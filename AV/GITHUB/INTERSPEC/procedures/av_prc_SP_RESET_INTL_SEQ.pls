     FROM DUAL;

   WHILE L_MAX_SEQ < L_MAX_ID
   LOOP
      SELECT PROPERTY_SEQ_INTL.NEXTVAL
        INTO L_MAX_SEQ
        FROM DUAL;
   END LOOP;

   SELECT MAX( SECTION_ID )
     INTO L_MAX_ID
     FROM SECTION;

   SELECT SECTION_SEQ_INTL.NEXTVAL
     INTO L_MAX_SEQ
     FROM DUAL;

   WHILE L_MAX_SEQ < L_MAX_ID
   LOOP
      SELECT SECTION_SEQ_INTL.NEXTVAL
        INTO L_MAX_SEQ
        FROM DUAL;
   END LOOP;

   SELECT MAX( SUB_SECTION_ID )
     INTO L_MAX_ID
     FROM SUB_SECTION;

   SELECT SUB_SECTION_SEQ_INTL.NEXTVAL
     INTO L_MAX_SEQ
     FROM DUAL;

   WHILE L_MAX_SEQ < L_MAX_ID
   LOOP
      SELECT SUB_SECTION_SEQ_INTL.NEXTVAL
        INTO L_MAX_SEQ
        FROM DUAL;
   END LOOP;

   SELECT MAX( TEST_METHOD )
     INTO L_MAX_ID
