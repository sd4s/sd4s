     FROM DUAL;

   WHILE L_MAX_SEQ < L_MAX_ID
   LOOP
      SELECT TEST_METHOD_SEQ_INTL.NEXTVAL
        INTO L_MAX_SEQ
        FROM DUAL;
   END LOOP;

   SELECT MAX( UOM_ID )
     INTO L_MAX_ID
     FROM UOM;

   SELECT UOM_SEQ_INTL.NEXTVAL
     INTO L_MAX_SEQ
     FROM DUAL;

   WHILE L_MAX_SEQ < L_MAX_ID
   LOOP
      SELECT SECTION_SEQ_INTL.NEXTVAL
        INTO L_MAX_SEQ
        FROM DUAL;
   END LOOP;

   SELECT MAX( CID )
     INTO L_MAX_ID
     FROM ITINGCFG;

   SELECT INGCFG_SEQ_INTL.NEXTVAL
     INTO L_MAX_SEQ
     FROM DUAL;

   WHILE L_MAX_SEQ < L_MAX_ID
   LOOP
      SELECT INGCFG_SEQ_INTL.NEXTVAL
        INTO L_MAX_SEQ
        FROM DUAL;
   END LOOP;
END SP_RESET_INTL_SEQ;