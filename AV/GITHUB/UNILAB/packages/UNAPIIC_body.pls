PACKAGE BODY unapiic AS

L_SQLERRM         VARCHAR2(255);
L_SQL_STRING      VARCHAR2(2000);
L_WHERE_CLAUSE    VARCHAR2(1000);
L_EVENT_TP        UTEV.EV_TP%TYPE;
L_RET_CODE        NUMBER;
L_RESULT          NUMBER;
L_FETCHED_ROWS    NUMBER;
L_EV_SEQ_NR       NUMBER;
L_EV_DETAILS      VARCHAR2(255);
L_ERRM            VARCHAR2(255);
STPERROR          EXCEPTION;

P_SCII_CURSOR     INTEGER;

CURSOR P_IPIE_CURSOR(C_IP VARCHAR2, C_IP_VERSION VARCHAR2) IS
   SELECT A.*
   FROM UTIPIE A
   WHERE A.IP = C_IP
   AND A.VERSION = C_IP_VERSION
   ORDER BY A.SEQ;

CURSOR L_SEQ_EVENT_CURSOR IS
   SELECT SEQ_EVENT_NR.NEXTVAL FROM DUAL;

FUNCTION GETVERSION
   RETURN VARCHAR2
IS
BEGIN
   RETURN('06.07.00.00_00.13');
EXCEPTION
   WHEN OTHERS THEN
      RETURN (NULL);
END GETVERSION;

FUNCTION GETSCINFOCARD
(A_SC             OUT      UNAPIGEN.VC20_TABLE_TYPE,  
 A_IC             OUT      UNAPIGEN.VC20_TABLE_TYPE,  
 A_ICNODE         OUT      UNAPIGEN.LONG_TABLE_TYPE,  
 A_IP_VERSION     OUT      UNAPIGEN.VC20_TABLE_TYPE,  
 A_DESCRIPTION    OUT      UNAPIGEN.VC40_TABLE_TYPE,  
 A_WINSIZE_X      OUT      UNAPIGEN.NUM_TABLE_TYPE,   
 A_WINSIZE_Y      OUT      UNAPIGEN.NUM_TABLE_TYPE,   
 A_IS_PROTECTED   OUT      UNAPIGEN.CHAR1_TABLE_TYPE, 
 A_HIDDEN         OUT      UNAPIGEN.CHAR1_TABLE_TYPE, 
 A_MANUALLY_ADDED OUT      UNAPIGEN.CHAR1_TABLE_TYPE, 
 A_NEXT_II        OUT      UNAPIGEN.VC20_TABLE_TYPE,  
 A_IC_CLASS       OUT      UNAPIGEN.VC2_TABLE_TYPE,   
 A_LOG_HS         OUT      UNAPIGEN.CHAR1_TABLE_TYPE, 
 A_LOG_HS_DETAILS OUT      UNAPIGEN.CHAR1_TABLE_TYPE, 
 A_ALLOW_MODIFY   OUT      UNAPIGEN.CHAR1_TABLE_TYPE, 
 A_AR             OUT      UNAPIGEN.CHAR1_TABLE_TYPE, 
 A_ACTIVE         OUT      UNAPIGEN.CHAR1_TABLE_TYPE, 
 A_LC             OUT      UNAPIGEN.VC2_TABLE_TYPE,   
 A_LC_VERSION     OUT      UNAPIGEN.VC20_TABLE_TYPE,  
 A_SS             OUT      UNAPIGEN.VC2_TABLE_TYPE,   
 A_NR_OF_ROWS     IN OUT   NUMBER,                    
 A_WHERE_CLAUSE   IN       VARCHAR2)                  
RETURN NUMBER IS

L_SC             VARCHAR2(20);
L_IC             VARCHAR2(20);
L_ICNODE         NUMBER(9);
L_IP_VERSION     VARCHAR2(20);
L_DESCRIPTION    VARCHAR2(40);
L_WINSIZE_X      NUMBER(4);
L_WINSIZE_Y      NUMBER(4);
L_IS_PROTECTED   CHAR(1);
L_HIDDEN         CHAR(1);
L_MANUALLY_ADDED CHAR(1);
L_NEXT_II        VARCHAR2(20);
L_IC_CLASS       VARCHAR2(2);
L_LOG_HS         CHAR(1);
L_LOG_HS_DETAILS CHAR(1);
L_ALLOW_MODIFY   CHAR(1);
L_AR             CHAR(1);
L_ACTIVE         CHAR(1);
L_LC             VARCHAR2(2);
L_LC_VERSION     VARCHAR2(20);
L_SS             VARCHAR2(2);
L_IC_CURSOR      INTEGER;

BEGIN

   IF NVL(A_NR_OF_ROWS,0) = 0 THEN
      A_NR_OF_ROWS := UNAPIGEN.P_DEFAULT_CHUNK_SIZE;
   ELSIF A_NR_OF_ROWS < 0 OR A_NR_OF_ROWS > UNAPIGEN.P_MAX_CHUNK_SIZE THEN
      RETURN(UNAPIGEN.DBERR_NROFROWS);
   END IF;

   IF NVL(A_WHERE_CLAUSE, ' ') = ' ' THEN
       RETURN(UNAPIGEN.DBERR_WHERECLAUSE);
   ELSIF
     UPPER(SUBSTR(A_WHERE_CLAUSE,1,6)) <> 'WHERE ' THEN
     L_WHERE_CLAUSE := 'WHERE sc = ''' || REPLACE(A_WHERE_CLAUSE, '''', '''''') || 
                       ''' ORDER BY icnode';
   ELSE
      L_WHERE_CLAUSE := A_WHERE_CLAUSE; 
   END IF;

   L_IC_CURSOR := DBMS_SQL.OPEN_CURSOR;
   L_SQL_STRING := 'SELECT sc, ic, icnode, ip_version, description, winsize_x, winsize_y,'||
                   'is_protected, hidden, manually_added, next_ii, ic_class,' ||
                   'log_hs, log_hs_details, allow_modify, ar, active, lc, lc_version, ss ' ||
                   'FROM dd' || UNAPIGEN.P_DD || '.uvscic ' || L_WHERE_CLAUSE;

   DBMS_SQL.PARSE(L_IC_CURSOR, L_SQL_STRING, DBMS_SQL.V7); 

   DBMS_SQL.DEFINE_COLUMN(L_IC_CURSOR, 1, L_SC, 20);
   DBMS_SQL.DEFINE_COLUMN(L_IC_CURSOR, 2, L_IC, 20);
   DBMS_SQL.DEFINE_COLUMN(L_IC_CURSOR, 3, L_ICNODE);
   DBMS_SQL.DEFINE_COLUMN(L_IC_CURSOR, 4, L_IP_VERSION, 20);
   DBMS_SQL.DEFINE_COLUMN(L_IC_CURSOR, 5, L_DESCRIPTION, 40);
   DBMS_SQL.DEFINE_COLUMN(L_IC_CURSOR, 6, L_WINSIZE_X);
   DBMS_SQL.DEFINE_COLUMN(L_IC_CURSOR, 7, L_WINSIZE_Y);
   DBMS_SQL.DEFINE_COLUMN_CHAR(L_IC_CURSOR, 8, L_IS_PROTECTED, 1);
   DBMS_SQL.DEFINE_COLUMN_CHAR(L_IC_CURSOR, 9, L_HIDDEN, 1);
   DBMS_SQL.DEFINE_COLUMN_CHAR(L_IC_CURSOR, 10, L_MANUALLY_ADDED, 1);
   DBMS_SQL.DEFINE_COLUMN(L_IC_CURSOR, 11, L_NEXT_II, 20);
   DBMS_SQL.DEFINE_COLUMN(L_IC_CURSOR, 12, L_IC_CLASS, 2);
   DBMS_SQL.DEFINE_COLUMN_CHAR(L_IC_CURSOR, 13, L_LOG_HS, 1);
   DBMS_SQL.DEFINE_COLUMN_CHAR(L_IC_CURSOR, 14, L_LOG_HS_DETAILS, 1);
   DBMS_SQL.DEFINE_COLUMN_CHAR(L_IC_CURSOR, 15, L_ALLOW_MODIFY, 1);
   DBMS_SQL.DEFINE_COLUMN_CHAR(L_IC_CURSOR, 16, L_AR, 1);
   DBMS_SQL.DEFINE_COLUMN_CHAR(L_IC_CURSOR, 17, L_ACTIVE, 1);
   DBMS_SQL.DEFINE_COLUMN(L_IC_CURSOR, 18, L_LC, 2);
   DBMS_SQL.DEFINE_COLUMN(L_IC_CURSOR, 19, L_LC_VERSION, 20);
   DBMS_SQL.DEFINE_COLUMN(L_IC_CURSOR, 20, L_SS, 2);
   L_RESULT := DBMS_SQL.EXECUTE_AND_FETCH(L_IC_CURSOR);
   L_FETCHED_ROWS := 0;

   LOOP

      EXIT WHEN L_RESULT = 0 OR L_FETCHED_ROWS >= A_NR_OF_ROWS;

      DBMS_SQL.COLUMN_VALUE(L_IC_CURSOR, 1, L_SC);
      DBMS_SQL.COLUMN_VALUE(L_IC_CURSOR, 2, L_IC);
      DBMS_SQL.COLUMN_VALUE(L_IC_CURSOR, 3, L_ICNODE);
      DBMS_SQL.COLUMN_VALUE(L_IC_CURSOR, 4, L_IP_VERSION);
      DBMS_SQL.COLUMN_VALUE(L_IC_CURSOR, 5, L_DESCRIPTION);
      DBMS_SQL.COLUMN_VALUE(L_IC_CURSOR, 6, L_WINSIZE_X);
      DBMS_SQL.COLUMN_VALUE(L_IC_CURSOR, 7, L_WINSIZE_Y);
      DBMS_SQL.COLUMN_VALUE_CHAR(L_IC_CURSOR, 8, L_IS_PROTECTED);
      DBMS_SQL.COLUMN_VALUE_CHAR(L_IC_CURSOR, 9, L_HIDDEN);
      DBMS_SQL.COLUMN_VALUE_CHAR(L_IC_CURSOR, 10, L_MANUALLY_ADDED);
      DBMS_SQL.COLUMN_VALUE(L_IC_CURSOR, 11, L_NEXT_II);
      DBMS_SQL.COLUMN_VALUE(L_IC_CURSOR, 12, L_IC_CLASS);
      DBMS_SQL.COLUMN_VALUE_CHAR(L_IC_CURSOR, 13, L_LOG_HS);
      DBMS_SQL.COLUMN_VALUE_CHAR(L_IC_CURSOR, 14, L_LOG_HS_DETAILS);
      DBMS_SQL.COLUMN_VALUE_CHAR(L_IC_CURSOR, 15, L_ALLOW_MODIFY);
      DBMS_SQL.COLUMN_VALUE_CHAR(L_IC_CURSOR, 16, L_AR);
      DBMS_SQL.COLUMN_VALUE_CHAR(L_IC_CURSOR, 17, L_ACTIVE);
      DBMS_SQL.COLUMN_VALUE(L_IC_CURSOR, 18, L_LC);
      DBMS_SQL.COLUMN_VALUE(L_IC_CURSOR, 19, L_LC_VERSION);
      DBMS_SQL.COLUMN_VALUE(L_IC_CURSOR, 20, L_SS);

      L_FETCHED_ROWS := L_FETCHED_ROWS + 1;
      A_SC(L_FETCHED_ROWS) := L_SC;
      A_IC(L_FETCHED_ROWS) := L_IC;
      A_ICNODE(L_FETCHED_ROWS) := L_ICNODE;
      A_IP_VERSION(L_FETCHED_ROWS) := L_IP_VERSION;
      A_DESCRIPTION(L_FETCHED_ROWS) := L_DESCRIPTION;
      A_WINSIZE_X(L_FETCHED_ROWS) := L_WINSIZE_X;
      A_WINSIZE_Y(L_FETCHED_ROWS) := L_WINSIZE_Y;
      A_IS_PROTECTED(L_FETCHED_ROWS) := L_IS_PROTECTED;
      A_HIDDEN(L_FETCHED_ROWS) := L_HIDDEN;
      A_MANUALLY_ADDED(L_FETCHED_ROWS) := L_MANUALLY_ADDED;
      A_NEXT_II(L_FETCHED_ROWS) := L_NEXT_II;
      A_IC_CLASS(L_FETCHED_ROWS) := L_IC_CLASS;
      A_LOG_HS(L_FETCHED_ROWS) := L_LOG_HS;
      A_LOG_HS_DETAILS(L_FETCHED_ROWS) := L_LOG_HS_DETAILS;
      A_ALLOW_MODIFY(L_FETCHED_ROWS) := L_ALLOW_MODIFY;
      A_AR(L_FETCHED_ROWS) := L_AR;
      A_ACTIVE(L_FETCHED_ROWS) := L_ACTIVE;
      A_LC(L_FETCHED_ROWS) := L_LC;
      A_LC_VERSION(L_FETCHED_ROWS) := L_LC_VERSION;
      A_SS(L_FETCHED_ROWS) := L_SS;

      IF L_FETCHED_ROWS < A_NR_OF_ROWS THEN
         L_RESULT := DBMS_SQL.FETCH_ROWS(L_IC_CURSOR);
      END IF;

   END LOOP;

   DBMS_SQL.CLOSE_CURSOR(L_IC_CURSOR);

   IF L_FETCHED_ROWS = 0 THEN
      L_RET_CODE := UNAPIGEN.DBERR_NORECORDS;
   ELSE
      A_NR_OF_ROWS := L_FETCHED_ROWS;
      L_RET_CODE := UNAPIGEN.DBERR_SUCCESS;
   END IF;

   RETURN(L_RET_CODE);

EXCEPTION
   WHEN OTHERS THEN
      L_SQLERRM := SQLERRM;
      UNAPIGEN.U4ROLLBACK;
      INSERT INTO UTERROR(CLIENT_ID, APPLIC, WHO, LOGDATE, LOGDATE_TZ, API_NAME, ERROR_MSG)
      VALUES(UNAPIGEN.P_CLIENT_ID, UNAPIGEN.P_APPLIC_NAME, UNAPIGEN.P_USER, CURRENT_TIMESTAMP, CURRENT_TIMESTAMP,
             'GetScInfoCard', L_SQLERRM);
      UNAPIGEN.U4COMMIT;
      IF DBMS_SQL.IS_OPEN(L_IC_CURSOR) THEN
         DBMS_SQL.CLOSE_CURSOR(L_IC_CURSOR);
      END IF;
      RETURN(UNAPIGEN.DBERR_GENFAIL);
END GETSCINFOCARD;

FUNCTION GETSCINFOFIELD
(A_SC               OUT    UNAPIGEN.VC20_TABLE_TYPE,    
 A_IC               OUT    UNAPIGEN.VC20_TABLE_TYPE,    
 A_ICNODE           OUT    UNAPIGEN.LONG_TABLE_TYPE,    
 A_II               OUT    UNAPIGEN.VC20_TABLE_TYPE,    
 A_IINODE           OUT    UNAPIGEN.LONG_TABLE_TYPE,    
 A_IE_VERSION       OUT    UNAPIGEN.VC20_TABLE_TYPE,    
 A_IIVALUE          OUT    UNAPIGEN.VC2000_TABLE_TYPE,  
 A_POS_X            OUT    UNAPIGEN.NUM_TABLE_TYPE,     
 A_POS_Y            OUT    UNAPIGEN.NUM_TABLE_TYPE,     
 A_IS_PROTECTED     OUT    UNAPIGEN.CHAR1_TABLE_TYPE,   
 A_MANDATORY        OUT    UNAPIGEN.CHAR1_TABLE_TYPE,   
 A_HIDDEN           OUT    UNAPIGEN.CHAR1_TABLE_TYPE,   
 A_DSP_TITLE        OUT    UNAPIGEN.VC40_TABLE_TYPE,    
 A_DSP_LEN          OUT    UNAPIGEN.NUM_TABLE_TYPE,     
 A_DSP_TP           OUT    UNAPIGEN.CHAR1_TABLE_TYPE,   
 A_DSP_ROWS         OUT    UNAPIGEN.NUM_TABLE_TYPE,     
 A_II_CLASS         OUT    UNAPIGEN.VC2_TABLE_TYPE,     
 A_LOG_HS           OUT    UNAPIGEN.CHAR1_TABLE_TYPE,   
 A_LOG_HS_DETAILS   OUT    UNAPIGEN.CHAR1_TABLE_TYPE,   
 A_ALLOW_MODIFY     OUT    UNAPIGEN.CHAR1_TABLE_TYPE,   
 A_AR               OUT    UNAPIGEN.CHAR1_TABLE_TYPE,   
 A_ACTIVE           OUT    UNAPIGEN.CHAR1_TABLE_TYPE,   
 A_LC               OUT    UNAPIGEN.VC2_TABLE_TYPE,     
 A_LC_VERSION       OUT    UNAPIGEN.VC20_TABLE_TYPE,    
 A_SS               OUT    UNAPIGEN.VC2_TABLE_TYPE,     
 A_NR_OF_ROWS       IN OUT NUMBER,                      
 A_WHERE_CLAUSE     IN     VARCHAR2,                    
 A_NEXT_ROWS        IN     NUMBER)                      
RETURN NUMBER IS

L_SC             VARCHAR2(20);
L_IC             VARCHAR2(20);
L_ICNODE         NUMBER(9);
L_II             VARCHAR2(20);
L_IINODE         NUMBER(9);
L_IE_VERSION     VARCHAR2(20);
L_IIVALUE        VARCHAR2(2000);
L_POS_X          NUMBER(4);
L_POS_Y          NUMBER(4);
L_IS_PROTECTED   CHAR(1);
L_MANDATORY      CHAR(1);
L_HIDDEN         CHAR(1);
L_DSP_TITLE      VARCHAR2(40);
L_DSP_TP         CHAR(1);
L_DSP_LEN        NUMBER(4);
L_DSP_ROWS       NUMBER(3);
L_II_CLASS       VARCHAR2(2);
L_LOG_HS         CHAR(1);
L_LOG_HS_DETAILS CHAR(1);
L_ALLOW_MODIFY   CHAR(1);
L_AR             CHAR(1);
L_ACTIVE         CHAR(1);
L_LC             VARCHAR2(2);
L_LC_VERSION     VARCHAR2(20);
L_SS             VARCHAR2(2);

BEGIN

   IF NVL(A_NR_OF_ROWS,0) = 0 THEN
      A_NR_OF_ROWS := UNAPIGEN.P_DEFAULT_CHUNK_SIZE;
   ELSIF A_NR_OF_ROWS < 0 OR A_NR_OF_ROWS > UNAPIGEN.P_MAX_CHUNK_SIZE THEN
      RETURN(UNAPIGEN.DBERR_NROFROWS);
   END IF;

   IF NVL(A_NEXT_ROWS, 0) NOT IN (-1, 0, 1) THEN
      RETURN(UNAPIGEN.DBERR_NEXTROWS);
   END IF;

   
   IF A_NEXT_ROWS = -1 THEN
      IF P_SCII_CURSOR IS NOT NULL THEN
         DBMS_SQL.CLOSE_CURSOR(P_SCII_CURSOR);
         P_SCII_CURSOR := NULL;
      END IF;
      RETURN (UNAPIGEN.DBERR_SUCCESS);
   END IF;

   
   IF A_NEXT_ROWS = 1 THEN
      IF P_SCII_CURSOR IS NULL THEN
         RETURN(UNAPIGEN.DBERR_NOCURSOR);
      END IF;
   END IF;

   
   IF NVL(A_NEXT_ROWS,0) = 0 THEN

      IF NVL(A_WHERE_CLAUSE, ' ') = ' ' THEN
          RETURN(UNAPIGEN.DBERR_WHERECLAUSE);
      ELSIF
        UPPER(SUBSTR(A_WHERE_CLAUSE,1,6)) <> 'WHERE ' THEN
        L_WHERE_CLAUSE := 'WHERE sc = ''' || REPLACE(A_WHERE_CLAUSE, '''', '''''') || 
                          ''' ORDER BY icnode, iinode';
      ELSE
         L_WHERE_CLAUSE := A_WHERE_CLAUSE; 
      END IF;

      IF P_SCII_CURSOR IS NULL THEN
         P_SCII_CURSOR := DBMS_SQL.OPEN_CURSOR;
      END IF;

      L_SQL_STRING := 'SELECT sc, ic, icnode, ii, iinode, ie_version, iivalue, pos_x, pos_y, ' ||
                      'is_protected, mandatory, hidden, dsp_title, dsp_len, ' ||
                      'dsp_tp, dsp_rows, ii_class, log_hs, log_hs_details, ' ||
                      'allow_modify, ar, active, lc, lc_version, ss ' ||
                      'FROM dd' || UNAPIGEN.P_DD || '.uvscii ' || L_WHERE_CLAUSE;

      DBMS_SQL.PARSE(P_SCII_CURSOR, L_SQL_STRING, DBMS_SQL.V7); 

      DBMS_SQL.DEFINE_COLUMN(P_SCII_CURSOR, 1, L_SC, 20);
      DBMS_SQL.DEFINE_COLUMN(P_SCII_CURSOR, 2, L_IC, 20);
      DBMS_SQL.DEFINE_COLUMN(P_SCII_CURSOR, 3, L_ICNODE);
      DBMS_SQL.DEFINE_COLUMN(P_SCII_CURSOR, 4, L_II, 20);
      DBMS_SQL.DEFINE_COLUMN(P_SCII_CURSOR, 5, L_IINODE);
      DBMS_SQL.DEFINE_COLUMN(P_SCII_CURSOR, 6, L_IE_VERSION, 20);
      DBMS_SQL.DEFINE_COLUMN(P_SCII_CURSOR, 7, L_IIVALUE, 2000);
      DBMS_SQL.DEFINE_COLUMN(P_SCII_CURSOR, 8, L_POS_X);
      DBMS_SQL.DEFINE_COLUMN(P_SCII_CURSOR, 9, L_POS_Y);
      DBMS_SQL.DEFINE_COLUMN_CHAR(P_SCII_CURSOR, 10, L_IS_PROTECTED, 1);
      DBMS_SQL.DEFINE_COLUMN_CHAR(P_SCII_CURSOR, 11, L_MANDATORY, 1);
      DBMS_SQL.DEFINE_COLUMN_CHAR(P_SCII_CURSOR, 12, L_HIDDEN, 1);
      DBMS_SQL.DEFINE_COLUMN(P_SCII_CURSOR, 13, L_DSP_TITLE, 40);
      DBMS_SQL.DEFINE_COLUMN(P_SCII_CURSOR, 14, L_DSP_LEN);
      DBMS_SQL.DEFINE_COLUMN_CHAR(P_SCII_CURSOR, 15, L_DSP_TP, 1);
      DBMS_SQL.DEFINE_COLUMN(P_SCII_CURSOR, 16, L_DSP_ROWS);
      DBMS_SQL.DEFINE_COLUMN(P_SCII_CURSOR, 17, L_II_CLASS, 2);
      DBMS_SQL.DEFINE_COLUMN_CHAR(P_SCII_CURSOR, 18, L_LOG_HS, 1);
      DBMS_SQL.DEFINE_COLUMN_CHAR(P_SCII_CURSOR, 19, L_LOG_HS_DETAILS, 1);
      DBMS_SQL.DEFINE_COLUMN_CHAR(P_SCII_CURSOR, 20, L_ALLOW_MODIFY, 1);
      DBMS_SQL.DEFINE_COLUMN_CHAR(P_SCII_CURSOR, 21, L_AR, 1);
      DBMS_SQL.DEFINE_COLUMN_CHAR(P_SCII_CURSOR, 22, L_ACTIVE, 1);
      DBMS_SQL.DEFINE_COLUMN(P_SCII_CURSOR, 23, L_LC, 2);
      DBMS_SQL.DEFINE_COLUMN(P_SCII_CURSOR, 24, L_LC_VERSION, 20);
      DBMS_SQL.DEFINE_COLUMN(P_SCII_CURSOR, 25, L_SS, 2);
      L_RESULT := DBMS_SQL.EXECUTE(P_SCII_CURSOR);
   END IF;
   
   L_RESULT := DBMS_SQL.FETCH_ROWS(P_SCII_CURSOR);
   L_FETCHED_ROWS := 0;

   LOOP
      EXIT WHEN L_RESULT = 0 OR L_FETCHED_ROWS >= A_NR_OF_ROWS;

      DBMS_SQL.COLUMN_VALUE(P_SCII_CURSOR, 1, L_SC);
      DBMS_SQL.COLUMN_VALUE(P_SCII_CURSOR, 2, L_IC);
      DBMS_SQL.COLUMN_VALUE(P_SCII_CURSOR, 3, L_ICNODE);
      DBMS_SQL.COLUMN_VALUE(P_SCII_CURSOR, 4, L_II);
      DBMS_SQL.COLUMN_VALUE(P_SCII_CURSOR, 5, L_IINODE);
      DBMS_SQL.COLUMN_VALUE(P_SCII_CURSOR, 6, L_IE_VERSION);
      DBMS_SQL.COLUMN_VALUE(P_SCII_CURSOR, 7, L_IIVALUE);
      DBMS_SQL.COLUMN_VALUE(P_SCII_CURSOR, 8, L_POS_X);
      DBMS_SQL.COLUMN_VALUE(P_SCII_CURSOR, 9, L_POS_Y);
      DBMS_SQL.COLUMN_VALUE_CHAR(P_SCII_CURSOR, 10, L_IS_PROTECTED);
      DBMS_SQL.COLUMN_VALUE_CHAR(P_SCII_CURSOR, 11, L_MANDATORY);
      DBMS_SQL.COLUMN_VALUE_CHAR(P_SCII_CURSOR, 12, L_HIDDEN);
      DBMS_SQL.COLUMN_VALUE(P_SCII_CURSOR, 13, L_DSP_TITLE);
      DBMS_SQL.COLUMN_VALUE(P_SCII_CURSOR, 14, L_DSP_LEN);
      DBMS_SQL.COLUMN_VALUE_CHAR(P_SCII_CURSOR, 15, L_DSP_TP);
      DBMS_SQL.COLUMN_VALUE(P_SCII_CURSOR, 16, L_DSP_ROWS);
      DBMS_SQL.COLUMN_VALUE(P_SCII_CURSOR, 17, L_II_CLASS);
      DBMS_SQL.COLUMN_VALUE_CHAR(P_SCII_CURSOR, 18, L_LOG_HS);
      DBMS_SQL.COLUMN_VALUE_CHAR(P_SCII_CURSOR, 19, L_LOG_HS_DETAILS);
      DBMS_SQL.COLUMN_VALUE_CHAR(P_SCII_CURSOR, 20, L_ALLOW_MODIFY);
      DBMS_SQL.COLUMN_VALUE_CHAR(P_SCII_CURSOR, 21, L_AR);
      DBMS_SQL.COLUMN_VALUE_CHAR(P_SCII_CURSOR, 22, L_ACTIVE);
      DBMS_SQL.COLUMN_VALUE(P_SCII_CURSOR, 23, L_LC);
      DBMS_SQL.COLUMN_VALUE(P_SCII_CURSOR, 24, L_LC_VERSION);
      DBMS_SQL.COLUMN_VALUE(P_SCII_CURSOR, 25, L_SS);

      L_FETCHED_ROWS := L_FETCHED_ROWS + 1;
      A_SC(L_FETCHED_ROWS) := L_SC;
      A_IC(L_FETCHED_ROWS) := L_IC;
      A_ICNODE(L_FETCHED_ROWS) := L_ICNODE;
      A_II(L_FETCHED_ROWS) := L_II;
      A_IINODE(L_FETCHED_ROWS) := L_IINODE;
      A_IE_VERSION(L_FETCHED_ROWS) := L_IE_VERSION;
      A_IIVALUE(L_FETCHED_ROWS) := L_IIVALUE;
      A_POS_X(L_FETCHED_ROWS) := L_POS_X;
      A_POS_Y(L_FETCHED_ROWS) := L_POS_Y;
      A_IS_PROTECTED(L_FETCHED_ROWS) := L_IS_PROTECTED;
      A_MANDATORY(L_FETCHED_ROWS) := L_MANDATORY;
      A_HIDDEN(L_FETCHED_ROWS) := L_HIDDEN;
      A_DSP_TITLE(L_FETCHED_ROWS) := L_DSP_TITLE;
      A_DSP_LEN(L_FETCHED_ROWS) := L_DSP_LEN;
      A_DSP_TP(L_FETCHED_ROWS) := L_DSP_TP;
      A_DSP_ROWS(L_FETCHED_ROWS) := L_DSP_ROWS;
      A_II_CLASS(L_FETCHED_ROWS) := L_II_CLASS;
      A_LOG_HS(L_FETCHED_ROWS) := L_LOG_HS;
      A_LOG_HS_DETAILS(L_FETCHED_ROWS) := L_LOG_HS_DETAILS;
      A_ALLOW_MODIFY(L_FETCHED_ROWS) := L_ALLOW_MODIFY;
      A_AR(L_FETCHED_ROWS) := L_AR;
      A_ACTIVE(L_FETCHED_ROWS) := L_ACTIVE;
      A_LC(L_FETCHED_ROWS) := L_LC;
      A_LC_VERSION(L_FETCHED_ROWS) := L_LC_VERSION;
      A_SS(L_FETCHED_ROWS) := L_SS;

      IF L_FETCHED_ROWS < A_NR_OF_ROWS THEN
         L_RESULT := DBMS_SQL.FETCH_ROWS(P_SCII_CURSOR);
      END IF;
   END LOOP;

   
   IF (L_FETCHED_ROWS = 0) THEN
       DBMS_SQL.CLOSE_CURSOR(P_SCII_CURSOR);
       P_SCII_CURSOR := NULL;
       RETURN(UNAPIGEN.DBERR_NORECORDS);
   ELSIF L_FETCHED_ROWS < A_NR_OF_ROWS THEN
      DBMS_SQL.CLOSE_CURSOR(P_SCII_CURSOR);
      P_SCII_CURSOR := NULL;
      A_NR_OF_ROWS := L_FETCHED_ROWS;
   END IF;
   RETURN(UNAPIGEN.DBERR_SUCCESS);

EXCEPTION
   WHEN OTHERS THEN
      L_SQLERRM := SQLERRM;
      UNAPIGEN.U4ROLLBACK;
      INSERT INTO UTERROR(CLIENT_ID, APPLIC, WHO, LOGDATE, LOGDATE_TZ, API_NAME, ERROR_MSG)
      VALUES(UNAPIGEN.P_CLIENT_ID, UNAPIGEN.P_APPLIC_NAME, UNAPIGEN.P_USER, CURRENT_TIMESTAMP, CURRENT_TIMESTAMP,
             'GetScInfoField', L_SQLERRM);
      UNAPIGEN.U4COMMIT;
      IF DBMS_SQL.IS_OPEN(P_SCII_CURSOR) THEN
         DBMS_SQL.CLOSE_CURSOR(P_SCII_CURSOR);
      END IF;
      RETURN(UNAPIGEN.DBERR_GENFAIL);
END GETSCINFOFIELD;

FUNCTION SAVESCINFOCARD
(A_SC             IN      UNAPIGEN.VC20_TABLE_TYPE,  
 A_IC             IN      UNAPIGEN.VC20_TABLE_TYPE,  
 A_ICNODE         IN OUT  UNAPIGEN.LONG_TABLE_TYPE,  
 A_IP_VERSION     IN      UNAPIGEN.VC20_TABLE_TYPE,  
 A_DESCRIPTION    IN      UNAPIGEN.VC40_TABLE_TYPE,  
 A_WINSIZE_X      IN      UNAPIGEN.NUM_TABLE_TYPE,   
 A_WINSIZE_Y      IN      UNAPIGEN.NUM_TABLE_TYPE,   
 A_IS_PROTECTED   IN      UNAPIGEN.CHAR1_TABLE_TYPE, 
 A_HIDDEN         IN      UNAPIGEN.CHAR1_TABLE_TYPE, 
 A_MANUALLY_ADDED IN      UNAPIGEN.CHAR1_TABLE_TYPE, 
 A_NEXT_II        IN      UNAPIGEN.VC20_TABLE_TYPE,  
 A_IC_CLASS       IN      UNAPIGEN.VC2_TABLE_TYPE,   
 A_LOG_HS         IN      UNAPIGEN.CHAR1_TABLE_TYPE, 
 A_LOG_HS_DETAILS IN      UNAPIGEN.CHAR1_TABLE_TYPE, 
 A_LC             IN      UNAPIGEN.VC2_TABLE_TYPE,   
 A_LC_VERSION     IN      UNAPIGEN.VC20_TABLE_TYPE,  
 A_MODIFY_FLAG    IN OUT  UNAPIGEN.NUM_TABLE_TYPE,   
 A_NR_OF_ROWS     IN      NUMBER,                    
 A_MODIFY_REASON  IN      VARCHAR2)                  
RETURN NUMBER IS

L_IP_VERSION             VARCHAR2(20);
L_LC                     VARCHAR2(2);
L_LC_VERSION             VARCHAR2(20);
L_SS                     VARCHAR2(2);
L_LOG_HS                 CHAR(1);
L_LOG_HS_DETAILS         CHAR(1);
L_ALLOW_MODIFY           CHAR(1);
L_ACTIVE                 CHAR(1);
L_INSERT                 BOOLEAN;
L_IC_RECORD_OK           BOOLEAN;
L_IC_HANDLED             BOOLEAN;
CURR_ITEM                INTEGER;
NEXT_ITEM                INTEGER;
PREV_NODE                NUMBER(9);
NEXT_NODE                NUMBER(9);
NODE_STEP                NUMBER(9);
L_SEQ_NO                 INTEGER;
NBR_OF_NODES_TO_CREATE   INTEGER;
L_COMPLETELY_SAVED       BOOLEAN;
L_DELETED_NODE           NUMBER(9);
L_CURRENT_TIMESTAMP                TIMESTAMP WITH TIME ZONE;
L_ST                     VARCHAR2(20);
L_ST_VERSION             VARCHAR2(20);
L_HS_DETAILS_SEQ_NR      INTEGER;

CURSOR L_SC_CURSOR(A_SC VARCHAR2) IS
   SELECT ST, ST_VERSION
   FROM UTSC 
   WHERE SC = A_SC;

CURSOR L_SCICOLD_CURSOR (A_SC IN VARCHAR2,
                         A_IC IN VARCHAR2, A_ICNODE IN NUMBER) IS
   SELECT A.*
   FROM UDSCIC A
   WHERE A.SC = A_SC
   AND A.IC = A_IC
   AND A.ICNODE = A_ICNODE;
L_SCICOLD_REC UDSCIC%ROWTYPE;
L_SCICNEW_REC UDSCIC%ROWTYPE;

BEGIN

   
   
   
   
   
   
   
   
   
   IF UNAPIGEN.BEGINTXN(UNAPIGEN.P_SINGLE_API_TXN) <>
      UNAPIGEN.DBERR_SUCCESS THEN
      RAISE STPERROR;
   END IF;

   L_CURRENT_TIMESTAMP := CURRENT_TIMESTAMP;
   L_HS_DETAILS_SEQ_NR := 0;
   
   
   
   CURR_ITEM := 1;
   WHILE CURR_ITEM <= A_NR_OF_ROWS LOOP
      IF A_MODIFY_FLAG(CURR_ITEM) IN (UNAPIGEN.MOD_FLAG_INSERT,
                                      UNAPIGEN.MOD_FLAG_INSERT_AND_CRAU,
                                      UNAPIGEN.MOD_FLAG_CREATE) THEN
         IF NVL(A_ICNODE(CURR_ITEM), 0) <> 0 THEN
            UNAPIGEN.P_TXN_ERROR := UNAPIGEN.DBERR_NEWNODENOTZERO;
            RAISE STPERROR;
         END IF;

         
         
         
         NEXT_ITEM := CURR_ITEM;
         NBR_OF_NODES_TO_CREATE := 0;
         WHILE (A_MODIFY_FLAG(NEXT_ITEM) IN (UNAPIGEN.MOD_FLAG_INSERT,
                                             UNAPIGEN.MOD_FLAG_INSERT_AND_CRAU,
                                             UNAPIGEN.MOD_FLAG_CREATE) AND
                NVL(A_SC(CURR_ITEM), ' ') = NVL(A_SC(NEXT_ITEM), ' ')) LOOP
            NBR_OF_NODES_TO_CREATE := NBR_OF_NODES_TO_CREATE + 1;
            IF NEXT_ITEM < A_NR_OF_ROWS THEN
               NEXT_ITEM := NEXT_ITEM + 1;
            ELSE
               EXIT;
            END IF;
         END LOOP;

         
         
         
         
         IF A_MODIFY_FLAG(NEXT_ITEM) NOT IN (UNAPIGEN.MOD_FLAG_INSERT,
                                             UNAPIGEN.MOD_FLAG_INSERT_AND_CRAU,
                                             UNAPIGEN.MOD_FLAG_CREATE) AND
                A_SC(CURR_ITEM) = A_SC(NEXT_ITEM) THEN
            NEXT_NODE := A_ICNODE(NEXT_ITEM);
            SELECT NVL(MAX(ICNODE), 0)
            INTO PREV_NODE
            FROM UTSCIC
            WHERE ICNODE < NEXT_NODE
              AND SC = A_SC(CURR_ITEM);
            NODE_STEP :=
               TRUNC(ABS((NEXT_NODE - PREV_NODE)) / (NBR_OF_NODES_TO_CREATE + 1));
            IF NODE_STEP < 1 THEN
               UNAPIGEN.P_TXN_ERROR := UNAPIGEN.DBERR_NODELIMITOVERF;
               RAISE STPERROR;
            END IF;
         ELSE
            
            SELECT NVL(MAX(ICNODE), 0)
            INTO PREV_NODE
            FROM UTSCIC
            WHERE SC = A_SC(CURR_ITEM);
            NODE_STEP := UNAPIGEN.DEFAULT_NODE_INTERVAL;
         END IF;

         
         
         
         FOR I IN 1..NBR_OF_NODES_TO_CREATE LOOP
            A_ICNODE(CURR_ITEM + I - 1) := PREV_NODE + (NODE_STEP * I);
         END LOOP;

         CURR_ITEM := CURR_ITEM + NBR_OF_NODES_TO_CREATE;
      ELSE
         CURR_ITEM := CURR_ITEM + 1;
      END IF;
   END LOOP;

   
   
   
   L_COMPLETELY_SAVED := TRUE;

   
   
   
   FOR L_SEQ_NO IN 1..A_NR_OF_ROWS LOOP
      L_IC_RECORD_OK := TRUE;          
      L_IC_HANDLED := TRUE;

      
      
      
      IF A_MODIFY_FLAG(L_SEQ_NO) IN (UNAPIGEN.MOD_FLAG_INSERT,
                                     UNAPIGEN.MOD_FLAG_INSERT_AND_CRAU,
                                     UNAPIGEN.MOD_FLAG_INSERT_WITH_NODES,
                                     UNAPIGEN.MOD_FLAG_INSERT_NODES_AND_CRAU,
                                     UNAPIGEN.MOD_FLAG_CREATE) THEN
         IF NVL(A_IP_VERSION(L_SEQ_NO), ' ') = ' ' THEN
            UNAPIGEN.P_TXN_ERROR := UNAPIGEN.DBERR_IPVERSION;
            RAISE STPERROR;
         END IF;
      END IF;

      IF NVL(A_SC(L_SEQ_NO), ' ') = ' ' OR
         NVL(A_IC(L_SEQ_NO), ' ') = ' ' THEN
         UNAPIGEN.P_TXN_ERROR := UNAPIGEN.DBERR_NOOBJID;
         RAISE STPERROR;

         
         
         
      ELSIF A_MODIFY_FLAG(L_SEQ_NO) IN (UNAPIGEN.MOD_FLAG_INSERT,
                                        UNAPIGEN.MOD_FLAG_INSERT_AND_CRAU,
                                        UNAPIGEN.MOD_FLAG_INSERT_WITH_NODES,
                                        UNAPIGEN.MOD_FLAG_INSERT_NODES_AND_CRAU,
                                        UNAPIGEN.MOD_FLAG_CREATE,                                        
                                        UNAPIGEN.MOD_FLAG_UPDATE,
                                        UNAPIGEN.MOD_FLAG_DELETE) THEN
         L_IP_VERSION := A_IP_VERSION(L_SEQ_NO);
         L_RET_CODE := UNAPIAUT.GETSCICAUTHORISATION(A_SC(L_SEQ_NO), A_IC(L_SEQ_NO), A_ICNODE(L_SEQ_NO), 
                                                     L_IP_VERSION, L_LC, L_LC_VERSION, L_SS, 
                                                     L_ALLOW_MODIFY, L_ACTIVE, L_LOG_HS, 
                                                     L_LOG_HS_DETAILS);

         IF L_RET_CODE = UNAPIGEN.DBERR_NOOBJECT THEN
            L_INSERT := TRUE;
         ELSIF L_RET_CODE = UNAPIGEN.DBERR_SUCCESS THEN
            L_INSERT := FALSE;
         ELSE
            A_MODIFY_FLAG(L_SEQ_NO) := L_RET_CODE;
            L_IC_RECORD_OK := FALSE;
         END IF;
      ELSIF A_MODIFY_FLAG(L_SEQ_NO) = UNAPIGEN.DBERR_SUCCESS THEN
         
         
         
         L_IC_RECORD_OK := FALSE;  
         L_IC_HANDLED := FALSE;    
      ELSE
         UNAPIGEN.P_TXN_ERROR := UNAPIGEN.DBERR_INVALMODFLAG;
         RAISE STPERROR;
      END IF;

      
      
      
      IF A_MODIFY_FLAG(L_SEQ_NO) IN (UNAPIGEN.MOD_FLAG_INSERT,
                                     UNAPIGEN.MOD_FLAG_INSERT_AND_CRAU,
                                     UNAPIGEN.MOD_FLAG_INSERT_WITH_NODES,
                                     UNAPIGEN.MOD_FLAG_INSERT_NODES_AND_CRAU,
                                     UNAPIGEN.MOD_FLAG_CREATE,
                                     UNAPIGEN.MOD_FLAG_UPDATE) THEN
         IF NVL(A_LOG_HS(L_SEQ_NO), ' ') NOT IN ('0', '1') THEN
            UNAPIGEN.P_TXN_ERROR := UNAPIGEN.DBERR_LOGHS;
            RAISE STPERROR;
         ELSIF NVL(A_LOG_HS_DETAILS(L_SEQ_NO), ' ') NOT IN ('0', '1') THEN
            UNAPIGEN.P_TXN_ERROR := UNAPIGEN.DBERR_LOGHSDETAILS;
            RAISE STPERROR;
         END IF;
      END IF;

      
      
      

      
      
      
      IF L_IC_RECORD_OK AND
         A_MODIFY_FLAG(L_SEQ_NO) = UNAPIGEN.MOD_FLAG_UPDATE THEN

         
         
         
         IF L_INSERT THEN
            
            A_MODIFY_FLAG(L_SEQ_NO) := UNAPIGEN.DBERR_NOOBJECT;
            L_IC_RECORD_OK := FALSE;
         ELSIF NVL(A_IS_PROTECTED(L_SEQ_NO), ' ') NOT IN ('0', '1') THEN
            UNAPIGEN.P_TXN_ERROR := UNAPIGEN.DBERR_PROTECTED;
            RAISE STPERROR;
         ELSIF NVL(A_HIDDEN(L_SEQ_NO), ' ') NOT IN ('0', '1') THEN
            UNAPIGEN.P_TXN_ERROR := UNAPIGEN.DBERR_HIDDEN;
            RAISE STPERROR;
         ELSIF NVL(A_MANUALLY_ADDED(L_SEQ_NO), ' ') NOT IN ('0', '1') THEN
            UNAPIGEN.P_TXN_ERROR := UNAPIGEN.DBERR_MANUALLYADDED;
            RAISE STPERROR;
         ELSE
            
            
            

            
            
            
            OPEN L_SCICOLD_CURSOR(A_SC(L_SEQ_NO),
                                  A_IC(L_SEQ_NO), A_ICNODE(L_SEQ_NO));
            FETCH L_SCICOLD_CURSOR
            INTO L_SCICOLD_REC;
            CLOSE L_SCICOLD_CURSOR;
            L_SCICNEW_REC := L_SCICOLD_REC;

            
            
            
            
            
            UPDATE UTSCIC
            SET DESCRIPTION    = A_DESCRIPTION(L_SEQ_NO),
                WINSIZE_X      = A_WINSIZE_X(L_SEQ_NO),
                WINSIZE_Y      = A_WINSIZE_Y(L_SEQ_NO),
                IS_PROTECTED   = A_IS_PROTECTED(L_SEQ_NO),
                HIDDEN         = A_HIDDEN(L_SEQ_NO),
                MANUALLY_ADDED = A_MANUALLY_ADDED(L_SEQ_NO),
                IC_CLASS       = A_IC_CLASS(L_SEQ_NO),
                LOG_HS         = A_LOG_HS(L_SEQ_NO),
                LOG_HS_DETAILS = A_LOG_HS_DETAILS(L_SEQ_NO),
                ALLOW_MODIFY   = '#'
            WHERE SC = A_SC(L_SEQ_NO)
              AND IC = A_IC(L_SEQ_NO)
              AND ICNODE = A_ICNODE(L_SEQ_NO)
            RETURNING DESCRIPTION, WINSIZE_X, WINSIZE_Y, IS_PROTECTED, HIDDEN, MANUALLY_ADDED, 
                      IC_CLASS, LOG_HS, LOG_HS_DETAILS, ALLOW_MODIFY
            INTO L_SCICNEW_REC.DESCRIPTION, L_SCICNEW_REC.WINSIZE_X, L_SCICNEW_REC.WINSIZE_Y, 
                 L_SCICNEW_REC.IS_PROTECTED, L_SCICNEW_REC.HIDDEN, L_SCICNEW_REC.MANUALLY_ADDED, 
                 L_SCICNEW_REC.IC_CLASS, L_SCICNEW_REC.LOG_HS, L_SCICNEW_REC.LOG_HS_DETAILS, 
                 L_SCICNEW_REC.ALLOW_MODIFY;

              
              
              
              L_EVENT_TP := 'InfoCardUpdated';
         END IF;

      
      
      
      ELSIF L_IC_RECORD_OK AND
            A_MODIFY_FLAG(L_SEQ_NO) IN (UNAPIGEN.MOD_FLAG_INSERT,
                                        UNAPIGEN.MOD_FLAG_INSERT_AND_CRAU,
                                        UNAPIGEN.MOD_FLAG_INSERT_WITH_NODES,
                                        UNAPIGEN.MOD_FLAG_INSERT_NODES_AND_CRAU,
                                        UNAPIGEN.MOD_FLAG_CREATE) THEN

         
         
         
         IF NOT L_INSERT THEN
            UNAPIGEN.P_TXN_ERROR := UNAPIGEN.DBERR_ICALREADYEXIST;
            RAISE STPERROR;
         ELSIF NVL(A_IS_PROTECTED(L_SEQ_NO), ' ') NOT IN ('0', '1') THEN
            UNAPIGEN.P_TXN_ERROR := UNAPIGEN.DBERR_PROTECTED;
            RAISE STPERROR;
         ELSIF NVL(A_HIDDEN(L_SEQ_NO), ' ') NOT IN ('0', '1') THEN
            UNAPIGEN.P_TXN_ERROR := UNAPIGEN.DBERR_HIDDEN;
            RAISE STPERROR;
         ELSIF NVL(A_MANUALLY_ADDED(L_SEQ_NO), ' ') NOT IN ('0', '1') THEN
            UNAPIGEN.P_TXN_ERROR := UNAPIGEN.DBERR_MANUALLYADDED;
            RAISE STPERROR;
         ELSE
            
            
            
            

            
            IF NVL(A_LC(L_SEQ_NO), ' ') <> ' ' THEN
               L_LC := A_LC(L_SEQ_NO);
            END IF;
            IF NVL(A_LC_VERSION(L_SEQ_NO), ' ') <> ' ' THEN
               L_LC_VERSION := A_LC_VERSION(L_SEQ_NO);
            END IF;

            INSERT INTO UTSCIC(SC, IC, ICNODE, IP_VERSION, DESCRIPTION, WINSIZE_X,
                               WINSIZE_Y, IS_PROTECTED, HIDDEN,
                               MANUALLY_ADDED, NEXT_II, IC_CLASS, LOG_HS, LOG_HS_DETAILS,
                               ALLOW_MODIFY, ACTIVE, LC, LC_VERSION)
            VALUES(A_SC(L_SEQ_NO), A_IC(L_SEQ_NO), A_ICNODE(L_SEQ_NO), L_IP_VERSION,
                   A_DESCRIPTION(L_SEQ_NO), A_WINSIZE_X(L_SEQ_NO),
                   A_WINSIZE_Y(L_SEQ_NO), A_IS_PROTECTED(L_SEQ_NO),
                   A_HIDDEN(L_SEQ_NO), A_MANUALLY_ADDED(L_SEQ_NO), NULL,
                   A_IC_CLASS(L_SEQ_NO), A_LOG_HS(L_SEQ_NO), A_LOG_HS_DETAILS(L_SEQ_NO),
                   '#', '0', L_LC, L_LC_VERSION);
            UNAPIAUT.UPDATELCINAUTHORISATIONBUFFER('ic', A_SC(L_SEQ_NO) || A_IC(L_SEQ_NO) || TO_CHAR(A_ICNODE(L_SEQ_NO)),
                                                           '', L_LC, L_LC_VERSION);                 
            
            
            
            L_EVENT_TP := 'InfoCardCreated';
         END IF;

      
      
      
      ELSIF L_IC_RECORD_OK AND
            A_MODIFY_FLAG(L_SEQ_NO) = UNAPIGEN.MOD_FLAG_DELETE THEN

         IF L_INSERT THEN
            
            UNAPIGEN.P_TXN_ERROR := UNAPIGEN.DBERR_NOOBJECT;
            RAISE STPERROR;

         
         
         
         ELSIF L_ACTIVE = '1' THEN
            A_MODIFY_FLAG(L_SEQ_NO) := UNAPIGEN.DBERR_OPACTIVE;
            L_IC_RECORD_OK := FALSE;
            L_COMPLETELY_SAVED := FALSE;
         ELSE
            
            IF UNAPIGEN.ISSYSTEM21CFR11COMPLIANT = UNAPIGEN.DBERR_SUCCESS THEN
               UNAPIGEN.P_TXN_ERROR := UNAPIGEN.DBERR_NOTALLOWEDIN21CFR11;
               RAISE STPERROR;
            END IF;

            
            
            
            DELETE FROM UTSCII
            WHERE SC     = A_SC(L_SEQ_NO)
              AND IC     = A_IC(L_SEQ_NO)
              AND ICNODE = A_ICNODE(L_SEQ_NO);

            
            
            
            DELETE FROM UTSCICAU
            WHERE SC     = A_SC(L_SEQ_NO)
              AND IC     = A_IC(L_SEQ_NO)
              AND ICNODE = A_ICNODE(L_SEQ_NO);

            DELETE FROM UTSCICHS
            WHERE SC     = A_SC(L_SEQ_NO)
              AND IC     = A_IC(L_SEQ_NO)
              AND ICNODE = A_ICNODE(L_SEQ_NO);

            DELETE FROM UTSCICHSDETAILS
            WHERE SC     = A_SC(L_SEQ_NO)
              AND IC     = A_IC(L_SEQ_NO)
              AND ICNODE = A_ICNODE(L_SEQ_NO);

            DELETE FROM UTSCIC
            WHERE SC     = A_SC(L_SEQ_NO)
              AND IC     = A_IC(L_SEQ_NO)
              AND ICNODE = A_ICNODE(L_SEQ_NO);

            
            
            
            DELETE FROM UTEVTIMED
            WHERE OBJECT_TP = 'ic'
              AND OBJECT_ID = A_IC(L_SEQ_NO)
              AND INSTR(EV_DETAILS, 'sc=' || A_SC(L_SEQ_NO)) <> 0
              AND INSTR(EV_DETAILS, 'icnode=' || TO_CHAR(A_ICNODE(L_SEQ_NO))) <> 0;

            DELETE FROM UTEVTIMED
            WHERE INSTR(EV_DETAILS, 'sc=' || A_SC(L_SEQ_NO)) <> 0
              AND INSTR(EV_DETAILS, 'ic=' || A_IC(L_SEQ_NO)) <> 0
              AND INSTR(EV_DETAILS, 'icnode=' || TO_CHAR(A_ICNODE(L_SEQ_NO))) <> 0;

            DELETE FROM UTEVRULESDELAYED
            WHERE OBJECT_TP = 'ic'
              AND OBJECT_ID = A_IC(L_SEQ_NO)
              AND INSTR(EV_DETAILS, 'sc=' || A_SC(L_SEQ_NO)) <> 0
              AND INSTR(EV_DETAILS, 'icnode=' || TO_CHAR(A_ICNODE(L_SEQ_NO))) <> 0;

            DELETE FROM UTEVRULESDELAYED
            WHERE INSTR(EV_DETAILS, 'sc=' || A_SC(L_SEQ_NO)) <> 0
              AND INSTR(EV_DETAILS, 'ic=' || A_IC(L_SEQ_NO)) <> 0
              AND INSTR(EV_DETAILS, 'icnode=' || TO_CHAR(A_ICNODE(L_SEQ_NO))) <> 0;

            L_DELETED_NODE := A_ICNODE(L_SEQ_NO);
            A_ICNODE(L_SEQ_NO) := 0;  

            
            
            
            L_EVENT_TP := 'InfoCardDeleted';
         END IF;
      END IF;

      
      
      
      IF L_IC_RECORD_OK THEN
         L_EV_SEQ_NR := -1;
         IF A_MODIFY_FLAG(L_SEQ_NO) <> UNAPIGEN.MOD_FLAG_DELETE THEN
            L_EV_DETAILS := 'sc=' || A_SC(L_SEQ_NO) || 
                            '#icnode=' || TO_CHAR(A_ICNODE(L_SEQ_NO)) || 
                            '#ip_version=' || L_IP_VERSION;
         ELSE
            L_EV_DETAILS := 'sc=' || A_SC(L_SEQ_NO) || 
                            '#icnode=' || TO_CHAR(L_DELETED_NODE) || 
                            '#ip_version=' || L_IP_VERSION;
         END IF;
         L_RESULT := UNAPIEV.INSERTEVENT('SaveScInfoCard', UNAPIGEN.P_EVMGR_NAME, 'ic', A_IC(L_SEQ_NO), 
                                        L_LC, L_LC_VERSION, L_SS, L_EVENT_TP, L_EV_DETAILS, L_EV_SEQ_NR);
         IF L_RESULT <> UNAPIGEN.DBERR_SUCCESS THEN
            UNAPIGEN.P_TXN_ERROR := L_RESULT;
            RAISE STPERROR;
         END IF;
      ELSE
         IF L_IC_HANDLED THEN
            L_COMPLETELY_SAVED := FALSE;
         END IF;
      END IF;

      
      
      
      IF L_IC_RECORD_OK AND L_LOG_HS <> A_LOG_HS(L_SEQ_NO) AND
         A_MODIFY_FLAG(L_SEQ_NO) <> UNAPIGEN.MOD_FLAG_DELETE THEN
         IF A_LOG_HS(L_SEQ_NO) = '1' THEN
            INSERT INTO UTSCICHS(SC, IC, ICNODE, WHO, WHO_DESCRIPTION, WHAT, 
                                 WHAT_DESCRIPTION, LOGDATE, LOGDATE_TZ, WHY, TR_SEQ, EV_SEQ)
            VALUES(A_SC(L_SEQ_NO), A_IC(L_SEQ_NO), A_ICNODE(L_SEQ_NO), UNAPIGEN.P_USER, 
                   UNAPIGEN.P_USER_DESCRIPTION, 'History switched ON', 
                   'Audit trail is turned on.',
                   L_CURRENT_TIMESTAMP, L_CURRENT_TIMESTAMP, A_MODIFY_REASON, UNAPIGEN.P_TR_SEQ, L_EV_SEQ_NR);
         ELSE
            INSERT INTO UTSCICHS(SC, IC, ICNODE, WHO, WHO_DESCRIPTION, WHAT, 
                                 WHAT_DESCRIPTION, LOGDATE, LOGDATE_TZ, WHY, TR_SEQ, EV_SEQ)
            VALUES(A_SC(L_SEQ_NO), A_IC(L_SEQ_NO), A_ICNODE(L_SEQ_NO), UNAPIGEN.P_USER,
                   UNAPIGEN.P_USER_DESCRIPTION, 'History switched OFF', 
                   'Audit trail is turned off.', 
                   L_CURRENT_TIMESTAMP, L_CURRENT_TIMESTAMP, A_MODIFY_REASON, UNAPIGEN.P_TR_SEQ, L_EV_SEQ_NR);
         END IF;
      END IF;

      
      
      
      IF L_IC_RECORD_OK AND L_LOG_HS_DETAILS <> A_LOG_HS_DETAILS(L_SEQ_NO) AND
         A_MODIFY_FLAG(L_SEQ_NO) <> UNAPIGEN.MOD_FLAG_DELETE THEN
         IF A_LOG_HS_DETAILS(L_SEQ_NO) = '1' THEN
            L_HS_DETAILS_SEQ_NR := L_HS_DETAILS_SEQ_NR + 1;
            INSERT INTO UTSCICHSDETAILS(SC, IC, ICNODE, TR_SEQ, EV_SEQ, SEQ, DETAILS)
            VALUES(A_SC(L_SEQ_NO), A_IC(L_SEQ_NO), A_ICNODE(L_SEQ_NO), UNAPIGEN.P_TR_SEQ, L_EV_SEQ_NR, 
                   L_HS_DETAILS_SEQ_NR, 'Audit trail is turned on.');
         ELSE
            L_HS_DETAILS_SEQ_NR := L_HS_DETAILS_SEQ_NR + 1;
            INSERT INTO UTSCICHSDETAILS(SC, IC, ICNODE, TR_SEQ, EV_SEQ, SEQ, DETAILS)
            VALUES(A_SC(L_SEQ_NO), A_IC(L_SEQ_NO), A_ICNODE(L_SEQ_NO), UNAPIGEN.P_TR_SEQ, L_EV_SEQ_NR, 
                   L_HS_DETAILS_SEQ_NR, 'Audit trail is turned off.');
         END IF;
      END IF;

      
      L_RET_CODE := UNAPIGEN.GETNEXTEVENTSEQNR(L_EV_SEQ_NR);

      IF L_IC_RECORD_OK AND (L_LOG_HS = '1' OR A_LOG_HS(L_SEQ_NO)='1') THEN
         
         
         
         IF L_EVENT_TP = 'InfoCardCreated' THEN
            INSERT INTO UTSCICHS(SC, IC, ICNODE, WHO, WHO_DESCRIPTION, WHAT, 
                                 WHAT_DESCRIPTION, LOGDATE, LOGDATE_TZ, WHY, TR_SEQ, EV_SEQ)
            VALUES(A_SC(L_SEQ_NO), A_IC(L_SEQ_NO), A_ICNODE(L_SEQ_NO), UNAPIGEN.P_USER,
                   UNAPIGEN.P_USER_DESCRIPTION, L_EVENT_TP, 
                   'info card "'||A_IC(L_SEQ_NO)||'" is created.', 
                   L_CURRENT_TIMESTAMP, L_CURRENT_TIMESTAMP, A_MODIFY_REASON, UNAPIGEN.P_TR_SEQ, L_EV_SEQ_NR);
         ELSIF L_EVENT_TP = 'InfoCardUpdated' THEN
            INSERT INTO UTSCICHS(SC, IC, ICNODE, WHO, WHO_DESCRIPTION, WHAT, 
                                 WHAT_DESCRIPTION, LOGDATE, LOGDATE_TZ, WHY, TR_SEQ, EV_SEQ)
            VALUES(A_SC(L_SEQ_NO), A_IC(L_SEQ_NO), A_ICNODE(L_SEQ_NO), UNAPIGEN.P_USER,
                   UNAPIGEN.P_USER_DESCRIPTION, L_EVENT_TP, 
                   'info card "'||A_IC(L_SEQ_NO)||'" is updated.', 
                   L_CURRENT_TIMESTAMP, L_CURRENT_TIMESTAMP, A_MODIFY_REASON, UNAPIGEN.P_TR_SEQ, L_EV_SEQ_NR);
         ELSE
            INSERT INTO UTSCHS(SC, WHO, WHO_DESCRIPTION, WHAT, 
                               WHAT_DESCRIPTION, LOGDATE, LOGDATE_TZ, WHY, TR_SEQ, EV_SEQ)
            VALUES(A_SC(L_SEQ_NO), UNAPIGEN.P_USER, UNAPIGEN.P_USER_DESCRIPTION, L_EVENT_TP, 
                   'info card "'||A_IC(L_SEQ_NO)||'" is deleted.', 
                   L_CURRENT_TIMESTAMP, L_CURRENT_TIMESTAMP, A_MODIFY_REASON, UNAPIGEN.P_TR_SEQ, L_EV_SEQ_NR);
         END IF;
      END IF;

      IF L_IC_RECORD_OK AND (L_LOG_HS_DETAILS = '1' OR A_LOG_HS_DETAILS(L_SEQ_NO)='1') THEN
         
         
         
         IF L_EVENT_TP = 'InfoCardCreated' THEN
            L_HS_DETAILS_SEQ_NR := L_HS_DETAILS_SEQ_NR + 1;
            INSERT INTO UTSCICHSDETAILS(SC, IC, ICNODE, TR_SEQ, EV_SEQ, SEQ, DETAILS)
            VALUES(A_SC(L_SEQ_NO), A_IC(L_SEQ_NO), A_ICNODE(L_SEQ_NO), UNAPIGEN.P_TR_SEQ, L_EV_SEQ_NR, 
                   L_HS_DETAILS_SEQ_NR, 'info card "'||A_IC(L_SEQ_NO)||'" is created.');
         ELSIF L_EVENT_TP = 'InfoCardUpdated' THEN
            L_HS_DETAILS_SEQ_NR := L_HS_DETAILS_SEQ_NR + 1;
            INSERT INTO UTSCICHSDETAILS(SC, IC, ICNODE, TR_SEQ, EV_SEQ, SEQ, DETAILS)
            VALUES(A_SC(L_SEQ_NO), A_IC(L_SEQ_NO), A_ICNODE(L_SEQ_NO), UNAPIGEN.P_TR_SEQ, L_EV_SEQ_NR, 
                   L_HS_DETAILS_SEQ_NR, 'info card "'||A_IC(L_SEQ_NO)||'" is updated.');

            UNAPIHSDETAILS.ADDSCICHSDETAILS(L_SCICOLD_REC, L_SCICNEW_REC, UNAPIGEN.P_TR_SEQ, 
                                            L_EV_SEQ_NR, L_HS_DETAILS_SEQ_NR); 
         ELSE
            L_HS_DETAILS_SEQ_NR := L_HS_DETAILS_SEQ_NR + 1;
            INSERT INTO UTSCHSDETAILS(SC, TR_SEQ, EV_SEQ, SEQ, DETAILS)
            VALUES(A_SC(L_SEQ_NO), UNAPIGEN.P_TR_SEQ, L_EV_SEQ_NR, L_HS_DETAILS_SEQ_NR, 
                   'info card "'||A_IC(L_SEQ_NO)||'" is deleted.');
         END IF;
      END IF;

      
      
      
      
      
      
      IF L_IC_RECORD_OK AND
         A_MODIFY_FLAG(L_SEQ_NO) IN (UNAPIGEN.MOD_FLAG_CREATE,
                                     UNAPIGEN.MOD_FLAG_INSERT_AND_CRAU,
                                     UNAPIGEN.MOD_FLAG_INSERT_NODES_AND_CRAU) THEN
         
         
         
         
         L_RET_CODE := UNAPIICP.INITANDSAVESCICATTRIBUTES(A_SC(L_SEQ_NO), 
                                                          A_IC(L_SEQ_NO), A_ICNODE(L_SEQ_NO));
         IF L_RET_CODE <> UNAPIGEN.DBERR_SUCCESS THEN
            UNAPIGEN.P_TXN_ERROR := L_RET_CODE;
            RAISE STPERROR;
         END IF;

         IF A_MODIFY_FLAG(L_SEQ_NO) = UNAPIGEN.MOD_FLAG_CREATE THEN
         
            OPEN L_SC_CURSOR(A_SC(L_SEQ_NO));
            FETCH L_SC_CURSOR
            INTO L_ST, L_ST_VERSION;
            IF L_SC_CURSOR%NOTFOUND THEN
               UNAPIGEN.P_TXN_ERROR := UNAPIGEN.DBERR_NOOBJECT;
               RAISE STPERROR;
            END IF;
            CLOSE L_SC_CURSOR;
            
            
            
            
            L_RET_CODE := UNAPIIC.CREATESCICDETAILS(L_ST, L_ST_VERSION, A_IC(L_SEQ_NO), 
                                                    L_IP_VERSION, L_SEQ_NO, A_SC(L_SEQ_NO), 
                                                    A_ICNODE(L_SEQ_NO), A_MODIFY_REASON);
            
            
            
            IF L_RET_CODE <> UNAPIGEN.DBERR_SUCCESS THEN
               L_ERRM := 'sc=' || A_SC(L_SEQ_NO)|| 
                         '#st='|| L_ST ||
                         '#st_version='|| L_ST_VERSION ||
                         '#ic=' || A_IC(L_SEQ_NO) ||
                         '#icnode=' || TO_CHAR(A_ICNODE(L_SEQ_NO)) ||
                         '#CreateScIcDetails#ErrorCode=' || TO_CHAR(L_RET_CODE);
               UNAPIGEN.P_TXN_ERROR := L_RET_CODE;
               RAISE STPERROR;
            END IF;
         END IF;
      END IF;

   
   
   
   END LOOP;

   IF UNAPIGEN.ENDTXN <> UNAPIGEN.DBERR_SUCCESS THEN
      RAISE STPERROR;
   END IF;

   
   
   
   FOR L_SEQ_NO IN 1..A_NR_OF_ROWS LOOP
      IF A_MODIFY_FLAG(L_SEQ_NO) < UNAPIGEN.DBERR_SUCCESS THEN
         A_MODIFY_FLAG(L_SEQ_NO) := UNAPIGEN.DBERR_SUCCESS;
      END IF;
   END LOOP;
   
   IF L_COMPLETELY_SAVED THEN
      RETURN(UNAPIGEN.DBERR_SUCCESS);
   ELSE
      RETURN(UNAPIGEN.DBERR_PARTIALSAVE);
   END IF;

EXCEPTION
WHEN OTHERS THEN
   IF SQLCODE <> 1 THEN
      UNAPIGEN.LOGERROR('SaveScInfoCard',SQLERRM);
   END IF;
   IF L_SC_CURSOR%ISOPEN THEN
      CLOSE L_SC_CURSOR;
   END IF;
   IF L_SCICOLD_CURSOR%ISOPEN THEN
      CLOSE L_SCICOLD_CURSOR;
   END IF;
   RETURN(UNAPIGEN.ABORTTXN(UNAPIGEN.P_TXN_ERROR, 'SaveScInfoCard'));
END SAVESCINFOCARD;

FUNCTION SAVESCINFOFIELD
(A_SC               IN       UNAPIGEN.VC20_TABLE_TYPE,    
 A_IC               IN       UNAPIGEN.VC20_TABLE_TYPE,    
 A_ICNODE           IN OUT   UNAPIGEN.LONG_TABLE_TYPE,    
 A_II               IN       UNAPIGEN.VC20_TABLE_TYPE,    
 A_IINODE           IN OUT   UNAPIGEN.LONG_TABLE_TYPE,    
 A_IE_VERSION       IN       UNAPIGEN.VC20_TABLE_TYPE,    
 A_IIVALUE          IN       UNAPIGEN.VC2000_TABLE_TYPE,  
 A_POS_X            IN       UNAPIGEN.NUM_TABLE_TYPE,     
 A_POS_Y            IN       UNAPIGEN.NUM_TABLE_TYPE,     
 A_IS_PROTECTED     IN       UNAPIGEN.CHAR1_TABLE_TYPE,   
 A_MANDATORY        IN       UNAPIGEN.CHAR1_TABLE_TYPE,   
 A_HIDDEN           IN       UNAPIGEN.CHAR1_TABLE_TYPE,   
 A_DSP_TITLE        IN       UNAPIGEN.VC40_TABLE_TYPE,    
 A_DSP_LEN          IN       UNAPIGEN.NUM_TABLE_TYPE,     
 A_DSP_TP           IN       UNAPIGEN.CHAR1_TABLE_TYPE,   
 A_DSP_ROWS         IN       UNAPIGEN.NUM_TABLE_TYPE,     
 A_II_CLASS         IN       UNAPIGEN.VC2_TABLE_TYPE,     
 A_LOG_HS           IN       UNAPIGEN.CHAR1_TABLE_TYPE,   
 A_LOG_HS_DETAILS   IN       UNAPIGEN.CHAR1_TABLE_TYPE,   
 A_LC               IN       UNAPIGEN.VC2_TABLE_TYPE,     
 A_LC_VERSION       IN       UNAPIGEN.VC20_TABLE_TYPE,    
 A_MODIFY_FLAG      IN OUT   UNAPIGEN.NUM_TABLE_TYPE,     
 A_NR_OF_ROWS       IN       NUMBER,                      
 A_MODIFY_REASON    IN       VARCHAR2)                    
RETURN NUMBER IS

L_IE_VERSION           VARCHAR2(20);
L_LC                   VARCHAR2(2);
L_LC_VERSION           VARCHAR2(20);
L_SS                   VARCHAR2(2);
L_LOG_HS               CHAR(1);
L_LOG_HS_DETAILS       CHAR(1);
L_ALLOW_MODIFY         CHAR(1);
L_ACTIVE               CHAR(1);
L_INSERT               BOOLEAN;
CURR_ITEM              INTEGER;
NEXT_ITEM              INTEGER;
PREV_NODE              NUMBER(9);
NEXT_NODE              NUMBER(9);
NODE_STEP              NUMBER(9);
L_SEQ_NO               INTEGER;
NBR_OF_NODES_TO_CREATE INTEGER;
L_COMPLETELY_SAVED     BOOLEAN;
L_II_RECORD_OK         BOOLEAN;
L_II_HANDLED           BOOLEAN;
L_DELETED_NODE         NUMBER(9);
L_CURRENT_TIMESTAMP              TIMESTAMP WITH TIME ZONE;
L_OLD_VALUE            VARCHAR2(2000);
L_NEW_VALUE            VARCHAR2(2000);
L_HS_DETAILS_SEQ_NR    INTEGER;

L_SVIIV_SEQ               INTEGER;
L_SVIIV_SC                UNAPIGEN.VC20_TABLE_TYPE;
L_SVIIV_IC                UNAPIGEN.VC20_TABLE_TYPE;
L_SVIIV_ICNODE            UNAPIGEN.LONG_TABLE_TYPE;
L_SVIIV_II                UNAPIGEN.VC20_TABLE_TYPE;
L_SVIIV_IINODE            UNAPIGEN.LONG_TABLE_TYPE;
L_SVIIV_IIVALUE           UNAPIGEN.VC2000_TABLE_TYPE;
L_SVIIV_MODIFY_FLAG       UNAPIGEN.NUM_TABLE_TYPE;
L_ROW                     INTEGER;

CURSOR L_SCIIOLD_CURSOR (A_SC IN VARCHAR2,
                         A_IC IN VARCHAR2, A_ICNODE IN NUMBER,
                         A_II IN VARCHAR2, A_IINODE IN NUMBER) IS
   SELECT A.*
   FROM UDSCII A
   WHERE A.SC = A_SC
   AND A.IC = A_IC
   AND A.ICNODE = A_ICNODE
   AND A.II = A_II
   AND A.IINODE = A_IINODE;
L_SCIIOLD_REC UDSCII%ROWTYPE;
L_SCIINEW_REC UDSCII%ROWTYPE;

BEGIN

   IF UNAPIGEN.BEGINTXN(UNAPIGEN.P_SINGLE_API_TXN) <> UNAPIGEN.DBERR_SUCCESS THEN
      RAISE STPERROR;
   END IF;

   L_SVIIV_SEQ := 0;
   L_CURRENT_TIMESTAMP := CURRENT_TIMESTAMP;
   L_ERRM := NULL;
   L_HS_DETAILS_SEQ_NR := 0;
   
   
   
   CURR_ITEM := 1;
   WHILE CURR_ITEM <= A_NR_OF_ROWS LOOP
      IF A_MODIFY_FLAG(CURR_ITEM) = UNAPIGEN.MOD_FLAG_INSERT THEN

         IF NVL(A_IINODE(CURR_ITEM), 0) <> 0 THEN
            UNAPIGEN.P_TXN_ERROR := UNAPIGEN.DBERR_NEWNODENOTZERO;
            RAISE STPERROR;
         END IF;

         
         
         
         NEXT_ITEM := CURR_ITEM;
         NBR_OF_NODES_TO_CREATE := 0;
         WHILE A_MODIFY_FLAG(NEXT_ITEM) = UNAPIGEN.MOD_FLAG_INSERT AND
               NVL(A_SC(NEXT_ITEM), ' ') = NVL(A_SC(CURR_ITEM),' ') AND
               NVL(A_IC(NEXT_ITEM), ' ') = NVL(A_IC(CURR_ITEM),' ') AND
               NVL(A_ICNODE(NEXT_ITEM), 0) = NVL(A_ICNODE(CURR_ITEM),0)
            LOOP
            NBR_OF_NODES_TO_CREATE := NBR_OF_NODES_TO_CREATE + 1;
            IF NEXT_ITEM < A_NR_OF_ROWS THEN
               NEXT_ITEM := NEXT_ITEM + 1;
            ELSE
               EXIT;
            END IF;
         END LOOP;

         
         
         
         
         IF A_MODIFY_FLAG(NEXT_ITEM) <> UNAPIGEN.MOD_FLAG_INSERT AND
            A_SC(NEXT_ITEM) = A_SC(CURR_ITEM) AND
            A_IC(NEXT_ITEM) = A_IC(CURR_ITEM) AND
            A_ICNODE(NEXT_ITEM) = A_ICNODE(CURR_ITEM) THEN
            NEXT_NODE := A_IINODE(NEXT_ITEM);
            SELECT NVL(MAX(IINODE), 0)
            INTO PREV_NODE
            FROM UTSCII
            WHERE IINODE < NEXT_NODE
              AND SC = A_SC(CURR_ITEM)
              AND IC = A_IC(CURR_ITEM)
              AND ICNODE = A_ICNODE(CURR_ITEM);
            NODE_STEP :=
               TRUNC(ABS((NEXT_NODE - PREV_NODE)) / (NBR_OF_NODES_TO_CREATE + 1));
            IF NODE_STEP < 1 THEN
               UNAPIGEN.P_TXN_ERROR := UNAPIGEN.DBERR_NODELIMITOVERF;
               RAISE STPERROR;
            END IF;
        ELSE
           
           SELECT NVL(MAX(IINODE), 0)
           INTO PREV_NODE
           FROM UTSCII
           WHERE SC = A_SC(CURR_ITEM)
             AND IC = A_IC(CURR_ITEM)
             AND ICNODE = A_ICNODE(CURR_ITEM);
           NODE_STEP := UNAPIGEN.DEFAULT_NODE_INTERVAL;
        END IF;

         
         
         
         FOR I IN 1..NBR_OF_NODES_TO_CREATE LOOP
            A_IINODE(CURR_ITEM + I - 1) := PREV_NODE + (NODE_STEP * I);
         END LOOP;

         CURR_ITEM := CURR_ITEM + NBR_OF_NODES_TO_CREATE;
      ELSE
         CURR_ITEM := CURR_ITEM + 1;
      END IF;
   END LOOP;

   
   
   
   L_COMPLETELY_SAVED := TRUE;

   
   
   
   FOR L_SEQ_NO IN 1..A_NR_OF_ROWS LOOP
      L_II_RECORD_OK := TRUE;          
      L_II_HANDLED := TRUE;

      
      
      
      IF A_MODIFY_FLAG(L_SEQ_NO) IN (UNAPIGEN.MOD_FLAG_INSERT,
                                     UNAPIGEN.MOD_FLAG_INSERT_WITH_NODES) THEN
         IF NVL(A_IE_VERSION(L_SEQ_NO), ' ') = ' ' THEN
            UNAPIGEN.P_TXN_ERROR := UNAPIGEN.DBERR_IEVERSION;
            RAISE STPERROR;
         END IF;
      END IF;

      IF NVL(A_SC(L_SEQ_NO), ' ') = ' ' OR
         NVL(A_IC(L_SEQ_NO), ' ') = ' ' OR
         NVL(A_II(L_SEQ_NO), ' ') = ' ' THEN
         UNAPIGEN.P_TXN_ERROR := UNAPIGEN.DBERR_NOOBJID;
         RAISE STPERROR;
      END IF;

      
      
      
      IF A_MODIFY_FLAG(L_SEQ_NO) IN (UNAPIGEN.MOD_FLAG_INSERT,
                                     UNAPIGEN.MOD_FLAG_INSERT_WITH_NODES,
                                     UNAPIGEN.MOD_FLAG_UPDATE,
                                     UNAPIGEN.MOD_FLAG_DELETE) THEN
         L_IE_VERSION := A_IE_VERSION(L_SEQ_NO);
         L_RET_CODE := UNAPIAUT.GETSCIIAUTHORISATION(A_SC(L_SEQ_NO), A_IC(L_SEQ_NO), A_ICNODE(L_SEQ_NO),
                                                     A_II(L_SEQ_NO), A_IINODE(L_SEQ_NO), L_IE_VERSION, 
                                                     L_LC, L_LC_VERSION, L_SS, L_ALLOW_MODIFY, 
                                                     L_ACTIVE, L_LOG_HS, L_LOG_HS_DETAILS);
         IF L_RET_CODE = UNAPIGEN.DBERR_NOOBJECT THEN
            L_INSERT := TRUE;
            
            L_LOG_HS := '1';
            L_LOG_HS_DETAILS := '1';
         ELSIF L_RET_CODE = UNAPIGEN.DBERR_SUCCESS THEN
            L_INSERT := FALSE;
         ELSE
            A_MODIFY_FLAG(L_SEQ_NO) := L_RET_CODE;
            L_II_RECORD_OK := FALSE;
         END IF;
      ELSIF A_MODIFY_FLAG(L_SEQ_NO) = UNAPIGEN.DBERR_SUCCESS THEN
         
         
         
         L_II_RECORD_OK := FALSE;
         L_II_HANDLED := FALSE;
      ELSE
         UNAPIGEN.P_TXN_ERROR := UNAPIGEN.DBERR_INVALMODFLAG;
         RAISE STPERROR;         
      END IF;

      
      
      

      
      
      
      IF L_II_RECORD_OK AND
         A_MODIFY_FLAG(L_SEQ_NO) = UNAPIGEN.MOD_FLAG_UPDATE THEN

         
         
         
         IF L_INSERT THEN
            
            A_MODIFY_FLAG(L_SEQ_NO) := UNAPIGEN.DBERR_NOOBJECT;
            L_II_RECORD_OK := FALSE;
         ELSIF NVL(A_IS_PROTECTED(L_SEQ_NO), ' ') NOT IN ('0', '1') THEN
            UNAPIGEN.P_TXN_ERROR := UNAPIGEN.DBERR_PROTECTED;
            RAISE STPERROR;
         ELSIF NVL(A_HIDDEN(L_SEQ_NO), ' ') NOT IN ('0', '1') THEN
            UNAPIGEN.P_TXN_ERROR := UNAPIGEN.DBERR_HIDDEN;
            RAISE STPERROR;
         ELSIF NVL(A_MANDATORY(L_SEQ_NO), ' ') NOT IN ('0', '1') THEN
            UNAPIGEN.P_TXN_ERROR := UNAPIGEN.DBERR_MANDATORY;
            RAISE STPERROR;
         ELSE
            
            
            

            
            
            
            OPEN L_SCIIOLD_CURSOR(A_SC(L_SEQ_NO),
                                  A_IC(L_SEQ_NO), A_ICNODE(L_SEQ_NO),
                                  A_II(L_SEQ_NO), A_IINODE(L_SEQ_NO));
            FETCH L_SCIIOLD_CURSOR
            INTO L_SCIIOLD_REC;
            CLOSE L_SCIIOLD_CURSOR;
            L_SCIINEW_REC := L_SCIIOLD_REC;
            L_NEW_VALUE := A_IIVALUE(L_SEQ_NO);

            
            
            
            
            
            
            UPDATE UTSCII
            SET POS_X          = A_POS_X(L_SEQ_NO),
                POS_Y          = A_POS_Y(L_SEQ_NO),
                IS_PROTECTED   = A_IS_PROTECTED(L_SEQ_NO),
                MANDATORY      = A_MANDATORY(L_SEQ_NO),
                HIDDEN         = A_HIDDEN(L_SEQ_NO),
                DSP_TITLE      = A_DSP_TITLE(L_SEQ_NO),
                DSP_LEN        = A_DSP_LEN(L_SEQ_NO),
                DSP_TP         = A_DSP_TP(L_SEQ_NO),
                DSP_ROWS       = A_DSP_ROWS(L_SEQ_NO),
                II_CLASS       = A_II_CLASS(L_SEQ_NO),
                LOG_HS         = A_LOG_HS(L_SEQ_NO),
                LOG_HS_DETAILS = A_LOG_HS_DETAILS(L_SEQ_NO),
                ALLOW_MODIFY   = '1'
            WHERE SC = A_SC(L_SEQ_NO)
              AND IC = A_IC(L_SEQ_NO)
              AND ICNODE = A_ICNODE(L_SEQ_NO)
              AND II = A_II(L_SEQ_NO)
              AND IINODE = A_IINODE(L_SEQ_NO)
            RETURNING POS_X, POS_Y, IS_PROTECTED, MANDATORY, HIDDEN, DSP_TITLE, DSP_LEN, DSP_TP, 
                      DSP_ROWS, II_CLASS, LOG_HS, LOG_HS_DETAILS, ALLOW_MODIFY
            INTO L_SCIINEW_REC.POS_X, L_SCIINEW_REC.POS_Y, L_SCIINEW_REC.IS_PROTECTED, 
                 L_SCIINEW_REC.MANDATORY, L_SCIINEW_REC.HIDDEN, L_SCIINEW_REC.DSP_TITLE,
                 L_SCIINEW_REC.DSP_LEN, L_SCIINEW_REC.DSP_TP, L_SCIINEW_REC.DSP_ROWS, 
                 L_SCIINEW_REC.II_CLASS, L_SCIINEW_REC.LOG_HS, L_SCIINEW_REC.LOG_HS_DETAILS, 
                 L_SCIINEW_REC.ALLOW_MODIFY;

            IF (NVL(L_SCIIOLD_REC.IIVALUE, ' ') <> NVL(L_NEW_VALUE, ' ')) OR
               (L_SCIIOLD_REC.IIVALUE IS NULL AND L_NEW_VALUE IS NOT NULL) OR
               (L_SCIIOLD_REC.IIVALUE IS NOT NULL AND L_NEW_VALUE IS NULL) THEN

               
               
               
               L_SVIIV_SEQ := L_SVIIV_SEQ + 1;
               L_SVIIV_SC(L_SVIIV_SEQ) := A_SC(L_SEQ_NO);
               L_SVIIV_IC(L_SVIIV_SEQ) := A_IC(L_SEQ_NO);
               L_SVIIV_ICNODE(L_SVIIV_SEQ) := A_ICNODE(L_SEQ_NO);
               L_SVIIV_II(L_SVIIV_SEQ) := A_II(L_SEQ_NO);
               L_SVIIV_IINODE(L_SVIIV_SEQ) := A_IINODE(L_SEQ_NO);
               L_SVIIV_IIVALUE(L_SVIIV_SEQ) := A_IIVALUE(L_SEQ_NO);
               L_SVIIV_MODIFY_FLAG(L_SVIIV_SEQ) := UNAPIGEN.MOD_FLAG_UPDATE;
            END IF;
            
            
            
            
            L_EVENT_TP := 'InfoFieldUpdated';
         END IF;

      
      
      
      ELSIF L_II_RECORD_OK AND
            A_MODIFY_FLAG(L_SEQ_NO) IN (UNAPIGEN.MOD_FLAG_INSERT,
                                        UNAPIGEN.MOD_FLAG_INSERT_WITH_NODES) THEN

         
         
         
         IF NOT L_INSERT THEN
            UNAPIGEN.P_TXN_ERROR := UNAPIGEN.DBERR_IIALREADYEXIST;
            RAISE STPERROR;
         ELSIF NVL(A_IS_PROTECTED(L_SEQ_NO), ' ') NOT IN ('0', '1') THEN
            UNAPIGEN.P_TXN_ERROR := UNAPIGEN.DBERR_PROTECTED;
            RAISE STPERROR;
         ELSIF NVL(A_HIDDEN(L_SEQ_NO), ' ') NOT IN ('0', '1') THEN
            UNAPIGEN.P_TXN_ERROR := UNAPIGEN.DBERR_HIDDEN;
            RAISE STPERROR;
         ELSIF NVL(A_MANDATORY(L_SEQ_NO), ' ') NOT IN ('0', '1') THEN
            UNAPIGEN.P_TXN_ERROR := UNAPIGEN.DBERR_MANDATORY;
            RAISE STPERROR;
         ELSE
            
            
            
            

            
            IF NVL(A_LC(L_SEQ_NO), ' ') <> ' ' THEN
               L_LC := A_LC(L_SEQ_NO);
            END IF;
            IF NVL(A_LC_VERSION(L_SEQ_NO), ' ') <> ' ' THEN
               L_LC_VERSION := A_LC_VERSION(L_SEQ_NO);
            END IF;

            
            
            INSERT INTO UTSCII(SC, IC, ICNODE, II, IINODE, IE_VERSION, POS_X,
                               POS_Y, IS_PROTECTED, MANDATORY, HIDDEN,
                               DSP_TITLE, DSP_LEN, DSP_TP, DSP_ROWS, II_CLASS,
                               LOG_HS, LOG_HS_DETAILS, ALLOW_MODIFY, ACTIVE, LC, LC_VERSION, SS)
            VALUES(A_SC(L_SEQ_NO), A_IC(L_SEQ_NO), A_ICNODE(L_SEQ_NO),
                   A_II(L_SEQ_NO), A_IINODE(L_SEQ_NO), L_IE_VERSION, A_POS_X(L_SEQ_NO),
                   A_POS_Y(L_SEQ_NO), A_IS_PROTECTED(L_SEQ_NO), A_MANDATORY(L_SEQ_NO),
                   A_HIDDEN(L_SEQ_NO), A_DSP_TITLE(L_SEQ_NO),
                   A_DSP_LEN(L_SEQ_NO), A_DSP_TP(L_SEQ_NO),
                   A_DSP_ROWS(L_SEQ_NO), A_II_CLASS(L_SEQ_NO),
                   A_LOG_HS(L_SEQ_NO), A_LOG_HS_DETAILS(L_SEQ_NO), '1', '1', L_LC, L_LC_VERSION,'');
            UNAPIAUT.UPDATELCINAUTHORISATIONBUFFER('ii', A_SC(L_SEQ_NO) || A_IC(L_SEQ_NO) || TO_CHAR(A_ICNODE(L_SEQ_NO))
                                                         || A_II(L_SEQ_NO) || TO_CHAR(A_IINODE(L_SEQ_NO)), '', L_LC, L_LC_VERSION);                 
                   

            IF A_IIVALUE(L_SEQ_NO) IS NOT NULL THEN

               
               
               
               L_SVIIV_SEQ := L_SVIIV_SEQ + 1;
               L_SVIIV_SC(L_SVIIV_SEQ) := A_SC(L_SEQ_NO);
               L_SVIIV_IC(L_SVIIV_SEQ) := A_IC(L_SEQ_NO);
               L_SVIIV_ICNODE(L_SVIIV_SEQ) := A_ICNODE(L_SEQ_NO);
               L_SVIIV_II(L_SVIIV_SEQ) := A_II(L_SEQ_NO);
               L_SVIIV_IINODE(L_SVIIV_SEQ) := A_IINODE(L_SEQ_NO);
               L_SVIIV_IIVALUE(L_SVIIV_SEQ) := A_IIVALUE(L_SEQ_NO);
               L_SVIIV_MODIFY_FLAG(L_SVIIV_SEQ) := UNAPIGEN.MOD_FLAG_UPDATE;

            END IF;

            
            
            
            L_EVENT_TP := 'InfoFieldCreated';
         END IF;

      
      
      
      ELSIF L_II_RECORD_OK AND
            A_MODIFY_FLAG(L_SEQ_NO) = UNAPIGEN.MOD_FLAG_DELETE THEN

         IF L_INSERT THEN
            
            UNAPIGEN.P_TXN_ERROR := UNAPIGEN.DBERR_NOOBJECT;
            RAISE STPERROR;
         
         
         
         ELSIF L_ACTIVE = '1' THEN
            UNAPIGEN.P_TXN_ERROR := UNAPIGEN.DBERR_OPACTIVE;
            RAISE STPERROR;
         ELSE
            
            IF UNAPIGEN.ISSYSTEM21CFR11COMPLIANT = UNAPIGEN.DBERR_SUCCESS THEN
               UNAPIGEN.P_TXN_ERROR := UNAPIGEN.DBERR_NOTALLOWEDIN21CFR11;
               RAISE STPERROR;
            END IF;

            
            L_OLD_VALUE := '';
            SELECT IIVALUE
            INTO   L_OLD_VALUE
            FROM   UTSCII
            WHERE SC = A_SC(L_SEQ_NO)
              AND IC = A_IC(L_SEQ_NO)
              AND ICNODE = A_ICNODE(L_SEQ_NO)
              AND II = A_II(L_SEQ_NO)
              AND IINODE = A_IINODE(L_SEQ_NO);

            
            
            
            DELETE FROM UTSCII
            WHERE SC = A_SC(L_SEQ_NO)
              AND IC = A_IC(L_SEQ_NO)
              AND ICNODE = A_ICNODE(L_SEQ_NO)
              AND II = A_II(L_SEQ_NO)
              AND IINODE = A_IINODE(L_SEQ_NO);
              
            DELETE FROM UTWSII
            WHERE SC = A_SC(L_SEQ_NO)
              AND IC = A_IC(L_SEQ_NO)
              AND ICNODE = A_ICNODE(L_SEQ_NO)
              AND II = A_II(L_SEQ_NO)
              AND IINODE = A_IINODE(L_SEQ_NO);
  
            
            
            
            DELETE FROM UTEVTIMED
            WHERE OBJECT_TP = 'ii'
              AND OBJECT_ID = A_II(L_SEQ_NO)
              AND INSTR(EV_DETAILS, 'sc=' || A_SC(L_SEQ_NO)) <> 0
              AND INSTR(EV_DETAILS, 'ic=' || A_IC(L_SEQ_NO)) <> 0
              AND INSTR(EV_DETAILS, 'icnode=' || TO_CHAR(A_ICNODE(L_SEQ_NO))) <> 0
              AND INSTR(EV_DETAILS, 'iinode=' || TO_CHAR(A_IINODE(L_SEQ_NO))) <> 0;

            DELETE FROM UTEVTIMED
            WHERE INSTR(EV_DETAILS, 'sc=' || A_SC(L_SEQ_NO)) <> 0
              AND INSTR(EV_DETAILS, 'ic=' || A_IC(L_SEQ_NO)) <> 0
              AND INSTR(EV_DETAILS, 'icnode=' || TO_CHAR(A_ICNODE(L_SEQ_NO))) <> 0
              AND INSTR(EV_DETAILS, 'ii=' || A_II(L_SEQ_NO)) <> 0
              AND INSTR(EV_DETAILS, 'iinode=' || TO_CHAR(A_IINODE(L_SEQ_NO))) <> 0;

            DELETE FROM UTEVRULESDELAYED
            WHERE OBJECT_TP = 'ii'
              AND OBJECT_ID = A_II(L_SEQ_NO)
              AND INSTR(EV_DETAILS, 'sc=' || A_SC(L_SEQ_NO)) <> 0
              AND INSTR(EV_DETAILS, 'ic=' || A_IC(L_SEQ_NO)) <> 0
              AND INSTR(EV_DETAILS, 'icnode=' || TO_CHAR(A_ICNODE(L_SEQ_NO))) <> 0
              AND INSTR(EV_DETAILS, 'iinode=' || TO_CHAR(A_IINODE(L_SEQ_NO))) <> 0;

            DELETE FROM UTEVRULESDELAYED
            WHERE INSTR(EV_DETAILS, 'sc=' || A_SC(L_SEQ_NO)) <> 0
              AND INSTR(EV_DETAILS, 'ic=' || A_IC(L_SEQ_NO)) <> 0
              AND INSTR(EV_DETAILS, 'icnode=' || TO_CHAR(A_ICNODE(L_SEQ_NO))) <> 0
              AND INSTR(EV_DETAILS, 'ii=' || A_II(L_SEQ_NO)) <> 0
              AND INSTR(EV_DETAILS, 'iinode=' || TO_CHAR(A_IINODE(L_SEQ_NO))) <> 0;

            L_DELETED_NODE := A_IINODE(L_SEQ_NO);
            A_IINODE(L_SEQ_NO) := 0;
            
            
            
            L_EVENT_TP := 'InfoFieldDeleted';
         END IF;
      END IF;

      
      
      
      IF L_II_RECORD_OK THEN
         L_EV_SEQ_NR := -1;
         L_EV_DETAILS := 'sc=' || A_SC(L_SEQ_NO) ||
                         '#ic=' || A_IC(L_SEQ_NO) ||
                         '#icnode=' || TO_CHAR(A_ICNODE(L_SEQ_NO)) ||
                         '#iinode=' || TO_CHAR(A_IINODE(L_SEQ_NO)) ||
                         '#ie_version=' || L_IE_VERSION;
                         
         IF L_EVENT_TP = 'InfoFieldCreated' THEN
            L_EV_DETAILS := L_EV_DETAILS || '#new_value=' || SUBSTR(A_IIVALUE(L_SEQ_NO),1,40);
         ELSIF L_EVENT_TP = 'InfoFieldDeleted' THEN
            L_EV_DETAILS := L_EV_DETAILS || '#old_value=' || SUBSTR(L_OLD_VALUE,1,40);
         END IF;
         L_RESULT := UNAPIEV.INSERTINFOFIELDEVENT('SaveScInfoField', UNAPIGEN.P_EVMGR_NAME,
                                                  'ii', A_II(L_SEQ_NO), L_LC, L_LC_VERSION, L_SS,
                                                  L_EVENT_TP, L_EV_DETAILS, L_EV_SEQ_NR);
         IF L_RESULT <> UNAPIGEN.DBERR_SUCCESS THEN
            UNAPIGEN.P_TXN_ERROR := L_RESULT;
            RAISE STPERROR;
         END IF;

         
         IF L_EV_SEQ_NR = -1 THEN
            
            
            OPEN L_SEQ_EVENT_CURSOR;
            FETCH L_SEQ_EVENT_CURSOR INTO L_EV_SEQ_NR;
            CLOSE L_SEQ_EVENT_CURSOR;
         END IF;
      ELSE
         IF L_II_HANDLED THEN
            L_COMPLETELY_SAVED := FALSE;
         END IF;
      END IF;
      
      
      
      
      IF L_II_RECORD_OK AND L_LOG_HS = '1' THEN
         IF L_EVENT_TP = 'InfoFieldCreated' THEN
            INSERT INTO UTSCICHS(SC, IC, ICNODE, WHO, WHO_DESCRIPTION, WHAT, 
                                 WHAT_DESCRIPTION, LOGDATE, LOGDATE_TZ, WHY, TR_SEQ, EV_SEQ)
            VALUES(A_SC(L_SEQ_NO), A_IC(L_SEQ_NO), A_ICNODE(L_SEQ_NO), UNAPIGEN.P_USER,
                   UNAPIGEN.P_USER_DESCRIPTION, L_EVENT_TP, 
                   'info field "'||A_II(L_SEQ_NO)||'" is created.', 
                   L_CURRENT_TIMESTAMP, L_CURRENT_TIMESTAMP, A_MODIFY_REASON, UNAPIGEN.P_TR_SEQ, L_EV_SEQ_NR);
         ELSIF L_EVENT_TP = 'InfoFieldUpdated' THEN
            INSERT INTO UTSCICHS(SC, IC, ICNODE, WHO, WHO_DESCRIPTION, WHAT, 
                                 WHAT_DESCRIPTION, LOGDATE, LOGDATE_TZ, WHY, TR_SEQ, EV_SEQ)
            VALUES(A_SC(L_SEQ_NO), A_IC(L_SEQ_NO), A_ICNODE(L_SEQ_NO), UNAPIGEN.P_USER,
                   UNAPIGEN.P_USER_DESCRIPTION, L_EVENT_TP, 
                   'info field "'||A_II(L_SEQ_NO)||'" is updated.', 
                   L_CURRENT_TIMESTAMP, L_CURRENT_TIMESTAMP, A_MODIFY_REASON, UNAPIGEN.P_TR_SEQ, L_EV_SEQ_NR);
         ELSE
            INSERT INTO UTSCICHS(SC, IC, ICNODE, WHO, WHO_DESCRIPTION, WHAT, 
                                 WHAT_DESCRIPTION, LOGDATE, LOGDATE_TZ, WHY, TR_SEQ, EV_SEQ)
            VALUES(A_SC(L_SEQ_NO), A_IC(L_SEQ_NO), A_ICNODE(L_SEQ_NO), UNAPIGEN.P_USER,
                   UNAPIGEN.P_USER_DESCRIPTION, L_EVENT_TP, 
                   'info field "'||A_II(L_SEQ_NO)||'" is deleted.', 
                   L_CURRENT_TIMESTAMP, L_CURRENT_TIMESTAMP, A_MODIFY_REASON, UNAPIGEN.P_TR_SEQ, L_EV_SEQ_NR);
         END IF;
      END IF;

      
      
      
      IF L_II_RECORD_OK AND L_LOG_HS_DETAILS = '1' THEN
         IF L_EVENT_TP = 'InfoFieldCreated' THEN
            L_HS_DETAILS_SEQ_NR := L_HS_DETAILS_SEQ_NR + 1;
            INSERT INTO UTSCICHSDETAILS(SC, IC, ICNODE, TR_SEQ, EV_SEQ, SEQ, DETAILS)
            VALUES(A_SC(L_SEQ_NO), A_IC(L_SEQ_NO), A_ICNODE(L_SEQ_NO), UNAPIGEN.P_TR_SEQ, L_EV_SEQ_NR, 
                   L_HS_DETAILS_SEQ_NR, 'info field "'||A_II(L_SEQ_NO)||'" is created.');
         ELSIF L_EVENT_TP = 'InfoFieldUpdated' THEN
            L_HS_DETAILS_SEQ_NR := L_HS_DETAILS_SEQ_NR + 1;
            INSERT INTO UTSCICHSDETAILS(SC, IC, ICNODE, TR_SEQ, EV_SEQ, SEQ, DETAILS)
            VALUES(A_SC(L_SEQ_NO), A_IC(L_SEQ_NO), A_ICNODE(L_SEQ_NO), UNAPIGEN.P_TR_SEQ, L_EV_SEQ_NR, 
                   L_HS_DETAILS_SEQ_NR, 'info field "'||A_II(L_SEQ_NO)||'" is updated.');

            UNAPIHSDETAILS.ADDSCIIHSDETAILS(L_SCIIOLD_REC, L_SCIINEW_REC, UNAPIGEN.P_TR_SEQ, 
                                            L_EV_SEQ_NR, L_HS_DETAILS_SEQ_NR); 
         ELSE
            L_HS_DETAILS_SEQ_NR := L_HS_DETAILS_SEQ_NR + 1;
            INSERT INTO UTSCICHSDETAILS(SC, IC, ICNODE, TR_SEQ, EV_SEQ, SEQ, DETAILS)
            VALUES(A_SC(L_SEQ_NO), A_IC(L_SEQ_NO), A_ICNODE(L_SEQ_NO), UNAPIGEN.P_TR_SEQ, L_EV_SEQ_NR, 
                   L_HS_DETAILS_SEQ_NR, 'info field "'||A_II(L_SEQ_NO)||'" is deleted.');
         END IF;
      END IF;
   
   
   
   END LOOP;

   
   
   
   IF L_SVIIV_SEQ <> 0 THEN
      L_RET_CODE := UNAPIIC.SAVESCIIVALUE(L_SVIIV_SC, L_SVIIV_IC, L_SVIIV_ICNODE,
                                          L_SVIIV_II, L_SVIIV_IINODE,
                                          L_SVIIV_IIVALUE, 
                                          L_SVIIV_MODIFY_FLAG,
                                          L_SVIIV_SEQ, A_MODIFY_REASON);
      IF L_RET_CODE = UNAPIGEN.DBERR_PARTIALSAVE THEN
         
         
         
         FOR L_ROW IN 1..L_SVIIV_SEQ LOOP
            IF L_SVIIV_MODIFY_FLAG(L_ROW) > UNAPIGEN.DBERR_SUCCESS THEN
               UNAPIGEN.P_TXN_ERROR := L_SVIIV_MODIFY_FLAG(L_ROW);
               L_ERRM := 'sc=' || L_SVIIV_SC(L_ROW) || '#ic=' || L_SVIIV_IC(L_ROW) ||
                         '#icnode=' || TO_CHAR(L_SVIIV_ICNODE(L_ROW)) ||
                         '#ii=' || L_SVIIV_II(L_ROW)||
                         '#iinode=' || TO_CHAR(L_SVIIV_IINODE(L_ROW)) ||
                         '#SaveScIiValue#ErrorCode=' || TO_CHAR(L_RET_CODE);
               RAISE STPERROR;
            END IF;
         END LOOP;
      ELSIF L_RET_CODE <> UNAPIGEN.DBERR_SUCCESS THEN
         UNAPIGEN.P_TXN_ERROR := L_RET_CODE;
         L_ERRM := 'sc(1)=' || L_SVIIV_SC(1) || '#ic(1)=' || L_SVIIV_IC(1) ||
                   '#icnode(1)=' || TO_CHAR(L_SVIIV_ICNODE(1)) ||
                   '#ii(1)=' || L_SVIIV_II(1)||
                   '#iinode(1)=' || TO_CHAR(L_SVIIV_IINODE(1)) ||
                   '#nr_of_rows=' || TO_CHAR(L_SVIIV_SEQ) ||
                   '#SaveScIiValue#ErrorCode=' || TO_CHAR(L_RET_CODE);
         RAISE STPERROR;
      END IF;                           
   END IF;

   IF UNAPIGEN.ENDTXN <> UNAPIGEN.DBERR_SUCCESS THEN
      RAISE STPERROR;
   END IF;

   
   
   
   FOR L_SEQ_NO IN 1..A_NR_OF_ROWS LOOP
      IF A_MODIFY_FLAG(L_SEQ_NO) < UNAPIGEN.DBERR_SUCCESS THEN
         A_MODIFY_FLAG(L_SEQ_NO) := UNAPIGEN.DBERR_SUCCESS;
      END IF;
   END LOOP;

   IF L_COMPLETELY_SAVED THEN
      RETURN(UNAPIGEN.DBERR_SUCCESS);
   ELSE
      RETURN(UNAPIGEN.DBERR_PARTIALSAVE);
   END IF;

EXCEPTION
WHEN OTHERS THEN
   IF SQLCODE <> 1 THEN
      UNAPIGEN.LOGERROR('SaveScInfoField',SQLERRM);
   ELSIF L_ERRM IS NOT NULL THEN
      UNAPIGEN.LOGERROR('SaveScInfoField',L_ERRM);   
   END IF;
   IF L_SCIIOLD_CURSOR%ISOPEN THEN
      CLOSE L_SCIIOLD_CURSOR;
   END IF;
   IF L_SEQ_EVENT_CURSOR%ISOPEN THEN
      CLOSE L_SEQ_EVENT_CURSOR;
   END IF;
   RETURN(UNAPIGEN.ABORTTXN(UNAPIGEN.P_TXN_ERROR, 'SaveScInfoField'));
END SAVESCINFOFIELD;

FUNCTION SAVESCIIVALUE
(A_SC               IN     UNAPIGEN.VC20_TABLE_TYPE,      
 A_IC               IN     UNAPIGEN.VC20_TABLE_TYPE,      
 A_ICNODE           IN OUT UNAPIGEN.LONG_TABLE_TYPE,      
 A_II               IN     UNAPIGEN.VC20_TABLE_TYPE,      
 A_IINODE           IN OUT UNAPIGEN.LONG_TABLE_TYPE,      
 A_IIVALUE          IN     UNAPIGEN.VC2000_TABLE_TYPE,    
 A_MODIFY_FLAG      IN OUT UNAPIGEN.NUM_TABLE_TYPE,       
 A_NR_OF_ROWS       IN     NUMBER,                        
 A_MODIFY_REASON    IN     VARCHAR2)                      
RETURN NUMBER IS

L_IE_VERSION             VARCHAR2(20);
L_LC                     VARCHAR2(2);
L_LC_VERSION             VARCHAR2(20);
L_SS                     VARCHAR2(2);
L_LOG_HS                 CHAR(1);
L_LOG_HS_DETAILS         CHAR(1);
L_ALLOW_MODIFY           CHAR(1);
L_ACTIVE                 CHAR(1);
L_CURRENT_TIMESTAMP                TIMESTAMP WITH TIME ZONE;
L_SEQ_NO                 INTEGER;
L_OLD_VALUE              VARCHAR2(2000);
L_NEW_VALUE              VARCHAR2(2000);
L_II_RECORD_OK           BOOLEAN;
L_II_OK                  BOOLEAN;
L_II_HANDLED             BOOLEAN;
L_COMPLETELY_SAVED       BOOLEAN;
L_SCAN_SEQ               INTEGER;
L_SCAN_PREV_SC           VARCHAR2(20);
L_SCAN_PREV_IC           VARCHAR2(20);
L_SCAN_PREV_ICNODE       NUMBER;
L_IC_EVENT_INSERTED      BOOLEAN;
L_NEXT_II                VARCHAR2(20);
L_OLD_NEXT_II            VARCHAR2(20);
L_DATE_STRING            VARCHAR2(255);
L_DATE                   TIMESTAMP WITH TIME ZONE;
L_SQLERRM2               VARCHAR2(255);
L_HS_DETAILS_SEQ_NR      INTEGER;
L_ST_VERSION             VARCHAR2(20);
L_IP_VERSION             VARCHAR2(20);
L_IC_LOG_HS              CHAR(1);
L_IC_LOG_HS_DETAILS      CHAR(1);
L_SHELF_LIFE_VAL         NUMBER;
L_SHELF_LIFE_UNIT        VARCHAR2(20);
L_TIMED_EVENT_TP         VARCHAR2(255);
L_REF_DATE               TIMESTAMP WITH TIME ZONE;
L_DELAYED_TILL           TIMESTAMP WITH TIME ZONE;
L_LINKED_II              BOOLEAN;

CURSOR L_NEXT_II_CURSOR (C_SC        VARCHAR2,
                         C_IC        VARCHAR2,
                         C_ICNODE    NUMBER) IS
   SELECT II
   FROM UTSCII
   WHERE SC = C_SC
     AND IC = C_IC
     AND ICNODE = C_ICNODE
     AND IIVALUE IS NULL
   ORDER BY IINODE;

CURSOR L_IEVALUE_CURSOR(A_IC VARCHAR2, A_IP_VERSION VARCHAR2, A_IE VARCHAR2, A_IE_VERSION VARCHAR2) IS
   SELECT B.IEVALUE, B.FORMAT
   FROM UTIE B
   WHERE B.DEF_VAL_TP = 'S'
     AND B.IE = A_IE
     AND B.VERSION = A_IE_VERSION
     AND (B.IE, B.VERSION) NOT IN (SELECT C.IE, UNAPIGEN.VALIDATEVERSION('ie',C.IE,C.IE_VERSION)
                                   FROM UTIPIE C
                                   WHERE C.IE = B.IE
                                     AND UNAPIGEN.VALIDATEVERSION('ie',C.IE,C.IE_VERSION) = B.VERSION
                                     AND C.IP = A_IC
                                     AND C.VERSION = A_IP_VERSION
                                     AND (C.DEF_VAL_TP<>'F' OR C.IEVALUE IS NOT NULL))
   UNION 
   SELECT B.IEVALUE, C.FORMAT
   FROM UTIPIE B, UTIE C
   WHERE B.IP = A_IC
     AND B.VERSION = A_IP_VERSION
     AND B.IE = C.IE
     AND UNAPIGEN.VALIDATEVERSION('ie',B.IE,B.IE_VERSION) = C.VERSION
     AND B.DEF_VAL_TP = 'S'
     AND C.IE = A_IE
     AND C.VERSION = A_IE_VERSION;
L_IEVALUE_REC       L_IEVALUE_CURSOR%ROWTYPE;

CURSOR L_SCIIOLD_CURSOR (A_SC IN VARCHAR2,
                         A_IC IN VARCHAR2, A_ICNODE IN NUMBER,
                         A_II IN VARCHAR2, A_IINODE IN NUMBER) IS
   SELECT A.*
   FROM UDSCII A
   WHERE A.SC = A_SC
   AND A.IC = A_IC
   AND A.ICNODE = A_ICNODE
   AND A.II = A_II
   AND A.IINODE = A_IINODE;
L_SCIIOLD_REC UDSCII%ROWTYPE;
L_SCIINEW_REC UDSCII%ROWTYPE;

CURSOR L_SC_CURSOR (A_SC IN VARCHAR2) IS
   SELECT A.*
   FROM UDSC A
   WHERE A.SC = A_SC;
L_SCOLD_REC UDSC%ROWTYPE;
L_SCNEW_REC UDSC%ROWTYPE;

CURSOR L_IPVERSION_CURSOR (C_SC VARCHAR2, C_IC VARCHAR2, C_ICNODE NUMBER) IS
   SELECT IP_VERSION
   FROM UTSCIC
   WHERE SC = C_SC
     AND IC = C_IC
     AND ICNODE = C_ICNODE;

CURSOR L_SCIC_CURSOR (C_SC VARCHAR2, C_IC VARCHAR2, C_ICNODE NUMBER) IS
   SELECT IP_VERSION, NEXT_II, LOG_HS, LOG_HS_DETAILS
   FROM UTSCIC
   WHERE SC     = C_SC
     AND IC     = C_IC
     AND ICNODE = C_ICNODE;

BEGIN
   IF UNAPIGEN.BEGINTXN(UNAPIGEN.P_SINGLE_API_TXN) <> UNAPIGEN.DBERR_SUCCESS THEN
      RAISE STPERROR;
   END IF;

   
   L_CURRENT_TIMESTAMP := CURRENT_TIMESTAMP;
   L_COMPLETELY_SAVED := TRUE;
   L_II_OK := FALSE;
   L_LINKED_II := FALSE;

   L_HS_DETAILS_SEQ_NR := 0;

   
   
   
   OPEN L_SC_CURSOR(A_SC(1));
   FETCH L_SC_CURSOR
   INTO L_SCOLD_REC;
   CLOSE L_SC_CURSOR;

   FOR L_SEQ_NO IN 1..A_NR_OF_ROWS LOOP
      L_II_RECORD_OK := TRUE;          
      L_II_HANDLED := TRUE;

      IF NVL(A_SC(L_SEQ_NO), ' ') = ' ' OR
         NVL(A_IC(L_SEQ_NO), ' ') = ' ' OR
         NVL(A_ICNODE(L_SEQ_NO), 0) = 0 OR
         NVL(A_II(L_SEQ_NO), ' ') = ' ' OR
         NVL(A_IINODE(L_SEQ_NO), 0) = 0 THEN
         UNAPIGEN.P_TXN_ERROR := UNAPIGEN.DBERR_NOOBJID;
         RAISE STPERROR;

      ELSIF A_MODIFY_FLAG(L_SEQ_NO) = UNAPIGEN.MOD_FLAG_UPDATE THEN
         
         
         
         L_RET_CODE := UNAPIAUT.GETSCIIAUTHORISATION(A_SC(L_SEQ_NO), A_IC(L_SEQ_NO), A_ICNODE(L_SEQ_NO),
                                                     A_II(L_SEQ_NO), A_IINODE(L_SEQ_NO), L_IE_VERSION,
                                                     L_LC, L_LC_VERSION, L_SS, L_ALLOW_MODIFY,
                                                     L_ACTIVE, L_LOG_HS, L_LOG_HS_DETAILS);
         IF L_RET_CODE <> UNAPIGEN.DBERR_SUCCESS THEN
            A_MODIFY_FLAG(L_SEQ_NO) := L_RET_CODE;
            L_II_RECORD_OK := FALSE;
         END IF;
         
      ELSIF A_MODIFY_FLAG(L_SEQ_NO) = UNAPIGEN.DBERR_SUCCESS THEN
         
         
         
         L_II_RECORD_OK := FALSE; 
         L_II_HANDLED := FALSE;   

      ELSE
         
         
         
         UNAPIGEN.P_TXN_ERROR := UNAPIGEN.DBERR_INVALMODFLAG;
         RAISE STPERROR;
         
      END IF;

      IF L_II_RECORD_OK THEN
         
         
         L_II_OK := TRUE;
         
         
         
         
         OPEN L_SCIIOLD_CURSOR(A_SC(L_SEQ_NO),
                               A_IC(L_SEQ_NO), A_ICNODE(L_SEQ_NO),
                               A_II(L_SEQ_NO), A_IINODE(L_SEQ_NO));
         FETCH L_SCIIOLD_CURSOR
         INTO L_SCIIOLD_REC;
         CLOSE L_SCIIOLD_CURSOR;
         L_SCIINEW_REC := L_SCIIOLD_REC;

         
         
         
         
         
         
         UPDATE UTSCII
         SET IIVALUE      = A_IIVALUE(L_SEQ_NO),
             ALLOW_MODIFY = '1'
         WHERE SC = A_SC(L_SEQ_NO)
           AND IC = A_IC(L_SEQ_NO)
           AND ICNODE = A_ICNODE(L_SEQ_NO)
           AND II = A_II(L_SEQ_NO)
           AND IINODE = A_IINODE(L_SEQ_NO)
         RETURNING IIVALUE, ALLOW_MODIFY
         INTO L_SCIINEW_REC.IIVALUE, L_SCIINEW_REC.ALLOW_MODIFY;

         OPEN L_IPVERSION_CURSOR(A_SC(L_SEQ_NO), A_IC(L_SEQ_NO), A_ICNODE(L_SEQ_NO));
         FETCH L_IPVERSION_CURSOR INTO L_IP_VERSION;
         CLOSE L_IPVERSION_CURSOR;

         
         
         
         OPEN L_IEVALUE_CURSOR(A_IC(L_SEQ_NO), L_IP_VERSION, A_II(L_SEQ_NO), L_IE_VERSION);
         FETCH L_IEVALUE_CURSOR
         INTO L_IEVALUE_REC;
         IF L_IEVALUE_CURSOR%FOUND THEN

            
            L_DATE := NULL;
            IF  L_IEVALUE_REC.IEVALUE IN ('sampling_date', 'creation_date', 'exec_start_date', 'exec_end_date', 
                                          'date1', 'date2', 'date3', 'date4', 'date5') THEN 
                L_DATE_STRING := SUBSTR(A_IIVALUE(L_SEQ_NO),1,40)||'@'||SUBSTR(L_IEVALUE_REC.FORMAT,2);
                L_RET_CODE := UNAPIGEN.DATEVALID(L_DATE_STRING, L_DATE, L_SQLERRM);
                IF L_RET_CODE <> UNAPIGEN.DBERR_SUCCESS THEN
                   L_SQLERRM2 := 'UNAPIGEN.DateValid('''||SUBSTR(A_IIVALUE(L_SEQ_NO),1,40)||'@'||SUBSTR(L_IEVALUE_REC.FORMAT,2)||''') ret_code='||
                                 TO_CHAR(L_RET_CODE)||'#sc='||A_SC(L_SEQ_NO)||'#ic='||A_IC(L_SEQ_NO)||'#icnode='||TO_CHAR(A_ICNODE(L_SEQ_NO))||
                                 '#ii='||A_II(L_SEQ_NO)||'#iinode='||TO_CHAR(A_IINODE(L_SEQ_NO));
                   RAISE STPERROR;
                END IF;
            END IF;

             
            IF L_IEVALUE_REC.IEVALUE = 'sc' THEN
               
               NULL;
            ELSIF L_IEVALUE_REC.IEVALUE = 'st' THEN
               IF L_SCIIOLD_REC.IIVALUE <> L_SCIINEW_REC.IIVALUE THEN
                  L_ST_VERSION := UNAPIGEN.VALIDATEVERSION('st', SUBSTR(A_IIVALUE(L_SEQ_NO),1,40), '~Current~');
                  L_RET_CODE := UNAPISC.CHANGESCSAMPLETYPE(A_SC(L_SEQ_NO), 
                                                           SUBSTR(A_IIVALUE(L_SEQ_NO),1,40), 
                                                           L_ST_VERSION, 
                                                           A_MODIFY_REASON);
                  IF L_RET_CODE <> UNAPIGEN.DBERR_SUCCESS THEN
                     L_SQLERRM := 'ChangeScSampleType returned '||TO_CHAR(L_RET_CODE)||
                                  '#sc='||A_SC(L_SEQ_NO) ||
                                  '#new_st='||SUBSTR(A_IIVALUE(L_SEQ_NO),1,40);
                     RAISE STPERROR;
                  END IF;
                  
                  
                  
                  
                  
                  OPEN L_SC_CURSOR(A_SC(1));
                  FETCH L_SC_CURSOR
                  INTO L_SCOLD_REC; 
                  CLOSE L_SC_CURSOR;
               END IF;
            ELSIF L_IEVALUE_REC.IEVALUE = 'description' THEN
               UPDATE UTSC SET DESCRIPTION= A_IIVALUE(L_SEQ_NO) WHERE SC=A_SC(L_SEQ_NO);
               
            ELSIF L_IEVALUE_REC.IEVALUE IN ('shelf_life_val','shelf_life_unit') THEN
               
               OPEN L_SC_CURSOR(A_SC(1));
               FETCH L_SC_CURSOR
               INTO L_SCNEW_REC;
               CLOSE L_SC_CURSOR;

               IF L_IEVALUE_REC.IEVALUE = 'shelf_life_val' THEN
                  L_SHELF_LIFE_VAL := NVL(A_IIVALUE(L_SEQ_NO), 0);
                  L_SHELF_LIFE_UNIT := L_SCNEW_REC.SHELF_LIFE_UNIT;
                  UPDATE UTSC SET SHELF_LIFE_VAL=L_SHELF_LIFE_VAL WHERE SC=A_SC(L_SEQ_NO);
               ELSE
                  IF A_IIVALUE(L_SEQ_NO) IS NULL THEN
                     L_SHELF_LIFE_VAL := 0;
                  ELSE
                     L_SHELF_LIFE_VAL := L_SCNEW_REC.SHELF_LIFE_VAL;
                  END IF;
                  L_SHELF_LIFE_UNIT := NVL(A_IIVALUE(L_SEQ_NO), L_SCNEW_REC.SHELF_LIFE_UNIT);
                  UPDATE UTSC SET SHELF_LIFE_VAL=L_SHELF_LIFE_VAL, SHELF_LIFE_UNIT=L_SHELF_LIFE_UNIT WHERE SC=A_SC(L_SEQ_NO);
               END IF;
               
               
               
               
               L_EV_SEQ_NR := -1;
               L_EV_DETAILS := 'st_version=' || L_SCNEW_REC.ST_VERSION;
               L_TIMED_EVENT_TP := 'ScShelfLifeExceeded';
               L_REF_DATE := NVL(L_SCNEW_REC.SAMPLING_DATE, L_CURRENT_TIMESTAMP);
               IF     (NVL(L_SCIIOLD_REC.IIVALUE,0) <> NVL(L_SCIINEW_REC.IIVALUE,0))
                  AND NOT(L_SCIIOLD_REC.IIVALUE IS NULL AND L_SCIINEW_REC.IIVALUE IS NULL) THEN
                  
                  
                  L_RET_CODE := UNAPIAUT.CALCULATEDELAY(L_SHELF_LIFE_VAL,
                                                        L_SHELF_LIFE_UNIT,
                                                        L_REF_DATE, L_DELAYED_TILL);
                  IF L_RET_CODE <> UNAPIGEN.DBERR_SUCCESS THEN
                     UNAPIGEN.P_TXN_ERROR := L_RET_CODE;
                     RAISE STPERROR;
                  END IF;
                  
                  
                  
                  
                  IF     (NVL(L_SCNEW_REC.SHELF_LIFE_VAL, 0) = 0) 
                     AND (L_IEVALUE_REC.IEVALUE <> 'shelf_life_unit') THEN
                     
                     L_EV_SEQ_NR  := -1;
                     L_RESULT := UNAPIEV.INSERTTIMEDEVENT('SaveScIiValue', UNAPIGEN.P_EVMGR_NAME, 'sc', A_SC(L_SEQ_NO), 
                                                          L_SCNEW_REC.LC, L_SCNEW_REC.LC_VERSION, L_SCNEW_REC.SS, 
                                                          L_TIMED_EVENT_TP, L_EV_DETAILS, L_EV_SEQ_NR, L_DELAYED_TILL);
                     IF L_RESULT <> UNAPIGEN.DBERR_SUCCESS THEN
                        UNAPIGEN.P_TXN_ERROR := L_RESULT;
                        RAISE STPERROR;
                     END IF;
                  
                  ELSE
                     
                     IF NVL(L_SHELF_LIFE_VAL, 0) <> 0 THEN
                        
                        L_RESULT := UNAPIEV.UPDATETIMEDEVENT('sc', A_SC(L_SEQ_NO), L_TIMED_EVENT_TP, L_EV_DETAILS, 
                                                             L_DELAYED_TILL);
                        IF L_RESULT <> UNAPIGEN.DBERR_SUCCESS THEN
                           UNAPIGEN.P_TXN_ERROR := L_RESULT;
                           RAISE STPERROR;
                        END IF;
                     
                     ELSE
                        
                        DELETE FROM UTEVTIMED
                        WHERE OBJECT_TP = 'sc'
                          AND OBJECT_ID = A_SC(L_SEQ_NO)
                          AND EV_TP = 'ScShelfLifeExceeded';
                        L_EVENT_TP := 'ScShelfLifeExceeded';
                        L_RESULT := UNAPIEV.INSERTEVENT('SaveScIiValue', UNAPIGEN.P_EVMGR_NAME, 'sc', A_SC(L_SEQ_NO), 
                                                        L_SCNEW_REC.LC, L_SCNEW_REC.LC_VERSION, L_SCNEW_REC.SS, 
                                                        L_EVENT_TP, L_EV_DETAILS, L_EV_SEQ_NR);
                        IF L_RESULT <> UNAPIGEN.DBERR_SUCCESS  THEN
                           UNAPIGEN.P_TXN_ERROR := L_RESULT;
                           RAISE STPERROR;
                        END IF;
                     END IF;
                  END IF;
               END IF;
            ELSIF L_IEVALUE_REC.IEVALUE = 'sampling_date' THEN
               UPDATE UTSC SET SAMPLING_DATE= L_DATE, SAMPLING_DATE_TZ= L_DATE WHERE SC=A_SC(L_SEQ_NO);
               
            ELSIF L_IEVALUE_REC.IEVALUE = 'creation_date' THEN
               UPDATE UTSC SET CREATION_DATE= L_DATE, CREATION_DATE_TZ= L_DATE WHERE SC=A_SC(L_SEQ_NO);
               
            ELSIF L_IEVALUE_REC.IEVALUE = 'created_by' THEN
               UPDATE UTSC SET CREATED_BY= A_IIVALUE(L_SEQ_NO) WHERE SC=A_SC(L_SEQ_NO);
               
            ELSIF L_IEVALUE_REC.IEVALUE = 'exec_start_date' THEN
               UPDATE UTSC SET EXEC_START_DATE= L_DATE, EXEC_START_DATE_TZ= L_DATE WHERE SC=A_SC(L_SEQ_NO);
               
            ELSIF L_IEVALUE_REC.IEVALUE = 'exec_end_date' THEN
               UPDATE UTSC SET EXEC_END_DATE= L_DATE, EXEC_END_DATE_TZ= L_DATE WHERE SC=A_SC(L_SEQ_NO);
               
            ELSIF L_IEVALUE_REC.IEVALUE = 'priority' THEN
               UPDATE UTSC SET PRIORITY= A_IIVALUE(L_SEQ_NO) WHERE SC=A_SC(L_SEQ_NO);
               
            ELSIF L_IEVALUE_REC.IEVALUE = 'descr_doc' THEN
               UPDATE UTSC SET DESCR_DOC= A_IIVALUE(L_SEQ_NO) WHERE SC=A_SC(L_SEQ_NO);

            ELSIF L_IEVALUE_REC.IEVALUE = 'label_format' THEN
               UPDATE UTSC SET LABEL_FORMAT= A_IIVALUE(L_SEQ_NO) WHERE SC=A_SC(L_SEQ_NO);
               
            ELSIF L_IEVALUE_REC.IEVALUE = 'rq' THEN
               
               
               
               IF L_SCIIOLD_REC.IIVALUE IS NOT NULL THEN
                  L_RET_CODE := UNAPIRQ.REMOVERQSAMPLE(L_SCIIOLD_REC.IIVALUE, A_SC(L_SEQ_NO),
                                                       'ii '||A_II(L_SEQ_NO)||' : '||
                                                       L_SCIIOLD_REC.IIVALUE || ' -> '||
                                                       L_SCIINEW_REC.IIVALUE);
                  IF L_RET_CODE <> UNAPIGEN.DBERR_SUCCESS THEN
                     RAISE STPERROR;
                  END IF;

                  
                  
                  
                  
                  OPEN L_SC_CURSOR(A_SC(1));
                  FETCH L_SC_CURSOR
                  INTO L_SCOLD_REC; 
                  CLOSE L_SC_CURSOR;
               END IF;

               IF L_SCIINEW_REC.IIVALUE IS NOT NULL THEN
                  L_RET_CODE := UNAPIRQ.SAVE1RQSAMPLE(L_SCIINEW_REC.IIVALUE, A_SC(L_SEQ_NO),
                                                      CURRENT_TIMESTAMP, UNAPIGEN.P_USER,
                                                      'ii '||A_II(L_SEQ_NO)||' : '||
                                                      L_SCIIOLD_REC.IIVALUE || ' -> '||
                                                      L_SCIINEW_REC.IIVALUE);
                  IF L_RET_CODE = UNAPIGEN.DBERR_SUCCESS THEN
                     
                     
                     
                     
                     UPDATE UTSC SET RQ= A_IIVALUE(L_SEQ_NO) WHERE SC=A_SC(L_SEQ_NO);
                  ELSE
                     L_COMPLETELY_SAVED := FALSE;
                  END IF;
               END IF;


            ELSIF L_IEVALUE_REC.IEVALUE = 'date1' THEN
               UPDATE UTSC SET DATE1= L_DATE, DATE1_TZ= L_DATE WHERE SC=A_SC(L_SEQ_NO);
               
            ELSIF L_IEVALUE_REC.IEVALUE = 'date2' THEN
               UPDATE UTSC SET DATE2= L_DATE, DATE2_TZ= L_DATE WHERE SC=A_SC(L_SEQ_NO);
               
            ELSIF L_IEVALUE_REC.IEVALUE = 'date3' THEN
               UPDATE UTSC SET DATE3= L_DATE, DATE3_TZ= L_DATE WHERE SC=A_SC(L_SEQ_NO);
               
            ELSIF L_IEVALUE_REC.IEVALUE = 'date4' THEN
               UPDATE UTSC SET DATE4= L_DATE, DATE4_TZ= L_DATE WHERE SC=A_SC(L_SEQ_NO);
               
            ELSIF L_IEVALUE_REC.IEVALUE = 'date5' THEN
               UPDATE UTSC SET DATE5= L_DATE, DATE5_TZ= L_DATE WHERE SC=A_SC(L_SEQ_NO);
               
            ELSIF L_IEVALUE_REC.IEVALUE = 'allow_any_pp' THEN
               UPDATE UTSC SET ALLOW_ANY_PP= A_IIVALUE(L_SEQ_NO) WHERE SC=A_SC(L_SEQ_NO);
               
            ELSIF L_IEVALUE_REC.IEVALUE = 'sc_class' THEN
               UPDATE UTSC SET SC_CLASS= A_IIVALUE(L_SEQ_NO) WHERE SC=A_SC(L_SEQ_NO);

            ELSE
               L_SQLERRM := L_IEVALUE_REC.IEVALUE || ' is not a valid sample standard property for info field '||A_II(L_SEQ_NO)||
                            ' in info profile '||A_IC(L_SEQ_NO) ||' sc=' || A_SC(L_SEQ_NO) || '#icnode=' ||
                            TO_CHAR(A_ICNODE(L_SEQ_NO)) || '#iinode=' || TO_CHAR(A_IINODE(L_SEQ_NO));
               RAISE STPERROR;
            END IF;
            L_LINKED_II := TRUE;
         END IF;
         CLOSE L_IEVALUE_CURSOR;
         
         
         
         
         L_EV_SEQ_NR := -1;
         L_EVENT_TP  := 'InfoFieldValueChanged';
         L_EV_DETAILS := 'sc=' || A_SC(L_SEQ_NO) || 
                         '#ic=' || A_IC(L_SEQ_NO) ||
                         '#icnode=' || TO_CHAR(A_ICNODE(L_SEQ_NO)) || 
                         '#iinode=' || TO_CHAR(A_IINODE(L_SEQ_NO)) || 
                         '#old_value=' || SUBSTR(L_SCIIOLD_REC.IIVALUE,1,40) || 
                         '#new_value=' || SUBSTR(L_SCIINEW_REC.IIVALUE,1,40) ||
                         '#ie_version=' || L_IE_VERSION;
         L_RESULT := UNAPIEV.INSERTINFOFIELDEVENT('SaveScIiValue', UNAPIGEN.P_EVMGR_NAME,
                                                  'ii', A_II(L_SEQ_NO), L_LC, L_LC_VERSION, L_SS,
                                                  L_EVENT_TP, L_EV_DETAILS, L_EV_SEQ_NR);
         IF L_RESULT <> UNAPIGEN.DBERR_SUCCESS THEN
            UNAPIGEN.P_TXN_ERROR := L_RESULT;
            RAISE STPERROR;
         END IF;

         
         IF L_EV_SEQ_NR = -1 THEN
            
            
            OPEN L_SEQ_EVENT_CURSOR;
            FETCH L_SEQ_EVENT_CURSOR INTO L_EV_SEQ_NR;
            CLOSE L_SEQ_EVENT_CURSOR;
         END IF;

         IF L_LOG_HS = '1' THEN
            IF NVL((L_SCIIOLD_REC.IIVALUE <> L_SCIINEW_REC.IIVALUE), TRUE) AND NOT(L_SCIIOLD_REC.IIVALUE IS NULL AND L_SCIINEW_REC.IIVALUE IS NULL) THEN 
               INSERT INTO UTSCICHS(SC, IC, ICNODE, WHO, WHO_DESCRIPTION, WHAT, 
                                    WHAT_DESCRIPTION, LOGDATE, LOGDATE_TZ, WHY, TR_SEQ, EV_SEQ)
               VALUES  (A_SC(L_SEQ_NO), A_IC(L_SEQ_NO), A_ICNODE(L_SEQ_NO), UNAPIGEN.P_USER, 
                        UNAPIGEN.P_USER_DESCRIPTION, L_EVENT_TP, 
                        'info field "'||A_II(L_SEQ_NO)||'" is updated: property <iivalue> changed value from "'||SUBSTR(L_SCIIOLD_REC.IIVALUE,1,40)||'" to "'||SUBSTR(L_SCIINEW_REC.IIVALUE,1,40)||'".',
                        CURRENT_TIMESTAMP, CURRENT_TIMESTAMP, A_MODIFY_REASON, UNAPIGEN.P_TR_SEQ, L_EV_SEQ_NR);
            END IF;
         END IF;

         IF L_LOG_HS_DETAILS = '1' THEN
            UNAPIHSDETAILS.ADDSCIIHSDETAILS(L_SCIIOLD_REC, L_SCIINEW_REC, UNAPIGEN.P_TR_SEQ, 
                                            L_EV_SEQ_NR, L_HS_DETAILS_SEQ_NR); 
         END IF;
      ELSE
         IF L_II_HANDLED THEN
            L_COMPLETELY_SAVED := FALSE;
         END IF;
      END IF;
   END LOOP;

   IF L_II_OK THEN
      
      
      
      OPEN L_SC_CURSOR(A_SC(1));
      FETCH L_SC_CURSOR
      INTO L_SCNEW_REC;
      CLOSE L_SC_CURSOR;

      
      
      
      IF (L_SCNEW_REC.LOG_HS = '1') AND L_LINKED_II THEN
         INSERT INTO UTSCHS(SC, WHO, WHO_DESCRIPTION, WHAT, WHAT_DESCRIPTION, 
                            LOGDATE, LOGDATE_TZ, WHY, TR_SEQ, EV_SEQ)
         VALUES(A_SC(1), UNAPIGEN.P_USER, UNAPIGEN.P_USER_DESCRIPTION, L_EVENT_TP, 
                'sample "'||A_SC(1)||'" is updated.',
                CURRENT_TIMESTAMP, CURRENT_TIMESTAMP, A_MODIFY_REASON, UNAPIGEN.P_TR_SEQ, L_EV_SEQ_NR);
      END IF;
      IF (L_SCNEW_REC.LOG_HS_DETAILS = '1') AND L_LINKED_II THEN
         L_HS_DETAILS_SEQ_NR := L_HS_DETAILS_SEQ_NR + 1;
         INSERT INTO UTSCHSDETAILS(SC, TR_SEQ, EV_SEQ, SEQ, DETAILS)
         VALUES  (A_SC(1), UNAPIGEN.P_TR_SEQ, L_EV_SEQ_NR, L_HS_DETAILS_SEQ_NR, 
                  'sample "'||A_SC(1)||'" is updated.');

         UNAPIHSDETAILS.ADDSCHSDETAILS(L_SCOLD_REC, L_SCNEW_REC, UNAPIGEN.P_TR_SEQ, 
                                       L_EV_SEQ_NR, L_HS_DETAILS_SEQ_NR); 
      END IF;
   END IF;

   
   
   
   
   
   
   L_SCAN_SEQ := 1;
   L_SCAN_PREV_SC := NULL;
   L_SCAN_PREV_IC := NULL;
   L_SCAN_PREV_ICNODE := NULL;
   L_IC_EVENT_INSERTED := FALSE;
   WHILE L_SCAN_SEQ <= A_NR_OF_ROWS LOOP

      OPEN L_SCIC_CURSOR(A_SC(L_SCAN_SEQ), A_IC(L_SCAN_SEQ), A_ICNODE(L_SCAN_SEQ));
      FETCH L_SCIC_CURSOR INTO L_IP_VERSION, L_OLD_NEXT_II, L_IC_LOG_HS, L_IC_LOG_HS_DETAILS;
      IF L_SCIC_CURSOR%NOTFOUND THEN
         UNAPIGEN.P_TXN_ERROR := UNAPIGEN.DBERR_NOOBJECT;
         RAISE STPERROR;
      END IF;
      CLOSE L_SCIC_CURSOR;

      IF NVL(L_SCAN_PREV_SC, ' ') <> NVL(A_SC(L_SCAN_SEQ), ' ') OR
         NVL(L_SCAN_PREV_IC, ' ') <> NVL(A_IC(L_SCAN_SEQ), ' ') OR
         NVL(L_SCAN_PREV_ICNODE, -1) <> NVL(A_ICNODE(L_SCAN_SEQ), -1) THEN

         
         
         OPEN L_NEXT_II_CURSOR(A_SC(L_SCAN_SEQ), A_IC(L_SCAN_SEQ), A_ICNODE(L_SCAN_SEQ));
         FETCH L_NEXT_II_CURSOR
         INTO L_NEXT_II;
         IF L_NEXT_II_CURSOR%NOTFOUND THEN
            L_NEXT_II := NULL;   
         END IF;
         CLOSE L_NEXT_II_CURSOR;

         
         
         UPDATE UTSCIC
         SET NEXT_II = L_NEXT_II
         WHERE SC = A_SC(L_SCAN_SEQ)
           AND IC = A_IC(L_SCAN_SEQ)
           AND ICNODE = A_ICNODE(L_SCAN_SEQ);

         L_SCAN_PREV_SC := A_SC(L_SCAN_SEQ);
         L_SCAN_PREV_IC := A_IC(L_SCAN_SEQ);
         L_SCAN_PREV_ICNODE := A_ICNODE(L_SCAN_SEQ);
         L_IC_EVENT_INSERTED := FALSE;

      END IF;

      IF A_MODIFY_FLAG(L_SCAN_SEQ) <= UNAPIGEN.DBERR_SUCCESS AND NOT L_IC_EVENT_INSERTED THEN
         L_EV_SEQ_NR := -1;
         L_EVENT_TP  := 'InfoFieldValuesChanged';
         L_EV_DETAILS := 'sc=' || A_SC(L_SCAN_SEQ) || 
                         '#icnode=' || TO_CHAR(A_ICNODE(L_SCAN_SEQ)) ||
                         '#ip_version=' || L_IP_VERSION;
         L_RESULT := UNAPIEV.INSERTEVENT('SaveScIiValue', UNAPIGEN.P_EVMGR_NAME,
                                         'ic', A_IC(L_SCAN_SEQ), L_LC, L_LC_VERSION, 
                                         L_SS, L_EVENT_TP, L_EV_DETAILS, L_EV_SEQ_NR);
         IF L_RESULT <> UNAPIGEN.DBERR_SUCCESS THEN
            UNAPIGEN.P_TXN_ERROR := L_RESULT;
            RAISE STPERROR;
         END IF;

         IF L_IC_LOG_HS = '1' THEN
            INSERT INTO UTSCICHS(SC, IC, ICNODE, WHO, WHO_DESCRIPTION, WHAT, 
                                 WHAT_DESCRIPTION, LOGDATE, LOGDATE_TZ, WHY, TR_SEQ, EV_SEQ)
            VALUES  (A_SC(L_SCAN_SEQ), A_IC(L_SCAN_SEQ), A_ICNODE(L_SCAN_SEQ), UNAPIGEN.P_USER, 
                     UNAPIGEN.P_USER_DESCRIPTION, L_EVENT_TP, 
                     'info card "'||A_IC(L_SCAN_SEQ)||'" info field values are updated.',
                     CURRENT_TIMESTAMP, CURRENT_TIMESTAMP, A_MODIFY_REASON, UNAPIGEN.P_TR_SEQ, L_EV_SEQ_NR);
         END IF;

         IF L_IC_LOG_HS_DETAILS = '1' THEN
            L_HS_DETAILS_SEQ_NR := L_HS_DETAILS_SEQ_NR + 1;
            INSERT INTO UTSCICHSDETAILS(SC, IC, ICNODE, TR_SEQ, EV_SEQ, SEQ, DETAILS)
            VALUES  (A_SC(L_SCAN_SEQ), A_IC(L_SCAN_SEQ), A_ICNODE(L_SCAN_SEQ), 
                     UNAPIGEN.P_TR_SEQ, L_EV_SEQ_NR, L_HS_DETAILS_SEQ_NR, 
                     'info card "'||A_IC(L_SCAN_SEQ)||'" info field values are updated.');

            IF NVL((L_OLD_NEXT_II <> L_NEXT_II), TRUE) AND NOT(L_OLD_NEXT_II IS NULL AND L_NEXT_II IS NULL) THEN 
               L_HS_DETAILS_SEQ_NR := L_HS_DETAILS_SEQ_NR + 1;
               INSERT INTO UTSCICHSDETAILS(SC, IC, ICNODE, TR_SEQ, EV_SEQ, SEQ, DETAILS)
               VALUES(A_SC(L_SCAN_SEQ), A_IC(L_SCAN_SEQ), A_ICNODE(L_SCAN_SEQ), UNAPIGEN.P_TR_SEQ, L_EV_SEQ_NR, L_HS_DETAILS_SEQ_NR,
                      'info card "'||A_IC(L_SCAN_SEQ)||'" is updated: property <next_ii> changed value from "'||L_OLD_NEXT_II||'" to "'||L_NEXT_II||'".');
            END IF;
         END IF;

         L_IC_EVENT_INSERTED := TRUE;
      END IF;

      L_SCAN_SEQ := L_SCAN_SEQ + 1;

   END LOOP;

   IF UNAPIGEN.ENDTXN <> UNAPIGEN.DBERR_SUCCESS THEN
      RAISE STPERROR;
   END IF;

   
   
   
   FOR L_SEQ_NO IN 1..A_NR_OF_ROWS LOOP
      IF A_MODIFY_FLAG(L_SEQ_NO) < UNAPIGEN.DBERR_SUCCESS THEN
         A_MODIFY_FLAG(L_SEQ_NO) := UNAPIGEN.DBERR_SUCCESS;
      END IF;
   END LOOP;

   IF L_COMPLETELY_SAVED THEN
      RETURN(UNAPIGEN.DBERR_SUCCESS);
   ELSE
      RETURN(UNAPIGEN.DBERR_PARTIALSAVE);
   END IF;

EXCEPTION
WHEN OTHERS THEN
   IF SQLCODE <> 1 THEN
      UNAPIGEN.LOGERROR('SaveScIiValue',SQLERRM);
   ELSIF L_SQLERRM IS NOT NULL THEN
      UNAPIGEN.LOGERROR('SaveScIiValue',L_SQLERRM);
      IF L_SQLERRM2 IS NOT NULL THEN
         UNAPIGEN.LOGERROR('SaveScIiValue',L_SQLERRM2);
      END IF;      
   END IF;
   IF L_NEXT_II_CURSOR%ISOPEN THEN
      CLOSE L_NEXT_II_CURSOR;
   END IF;
   IF L_IEVALUE_CURSOR%ISOPEN THEN
      CLOSE L_IEVALUE_CURSOR;
   END IF;   
   IF L_SCIIOLD_CURSOR%ISOPEN THEN
      CLOSE L_SCIIOLD_CURSOR;
   END IF;
   IF L_SC_CURSOR%ISOPEN THEN
      CLOSE L_SC_CURSOR;
   END IF;
   IF L_SEQ_EVENT_CURSOR%ISOPEN THEN
      CLOSE L_SEQ_EVENT_CURSOR;
   END IF;
   IF L_IPVERSION_CURSOR%ISOPEN THEN
      CLOSE L_IPVERSION_CURSOR;
   END IF;
   IF L_SCIC_CURSOR%ISOPEN THEN
      CLOSE L_SCIC_CURSOR;
   END IF;
   RETURN(UNAPIGEN.ABORTTXN(UNAPIGEN.P_TXN_ERROR, 'SaveScIiValue'));
END SAVESCIIVALUE;

FUNCTION COPYSCINFOVALUES
(A_SC_FROM        IN        VARCHAR2,                 
 A_IC_FROM        IN        VARCHAR2,                 
 A_ICNODE_FROM    IN        NUMBER,                   
 A_ST_TO          IN        VARCHAR2,                 
 A_ST_TO_VERSION  IN        VARCHAR2,                 
 A_SC_TO          IN        VARCHAR2,                 
 A_IC_TO          IN        VARCHAR2,                 
 A_ICNODE_TO      IN        NUMBER,                   
 A_MODIFY_REASON  IN        VARCHAR2)                 
RETURN NUMBER IS

L_SQLERRM2               VARCHAR2(255);


L_SVIIV_SEQ               INTEGER;
L_SVIIV_SC                UNAPIGEN.VC20_TABLE_TYPE;
L_SVIIV_IC                UNAPIGEN.VC20_TABLE_TYPE;
L_SVIIV_ICNODE            UNAPIGEN.LONG_TABLE_TYPE;
L_SVIIV_II                UNAPIGEN.VC20_TABLE_TYPE;
L_SVIIV_IINODE            UNAPIGEN.LONG_TABLE_TYPE;
L_SVIIV_IIVALUE           UNAPIGEN.VC2000_TABLE_TYPE;
L_SVIIV_MODIFY_FLAG       UNAPIGEN.NUM_TABLE_TYPE;
L_ROW                     INTEGER;
L_IP_VERSION              VARCHAR2(20);

CURSOR L_IP_VERSION_CURSOR(A_SC VARCHAR2, A_IC VARCHAR2, A_ICNODE NUMBER) IS
   SELECT IP_VERSION
   FROM UTSCIC
   WHERE SC = A_SC
   AND IC = A_IC
   AND ICNODE = A_ICNODE;

CURSOR L_ALLSCII_CURSOR(A_SC VARCHAR2, A_IC VARCHAR2, A_ICNODE NUMBER) IS
   SELECT II, IINODE, IIVALUE
   FROM UTSCII
   WHERE SC = A_SC
   AND IC = A_IC
   AND ICNODE = A_ICNODE
   ORDER BY IINODE;










CURSOR L_SCII_POS_CURSOR (A_SC_FROM VARCHAR2, A_IC_FROM VARCHAR2, A_ICNODE_FROM NUMBER,
                          A_SC_TO   VARCHAR2, A_IC_TO   VARCHAR2, A_ICNODE_TO   NUMBER, 
                          A_II VARCHAR2, A_IP_VERSION VARCHAR2) IS
SELECT A.II_TO, A.IINODE_TO, B.II_FROM, B.IINODE_FROM, B.IIVALUE 
FROM (SELECT II II_TO, IINODE IINODE_TO, ROWNUM POSITION
      FROM (SELECT II,IINODE FROM UTSCII
            WHERE SC=A_SC_TO 
            AND IC=A_IC_TO
            AND ICNODE=A_ICNODE_TO
            AND II=A_II         
            GROUP BY II,IINODE
            ORDER BY II,IINODE)) A,
     (SELECT II II_FROM, IINODE IINODE_FROM, ROWNUM POSITION, IIVALUE
      FROM (SELECT II,IINODE, IIVALUE FROM UTSCII
            WHERE SC=A_SC_FROM 
            AND IC=A_IC_FROM
            AND ICNODE=A_ICNODE_FROM
            AND II=A_II         
            GROUP BY II,IINODE, IIVALUE
            ORDER BY II,IINODE, IIVALUE)) B
WHERE A.POSITION = B.POSITION(+)
AND A.II_TO NOT IN (SELECT A.II
                    FROM UTSCII A, UTIE B
                    WHERE A.SC = A_SC_TO
                      AND A.IC = A_IC_TO
                      AND A.ICNODE = A_ICNODE_TO
                      AND A.II = B.IE
                      AND A.IE_VERSION = B.VERSION
                      AND B.DEF_VAL_TP = 'S'
                      AND B.IEVALUE IS NOT NULL
                      AND (B.IE,B.VERSION) NOT IN 
                         (SELECT C.IE, UNAPIGEN.VALIDATEVERSION('ie',C.IE,C.IE_VERSION)
                          FROM UTIPIE C
                          WHERE C.IP = A.IC
                            AND C.VERSION = A_IP_VERSION
                            AND C.IE = A.II
                            AND UNAPIGEN.VALIDATEVERSION('ie',C.IE,C.IE_VERSION) = A.IE_VERSION
                            AND (C.DEF_VAL_TP<>'F' OR C.IEVALUE IS NOT NULL))
                    UNION 
                    SELECT A.II
                    FROM UTSCII A, UTIPIE B
                    WHERE A.SC = A_SC_TO
                      AND A.IC = A_IC_TO
                      AND A.ICNODE = A_ICNODE_TO
                      AND A.IC = B.IP
                      AND A_IP_VERSION = B.VERSION
                      AND A.II = B.IE
                      AND A.IE_VERSION = UNAPIGEN.VALIDATEVERSION('ie',B.IE,B.IE_VERSION)
                      AND B.DEF_VAL_TP = 'S'
                      AND B.IEVALUE IS NOT NULL);




L_SCII_POS_REC     L_SCII_POS_CURSOR%ROWTYPE;    
L_SCII_POS_FOUND   BOOLEAN;    


BEGIN

   L_SQLERRM := NULL;
   L_SQLERRM2 := NULL;
   L_SVIIV_SEQ := 0;
   IF UNAPIGEN.BEGINTXN(UNAPIGEN.P_SINGLE_API_TXN) <> UNAPIGEN.DBERR_SUCCESS THEN
      RAISE STPERROR;
   END IF;
   
   
   OPEN L_IP_VERSION_CURSOR(A_SC_TO, A_IC_TO, A_ICNODE_TO);
   FETCH L_IP_VERSION_CURSOR INTO L_IP_VERSION;
   CLOSE L_IP_VERSION_CURSOR;

   
   FOR L_ALLSCII_REC IN L_ALLSCII_CURSOR(A_SC_TO, A_IC_TO, A_ICNODE_TO) LOOP
      
      

      L_SCII_POS_FOUND := FALSE;
      OPEN L_SCII_POS_CURSOR(A_SC_FROM, A_IC_FROM, A_ICNODE_FROM, A_SC_TO, A_IC_TO, A_ICNODE_TO, 
                             L_ALLSCII_REC.II, L_IP_VERSION);
      LOOP
         FETCH L_SCII_POS_CURSOR
         INTO L_SCII_POS_REC;
         IF L_SCII_POS_REC.II_TO = L_ALLSCII_REC.II AND
            L_SCII_POS_REC.IINODE_TO = L_ALLSCII_REC.IINODE THEN
            L_SCII_POS_FOUND := TRUE;
            EXIT;
         END IF;
         EXIT WHEN L_SCII_POS_CURSOR%NOTFOUND;
      END LOOP;
      CLOSE L_SCII_POS_CURSOR;

      IF L_SCII_POS_FOUND AND 
         L_SCII_POS_REC.II_FROM IS NOT NULL AND
         NVL(L_SCII_POS_REC.IIVALUE,' ') <> NVL(L_ALLSCII_REC.IIVALUE,' ') THEN
         
         
         L_SVIIV_SEQ := L_SVIIV_SEQ + 1;
         L_SVIIV_SC(L_SVIIV_SEQ) := A_SC_TO;
         L_SVIIV_IC(L_SVIIV_SEQ) := A_IC_TO;
         L_SVIIV_ICNODE(L_SVIIV_SEQ) := A_ICNODE_TO;
         L_SVIIV_II(L_SVIIV_SEQ) := L_SCII_POS_REC.II_TO;
         L_SVIIV_IINODE(L_SVIIV_SEQ) := L_SCII_POS_REC.IINODE_TO;
         L_SVIIV_IIVALUE(L_SVIIV_SEQ) := L_SCII_POS_REC.IIVALUE;
         L_SVIIV_MODIFY_FLAG(L_SVIIV_SEQ) := UNAPIGEN.MOD_FLAG_UPDATE;
      END IF;
   END LOOP;         
   
   
   
   
   
   IF L_SVIIV_SEQ <> 0 THEN
      L_RET_CODE := UNAPIIC.SAVESCIIVALUE(L_SVIIV_SC, L_SVIIV_IC, L_SVIIV_ICNODE, L_SVIIV_II, 
                                          L_SVIIV_IINODE, L_SVIIV_IIVALUE, L_SVIIV_MODIFY_FLAG,
                                          L_SVIIV_SEQ, A_MODIFY_REASON);
      IF L_RET_CODE = UNAPIGEN.DBERR_PARTIALSAVE THEN
         
         
         
         FOR L_ROW IN 1..L_SVIIV_SEQ LOOP
            IF L_SVIIV_MODIFY_FLAG(L_ROW) > UNAPIGEN.DBERR_SUCCESS THEN
               UNAPIGEN.P_TXN_ERROR := L_SVIIV_MODIFY_FLAG(L_ROW);
               L_SQLERRM := 'sc=' || L_SVIIV_SC(L_ROW) || 
                            '#ic=' || L_SVIIV_IC(L_ROW) ||
                            '#icnode=' || TO_CHAR(L_SVIIV_ICNODE(L_ROW)) ||
                            '#ii=' || L_SVIIV_II(L_ROW)||
                            '#iinode=' || TO_CHAR(L_SVIIV_IINODE(L_ROW)) ||
                            '#SaveScIiValue#ErrorCode=' || TO_CHAR(L_RET_CODE);
               IF UNAPIAUT.P_NOT_AUTHORISED IS NOT NULL THEN
                  L_SQLERRM2 := UNAPIAUT.P_NOT_AUTHORISED;
               END IF;
               RAISE STPERROR;
            END IF;
         END LOOP;
      ELSIF L_RET_CODE <> UNAPIGEN.DBERR_SUCCESS THEN
         UNAPIGEN.P_TXN_ERROR := L_RET_CODE;
         L_SQLERRM := 'sc(1)=' || L_SVIIV_SC(1) || '#ic(1)=' || L_SVIIV_IC(1) ||
                      '#icnode(1)=' || TO_CHAR(L_SVIIV_ICNODE(1)) ||
                      '#ii(1)=' || L_SVIIV_II(1)||
                      '#iinode(1)=' || TO_CHAR(L_SVIIV_IINODE(1)) ||
                      '#nr_of_rows=' || TO_CHAR(L_SVIIV_SEQ) ||
                      '#SaveScIiValue#ErrorCode=' || TO_CHAR(L_RET_CODE);
         RAISE STPERROR;
      END IF;                           
   END IF;

   IF UNAPIGEN.ENDTXN <> UNAPIGEN.DBERR_SUCCESS THEN
      RAISE STPERROR;
   END IF;

   RETURN(UNAPIGEN.DBERR_SUCCESS);

EXCEPTION
WHEN OTHERS THEN
   IF SQLCODE <> 1 THEN
      UNAPIGEN.LOGERROR('CopyScInfoValues',SQLERRM);
   ELSIF L_SQLERRM IS NOT NULL THEN
      UNAPIGEN.LOGERROR('CopyScInfoValues',L_SQLERRM);
      IF L_SQLERRM2 IS NOT NULL THEN
         UNAPIGEN.LOGERROR('SaveScIiValue',L_SQLERRM2);
      END IF;      
   END IF;
   IF L_SCII_POS_CURSOR%ISOPEN THEN
      CLOSE L_SCII_POS_CURSOR;
   END IF;   
   IF L_IP_VERSION_CURSOR%ISOPEN THEN
      CLOSE L_IP_VERSION_CURSOR;
   END IF;   
   RETURN(UNAPIGEN.ABORTTXN(UNAPIGEN.P_TXN_ERROR, 'CopyScInfoValues'));
END COPYSCINFOVALUES;

FUNCTION CREATESCINFODETAILS
(A_ST             IN        VARCHAR2,                 
 A_ST_VERSION     IN OUT    VARCHAR2,                 
 A_SC             IN        VARCHAR2,                 
 A_FILTER_FREQ    IN        CHAR,                     
 A_REF_DATE       IN        DATE,                     
 A_MODIFY_REASON  IN        VARCHAR2)                 
RETURN NUMBER IS

L_SC                 UNAPIGEN.VC20_TABLE_TYPE;
L_IC                 UNAPIGEN.VC20_TABLE_TYPE;
L_ICNODE             UNAPIGEN.LONG_TABLE_TYPE;
L_IP_VERSION         UNAPIGEN.VC20_TABLE_TYPE;
L_DESCRIPTION        UNAPIGEN.VC40_TABLE_TYPE;
L_WINSIZE_X          UNAPIGEN.NUM_TABLE_TYPE;
L_WINSIZE_Y          UNAPIGEN.NUM_TABLE_TYPE;
L_IS_PROTECTED       UNAPIGEN.CHAR1_TABLE_TYPE;
L_HIDDEN             UNAPIGEN.CHAR1_TABLE_TYPE;
L_MANUALLY_ADDED     UNAPIGEN.CHAR1_TABLE_TYPE;
L_NEXT_II            UNAPIGEN.VC20_TABLE_TYPE;
L_IC_CLASS           UNAPIGEN.VC2_TABLE_TYPE;
L_LOG_HS             UNAPIGEN.CHAR1_TABLE_TYPE;
L_LOG_HS_DETAILS     UNAPIGEN.CHAR1_TABLE_TYPE;
L_LC                 UNAPIGEN.VC2_TABLE_TYPE;
L_LC_VERSION         UNAPIGEN.VC20_TABLE_TYPE;
L_MODIFY_FLAG        UNAPIGEN.NUM_TABLE_TYPE;
L_NR_OF_ROWS         NUMBER;
L_REF_DATE           TIMESTAMP WITH TIME ZONE;
L_FILTER_FREQ        CHAR(1);
L_ERRM               VARCHAR2(255);
L_SEQ                NUMBER(5);
L_LOG_HS_SC          CHAR(1);
L_LOG_HS_DETAILS_SC  CHAR(1);
L_HS_DETAILS_SEQ_NR  INTEGER;

BEGIN

   IF UNAPIGEN.BEGINTXN(UNAPIGEN.P_SINGLE_API_TXN) <> UNAPIGEN.DBERR_SUCCESS THEN
      RAISE STPERROR;
   END IF;

   IF NVL(A_SC, ' ') = ' ' OR
      NVL(A_ST, ' ') = ' ' THEN
      UNAPIGEN.P_TXN_ERROR := UNAPIGEN.DBERR_NOOBJID;
      RAISE STPERROR;
   END IF;

   IF A_REF_DATE IS NULL THEN
      L_REF_DATE := CURRENT_TIMESTAMP;
   ELSE
      L_REF_DATE := A_REF_DATE;
   END IF;

   L_ERRM := NULL;
   L_FILTER_FREQ := NVL(A_FILTER_FREQ, '1');

   
   A_ST_VERSION := UNAPIGEN.VALIDATEVERSION('st', A_ST, A_ST_VERSION);
   
   
   
   
   L_RET_CODE := UNAPIIC.INITSCINFODETAILS(A_ST, A_ST_VERSION, A_SC, L_FILTER_FREQ, L_REF_DATE, 
                                           L_IC, L_IP_VERSION, L_DESCRIPTION, L_WINSIZE_X,
                                           L_WINSIZE_Y, L_IS_PROTECTED, L_HIDDEN, L_MANUALLY_ADDED, 
                                           L_NEXT_II, L_IC_CLASS, L_LOG_HS, L_LOG_HS_DETAILS,
                                           L_LC, L_LC_VERSION, L_NR_OF_ROWS);
   IF L_RET_CODE <> UNAPIGEN.DBERR_SUCCESS THEN
      UNAPIGEN.P_TXN_ERROR := L_RET_CODE;
      L_ERRM := 'sc=' || A_SC || 
                '#st=' || A_ST || '#st_version=' || A_ST_VERSION ||
                '#filter_freq=' || L_FILTER_FREQ ||
                '#ref_date=' || L_REF_DATE ||
                '#nr_of_rows=' || L_NR_OF_ROWS ||
                '#InitScInfoDetails#ErrorCode=' || TO_CHAR(L_RET_CODE);
      RAISE STPERROR;
   END IF;

   
   
   
   FOR L_ROW IN 1..L_NR_OF_ROWS LOOP
      L_SC(L_ROW) := A_SC;
      L_MODIFY_FLAG(L_ROW) := UNAPIGEN.MOD_FLAG_INSERT;
      L_ICNODE(L_ROW) := 0;
   END LOOP;

   
   
   
   IF L_NR_OF_ROWS > 0 THEN
      L_RET_CODE := UNAPIIC.SAVESCINFOCARD(L_SC, L_IC, L_ICNODE, L_IP_VERSION, L_DESCRIPTION,
                                           L_WINSIZE_X, L_WINSIZE_Y,
                                           L_IS_PROTECTED,
                                           L_HIDDEN, L_MANUALLY_ADDED, L_NEXT_II, 
                                           L_IC_CLASS, L_LOG_HS, L_LOG_HS_DETAILS, 
                                           L_LC, L_LC_VERSION, L_MODIFY_FLAG,
                                           L_NR_OF_ROWS, A_MODIFY_REASON);
      IF L_RET_CODE = UNAPIGEN.DBERR_PARTIALSAVE THEN
         
         
         
         FOR L_ROW IN 1..L_NR_OF_ROWS LOOP
            IF L_MODIFY_FLAG(L_ROW) > UNAPIGEN.DBERR_SUCCESS THEN
               UNAPIGEN.P_TXN_ERROR := L_MODIFY_FLAG(L_ROW);
               L_ERRM := 'sc=' || L_SC(L_ROW) ||
                          '#ic=' || L_IC(L_ROW) ||
                          '#icnode=' || TO_CHAR(L_ICNODE(L_ROW)) ||
                          '#SaveScInfoCard#ErrorCode='||TO_CHAR(L_RET_CODE);
               RAISE STPERROR;
            END IF;
         END LOOP;
      ELSIF L_RET_CODE <> UNAPIGEN.DBERR_SUCCESS THEN
         UNAPIGEN.P_TXN_ERROR := L_RET_CODE;
         RAISE STPERROR;
      END IF;
   END IF;

   
   
   
   FOR L_ROW IN 1..L_NR_OF_ROWS LOOP

      
      
      
      L_RET_CODE := UNAPIICP.INITANDSAVESCICATTRIBUTES(A_SC, L_IC(L_ROW), L_ICNODE(L_ROW));
      
      
      
      IF L_RET_CODE <> UNAPIGEN.DBERR_SUCCESS THEN
         L_ERRM := 'sc=' || A_SC|| 
                   '#ic=' || L_IC(L_ROW) ||
                   '#icnode=' || TO_CHAR(L_ICNODE(L_ROW)) ||
                   '#InitAndSaveScIcAttributes#ErrorCode=' || TO_CHAR(L_RET_CODE);
         UNAPIGEN.P_TXN_ERROR := L_RET_CODE;
         RAISE STPERROR;
      END IF;

      
      
      
      L_RET_CODE := UNAPIIC.CREATESCICDETAILS(A_ST, A_ST_VERSION, L_IC(L_ROW), L_IP_VERSION(L_ROW), 
                                              L_SEQ, A_SC, L_ICNODE(L_ROW), A_MODIFY_REASON);

      
      
      
      IF L_RET_CODE <> UNAPIGEN.DBERR_SUCCESS THEN
         L_ERRM := 'sc=' || A_SC|| 
                   '#st='|| A_ST ||
                   '#st_version='|| A_ST_VERSION ||
                   '#ic=' || L_IC(L_ROW) ||
                   '#icnode=' || TO_CHAR(L_ICNODE(L_ROW)) ||
                   '#CreateScIcDetails#ErrorCode=' || TO_CHAR(L_RET_CODE);
         UNAPIGEN.P_TXN_ERROR := L_RET_CODE;
         RAISE STPERROR;
      END IF;
   END LOOP;

   
   
   
   L_EV_SEQ_NR := -1;
   L_EV_DETAILS := 'st=' || A_ST ||
                   '#st_version=' || A_ST_VERSION;
   L_EVENT_TP := 'ScInfoCreated';
   L_RESULT := UNAPIEV.INSERTEVENT('CreateScInfoDetails', UNAPIGEN.P_EVMGR_NAME, 'sc', A_SC, 
                                   '', '', '', L_EVENT_TP, L_EV_DETAILS, L_EV_SEQ_NR);
   IF L_RESULT <> UNAPIGEN.DBERR_SUCCESS THEN
      UNAPIGEN.P_TXN_ERROR := L_RESULT;
      RAISE STPERROR;
   END IF;

   BEGIN
      SELECT LOG_HS, LOG_HS_DETAILS
      INTO L_LOG_HS_SC, L_LOG_HS_DETAILS_SC
      FROM UTSC
      WHERE SC = A_SC;
   EXCEPTION
   WHEN NO_DATA_FOUND THEN
      UNAPIGEN.P_TXN_ERROR := UNAPIGEN.DBERR_NOOBJECT;
      RAISE STPERROR;
   END;

   IF L_LOG_HS_SC = '1' THEN
      INSERT INTO UTSCHS(SC, WHO, WHO_DESCRIPTION, WHAT, 
                         WHAT_DESCRIPTION, LOGDATE, LOGDATE_TZ, WHY, TR_SEQ, EV_SEQ)
      VALUES  (A_SC, UNAPIGEN.P_USER, UNAPIGEN.P_USER_DESCRIPTION, L_EVENT_TP, 
               'Info cards for sample "'||A_SC||'" are created',
               CURRENT_TIMESTAMP, CURRENT_TIMESTAMP, A_MODIFY_REASON, UNAPIGEN.P_TR_SEQ, L_EV_SEQ_NR);
   END IF;
   
   L_HS_DETAILS_SEQ_NR := 0;
   IF L_LOG_HS_DETAILS_SC = '1' THEN
      L_HS_DETAILS_SEQ_NR := L_HS_DETAILS_SEQ_NR + 1;
      INSERT INTO UTSCHSDETAILS(SC, TR_SEQ, EV_SEQ, SEQ, DETAILS)
      VALUES  (A_SC, UNAPIGEN.P_TR_SEQ, L_EV_SEQ_NR, L_HS_DETAILS_SEQ_NR, 
               'Info cards for sample "'||A_SC||'" are created');
   END IF;

   IF UNAPIGEN.ENDTXN <> UNAPIGEN.DBERR_SUCCESS THEN
      RAISE STPERROR;
   END IF;

   RETURN(UNAPIGEN.DBERR_SUCCESS);

EXCEPTION
WHEN OTHERS THEN
   IF SQLCODE <> 1 THEN
      UNAPIGEN.LOGERROR('CreateScInfoDetails', SQLERRM);
   ELSIF L_ERRM IS NOT NULL THEN
      UNAPIGEN.LOGERROR('CreateScInfoDetails', L_ERRM);
   END IF;
   RETURN(UNAPIGEN.ABORTTXN(UNAPIGEN.P_TXN_ERROR,'CreateScInfoDetails'));
END CREATESCINFODETAILS;

