      DBMS_SQL.COLUMN_VALUE(P_HS_DETAILS_CURSOR, 4, L_TR_SEQ);
      DBMS_SQL.COLUMN_VALUE(P_HS_DETAILS_CURSOR, 5, L_EV_SEQ);
