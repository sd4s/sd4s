PACKAGE BODY unapirqic AS

L_SQLERRM         VARCHAR2(255);
L_SQL_STRING      VARCHAR2(2000);
L_WHERE_CLAUSE    VARCHAR2(1000);
L_EVENT_TP        UTEV.EV_TP%TYPE;
L_RET_CODE        NUMBER;
L_RESULT          NUMBER;
L_FETCHED_ROWS    NUMBER;
L_EV_SEQ_NR       NUMBER;
L_EV_DETAILS      VARCHAR2(255);
L_ERRM            VARCHAR2(255);
STPERROR          EXCEPTION;

P_RQII_CURSOR     INTEGER;

CURSOR P_IPIE_CURSOR(C_IP VARCHAR2, C_IP_VERSION VARCHAR2) IS
   SELECT A.*
   FROM UTIPIE A
   WHERE A.IP = C_IP
   AND A.VERSION = C_IP_VERSION
   ORDER BY A.SEQ;

CURSOR L_SEQ_EVENT_CURSOR IS
   SELECT SEQ_EVENT_NR.NEXTVAL FROM DUAL;

FUNCTION GETVERSION
   RETURN VARCHAR2
IS
BEGIN
   RETURN('06.07.00.00_00.13');
EXCEPTION
   WHEN OTHERS THEN
      RETURN (NULL);
END GETVERSION;

FUNCTION GETRQINFOCARD                                
(A_RQ             OUT      UNAPIGEN.VC20_TABLE_TYPE,  
 A_IC             OUT      UNAPIGEN.VC20_TABLE_TYPE,  
 A_ICNODE         OUT      UNAPIGEN.LONG_TABLE_TYPE,  
 A_IP_VERSION     OUT      UNAPIGEN.VC20_TABLE_TYPE,  
 A_DESCRIPTION    OUT      UNAPIGEN.VC40_TABLE_TYPE,  
 A_WINSIZE_X      OUT      UNAPIGEN.NUM_TABLE_TYPE,   
 A_WINSIZE_Y      OUT      UNAPIGEN.NUM_TABLE_TYPE,   
 A_IS_PROTECTED   OUT      UNAPIGEN.CHAR1_TABLE_TYPE, 
 A_HIDDEN         OUT      UNAPIGEN.CHAR1_TABLE_TYPE, 
 A_MANUALLY_ADDED OUT      UNAPIGEN.CHAR1_TABLE_TYPE, 
 A_NEXT_II        OUT      UNAPIGEN.VC20_TABLE_TYPE,  
 A_IC_CLASS       OUT      UNAPIGEN.VC2_TABLE_TYPE,   
 A_LOG_HS         OUT      UNAPIGEN.CHAR1_TABLE_TYPE, 
 A_LOG_HS_DETAILS OUT      UNAPIGEN.CHAR1_TABLE_TYPE, 
 A_ALLOW_MODIFY   OUT      UNAPIGEN.CHAR1_TABLE_TYPE, 
 A_AR             OUT      UNAPIGEN.CHAR1_TABLE_TYPE, 
 A_ACTIVE         OUT      UNAPIGEN.CHAR1_TABLE_TYPE, 
 A_LC             OUT      UNAPIGEN.VC2_TABLE_TYPE,   
 A_LC_VERSION     OUT      UNAPIGEN.VC20_TABLE_TYPE,  
 A_SS             OUT      UNAPIGEN.VC2_TABLE_TYPE,   
 A_NR_OF_ROWS     IN OUT   NUMBER,                    
 A_WHERE_CLAUSE   IN       VARCHAR2)                  
RETURN NUMBER IS

L_RQ             VARCHAR2(20);
L_IC             VARCHAR2(20);
L_ICNODE         NUMBER(9);
L_IP_VERSION  VARCHAR2(20); 
L_DESCRIPTION    VARCHAR2(40);
L_WINSIZE_X      NUMBER(4);
L_WINSIZE_Y      NUMBER(4);
L_IS_PROTECTED   CHAR(1);
L_HIDDEN         CHAR(1);
L_MANUALLY_ADDED CHAR(1);
L_NEXT_II        VARCHAR2(20);
L_IC_CLASS       VARCHAR2(2);
L_LOG_HS         CHAR(1);
L_LOG_HS_DETAILS CHAR(1);
L_ALLOW_MODIFY   CHAR(1);
L_AR             CHAR(1);
L_ACTIVE         CHAR(1);
L_LC             VARCHAR2(2);
L_LC_VERSION     VARCHAR2(20);
L_SS             VARCHAR2(2);
L_IC_CURSOR      INTEGER;

BEGIN

   IF NVL(A_NR_OF_ROWS,0) = 0 THEN
      A_NR_OF_ROWS := UNAPIGEN.P_DEFAULT_CHUNK_SIZE;
   ELSIF A_NR_OF_ROWS < 0 OR A_NR_OF_ROWS > UNAPIGEN.P_MAX_CHUNK_SIZE THEN
      RETURN(UNAPIGEN.DBERR_NROFROWS);
   END IF;

   IF NVL(A_WHERE_CLAUSE, ' ') = ' ' THEN
       RETURN(UNAPIGEN.DBERR_WHERECLAUSE);
   ELSIF
     UPPER(SUBSTR(A_WHERE_CLAUSE,1,6)) <> 'WHERE ' THEN
     L_WHERE_CLAUSE := 'WHERE rq = ''' || REPLACE(A_WHERE_CLAUSE, '''', '''''') || 
                       ''' ORDER BY icnode';
   ELSE
      L_WHERE_CLAUSE := A_WHERE_CLAUSE; 
   END IF;

   L_IC_CURSOR := DBMS_SQL.OPEN_CURSOR;
   L_SQL_STRING := 'SELECT rq, ic, icnode, ip_version, description, winsize_x, winsize_y,'||
                   'is_protected, hidden, manually_added, next_ii, ic_class,' ||
                   'log_hs, log_hs_details, allow_modify, ar, active, lc, lc_version, ss ' ||
                   'FROM dd' || UNAPIGEN.P_DD || '.uvrqic ' || L_WHERE_CLAUSE;

   DBMS_SQL.PARSE(L_IC_CURSOR, L_SQL_STRING, DBMS_SQL.V7); 

   DBMS_SQL.DEFINE_COLUMN(L_IC_CURSOR, 1, L_RQ, 20);
   DBMS_SQL.DEFINE_COLUMN(L_IC_CURSOR, 2, L_IC, 20);
   DBMS_SQL.DEFINE_COLUMN(L_IC_CURSOR, 3, L_ICNODE);
   DBMS_SQL.DEFINE_COLUMN(L_IC_CURSOR, 4, L_IP_VERSION, 20);
   DBMS_SQL.DEFINE_COLUMN(L_IC_CURSOR, 5, L_DESCRIPTION, 40);
   DBMS_SQL.DEFINE_COLUMN(L_IC_CURSOR, 6, L_WINSIZE_X);
   DBMS_SQL.DEFINE_COLUMN(L_IC_CURSOR, 7, L_WINSIZE_Y);
   DBMS_SQL.DEFINE_COLUMN_CHAR(L_IC_CURSOR, 8, L_IS_PROTECTED, 1);
   DBMS_SQL.DEFINE_COLUMN_CHAR(L_IC_CURSOR, 9, L_HIDDEN, 1);
   DBMS_SQL.DEFINE_COLUMN_CHAR(L_IC_CURSOR, 10, L_MANUALLY_ADDED, 1);
   DBMS_SQL.DEFINE_COLUMN(L_IC_CURSOR, 11, L_NEXT_II, 20);
   DBMS_SQL.DEFINE_COLUMN(L_IC_CURSOR, 12, L_IC_CLASS, 2);
   DBMS_SQL.DEFINE_COLUMN_CHAR(L_IC_CURSOR, 13, L_LOG_HS, 1);
   DBMS_SQL.DEFINE_COLUMN_CHAR(L_IC_CURSOR, 14, L_LOG_HS_DETAILS, 1);
   DBMS_SQL.DEFINE_COLUMN_CHAR(L_IC_CURSOR, 15, L_ALLOW_MODIFY, 1);
   DBMS_SQL.DEFINE_COLUMN_CHAR(L_IC_CURSOR, 16, L_AR, 1);
   DBMS_SQL.DEFINE_COLUMN_CHAR(L_IC_CURSOR, 17, L_ACTIVE, 1);
   DBMS_SQL.DEFINE_COLUMN(L_IC_CURSOR, 18, L_LC, 2);
   DBMS_SQL.DEFINE_COLUMN(L_IC_CURSOR, 19, L_LC_VERSION, 20);
   DBMS_SQL.DEFINE_COLUMN(L_IC_CURSOR, 20, L_SS, 2);
   L_RESULT := DBMS_SQL.EXECUTE_AND_FETCH(L_IC_CURSOR);
   L_FETCHED_ROWS := 0;

   LOOP

      EXIT WHEN L_RESULT = 0 OR L_FETCHED_ROWS >= A_NR_OF_ROWS;

      DBMS_SQL.COLUMN_VALUE(L_IC_CURSOR, 1, L_RQ);
      DBMS_SQL.COLUMN_VALUE(L_IC_CURSOR, 2, L_IC);
      DBMS_SQL.COLUMN_VALUE(L_IC_CURSOR, 3, L_ICNODE);
      DBMS_SQL.COLUMN_VALUE(L_IC_CURSOR, 4, L_IP_VERSION);
      DBMS_SQL.COLUMN_VALUE(L_IC_CURSOR, 5, L_DESCRIPTION);
      DBMS_SQL.COLUMN_VALUE(L_IC_CURSOR, 6, L_WINSIZE_X);
      DBMS_SQL.COLUMN_VALUE(L_IC_CURSOR, 7, L_WINSIZE_Y);
      DBMS_SQL.COLUMN_VALUE_CHAR(L_IC_CURSOR, 8, L_IS_PROTECTED);
      DBMS_SQL.COLUMN_VALUE_CHAR(L_IC_CURSOR, 9, L_HIDDEN);
      DBMS_SQL.COLUMN_VALUE_CHAR(L_IC_CURSOR, 10, L_MANUALLY_ADDED);
      DBMS_SQL.COLUMN_VALUE(L_IC_CURSOR, 11, L_NEXT_II);
      DBMS_SQL.COLUMN_VALUE(L_IC_CURSOR, 12, L_IC_CLASS);
      DBMS_SQL.COLUMN_VALUE_CHAR(L_IC_CURSOR, 13, L_LOG_HS);
      DBMS_SQL.COLUMN_VALUE_CHAR(L_IC_CURSOR, 14, L_LOG_HS_DETAILS);
      DBMS_SQL.COLUMN_VALUE_CHAR(L_IC_CURSOR, 15, L_ALLOW_MODIFY);
      DBMS_SQL.COLUMN_VALUE_CHAR(L_IC_CURSOR, 16, L_AR);
      DBMS_SQL.COLUMN_VALUE_CHAR(L_IC_CURSOR, 17, L_ACTIVE);
      DBMS_SQL.COLUMN_VALUE(L_IC_CURSOR, 18, L_LC);
      DBMS_SQL.COLUMN_VALUE(L_IC_CURSOR, 19, L_LC_VERSION);
      DBMS_SQL.COLUMN_VALUE(L_IC_CURSOR, 20, L_SS);

      L_FETCHED_ROWS := L_FETCHED_ROWS + 1;
      A_RQ(L_FETCHED_ROWS) := L_RQ;
      A_IC(L_FETCHED_ROWS) := L_IC;
      A_ICNODE(L_FETCHED_ROWS) := L_ICNODE;
      A_IP_VERSION(L_FETCHED_ROWS) := L_IP_VERSION;
      A_DESCRIPTION(L_FETCHED_ROWS) := L_DESCRIPTION;
      A_WINSIZE_X(L_FETCHED_ROWS) := L_WINSIZE_X;
      A_WINSIZE_Y(L_FETCHED_ROWS) := L_WINSIZE_Y;
      A_IS_PROTECTED(L_FETCHED_ROWS) := L_IS_PROTECTED;
      A_HIDDEN(L_FETCHED_ROWS) := L_HIDDEN;
      A_MANUALLY_ADDED(L_FETCHED_ROWS) := L_MANUALLY_ADDED;
      A_NEXT_II(L_FETCHED_ROWS) := L_NEXT_II;
      A_IC_CLASS(L_FETCHED_ROWS) := L_IC_CLASS;
      A_LOG_HS(L_FETCHED_ROWS) := L_LOG_HS;
      A_LOG_HS_DETAILS(L_FETCHED_ROWS) := L_LOG_HS_DETAILS;
      A_ALLOW_MODIFY(L_FETCHED_ROWS) := L_ALLOW_MODIFY;
      A_AR(L_FETCHED_ROWS) := L_AR;
      A_ACTIVE(L_FETCHED_ROWS) := L_ACTIVE;
      A_LC(L_FETCHED_ROWS) := L_LC;
      A_LC_VERSION(L_FETCHED_ROWS) := L_LC_VERSION;
      A_SS(L_FETCHED_ROWS) := L_SS;

      IF L_FETCHED_ROWS < A_NR_OF_ROWS THEN
         L_RESULT := DBMS_SQL.FETCH_ROWS(L_IC_CURSOR);
      END IF;

   END LOOP;

   DBMS_SQL.CLOSE_CURSOR(L_IC_CURSOR);

   IF L_FETCHED_ROWS = 0 THEN
      L_RET_CODE := UNAPIGEN.DBERR_NORECORDS;
   ELSE
      A_NR_OF_ROWS := L_FETCHED_ROWS;
      L_RET_CODE := UNAPIGEN.DBERR_SUCCESS;
   END IF;

   RETURN(L_RET_CODE);

EXCEPTION
   WHEN OTHERS THEN
      L_SQLERRM := SQLERRM;
      UNAPIGEN.U4ROLLBACK;
      INSERT INTO UTERROR(CLIENT_ID, APPLIC, WHO, LOGDATE, LOGDATE_TZ, API_NAME, ERROR_MSG)
      VALUES(UNAPIGEN.P_CLIENT_ID, UNAPIGEN.P_APPLIC_NAME, UNAPIGEN.P_USER, CURRENT_TIMESTAMP, CURRENT_TIMESTAMP,
             'GetRqInfoCard', L_SQLERRM);
      UNAPIGEN.U4COMMIT;
      IF DBMS_SQL.IS_OPEN(L_IC_CURSOR) THEN
         DBMS_SQL.CLOSE_CURSOR(L_IC_CURSOR);
      END IF;
      RETURN(UNAPIGEN.DBERR_GENFAIL);
END GETRQINFOCARD;

FUNCTION GETRQINFOFIELD                                 
(A_RQ               OUT    UNAPIGEN.VC20_TABLE_TYPE,    
 A_IC               OUT    UNAPIGEN.VC20_TABLE_TYPE,    
 A_ICNODE           OUT    UNAPIGEN.LONG_TABLE_TYPE,    
 A_II               OUT    UNAPIGEN.VC20_TABLE_TYPE,    
 A_IINODE           OUT    UNAPIGEN.LONG_TABLE_TYPE,    
 A_IE_VERSION       OUT    UNAPIGEN.VC20_TABLE_TYPE,    
 A_IIVALUE          OUT    UNAPIGEN.VC2000_TABLE_TYPE,  
 A_POS_X            OUT    UNAPIGEN.NUM_TABLE_TYPE,     
 A_POS_Y            OUT    UNAPIGEN.NUM_TABLE_TYPE,     
 A_IS_PROTECTED     OUT    UNAPIGEN.CHAR1_TABLE_TYPE,   
 A_MANDATORY        OUT    UNAPIGEN.CHAR1_TABLE_TYPE,   
 A_HIDDEN           OUT    UNAPIGEN.CHAR1_TABLE_TYPE,   
 A_DSP_TITLE        OUT    UNAPIGEN.VC40_TABLE_TYPE,    
 A_DSP_LEN          OUT    UNAPIGEN.NUM_TABLE_TYPE,     
 A_DSP_TP           OUT    UNAPIGEN.CHAR1_TABLE_TYPE,   
 A_DSP_ROWS         OUT    UNAPIGEN.NUM_TABLE_TYPE,     
 A_II_CLASS         OUT    UNAPIGEN.VC2_TABLE_TYPE,     
 A_LOG_HS           OUT    UNAPIGEN.CHAR1_TABLE_TYPE,   
 A_LOG_HS_DETAILS   OUT    UNAPIGEN.CHAR1_TABLE_TYPE,   
 A_ALLOW_MODIFY     OUT    UNAPIGEN.CHAR1_TABLE_TYPE,   
 A_AR               OUT    UNAPIGEN.CHAR1_TABLE_TYPE,   
 A_ACTIVE           OUT    UNAPIGEN.CHAR1_TABLE_TYPE,   
 A_LC               OUT    UNAPIGEN.VC2_TABLE_TYPE,     
 A_LC_VERSION       OUT    UNAPIGEN.VC20_TABLE_TYPE,    
 A_SS               OUT    UNAPIGEN.VC2_TABLE_TYPE,     
 A_NR_OF_ROWS       IN OUT NUMBER,                      
 A_WHERE_CLAUSE     IN     VARCHAR2,                    
 A_NEXT_ROWS        IN     NUMBER)                      
RETURN NUMBER IS

L_RQ             VARCHAR2(20);
L_IC             VARCHAR2(20);
L_ICNODE         NUMBER(9);
L_II             VARCHAR2(20);
L_IINODE         NUMBER(9);
L_IE_VERSION     VARCHAR2(20);
L_IIVALUE        VARCHAR2(2000);
L_POS_X          NUMBER(4);
L_POS_Y          NUMBER(4);
L_IS_PROTECTED   CHAR(1);
L_MANDATORY      CHAR(1);
L_HIDDEN         CHAR(1);
L_DSP_TITLE      VARCHAR2(40);
L_DSP_TP         CHAR(1);
L_DSP_LEN        NUMBER(4);
L_DSP_ROWS       NUMBER(3);
L_II_CLASS       VARCHAR2(2);
L_LOG_HS         CHAR(1);
L_LOG_HS_DETAILS CHAR(1);
L_ALLOW_MODIFY   CHAR(1);
L_AR             CHAR(1);
L_ACTIVE         CHAR(1);
L_LC             VARCHAR2(2);
L_LC_VERSION     VARCHAR2(20);
L_SS             VARCHAR2(2);

BEGIN

   IF NVL(A_NR_OF_ROWS,0) = 0 THEN
      A_NR_OF_ROWS := UNAPIGEN.P_DEFAULT_CHUNK_SIZE;
   ELSIF A_NR_OF_ROWS < 0 OR A_NR_OF_ROWS > UNAPIGEN.P_MAX_CHUNK_SIZE THEN
      RETURN(UNAPIGEN.DBERR_NROFROWS);
   END IF;

   IF NVL(A_NEXT_ROWS, 0) NOT IN (-1, 0, 1) THEN
      RETURN(UNAPIGEN.DBERR_NEXTROWS);
   END IF;

   
   IF A_NEXT_ROWS = -1 THEN
      IF P_RQII_CURSOR IS NOT NULL THEN
         DBMS_SQL.CLOSE_CURSOR(P_RQII_CURSOR);
         P_RQII_CURSOR := NULL;
      END IF;
      RETURN (UNAPIGEN.DBERR_SUCCESS);
   END IF;

   
   IF A_NEXT_ROWS = 1 THEN
      IF P_RQII_CURSOR IS NULL THEN
         RETURN(UNAPIGEN.DBERR_NOCURSOR);
      END IF;
   END IF;

   
   IF NVL(A_NEXT_ROWS,0) = 0 THEN

      IF NVL(A_WHERE_CLAUSE, ' ') = ' ' THEN
          RETURN(UNAPIGEN.DBERR_WHERECLAUSE);
      ELSIF
        UPPER(SUBSTR(A_WHERE_CLAUSE,1,6)) <> 'WHERE ' THEN
        L_WHERE_CLAUSE := 'WHERE rq = ''' || REPLACE(A_WHERE_CLAUSE, '''', '''''') || 
                          ''' ORDER BY icnode, iinode';
      ELSE
         L_WHERE_CLAUSE := A_WHERE_CLAUSE; 
      END IF;

      IF P_RQII_CURSOR IS NULL THEN
         P_RQII_CURSOR := DBMS_SQL.OPEN_CURSOR;
      END IF;

      L_SQL_STRING := 'SELECT rq, ic, icnode, ii, iinode, ie_version, iivalue, pos_x, pos_y, ' ||
                      'is_protected, mandatory, hidden, dsp_title, dsp_len, ' ||
                      'dsp_tp, dsp_rows, ii_class, log_hs, log_hs_details, ' ||
                      'allow_modify, ar, active, lc, lc_version, ss ' ||
                      'FROM dd' || UNAPIGEN.P_DD || '.uvrqii ' || L_WHERE_CLAUSE;

      DBMS_SQL.PARSE(P_RQII_CURSOR, L_SQL_STRING, DBMS_SQL.V7); 

      DBMS_SQL.DEFINE_COLUMN(P_RQII_CURSOR, 1, L_RQ, 20);
      DBMS_SQL.DEFINE_COLUMN(P_RQII_CURSOR, 2, L_IC, 20);
      DBMS_SQL.DEFINE_COLUMN(P_RQII_CURSOR, 3, L_ICNODE);
      DBMS_SQL.DEFINE_COLUMN(P_RQII_CURSOR, 4, L_II, 20);
      DBMS_SQL.DEFINE_COLUMN(P_RQII_CURSOR, 5, L_IINODE);
      DBMS_SQL.DEFINE_COLUMN(P_RQII_CURSOR, 6, L_IE_VERSION, 20);
      DBMS_SQL.DEFINE_COLUMN(P_RQII_CURSOR, 7, L_IIVALUE, 2000);
      DBMS_SQL.DEFINE_COLUMN(P_RQII_CURSOR, 8, L_POS_X);
      DBMS_SQL.DEFINE_COLUMN(P_RQII_CURSOR, 9, L_POS_Y);
      DBMS_SQL.DEFINE_COLUMN_CHAR(P_RQII_CURSOR, 10, L_IS_PROTECTED, 1);
      DBMS_SQL.DEFINE_COLUMN_CHAR(P_RQII_CURSOR, 11, L_MANDATORY, 1);
      DBMS_SQL.DEFINE_COLUMN_CHAR(P_RQII_CURSOR, 12, L_HIDDEN, 1);
      DBMS_SQL.DEFINE_COLUMN(P_RQII_CURSOR, 13, L_DSP_TITLE, 40);
      DBMS_SQL.DEFINE_COLUMN(P_RQII_CURSOR, 14, L_DSP_LEN);
      DBMS_SQL.DEFINE_COLUMN_CHAR(P_RQII_CURSOR, 15, L_DSP_TP, 1);
      DBMS_SQL.DEFINE_COLUMN(P_RQII_CURSOR, 16, L_DSP_ROWS);
      DBMS_SQL.DEFINE_COLUMN(P_RQII_CURSOR, 17, L_II_CLASS, 2);
      DBMS_SQL.DEFINE_COLUMN_CHAR(P_RQII_CURSOR, 18, L_LOG_HS, 1);
      DBMS_SQL.DEFINE_COLUMN_CHAR(P_RQII_CURSOR, 19, L_LOG_HS_DETAILS, 1);
      DBMS_SQL.DEFINE_COLUMN_CHAR(P_RQII_CURSOR, 20, L_ALLOW_MODIFY, 1);
      DBMS_SQL.DEFINE_COLUMN_CHAR(P_RQII_CURSOR, 21, L_AR, 1);
      DBMS_SQL.DEFINE_COLUMN_CHAR(P_RQII_CURSOR, 22, L_ACTIVE, 1);
      DBMS_SQL.DEFINE_COLUMN(P_RQII_CURSOR, 23, L_LC, 2);
      DBMS_SQL.DEFINE_COLUMN(P_RQII_CURSOR, 24, L_LC_VERSION, 20);
      DBMS_SQL.DEFINE_COLUMN(P_RQII_CURSOR, 25, L_SS, 2);
      L_RESULT := DBMS_SQL.EXECUTE(P_RQII_CURSOR);
   END IF;
   
   L_RESULT := DBMS_SQL.FETCH_ROWS(P_RQII_CURSOR);
   L_FETCHED_ROWS := 0;

   LOOP
      EXIT WHEN L_RESULT = 0 OR L_FETCHED_ROWS >= A_NR_OF_ROWS;

      DBMS_SQL.COLUMN_VALUE(P_RQII_CURSOR, 1, L_RQ);
      DBMS_SQL.COLUMN_VALUE(P_RQII_CURSOR, 2, L_IC);
      DBMS_SQL.COLUMN_VALUE(P_RQII_CURSOR, 3, L_ICNODE);
      DBMS_SQL.COLUMN_VALUE(P_RQII_CURSOR, 4, L_II);
      DBMS_SQL.COLUMN_VALUE(P_RQII_CURSOR, 5, L_IINODE);
      DBMS_SQL.COLUMN_VALUE(P_RQII_CURSOR, 6, L_IE_VERSION);
      DBMS_SQL.COLUMN_VALUE(P_RQII_CURSOR, 7, L_IIVALUE);
      DBMS_SQL.COLUMN_VALUE(P_RQII_CURSOR, 8, L_POS_X);
      DBMS_SQL.COLUMN_VALUE(P_RQII_CURSOR, 9, L_POS_Y);
      DBMS_SQL.COLUMN_VALUE_CHAR(P_RQII_CURSOR, 10, L_IS_PROTECTED);
      DBMS_SQL.COLUMN_VALUE_CHAR(P_RQII_CURSOR, 11, L_MANDATORY);
      DBMS_SQL.COLUMN_VALUE_CHAR(P_RQII_CURSOR, 12, L_HIDDEN);
      DBMS_SQL.COLUMN_VALUE(P_RQII_CURSOR, 13, L_DSP_TITLE);
      DBMS_SQL.COLUMN_VALUE(P_RQII_CURSOR, 14, L_DSP_LEN);
      DBMS_SQL.COLUMN_VALUE_CHAR(P_RQII_CURSOR, 15, L_DSP_TP);
      DBMS_SQL.COLUMN_VALUE(P_RQII_CURSOR, 16, L_DSP_ROWS);
      DBMS_SQL.COLUMN_VALUE(P_RQII_CURSOR, 17, L_II_CLASS);
      DBMS_SQL.COLUMN_VALUE_CHAR(P_RQII_CURSOR, 18, L_LOG_HS);
      DBMS_SQL.COLUMN_VALUE_CHAR(P_RQII_CURSOR, 19, L_LOG_HS_DETAILS);
      DBMS_SQL.COLUMN_VALUE_CHAR(P_RQII_CURSOR, 20, L_ALLOW_MODIFY);
      DBMS_SQL.COLUMN_VALUE_CHAR(P_RQII_CURSOR, 21, L_AR);
      DBMS_SQL.COLUMN_VALUE_CHAR(P_RQII_CURSOR, 22, L_ACTIVE);
      DBMS_SQL.COLUMN_VALUE(P_RQII_CURSOR, 23, L_LC);
      DBMS_SQL.COLUMN_VALUE(P_RQII_CURSOR, 24, L_LC_VERSION);
      DBMS_SQL.COLUMN_VALUE(P_RQII_CURSOR, 25, L_SS);

      L_FETCHED_ROWS := L_FETCHED_ROWS + 1;
      A_RQ(L_FETCHED_ROWS) := L_RQ;
      A_IC(L_FETCHED_ROWS) := L_IC;
      A_ICNODE(L_FETCHED_ROWS) := L_ICNODE;
      A_II(L_FETCHED_ROWS) := L_II;
      A_IINODE(L_FETCHED_ROWS) := L_IINODE;
      A_IE_VERSION(L_FETCHED_ROWS) := L_IE_VERSION;
      A_IIVALUE(L_FETCHED_ROWS) := L_IIVALUE;
      A_POS_X(L_FETCHED_ROWS) := L_POS_X;
      A_POS_Y(L_FETCHED_ROWS) := L_POS_Y;
      A_IS_PROTECTED(L_FETCHED_ROWS) := L_IS_PROTECTED;
      A_MANDATORY(L_FETCHED_ROWS) := L_MANDATORY;
      A_HIDDEN(L_FETCHED_ROWS) := L_HIDDEN;
      A_DSP_TITLE(L_FETCHED_ROWS) := L_DSP_TITLE;
      A_DSP_LEN(L_FETCHED_ROWS) := L_DSP_LEN;
      A_DSP_TP(L_FETCHED_ROWS) := L_DSP_TP;
      A_DSP_ROWS(L_FETCHED_ROWS) := L_DSP_ROWS;
      A_II_CLASS(L_FETCHED_ROWS) := L_II_CLASS;
      A_LOG_HS(L_FETCHED_ROWS) := L_LOG_HS;
      A_LOG_HS_DETAILS(L_FETCHED_ROWS) := L_LOG_HS_DETAILS;
      A_ALLOW_MODIFY(L_FETCHED_ROWS) := L_ALLOW_MODIFY;
      A_AR(L_FETCHED_ROWS) := L_AR;
      A_ACTIVE(L_FETCHED_ROWS) := L_ACTIVE;
      A_LC(L_FETCHED_ROWS) := L_LC;
      A_LC_VERSION(L_FETCHED_ROWS) := L_LC_VERSION;
      A_SS(L_FETCHED_ROWS) := L_SS;

      IF L_FETCHED_ROWS < A_NR_OF_ROWS THEN
         L_RESULT := DBMS_SQL.FETCH_ROWS(P_RQII_CURSOR);
      END IF;
   END LOOP;

   
   IF (L_FETCHED_ROWS = 0) THEN
       DBMS_SQL.CLOSE_CURSOR(P_RQII_CURSOR);
       P_RQII_CURSOR := NULL;
       RETURN(UNAPIGEN.DBERR_NORECORDS);
   ELSIF L_FETCHED_ROWS < A_NR_OF_ROWS THEN
      DBMS_SQL.CLOSE_CURSOR(P_RQII_CURSOR);
      P_RQII_CURSOR := NULL;
      A_NR_OF_ROWS := L_FETCHED_ROWS;
   END IF;
   RETURN(UNAPIGEN.DBERR_SUCCESS);

EXCEPTION
   WHEN OTHERS THEN
      L_SQLERRM := SQLERRM;
      UNAPIGEN.U4ROLLBACK;
      INSERT INTO UTERROR(CLIENT_ID, APPLIC, WHO, LOGDATE, LOGDATE_TZ, API_NAME, ERROR_MSG)
      VALUES(UNAPIGEN.P_CLIENT_ID, UNAPIGEN.P_APPLIC_NAME, UNAPIGEN.P_USER, CURRENT_TIMESTAMP, CURRENT_TIMESTAMP, 
             'GetRqInfoField', L_SQLERRM);
      UNAPIGEN.U4COMMIT;
      IF DBMS_SQL.IS_OPEN(P_RQII_CURSOR) THEN
         DBMS_SQL.CLOSE_CURSOR(P_RQII_CURSOR);
      END IF;
      RETURN(UNAPIGEN.DBERR_GENFAIL);
END GETRQINFOFIELD;

FUNCTION SAVERQINFOCARD                              
(A_RQ             IN      UNAPIGEN.VC20_TABLE_TYPE,  
 A_IC             IN      UNAPIGEN.VC20_TABLE_TYPE,  
 A_ICNODE         IN OUT  UNAPIGEN.LONG_TABLE_TYPE,  
 A_IP_VERSION     IN      UNAPIGEN.VC20_TABLE_TYPE,  
 A_DESCRIPTION    IN      UNAPIGEN.VC40_TABLE_TYPE,  
 A_WINSIZE_X      IN      UNAPIGEN.NUM_TABLE_TYPE,   
 A_WINSIZE_Y      IN      UNAPIGEN.NUM_TABLE_TYPE,   
 A_IS_PROTECTED   IN      UNAPIGEN.CHAR1_TABLE_TYPE, 
 A_HIDDEN         IN      UNAPIGEN.CHAR1_TABLE_TYPE, 
 A_MANUALLY_ADDED IN      UNAPIGEN.CHAR1_TABLE_TYPE, 
 A_NEXT_II        IN      UNAPIGEN.VC20_TABLE_TYPE,  
 A_IC_CLASS       IN      UNAPIGEN.VC2_TABLE_TYPE,   
 A_LOG_HS         IN      UNAPIGEN.CHAR1_TABLE_TYPE, 
 A_LOG_HS_DETAILS IN      UNAPIGEN.CHAR1_TABLE_TYPE, 
 A_LC             IN      UNAPIGEN.VC2_TABLE_TYPE,   
 A_LC_VERSION     IN      UNAPIGEN.VC20_TABLE_TYPE,  
 A_MODIFY_FLAG    IN OUT  UNAPIGEN.NUM_TABLE_TYPE,   
 A_NR_OF_ROWS     IN      NUMBER,                    
 A_MODIFY_REASON  IN      VARCHAR2)                  
RETURN NUMBER IS

L_LC                     VARCHAR2(2);
L_LC_VERSION             VARCHAR2(20);
L_SS                     VARCHAR2(2);
L_LOG_HS                 CHAR(1);
L_LOG_HS_DETAILS         CHAR(1);
L_ALLOW_MODIFY           CHAR(1);
L_ACTIVE                 CHAR(1);
L_INSERT                 BOOLEAN;
L_IC_RECORD_OK           BOOLEAN;
L_IC_HANDLED             BOOLEAN;
CURR_ITEM                INTEGER;
NEXT_ITEM                INTEGER;
PREV_NODE                NUMBER(9);
NEXT_NODE                NUMBER(9);
NODE_STEP                NUMBER(9);
L_SEQ_NO                 INTEGER;
NBR_OF_NODES_TO_CREATE   INTEGER;
L_COMPLETELY_SAVED       BOOLEAN;
L_DELETED_NODE           NUMBER(9);
L_CURRENT_TIMESTAMP                TIMESTAMP WITH TIME ZONE;
L_RT                     VARCHAR2(20);
L_RT_VERSION             VARCHAR2(20);
L_HS_DETAILS_SEQ_NR      INTEGER;
L_IP_VERSION             VARCHAR2(20);

CURSOR L_RQ_CURSOR(A_RQ VARCHAR2) IS
   SELECT RT, RT_VERSION
   FROM UTRQ 
   WHERE RQ = A_RQ;

CURSOR L_RQICOLD_CURSOR (A_RQ IN VARCHAR2,
                         A_IC IN VARCHAR2, A_ICNODE IN NUMBER) IS
   SELECT A.*
   FROM UDRQIC A
   WHERE A.RQ = A_RQ
   AND A.IC = A_IC
   AND A.ICNODE = A_ICNODE;
L_RQICOLD_REC UDRQIC%ROWTYPE;
L_RQICNEW_REC UDRQIC%ROWTYPE;

BEGIN

   
   
   
   
   
   
   
   
   
   IF UNAPIGEN.BEGINTXN(UNAPIGEN.P_SINGLE_API_TXN) <>
      UNAPIGEN.DBERR_SUCCESS THEN
      RAISE STPERROR;
   END IF;

   L_CURRENT_TIMESTAMP := CURRENT_TIMESTAMP;
   L_HS_DETAILS_SEQ_NR := 0;
   
   
   
   CURR_ITEM := 1;
   WHILE CURR_ITEM <= A_NR_OF_ROWS LOOP
      IF A_MODIFY_FLAG(CURR_ITEM) IN (UNAPIGEN.MOD_FLAG_INSERT,
                                      UNAPIGEN.MOD_FLAG_INSERT_AND_CRAU,
                                      UNAPIGEN.MOD_FLAG_CREATE) THEN
         IF NVL(A_ICNODE(CURR_ITEM), 0) <> 0 THEN
            UNAPIGEN.P_TXN_ERROR := UNAPIGEN.DBERR_NEWNODENOTZERO;
            RAISE STPERROR;
         END IF;

         
         
         
         NEXT_ITEM := CURR_ITEM;
         NBR_OF_NODES_TO_CREATE := 0;
         WHILE (A_MODIFY_FLAG(NEXT_ITEM) IN (UNAPIGEN.MOD_FLAG_INSERT,
                                             UNAPIGEN.MOD_FLAG_INSERT_AND_CRAU,
                                             UNAPIGEN.MOD_FLAG_CREATE) AND
                NVL(A_RQ(CURR_ITEM), ' ') = NVL(A_RQ(NEXT_ITEM), ' ')) LOOP
            NBR_OF_NODES_TO_CREATE := NBR_OF_NODES_TO_CREATE + 1;
            IF NEXT_ITEM < A_NR_OF_ROWS THEN
               NEXT_ITEM := NEXT_ITEM + 1;
            ELSE
               EXIT;
            END IF;
         END LOOP;

         
         
         
         
         IF A_MODIFY_FLAG(NEXT_ITEM) NOT IN (UNAPIGEN.MOD_FLAG_INSERT,
                                             UNAPIGEN.MOD_FLAG_INSERT_AND_CRAU,
                                             UNAPIGEN.MOD_FLAG_CREATE) AND
                A_RQ(CURR_ITEM) = A_RQ(NEXT_ITEM) THEN
            NEXT_NODE := A_ICNODE(NEXT_ITEM);
            SELECT NVL(MAX(ICNODE), 0)
            INTO PREV_NODE
            FROM UTRQIC
            WHERE ICNODE < NEXT_NODE
              AND RQ = A_RQ(CURR_ITEM);
            NODE_STEP :=
               TRUNC(ABS((NEXT_NODE - PREV_NODE)) / (NBR_OF_NODES_TO_CREATE + 1));
            IF NODE_STEP < 1 THEN
               UNAPIGEN.P_TXN_ERROR := UNAPIGEN.DBERR_NODELIMITOVERF;
               RAISE STPERROR;
            END IF;
         ELSE
            
            SELECT NVL(MAX(ICNODE), 0)
            INTO PREV_NODE
            FROM UTRQIC
            WHERE RQ = A_RQ(CURR_ITEM);
            NODE_STEP := UNAPIGEN.DEFAULT_NODE_INTERVAL;
         END IF;

         
         
         
         FOR I IN 1..NBR_OF_NODES_TO_CREATE LOOP
            A_ICNODE(CURR_ITEM + I - 1) := PREV_NODE + (NODE_STEP * I);
         END LOOP;

         CURR_ITEM := CURR_ITEM + NBR_OF_NODES_TO_CREATE;
      ELSE
         CURR_ITEM := CURR_ITEM + 1;
      END IF;
   END LOOP;

   
   
   
   L_COMPLETELY_SAVED := TRUE;

   
   
   
   FOR L_SEQ_NO IN 1..A_NR_OF_ROWS LOOP
      L_IC_RECORD_OK := TRUE;          
      L_IC_HANDLED := TRUE;

      
      
      
      IF A_MODIFY_FLAG(L_SEQ_NO) IN (UNAPIGEN.MOD_FLAG_INSERT,
                                     UNAPIGEN.MOD_FLAG_INSERT_AND_CRAU,
                                     UNAPIGEN.MOD_FLAG_INSERT_WITH_NODES,
                                     UNAPIGEN.MOD_FLAG_INSERT_NODES_AND_CRAU,
                                     UNAPIGEN.MOD_FLAG_CREATE) THEN
         IF NVL(A_IP_VERSION(L_SEQ_NO), ' ') = ' ' THEN
            UNAPIGEN.P_TXN_ERROR := UNAPIGEN.DBERR_IPVERSION;
            RAISE STPERROR;
         END IF;
      END IF;

      IF NVL(A_RQ(L_SEQ_NO), ' ') = ' ' OR
         NVL(A_IC(L_SEQ_NO), ' ') = ' ' THEN
         UNAPIGEN.P_TXN_ERROR := UNAPIGEN.DBERR_NOOBJID;
         RAISE STPERROR;

         
         
         
      ELSIF A_MODIFY_FLAG(L_SEQ_NO) IN (UNAPIGEN.MOD_FLAG_INSERT,
                                        UNAPIGEN.MOD_FLAG_INSERT_AND_CRAU,
                                        UNAPIGEN.MOD_FLAG_INSERT_WITH_NODES,
                                        UNAPIGEN.MOD_FLAG_INSERT_NODES_AND_CRAU,
                                        UNAPIGEN.MOD_FLAG_CREATE,                                        
                                        UNAPIGEN.MOD_FLAG_UPDATE,
                                        UNAPIGEN.MOD_FLAG_DELETE) THEN
         L_IP_VERSION := A_IP_VERSION(L_SEQ_NO);
         L_RET_CODE := UNAPIAUT.GETRQICAUTHORISATION(A_RQ(L_SEQ_NO), A_IC(L_SEQ_NO),
                                                     A_ICNODE(L_SEQ_NO), L_IP_VERSION, L_LC, 
                                                     L_LC_VERSION, L_SS, L_ALLOW_MODIFY, L_ACTIVE, 
                                                     L_LOG_HS, L_LOG_HS_DETAILS);
         IF L_RET_CODE = UNAPIGEN.DBERR_NOOBJECT THEN
            L_INSERT := TRUE;
         ELSIF L_RET_CODE = UNAPIGEN.DBERR_SUCCESS THEN
            L_INSERT := FALSE;
         ELSE
            A_MODIFY_FLAG(L_SEQ_NO) := L_RET_CODE;
            L_IC_RECORD_OK := FALSE;
         END IF;
      ELSIF A_MODIFY_FLAG(L_SEQ_NO) = UNAPIGEN.DBERR_SUCCESS THEN
         
         
         
         L_IC_RECORD_OK := FALSE;  
         L_IC_HANDLED := FALSE;    
      ELSE
         UNAPIGEN.P_TXN_ERROR := UNAPIGEN.DBERR_INVALMODFLAG;
         RAISE STPERROR;         
      END IF;

      
      
      
      IF A_MODIFY_FLAG(L_SEQ_NO) IN (UNAPIGEN.MOD_FLAG_INSERT,
                                     UNAPIGEN.MOD_FLAG_INSERT_AND_CRAU,
                                     UNAPIGEN.MOD_FLAG_INSERT_WITH_NODES,      
                                     UNAPIGEN.MOD_FLAG_INSERT_NODES_AND_CRAU,
                                     UNAPIGEN.MOD_FLAG_CREATE,
                                     UNAPIGEN.MOD_FLAG_UPDATE) THEN
         IF NVL(A_LOG_HS(L_SEQ_NO), ' ') NOT IN ('0', '1') THEN
            UNAPIGEN.P_TXN_ERROR := UNAPIGEN.DBERR_LOGHS;
            RAISE STPERROR;
         ELSIF NVL(A_LOG_HS_DETAILS(L_SEQ_NO), ' ') NOT IN ('0', '1') THEN
            UNAPIGEN.P_TXN_ERROR := UNAPIGEN.DBERR_LOGHSDETAILS;
            RAISE STPERROR;
         END IF;
      END IF;

      
      
      

      
      
      
      IF L_IC_RECORD_OK AND
         A_MODIFY_FLAG(L_SEQ_NO) = UNAPIGEN.MOD_FLAG_UPDATE THEN

         
         
         
         IF L_INSERT THEN
            
            A_MODIFY_FLAG(L_SEQ_NO) := UNAPIGEN.DBERR_NOOBJECT;
            L_IC_RECORD_OK := FALSE;
         ELSIF NVL(A_IS_PROTECTED(L_SEQ_NO), ' ') NOT IN ('0', '1') AND 
               NOT (UNAPIGEN.P_RNDSUITESESSION = '1' AND NVL(A_IS_PROTECTED(L_SEQ_NO), ' ') IN ('0', '1', '4', '5')) THEN
            UNAPIGEN.P_TXN_ERROR := UNAPIGEN.DBERR_PROTECTED;
            RAISE STPERROR;
         ELSIF NVL(A_HIDDEN(L_SEQ_NO), ' ') NOT IN ('0', '1') THEN
            UNAPIGEN.P_TXN_ERROR := UNAPIGEN.DBERR_HIDDEN;
            RAISE STPERROR;
         ELSIF NVL(A_MANUALLY_ADDED(L_SEQ_NO), ' ') NOT IN ('0', '1') THEN
            UNAPIGEN.P_TXN_ERROR := UNAPIGEN.DBERR_MANUALLYADDED;
            RAISE STPERROR;
         ELSE
            
            
            

            
            
            
            OPEN L_RQICOLD_CURSOR(A_RQ(L_SEQ_NO),
                                  A_IC(L_SEQ_NO), A_ICNODE(L_SEQ_NO));
            FETCH L_RQICOLD_CURSOR
            INTO L_RQICOLD_REC;
            CLOSE L_RQICOLD_CURSOR;
            L_RQICNEW_REC := L_RQICOLD_REC;

            
            
            
            
            
            UPDATE UTRQIC
            SET DESCRIPTION    = A_DESCRIPTION(L_SEQ_NO),
                WINSIZE_X      = A_WINSIZE_X(L_SEQ_NO),
                WINSIZE_Y      = A_WINSIZE_Y(L_SEQ_NO),
                IS_PROTECTED   = A_IS_PROTECTED(L_SEQ_NO),
                HIDDEN         = A_HIDDEN(L_SEQ_NO),
                MANUALLY_ADDED = A_MANUALLY_ADDED(L_SEQ_NO),
                IC_CLASS       = A_IC_CLASS(L_SEQ_NO),
                LOG_HS         = A_LOG_HS(L_SEQ_NO),
                LOG_HS_DETAILS = A_LOG_HS_DETAILS(L_SEQ_NO),
                ALLOW_MODIFY   = '#'
            WHERE RQ = A_RQ(L_SEQ_NO)
              AND IC = A_IC(L_SEQ_NO)
              AND ICNODE = A_ICNODE(L_SEQ_NO)
            RETURNING DESCRIPTION, WINSIZE_X, WINSIZE_Y, IS_PROTECTED, HIDDEN, MANUALLY_ADDED, 
                      IC_CLASS, LOG_HS, LOG_HS_DETAILS, ALLOW_MODIFY
            INTO L_RQICNEW_REC.DESCRIPTION, L_RQICNEW_REC.WINSIZE_X, L_RQICNEW_REC.WINSIZE_Y, 
                 L_RQICNEW_REC.IS_PROTECTED, L_RQICNEW_REC.HIDDEN, L_RQICNEW_REC.MANUALLY_ADDED, 
                 L_RQICNEW_REC.IC_CLASS, L_RQICNEW_REC.LOG_HS, L_RQICNEW_REC.LOG_HS_DETAILS, 
                 L_RQICNEW_REC.ALLOW_MODIFY;

              
              
              
              L_EVENT_TP := 'RqInfoCardUpdated';
         END IF;

      
      
      
      ELSIF L_IC_RECORD_OK AND
            A_MODIFY_FLAG(L_SEQ_NO) IN (UNAPIGEN.MOD_FLAG_INSERT,
                                        UNAPIGEN.MOD_FLAG_INSERT_AND_CRAU,
                                        UNAPIGEN.MOD_FLAG_INSERT_WITH_NODES,
                                        UNAPIGEN.MOD_FLAG_INSERT_NODES_AND_CRAU,
                                        UNAPIGEN.MOD_FLAG_CREATE) THEN

         
         
         
         IF NOT L_INSERT THEN
            UNAPIGEN.P_TXN_ERROR := UNAPIGEN.DBERR_ICALREADYEXIST;
            RAISE STPERROR;
         ELSIF NVL(A_IS_PROTECTED(L_SEQ_NO), ' ') NOT IN ('0', '1') AND 
               NOT (UNAPIGEN.P_RNDSUITESESSION = '1' AND NVL(A_IS_PROTECTED(L_SEQ_NO), ' ') IN ('0', '1', '4', '5')) THEN
            UNAPIGEN.P_TXN_ERROR := UNAPIGEN.DBERR_PROTECTED;
            RAISE STPERROR;
         ELSIF NVL(A_HIDDEN(L_SEQ_NO), ' ') NOT IN ('0', '1') THEN
            UNAPIGEN.P_TXN_ERROR := UNAPIGEN.DBERR_HIDDEN;
            RAISE STPERROR;
         ELSIF NVL(A_MANUALLY_ADDED(L_SEQ_NO), ' ') NOT IN ('0', '1') THEN
            UNAPIGEN.P_TXN_ERROR := UNAPIGEN.DBERR_MANUALLYADDED;
            RAISE STPERROR;
         ELSE
            
            
            
            

            
            IF NVL(A_LC(L_SEQ_NO), ' ') <> ' ' THEN
               L_LC := A_LC(L_SEQ_NO);
            END IF;
            IF NVL(A_LC_VERSION(L_SEQ_NO), ' ') <> ' ' THEN
               L_LC_VERSION := A_LC_VERSION(L_SEQ_NO);
            END IF;

            INSERT INTO UTRQIC(RQ, IC, ICNODE, IP_VERSION, DESCRIPTION, WINSIZE_X,
                               WINSIZE_Y, IS_PROTECTED, HIDDEN,
                               MANUALLY_ADDED, NEXT_II, IC_CLASS, LOG_HS, LOG_HS_DETAILS,
                               ALLOW_MODIFY, ACTIVE, LC, LC_VERSION)
            VALUES(A_RQ(L_SEQ_NO), A_IC(L_SEQ_NO),  A_ICNODE(L_SEQ_NO), L_IP_VERSION,
                   A_DESCRIPTION(L_SEQ_NO), A_WINSIZE_X(L_SEQ_NO),
                   A_WINSIZE_Y(L_SEQ_NO), A_IS_PROTECTED(L_SEQ_NO),
                   A_HIDDEN(L_SEQ_NO), A_MANUALLY_ADDED(L_SEQ_NO), NULL, A_IC_CLASS(L_SEQ_NO), 
                   A_LOG_HS(L_SEQ_NO), A_LOG_HS_DETAILS(L_SEQ_NO), '#', '0', L_LC, L_LC_VERSION);
            UNAPIAUT.UPDATELCINAUTHORISATIONBUFFER('ic', A_RQ(L_SEQ_NO) || A_IC(L_SEQ_NO) || TO_CHAR(A_ICNODE(L_SEQ_NO)),
                                                           '', L_LC, L_LC_VERSION);                 
            
            
            
            L_EVENT_TP := 'RqInfoCardCreated';
         END IF;

      
      
      
      ELSIF L_IC_RECORD_OK AND
            A_MODIFY_FLAG(L_SEQ_NO) = UNAPIGEN.MOD_FLAG_DELETE THEN

         IF L_INSERT THEN
            
            UNAPIGEN.P_TXN_ERROR := UNAPIGEN.DBERR_NOOBJECT;
            RAISE STPERROR;

         ELSE
            IF UNAPIGEN.P_RNDSUITESESSION = '0' THEN
               
               IF UNAPIGEN.ISSYSTEM21CFR11COMPLIANT = UNAPIGEN.DBERR_SUCCESS THEN
                  UNAPIGEN.P_TXN_ERROR := UNAPIGEN.DBERR_NOTALLOWEDIN21CFR11;
                  RAISE STPERROR;
               
               
               
               ELSIF L_ACTIVE = '1' THEN
                  UNAPIGEN.P_TXN_ERROR := UNAPIGEN.DBERR_OPACTIVE;
                  RAISE STPERROR;
               END IF;
            END IF;

            
            
            
            DELETE FROM UTRQII
            WHERE RQ     = A_RQ(L_SEQ_NO)
              AND IC     = A_IC(L_SEQ_NO)
              AND ICNODE = A_ICNODE(L_SEQ_NO);

            
            
            
            DELETE FROM UTRQICAU
            WHERE RQ     = A_RQ(L_SEQ_NO)
              AND IC     = A_IC(L_SEQ_NO)
              AND ICNODE = A_ICNODE(L_SEQ_NO);

            DELETE FROM UTRQICHS
            WHERE RQ     = A_RQ(L_SEQ_NO)
              AND IC     = A_IC(L_SEQ_NO)
              AND ICNODE = A_ICNODE(L_SEQ_NO);

            DELETE FROM UTRQICHSDETAILS
            WHERE RQ     = A_RQ(L_SEQ_NO)
              AND IC     = A_IC(L_SEQ_NO)
              AND ICNODE = A_ICNODE(L_SEQ_NO);

            DELETE FROM UTRQIC
            WHERE RQ     = A_RQ(L_SEQ_NO)
              AND IC     = A_IC(L_SEQ_NO)
              AND ICNODE = A_ICNODE(L_SEQ_NO);

            
            
            
            DELETE FROM UTEVTIMED
            WHERE OBJECT_TP = 'rqic'
              AND OBJECT_ID = A_IC(L_SEQ_NO)
              AND INSTR(EV_DETAILS, 'rq=' || A_RQ(L_SEQ_NO)) <> 0
              AND INSTR(EV_DETAILS, 'icnode=' || TO_CHAR(A_ICNODE(L_SEQ_NO))) <> 0;

            DELETE FROM UTEVTIMED
            WHERE INSTR(EV_DETAILS, 'rq=' || A_RQ(L_SEQ_NO)) <> 0
              AND INSTR(EV_DETAILS, 'ic=' || A_IC(L_SEQ_NO)) <> 0
              AND INSTR(EV_DETAILS, 'icnode=' || TO_CHAR(A_ICNODE(L_SEQ_NO))) <> 0;

            DELETE FROM UTEVRULESDELAYED
            WHERE OBJECT_TP = 'rqic'
              AND OBJECT_ID = A_IC(L_SEQ_NO)
              AND INSTR(EV_DETAILS, 'rq=' || A_RQ(L_SEQ_NO)) <> 0
              AND INSTR(EV_DETAILS, 'icnode=' || TO_CHAR(A_ICNODE(L_SEQ_NO))) <> 0;

            DELETE FROM UTEVRULESDELAYED
            WHERE INSTR(EV_DETAILS, 'rq=' || A_RQ(L_SEQ_NO)) <> 0
              AND INSTR(EV_DETAILS, 'ic=' || A_IC(L_SEQ_NO)) <> 0
              AND INSTR(EV_DETAILS, 'icnode=' || TO_CHAR(A_ICNODE(L_SEQ_NO))) <> 0;

            L_DELETED_NODE := A_ICNODE(L_SEQ_NO);
            A_ICNODE(L_SEQ_NO) := 0;  

            
            
            
            L_EVENT_TP := 'RqInfoCardDeleted';
         END IF;
      END IF;

      
      
      
      IF L_IC_RECORD_OK THEN
         L_EV_SEQ_NR := -1;
         IF A_MODIFY_FLAG(L_SEQ_NO) <> UNAPIGEN.MOD_FLAG_DELETE THEN
            L_EV_DETAILS := 'rq=' || A_RQ(L_SEQ_NO) || 
                            '#icnode=' || TO_CHAR(A_ICNODE(L_SEQ_NO)) || 
                            '#ip_version=' || L_IP_VERSION;
         ELSE
            L_EV_DETAILS := 'rq=' || A_RQ(L_SEQ_NO) || 
                            '#icnode=' || TO_CHAR(L_DELETED_NODE) ||
                            '#ip_version=' || L_IP_VERSION;
         END IF;
         L_RESULT := UNAPIEV.INSERTEVENT('SaveRqInfoCard', UNAPIGEN.P_EVMGR_NAME,
                                         'rqic', A_IC(L_SEQ_NO), L_LC, L_LC_VERSION, L_SS,
                                         L_EVENT_TP, L_EV_DETAILS, L_EV_SEQ_NR);
         IF L_RESULT <> UNAPIGEN.DBERR_SUCCESS THEN
            UNAPIGEN.P_TXN_ERROR := L_RESULT;
            RAISE STPERROR;
         END IF;
      ELSE
         IF L_IC_HANDLED THEN
            L_COMPLETELY_SAVED := FALSE;
         END IF;
      END IF;

      
      
      
      IF L_IC_RECORD_OK AND L_LOG_HS <> A_LOG_HS(L_SEQ_NO) AND
         A_MODIFY_FLAG(L_SEQ_NO) <> UNAPIGEN.MOD_FLAG_DELETE THEN
         IF A_LOG_HS(L_SEQ_NO) = '1' THEN
            INSERT INTO UTRQICHS(RQ, IC, ICNODE, WHO, WHO_DESCRIPTION, WHAT, WHAT_DESCRIPTION, 
                                 LOGDATE, LOGDATE_TZ, WHY, TR_SEQ, EV_SEQ)
            VALUES(A_RQ(L_SEQ_NO), A_IC(L_SEQ_NO), A_ICNODE(L_SEQ_NO), UNAPIGEN.P_USER,
                   UNAPIGEN.P_USER_DESCRIPTION, 'History switched ON', 'Audit trail is turned on.', 
                   L_CURRENT_TIMESTAMP, L_CURRENT_TIMESTAMP, A_MODIFY_REASON, UNAPIGEN.P_TR_SEQ, L_EV_SEQ_NR);
         ELSE
            INSERT INTO UTRQICHS(RQ, IC, ICNODE, WHO, WHO_DESCRIPTION, WHAT, WHAT_DESCRIPTION, 
                                 LOGDATE, LOGDATE_TZ, WHY, TR_SEQ, EV_SEQ)
            VALUES(A_RQ(L_SEQ_NO), A_IC(L_SEQ_NO), A_ICNODE(L_SEQ_NO), UNAPIGEN.P_USER,
                   UNAPIGEN.P_USER_DESCRIPTION, 'History switched OFF', 'Audit trail is turned off.', 
                   L_CURRENT_TIMESTAMP, L_CURRENT_TIMESTAMP, A_MODIFY_REASON, UNAPIGEN.P_TR_SEQ, L_EV_SEQ_NR);
         END IF;
      END IF;

      
      
      
      IF L_IC_RECORD_OK AND L_LOG_HS_DETAILS <> A_LOG_HS_DETAILS(L_SEQ_NO) AND
         A_MODIFY_FLAG(L_SEQ_NO) <> UNAPIGEN.MOD_FLAG_DELETE THEN
         IF A_LOG_HS_DETAILS(L_SEQ_NO) = '1' THEN
            L_HS_DETAILS_SEQ_NR := L_HS_DETAILS_SEQ_NR + 1;
            INSERT INTO UTRQICHSDETAILS(RQ, IC, ICNODE, TR_SEQ, EV_SEQ, SEQ, DETAILS)
            VALUES(A_RQ(L_SEQ_NO), A_IC(L_SEQ_NO), A_ICNODE(L_SEQ_NO), UNAPIGEN.P_TR_SEQ, L_EV_SEQ_NR, 
                   L_HS_DETAILS_SEQ_NR, 'Audit trail is turned on.');
         ELSE
            L_HS_DETAILS_SEQ_NR := L_HS_DETAILS_SEQ_NR + 1;
            INSERT INTO UTRQICHSDETAILS(RQ, IC, ICNODE, TR_SEQ, EV_SEQ, SEQ, DETAILS)
            VALUES(A_RQ(L_SEQ_NO), A_IC(L_SEQ_NO), A_ICNODE(L_SEQ_NO), UNAPIGEN.P_TR_SEQ, L_EV_SEQ_NR, 
                   L_HS_DETAILS_SEQ_NR, 'Audit trail is turned off.');
         END IF;
      END IF;

      
      L_RET_CODE := UNAPIGEN.GETNEXTEVENTSEQNR(L_EV_SEQ_NR);

      IF L_IC_RECORD_OK AND (L_LOG_HS = '1' OR A_LOG_HS(L_SEQ_NO)='1') THEN
         IF A_MODIFY_FLAG(L_SEQ_NO) = UNAPIGEN.MOD_FLAG_DELETE THEN
            
            
            
            INSERT INTO UTRQHS(RQ, WHO, WHO_DESCRIPTION, WHAT, WHAT_DESCRIPTION, 
                               LOGDATE, LOGDATE_TZ, WHY, TR_SEQ, EV_SEQ)
            VALUES(A_RQ(L_SEQ_NO), UNAPIGEN.P_USER, UNAPIGEN.P_USER_DESCRIPTION, L_EVENT_TP, 
                   'info card "'||A_IC(L_SEQ_NO)||'" is deleted.', 
                   L_CURRENT_TIMESTAMP, L_CURRENT_TIMESTAMP, A_MODIFY_REASON, UNAPIGEN.P_TR_SEQ, L_EV_SEQ_NR);
         ELSE
            
            
            
            IF L_EVENT_TP = 'RqInfoCardCreated' THEN
               INSERT INTO UTRQICHS(RQ, IC, ICNODE, WHO, WHO_DESCRIPTION, WHAT, WHAT_DESCRIPTION, 
                                    LOGDATE, LOGDATE_TZ, WHY, TR_SEQ, EV_SEQ)
               VALUES(A_RQ(L_SEQ_NO), A_IC(L_SEQ_NO), A_ICNODE(L_SEQ_NO), UNAPIGEN.P_USER,
                      UNAPIGEN.P_USER_DESCRIPTION, L_EVENT_TP, 
                      'info card "'||A_IC(L_SEQ_NO)||'" is created.', 
                      L_CURRENT_TIMESTAMP, L_CURRENT_TIMESTAMP, A_MODIFY_REASON, UNAPIGEN.P_TR_SEQ, L_EV_SEQ_NR);
            ELSE
               INSERT INTO UTRQICHS(RQ, IC, ICNODE, WHO, WHO_DESCRIPTION, WHAT, WHAT_DESCRIPTION, 
                                    LOGDATE, LOGDATE_TZ, WHY, TR_SEQ, EV_SEQ)
               VALUES(A_RQ(L_SEQ_NO), A_IC(L_SEQ_NO), A_ICNODE(L_SEQ_NO), UNAPIGEN.P_USER,
                      UNAPIGEN.P_USER_DESCRIPTION, L_EVENT_TP, 
                      'info card "'||A_IC(L_SEQ_NO)||'" is updated.', 
                      L_CURRENT_TIMESTAMP, L_CURRENT_TIMESTAMP, A_MODIFY_REASON, UNAPIGEN.P_TR_SEQ, L_EV_SEQ_NR);
            END IF;
         END IF;
      END IF;

      IF L_IC_RECORD_OK AND (L_LOG_HS_DETAILS = '1' OR A_LOG_HS_DETAILS(L_SEQ_NO)='1') THEN
         IF A_MODIFY_FLAG(L_SEQ_NO) = UNAPIGEN.MOD_FLAG_DELETE THEN
            
            
            
            L_HS_DETAILS_SEQ_NR := L_HS_DETAILS_SEQ_NR + 1;
            INSERT INTO UTRQHSDETAILS(RQ, TR_SEQ, EV_SEQ, SEQ, DETAILS)
            VALUES(A_RQ(L_SEQ_NO), UNAPIGEN.P_TR_SEQ, L_EV_SEQ_NR, L_HS_DETAILS_SEQ_NR,
                   'info card "'||A_IC(L_SEQ_NO)||'" is deleted.');
         ELSE
            
            
            
            IF L_EVENT_TP = 'RqInfoCardCreated' THEN
               L_HS_DETAILS_SEQ_NR := L_HS_DETAILS_SEQ_NR + 1;
               INSERT INTO UTRQICHSDETAILS(RQ, IC, ICNODE, TR_SEQ, EV_SEQ, SEQ, DETAILS)
               VALUES(A_RQ(L_SEQ_NO), A_IC(L_SEQ_NO), A_ICNODE(L_SEQ_NO), UNAPIGEN.P_TR_SEQ, L_EV_SEQ_NR,
                      L_HS_DETAILS_SEQ_NR, 'info card "'||A_IC(L_SEQ_NO)||'" is created.');
            ELSE
               L_HS_DETAILS_SEQ_NR := L_HS_DETAILS_SEQ_NR + 1;
               INSERT INTO UTRQICHSDETAILS(RQ, IC, ICNODE, TR_SEQ, EV_SEQ, SEQ, DETAILS)
               VALUES(A_RQ(L_SEQ_NO), A_IC(L_SEQ_NO), A_ICNODE(L_SEQ_NO), UNAPIGEN.P_TR_SEQ, L_EV_SEQ_NR,
                      L_HS_DETAILS_SEQ_NR, 'info card "'||A_IC(L_SEQ_NO)||'" is updated.');

               UNAPIHSDETAILS.ADDRQICHSDETAILS(L_RQICOLD_REC, L_RQICNEW_REC, UNAPIGEN.P_TR_SEQ, 
                                               L_EV_SEQ_NR, L_HS_DETAILS_SEQ_NR); 
            END IF;
         END IF;
      END IF;

      
      
      
      
      
      
      IF L_IC_RECORD_OK AND
         A_MODIFY_FLAG(L_SEQ_NO) IN (UNAPIGEN.MOD_FLAG_CREATE,
                                     UNAPIGEN.MOD_FLAG_INSERT_AND_CRAU,
                                     UNAPIGEN.MOD_FLAG_INSERT_NODES_AND_CRAU) THEN
         
         
         
         
         L_RET_CODE := UNAPIRQIC2.INITANDSAVERQICATTRIBUTES(A_RQ(L_SEQ_NO), 
                                                             A_IC(L_SEQ_NO), A_ICNODE(L_SEQ_NO));
         IF L_RET_CODE <> UNAPIGEN.DBERR_SUCCESS THEN
            UNAPIGEN.P_TXN_ERROR := L_RET_CODE;
            RAISE STPERROR;
         END IF;

         IF A_MODIFY_FLAG(L_SEQ_NO) = UNAPIGEN.MOD_FLAG_CREATE THEN

            OPEN L_RQ_CURSOR(A_RQ(L_SEQ_NO));
            FETCH L_RQ_CURSOR
            INTO L_RT, L_RT_VERSION;
            IF L_RQ_CURSOR%NOTFOUND THEN
               UNAPIGEN.P_TXN_ERROR := UNAPIGEN.DBERR_NOOBJECT;
               RAISE STPERROR;
            END IF;
            CLOSE L_RQ_CURSOR;

            
            
            
            L_RET_CODE := UNAPIRQIC.CREATERQICDETAILS(L_RT, L_RT_VERSION, A_IC(L_SEQ_NO), 
                                                      L_IP_VERSION, L_SEQ_NO, A_RQ(L_SEQ_NO), 
                                                      A_ICNODE(L_SEQ_NO), A_MODIFY_REASON);
            
            
            
            IF L_RET_CODE <> UNAPIGEN.DBERR_SUCCESS THEN
               L_ERRM := 'rq=' || A_RQ(L_SEQ_NO)|| 
                         '#rt='|| L_RT || '#rt_version='|| L_RT_VERSION ||
                         '#ic=' || A_IC(L_SEQ_NO) || '#ip_version=' || L_IP_VERSION ||
                         '#icnode=' || TO_CHAR(A_ICNODE(L_SEQ_NO)) ||
                         '#CreateRqIcDetails#ErrorCode=' || TO_CHAR(L_RET_CODE);
               UNAPIGEN.P_TXN_ERROR := L_RET_CODE;
               RAISE STPERROR;
            END IF;
         END IF;
      END IF;

   
   
   
   END LOOP;

   IF UNAPIGEN.ENDTXN <> UNAPIGEN.DBERR_SUCCESS THEN
      RAISE STPERROR;
   END IF;

   
   
   
   FOR L_SEQ_NO IN 1..A_NR_OF_ROWS LOOP
      IF A_MODIFY_FLAG(L_SEQ_NO) < UNAPIGEN.DBERR_SUCCESS THEN
         A_MODIFY_FLAG(L_SEQ_NO) := UNAPIGEN.DBERR_SUCCESS;
      END IF;
   END LOOP;

   IF L_COMPLETELY_SAVED THEN
      RETURN(UNAPIGEN.DBERR_SUCCESS);
   ELSE
      RETURN(UNAPIGEN.DBERR_PARTIALSAVE);
   END IF;

EXCEPTION
WHEN OTHERS THEN
   IF SQLCODE <> 1 THEN
      UNAPIGEN.LOGERROR('SaveRqInfoCard',SQLERRM);
   END IF;
   IF L_RQ_CURSOR%ISOPEN THEN
      CLOSE L_RQ_CURSOR;
   END IF;
   IF L_RQICOLD_CURSOR%ISOPEN THEN
      CLOSE L_RQICOLD_CURSOR;
   END IF;
   RETURN(UNAPIGEN.ABORTTXN(UNAPIGEN.P_TXN_ERROR, 'SaveRqInfoCard'));
END SAVERQINFOCARD;

FUNCTION SAVERQINFOFIELD                                  
(A_RQ               IN       UNAPIGEN.VC20_TABLE_TYPE,    
 A_IC               IN       UNAPIGEN.VC20_TABLE_TYPE,    
 A_ICNODE           IN OUT   UNAPIGEN.LONG_TABLE_TYPE,    
 A_II               IN       UNAPIGEN.VC20_TABLE_TYPE,    
 A_IINODE           IN OUT   UNAPIGEN.LONG_TABLE_TYPE,    
 A_IE_VERSION       IN       UNAPIGEN.VC20_TABLE_TYPE,    
 A_IIVALUE          IN       UNAPIGEN.VC2000_TABLE_TYPE,  
 A_POS_X            IN       UNAPIGEN.NUM_TABLE_TYPE,     
 A_POS_Y            IN       UNAPIGEN.NUM_TABLE_TYPE,     
 A_IS_PROTECTED     IN       UNAPIGEN.CHAR1_TABLE_TYPE,   
 A_MANDATORY        IN       UNAPIGEN.CHAR1_TABLE_TYPE,   
 A_HIDDEN           IN       UNAPIGEN.CHAR1_TABLE_TYPE,   
 A_DSP_TITLE        IN       UNAPIGEN.VC40_TABLE_TYPE,    
 A_DSP_LEN          IN       UNAPIGEN.NUM_TABLE_TYPE,     
 A_DSP_TP           IN       UNAPIGEN.CHAR1_TABLE_TYPE,   
 A_DSP_ROWS         IN       UNAPIGEN.NUM_TABLE_TYPE,     
 A_II_CLASS         IN       UNAPIGEN.VC2_TABLE_TYPE,     
 A_LOG_HS           IN       UNAPIGEN.CHAR1_TABLE_TYPE,   
 A_LOG_HS_DETAILS   IN       UNAPIGEN.CHAR1_TABLE_TYPE,   
 A_LC               IN       UNAPIGEN.VC2_TABLE_TYPE,     
 A_LC_VERSION       IN       UNAPIGEN.VC20_TABLE_TYPE,    
 A_MODIFY_FLAG      IN OUT   UNAPIGEN.NUM_TABLE_TYPE,     
 A_NR_OF_ROWS       IN       NUMBER,                      
 A_MODIFY_REASON    IN       VARCHAR2)                    
RETURN NUMBER IS

L_LC                   VARCHAR2(2);
L_LC_VERSION           VARCHAR2(20);
L_SS                   VARCHAR2(2);
L_LOG_HS               CHAR(1);
L_LOG_HS_DETAILS       CHAR(1);
L_ALLOW_MODIFY         CHAR(1);
L_ACTIVE               CHAR(1);
L_INSERT               BOOLEAN;
CURR_ITEM              INTEGER;
NEXT_ITEM              INTEGER;
PREV_NODE              NUMBER(9);
NEXT_NODE              NUMBER(9);
NODE_STEP              NUMBER(9);
L_SEQ_NO               INTEGER;
NBR_OF_NODES_TO_CREATE INTEGER;
L_COMPLETELY_SAVED     BOOLEAN;
L_II_RECORD_OK         BOOLEAN;
L_II_HANDLED           BOOLEAN;
L_DELETED_NODE         NUMBER(9);
L_CURRENT_TIMESTAMP              TIMESTAMP WITH TIME ZONE;
L_OLD_VALUE            VARCHAR2(2000);
L_NEW_VALUE            VARCHAR2(2000);
L_HS_DETAILS_SEQ_NR    INTEGER;
L_IE_VERSION           VARCHAR2(20);

L_SVIIV_SEQ               INTEGER;
L_SVIIV_RQ                UNAPIGEN.VC20_TABLE_TYPE;
L_SVIIV_IC                UNAPIGEN.VC20_TABLE_TYPE;
L_SVIIV_ICNODE            UNAPIGEN.LONG_TABLE_TYPE;
L_SVIIV_II                UNAPIGEN.VC20_TABLE_TYPE;
L_SVIIV_IINODE            UNAPIGEN.LONG_TABLE_TYPE;
L_SVIIV_IIVALUE           UNAPIGEN.VC2000_TABLE_TYPE;
L_SVIIV_MODIFY_FLAG       UNAPIGEN.NUM_TABLE_TYPE;
L_ROW                     INTEGER;

CURSOR L_RQIIOLD_CURSOR (A_RQ IN VARCHAR2,
                         A_IC IN VARCHAR2, A_ICNODE IN NUMBER,
                         A_II IN VARCHAR2, A_IINODE IN NUMBER) IS
   SELECT A.*
   FROM UDRQII A
   WHERE A.RQ = A_RQ
   AND A.IC = A_IC
   AND A.ICNODE = A_ICNODE
   AND A.II = A_II
   AND A.IINODE = A_IINODE;
L_RQIIOLD_REC UDRQII%ROWTYPE;
L_RQIINEW_REC UDRQII%ROWTYPE;

BEGIN

   IF UNAPIGEN.BEGINTXN(UNAPIGEN.P_SINGLE_API_TXN) <> UNAPIGEN.DBERR_SUCCESS THEN
      RAISE STPERROR;
   END IF;

   L_SVIIV_SEQ := 0;
   L_CURRENT_TIMESTAMP := CURRENT_TIMESTAMP;
   L_ERRM := NULL;
   L_HS_DETAILS_SEQ_NR := 0;
   
   
   
   CURR_ITEM := 1;
   WHILE CURR_ITEM <= A_NR_OF_ROWS LOOP
      IF A_MODIFY_FLAG(CURR_ITEM) = UNAPIGEN.MOD_FLAG_INSERT THEN

         IF NVL(A_IINODE(CURR_ITEM), 0) <> 0 THEN
            UNAPIGEN.P_TXN_ERROR := UNAPIGEN.DBERR_NEWNODENOTZERO;
            RAISE STPERROR;
         END IF;

         
         
         
         NEXT_ITEM := CURR_ITEM;
         NBR_OF_NODES_TO_CREATE := 0;
         WHILE A_MODIFY_FLAG(NEXT_ITEM) = UNAPIGEN.MOD_FLAG_INSERT AND
               NVL(A_RQ(NEXT_ITEM), ' ') = NVL(A_RQ(CURR_ITEM),' ') AND
               NVL(A_IC(NEXT_ITEM), ' ') = NVL(A_IC(CURR_ITEM),' ') AND
               NVL(A_ICNODE(NEXT_ITEM), 0) = NVL(A_ICNODE(CURR_ITEM),0)
            LOOP
            NBR_OF_NODES_TO_CREATE := NBR_OF_NODES_TO_CREATE + 1;
            IF NEXT_ITEM < A_NR_OF_ROWS THEN
               NEXT_ITEM := NEXT_ITEM + 1;
            ELSE
               EXIT;
            END IF;
         END LOOP;

         
         
         
         
         IF A_MODIFY_FLAG(NEXT_ITEM) <> UNAPIGEN.MOD_FLAG_INSERT AND
            A_RQ(NEXT_ITEM) = A_RQ(CURR_ITEM) AND
            A_IC(NEXT_ITEM) = A_IC(CURR_ITEM) AND
            A_ICNODE(NEXT_ITEM) = A_ICNODE(CURR_ITEM) THEN
            NEXT_NODE := A_IINODE(NEXT_ITEM);
            SELECT NVL(MAX(IINODE), 0)
            INTO PREV_NODE
            FROM UTRQII
            WHERE IINODE < NEXT_NODE
              AND RQ = A_RQ(CURR_ITEM)
              AND IC = A_IC(CURR_ITEM)
              AND ICNODE = A_ICNODE(CURR_ITEM);
            NODE_STEP :=
               TRUNC(ABS((NEXT_NODE - PREV_NODE)) / (NBR_OF_NODES_TO_CREATE + 1));
            IF NODE_STEP < 1 THEN
               UNAPIGEN.P_TXN_ERROR := UNAPIGEN.DBERR_NODELIMITOVERF;
               RAISE STPERROR;
            END IF;
        ELSE
           
           SELECT NVL(MAX(IINODE), 0)
           INTO PREV_NODE
           FROM UTRQII
           WHERE RQ = A_RQ(CURR_ITEM)
             AND IC = A_IC(CURR_ITEM)
             AND ICNODE = A_ICNODE(CURR_ITEM);
           NODE_STEP := UNAPIGEN.DEFAULT_NODE_INTERVAL;
        END IF;

         
         
         
         FOR I IN 1..NBR_OF_NODES_TO_CREATE LOOP
            A_IINODE(CURR_ITEM + I - 1) := PREV_NODE + (NODE_STEP * I);
         END LOOP;

         CURR_ITEM := CURR_ITEM + NBR_OF_NODES_TO_CREATE;
      ELSE
         CURR_ITEM := CURR_ITEM + 1;
      END IF;
   END LOOP;

   
   
   
   L_COMPLETELY_SAVED := TRUE;

   
   
   
   FOR L_SEQ_NO IN 1..A_NR_OF_ROWS LOOP
      L_II_RECORD_OK := TRUE;          
      L_II_HANDLED := TRUE;

      
      
      
      IF A_MODIFY_FLAG(L_SEQ_NO) IN (UNAPIGEN.MOD_FLAG_INSERT,
                                     UNAPIGEN.MOD_FLAG_INSERT_WITH_NODES) THEN
         IF NVL(A_IE_VERSION(L_SEQ_NO), ' ') = ' ' THEN
            UNAPIGEN.P_TXN_ERROR := UNAPIGEN.DBERR_IEVERSION;
            RAISE STPERROR;
         END IF;
      END IF;

      IF NVL(A_RQ(L_SEQ_NO), ' ') = ' ' OR
         NVL(A_IC(L_SEQ_NO), ' ') = ' ' OR
         NVL(A_II(L_SEQ_NO), ' ') = ' ' THEN
         UNAPIGEN.P_TXN_ERROR := UNAPIGEN.DBERR_NOOBJID;
         RAISE STPERROR;
      END IF;

      
      
      
      IF A_MODIFY_FLAG(L_SEQ_NO) IN (UNAPIGEN.MOD_FLAG_INSERT,
                                     UNAPIGEN.MOD_FLAG_INSERT_WITH_NODES,
                                     UNAPIGEN.MOD_FLAG_UPDATE,
                                     UNAPIGEN.MOD_FLAG_DELETE) THEN
         L_IE_VERSION := A_IE_VERSION(L_SEQ_NO);
         L_RET_CODE := UNAPIAUT.GETRQIIAUTHORISATION(A_RQ(L_SEQ_NO), A_IC(L_SEQ_NO), A_ICNODE(L_SEQ_NO),
                                                     A_II(L_SEQ_NO), A_IINODE(L_SEQ_NO), L_IE_VERSION, 
                                                     L_LC, L_LC_VERSION, L_SS, L_ALLOW_MODIFY,
                                                     L_ACTIVE, L_LOG_HS, L_LOG_HS_DETAILS);
         IF L_RET_CODE = UNAPIGEN.DBERR_NOOBJECT THEN
            L_INSERT := TRUE;
            L_LOG_HS := '1';
            L_LOG_HS_DETAILS := '1';
         ELSIF L_RET_CODE = UNAPIGEN.DBERR_SUCCESS THEN
            L_INSERT := FALSE;
         ELSE
            A_MODIFY_FLAG(L_SEQ_NO) := L_RET_CODE;
            L_II_RECORD_OK := FALSE;
         END IF;
      ELSIF A_MODIFY_FLAG(L_SEQ_NO) = UNAPIGEN.DBERR_SUCCESS THEN
         
         
         
         L_II_RECORD_OK := FALSE;
         L_II_HANDLED := FALSE;
      ELSE
         UNAPIGEN.P_TXN_ERROR := UNAPIGEN.DBERR_INVALMODFLAG;
         RAISE STPERROR;         
      END IF;

      
      
      

      
      
      
      IF L_II_RECORD_OK AND
         A_MODIFY_FLAG(L_SEQ_NO) = UNAPIGEN.MOD_FLAG_UPDATE THEN

         
         
         
         IF L_INSERT THEN
            
            A_MODIFY_FLAG(L_SEQ_NO) := UNAPIGEN.DBERR_NOOBJECT;
            L_II_RECORD_OK := FALSE;
         ELSIF NVL(A_IS_PROTECTED(L_SEQ_NO), ' ') NOT IN ('0', '1') AND 
               NOT (UNAPIGEN.P_RNDSUITESESSION = '1' AND NVL(A_IS_PROTECTED(L_SEQ_NO), ' ') IN ('0', '1', '4', '5')) THEN
            UNAPIGEN.P_TXN_ERROR := UNAPIGEN.DBERR_PROTECTED;
            RAISE STPERROR;
         ELSIF NVL(A_HIDDEN(L_SEQ_NO), ' ') NOT IN ('0', '1') THEN
            UNAPIGEN.P_TXN_ERROR := UNAPIGEN.DBERR_HIDDEN;
            RAISE STPERROR;
         ELSIF NVL(A_MANDATORY(L_SEQ_NO), ' ') NOT IN ('0', '1') THEN
            UNAPIGEN.P_TXN_ERROR := UNAPIGEN.DBERR_MANDATORY;
            RAISE STPERROR;
         ELSE
            
            
            

            
            
            
            OPEN L_RQIIOLD_CURSOR(A_RQ(L_SEQ_NO),
                                  A_IC(L_SEQ_NO), A_ICNODE(L_SEQ_NO),
                                  A_II(L_SEQ_NO), A_IINODE(L_SEQ_NO));
            FETCH L_RQIIOLD_CURSOR
            INTO L_RQIIOLD_REC;
            CLOSE L_RQIIOLD_CURSOR;
            L_RQIINEW_REC := L_RQIIOLD_REC;
            L_NEW_VALUE := A_IIVALUE(L_SEQ_NO);

            
            
            
            
            
            
            UPDATE UTRQII
            SET POS_X          = A_POS_X(L_SEQ_NO),
                POS_Y          = A_POS_Y(L_SEQ_NO),
                IS_PROTECTED   = A_IS_PROTECTED(L_SEQ_NO),
                MANDATORY      = A_MANDATORY(L_SEQ_NO),
                HIDDEN         = A_HIDDEN(L_SEQ_NO),
                DSP_TITLE      = A_DSP_TITLE(L_SEQ_NO),
                DSP_LEN        = A_DSP_LEN(L_SEQ_NO),
                DSP_TP         = A_DSP_TP(L_SEQ_NO),
                DSP_ROWS       = A_DSP_ROWS(L_SEQ_NO),
                II_CLASS       = A_II_CLASS(L_SEQ_NO),
                LOG_HS         = A_LOG_HS(L_SEQ_NO),
                LOG_HS_DETAILS = A_LOG_HS_DETAILS(L_SEQ_NO),
                ALLOW_MODIFY   = '1'
            WHERE RQ = A_RQ(L_SEQ_NO)
              AND IC = A_IC(L_SEQ_NO)
              AND ICNODE = A_ICNODE(L_SEQ_NO)
              AND II = A_II(L_SEQ_NO)
              AND IINODE = A_IINODE(L_SEQ_NO)
            RETURNING POS_X, POS_Y, IS_PROTECTED, MANDATORY, HIDDEN, DSP_TITLE, DSP_LEN, DSP_TP, 
                      DSP_ROWS, II_CLASS, LOG_HS, LOG_HS_DETAILS, ALLOW_MODIFY
            INTO L_RQIINEW_REC.POS_X, L_RQIINEW_REC.POS_Y, L_RQIINEW_REC.IS_PROTECTED, 
                 L_RQIINEW_REC.MANDATORY, L_RQIINEW_REC.HIDDEN, L_RQIINEW_REC.DSP_TITLE, 
                 L_RQIINEW_REC.DSP_LEN, L_RQIINEW_REC.DSP_TP, L_RQIINEW_REC.DSP_ROWS, 
                 L_RQIINEW_REC.II_CLASS, L_RQIINEW_REC.LOG_HS, L_RQIINEW_REC.LOG_HS_DETAILS, 
                 L_RQIINEW_REC.ALLOW_MODIFY;

            IF (NVL(L_RQIIOLD_REC.IIVALUE, ' ') <> NVL(L_NEW_VALUE, ' ')) OR
               (L_RQIIOLD_REC.IIVALUE IS NULL AND L_NEW_VALUE IS NOT NULL) OR
               (L_RQIIOLD_REC.IIVALUE IS NOT NULL AND L_NEW_VALUE IS NULL) THEN

               
               
               
               L_SVIIV_SEQ := L_SVIIV_SEQ + 1;
               L_SVIIV_RQ(L_SVIIV_SEQ) := A_RQ(L_SEQ_NO);
               L_SVIIV_IC(L_SVIIV_SEQ) := A_IC(L_SEQ_NO);
               L_SVIIV_ICNODE(L_SVIIV_SEQ) := A_ICNODE(L_SEQ_NO);
               L_SVIIV_II(L_SVIIV_SEQ) := A_II(L_SEQ_NO);
               L_SVIIV_IINODE(L_SVIIV_SEQ) := A_IINODE(L_SEQ_NO);
               L_SVIIV_IIVALUE(L_SVIIV_SEQ) := A_IIVALUE(L_SEQ_NO);
               L_SVIIV_MODIFY_FLAG(L_SVIIV_SEQ) := UNAPIGEN.MOD_FLAG_UPDATE;
            END IF;
            
            
            
            
            L_EVENT_TP := 'RqInfoFieldUpdated';
         END IF;

      
      
      
      ELSIF L_II_RECORD_OK AND
            A_MODIFY_FLAG(L_SEQ_NO) IN (UNAPIGEN.MOD_FLAG_INSERT,
                                        UNAPIGEN.MOD_FLAG_INSERT_WITH_NODES)THEN

         
         
         
         IF NOT L_INSERT THEN
            UNAPIGEN.P_TXN_ERROR := UNAPIGEN.DBERR_IIALREADYEXIST;
            RAISE STPERROR;
         ELSIF NVL(A_IS_PROTECTED(L_SEQ_NO), ' ') NOT IN ('0', '1') AND 
               NOT (UNAPIGEN.P_RNDSUITESESSION = '1' AND NVL(A_IS_PROTECTED(L_SEQ_NO), ' ') IN ('0', '1', '4', '5')) THEN
            UNAPIGEN.P_TXN_ERROR := UNAPIGEN.DBERR_PROTECTED;
            RAISE STPERROR;
         ELSIF NVL(A_HIDDEN(L_SEQ_NO), ' ') NOT IN ('0', '1') THEN
            UNAPIGEN.P_TXN_ERROR := UNAPIGEN.DBERR_HIDDEN;
            RAISE STPERROR;
         ELSIF NVL(A_MANDATORY(L_SEQ_NO), ' ') NOT IN ('0', '1') THEN
            UNAPIGEN.P_TXN_ERROR := UNAPIGEN.DBERR_MANDATORY;
            RAISE STPERROR;
         ELSE
            
            
            
            

            
            IF NVL(A_LC(L_SEQ_NO), ' ') <> ' ' THEN
               L_LC := A_LC(L_SEQ_NO);
            END IF;
            IF NVL(A_LC_VERSION(L_SEQ_NO), ' ') <> ' ' THEN
               L_LC_VERSION := A_LC_VERSION(L_SEQ_NO);
            END IF;

            
            
            INSERT INTO UTRQII(RQ, IC, ICNODE, II, IINODE, IE_VERSION, POS_X,
                               POS_Y, IS_PROTECTED, MANDATORY, HIDDEN,
                               DSP_TITLE, DSP_LEN, DSP_TP, DSP_ROWS, II_CLASS,
                               LOG_HS, LOG_HS_DETAILS, ALLOW_MODIFY, ACTIVE, LC, LC_VERSION, SS)
            VALUES(A_RQ(L_SEQ_NO), A_IC(L_SEQ_NO), A_ICNODE(L_SEQ_NO),
                   A_II(L_SEQ_NO),
                   A_IINODE(L_SEQ_NO), L_IE_VERSION,
                   A_POS_X(L_SEQ_NO), A_POS_Y(L_SEQ_NO),
                   A_IS_PROTECTED(L_SEQ_NO), A_MANDATORY(L_SEQ_NO),
                   A_HIDDEN(L_SEQ_NO), A_DSP_TITLE(L_SEQ_NO),
                   A_DSP_LEN(L_SEQ_NO), A_DSP_TP(L_SEQ_NO),
                   A_DSP_ROWS(L_SEQ_NO), A_II_CLASS(L_SEQ_NO),
                   A_LOG_HS(L_SEQ_NO), A_LOG_HS_DETAILS(L_SEQ_NO), '1', '1', L_LC, L_LC_VERSION, '');
            UNAPIAUT.UPDATELCINAUTHORISATIONBUFFER('ii', A_RQ(L_SEQ_NO) || A_IC(L_SEQ_NO) || TO_CHAR(A_ICNODE(L_SEQ_NO))
                                                         || A_II(L_SEQ_NO) || TO_CHAR(A_IINODE(L_SEQ_NO)), '', L_LC, L_LC_VERSION);                 

            IF A_IIVALUE(L_SEQ_NO) IS NOT NULL THEN

               
               
               
               L_SVIIV_SEQ := L_SVIIV_SEQ + 1;
               L_SVIIV_RQ(L_SVIIV_SEQ) := A_RQ(L_SEQ_NO);
               L_SVIIV_IC(L_SVIIV_SEQ) := A_IC(L_SEQ_NO);
               L_SVIIV_ICNODE(L_SVIIV_SEQ) := A_ICNODE(L_SEQ_NO);
               L_SVIIV_II(L_SVIIV_SEQ) := A_II(L_SEQ_NO);
               L_SVIIV_IINODE(L_SVIIV_SEQ) := A_IINODE(L_SEQ_NO);
               L_SVIIV_IIVALUE(L_SVIIV_SEQ) := A_IIVALUE(L_SEQ_NO);
               L_SVIIV_MODIFY_FLAG(L_SVIIV_SEQ) := UNAPIGEN.MOD_FLAG_UPDATE;

            END IF;

            
            
            
            L_EVENT_TP := 'RqInfoFieldCreated';
         END IF;

      
      
      
      ELSIF L_II_RECORD_OK AND
            A_MODIFY_FLAG(L_SEQ_NO) = UNAPIGEN.MOD_FLAG_DELETE THEN

         IF L_INSERT THEN
            
            UNAPIGEN.P_TXN_ERROR := UNAPIGEN.DBERR_NOOBJECT;
            RAISE STPERROR;
         ELSE
            
            IF UNAPIGEN.P_RNDSUITESESSION = '0' THEN
               
               IF UNAPIGEN.ISSYSTEM21CFR11COMPLIANT = UNAPIGEN.DBERR_SUCCESS THEN
                  UNAPIGEN.P_TXN_ERROR := UNAPIGEN.DBERR_NOTALLOWEDIN21CFR11;
                  RAISE STPERROR;
               
               
               
               ELSIF L_ACTIVE = '1' THEN
                  UNAPIGEN.P_TXN_ERROR := UNAPIGEN.DBERR_OPACTIVE;
                  RAISE STPERROR;
               END IF;
            END IF;

            
            L_OLD_VALUE := '';
            SELECT IIVALUE
            INTO   L_OLD_VALUE
            FROM   UTRQII
            WHERE RQ = A_RQ(L_SEQ_NO)
              AND IC = A_IC(L_SEQ_NO)
              AND ICNODE = A_ICNODE(L_SEQ_NO)
              AND II = A_II(L_SEQ_NO)
              AND IINODE = A_IINODE(L_SEQ_NO);

            
            
            
            DELETE FROM UTRQII
            WHERE RQ = A_RQ(L_SEQ_NO)
              AND IC = A_IC(L_SEQ_NO)
              AND ICNODE = A_ICNODE(L_SEQ_NO)
              AND II = A_II(L_SEQ_NO)
              AND IINODE = A_IINODE(L_SEQ_NO);

            
            
            
            DELETE FROM UTEVTIMED
            WHERE OBJECT_TP = 'rqii'
              AND OBJECT_ID = A_II(L_SEQ_NO)
              AND INSTR(EV_DETAILS, 'rq=' || A_RQ(L_SEQ_NO)) <> 0
              AND INSTR(EV_DETAILS, 'ic=' || A_IC(L_SEQ_NO)) <> 0
              AND INSTR(EV_DETAILS, 'icnode=' || TO_CHAR(A_ICNODE(L_SEQ_NO))) <> 0
              AND INSTR(EV_DETAILS, 'iinode=' || TO_CHAR(A_IINODE(L_SEQ_NO))) <> 0;

            DELETE FROM UTEVTIMED
            WHERE INSTR(EV_DETAILS, 'rq=' || A_RQ(L_SEQ_NO)) <> 0
              AND INSTR(EV_DETAILS, 'ic=' || A_IC(L_SEQ_NO)) <> 0
              AND INSTR(EV_DETAILS, 'icnode=' || TO_CHAR(A_ICNODE(L_SEQ_NO))) <> 0
              AND INSTR(EV_DETAILS, 'ii=' || A_II(L_SEQ_NO)) <> 0
              AND INSTR(EV_DETAILS, 'iinode=' || TO_CHAR(A_IINODE(L_SEQ_NO))) <> 0;

            DELETE FROM UTEVRULESDELAYED
            WHERE OBJECT_TP = 'rqii'
              AND OBJECT_ID = A_II(L_SEQ_NO)
              AND INSTR(EV_DETAILS, 'rq=' || A_RQ(L_SEQ_NO)) <> 0
              AND INSTR(EV_DETAILS, 'ic=' || A_IC(L_SEQ_NO)) <> 0
              AND INSTR(EV_DETAILS, 'icnode=' || TO_CHAR(A_ICNODE(L_SEQ_NO))) <> 0
              AND INSTR(EV_DETAILS, 'iinode=' || TO_CHAR(A_IINODE(L_SEQ_NO))) <> 0;

            DELETE FROM UTEVRULESDELAYED
            WHERE INSTR(EV_DETAILS, 'rq=' || A_RQ(L_SEQ_NO)) <> 0
              AND INSTR(EV_DETAILS, 'ic=' || A_IC(L_SEQ_NO)) <> 0
              AND INSTR(EV_DETAILS, 'icnode=' || TO_CHAR(A_ICNODE(L_SEQ_NO))) <> 0
              AND INSTR(EV_DETAILS, 'ii=' || A_II(L_SEQ_NO)) <> 0
              AND INSTR(EV_DETAILS, 'iinode=' || TO_CHAR(A_IINODE(L_SEQ_NO))) <> 0;

            L_DELETED_NODE := A_IINODE(L_SEQ_NO);
            A_IINODE(L_SEQ_NO) := 0;
            
            
            
            L_EVENT_TP := 'RqInfoFieldDeleted';
         END IF;
      END IF;

      
      
      
      IF L_II_RECORD_OK THEN
         L_EV_SEQ_NR := -1;
         L_EV_DETAILS := 'rq=' || A_RQ(L_SEQ_NO) || 
                         '#ic=' || A_IC(L_SEQ_NO) ||
                         '#icnode=' || TO_CHAR(A_ICNODE(L_SEQ_NO)) || 
                         '#iinode=' || TO_CHAR(A_IINODE(L_SEQ_NO)) ||
                         '#ie_version=' || L_IE_VERSION;
         IF L_EVENT_TP = 'RqInfoFieldCreated' THEN
            L_EV_DETAILS := L_EV_DETAILS || '#new_value=' || SUBSTR(A_IIVALUE(L_SEQ_NO),1,40);
         ELSIF L_EVENT_TP = 'RqInfoFieldDeleted' THEN
            L_EV_DETAILS := L_EV_DETAILS || '#old_value=' || SUBSTR(L_OLD_VALUE,1,40);
         END IF;

         L_RESULT := UNAPIEV.INSERTINFOFIELDEVENT('SaveRqInfoField', UNAPIGEN.P_EVMGR_NAME, 
                                                  'rqii', A_II(L_SEQ_NO), L_LC, L_LC_VERSION, 
                                                  L_SS, L_EVENT_TP, L_EV_DETAILS, L_EV_SEQ_NR);
         IF L_RESULT <> UNAPIGEN.DBERR_SUCCESS THEN
            UNAPIGEN.P_TXN_ERROR := L_RESULT;
            RAISE STPERROR;
         END IF;

         
         IF L_EV_SEQ_NR = -1 THEN
            
            
            OPEN L_SEQ_EVENT_CURSOR;
            FETCH L_SEQ_EVENT_CURSOR INTO L_EV_SEQ_NR;
            CLOSE L_SEQ_EVENT_CURSOR;
         END IF;
      ELSE
         IF L_II_HANDLED THEN
            L_COMPLETELY_SAVED := FALSE;
         END IF;
      END IF;

      
      
      
      IF L_II_RECORD_OK AND L_LOG_HS = '1' THEN
         IF L_EVENT_TP = 'RqInfoFieldDeleted' THEN
            INSERT INTO UTRQICHS(RQ, IC, ICNODE, WHO, WHO_DESCRIPTION, WHAT, WHAT_DESCRIPTION, 
                                 LOGDATE, LOGDATE_TZ, WHY, TR_SEQ, EV_SEQ)
            VALUES(A_RQ(L_SEQ_NO), A_IC(L_SEQ_NO), A_ICNODE(L_SEQ_NO), UNAPIGEN.P_USER, 
                   UNAPIGEN.P_USER_DESCRIPTION, L_EVENT_TP, 
                   'info field "'||A_II(L_SEQ_NO)||'" is deleted.', 
                   L_CURRENT_TIMESTAMP, L_CURRENT_TIMESTAMP, A_MODIFY_REASON, UNAPIGEN.P_TR_SEQ, L_EV_SEQ_NR);
         ELSIF L_EVENT_TP = 'RqInfoFieldCreated' THEN
            INSERT INTO UTRQICHS(RQ, IC, ICNODE, WHO, WHO_DESCRIPTION, WHAT, WHAT_DESCRIPTION, 
                                 LOGDATE, LOGDATE_TZ, WHY, TR_SEQ, EV_SEQ)
            VALUES(A_RQ(L_SEQ_NO), A_IC(L_SEQ_NO), A_ICNODE(L_SEQ_NO), UNAPIGEN.P_USER,
                   UNAPIGEN.P_USER_DESCRIPTION, L_EVENT_TP, 
                   'info field "'||A_II(L_SEQ_NO)||'" is created.', 
                   L_CURRENT_TIMESTAMP, L_CURRENT_TIMESTAMP, A_MODIFY_REASON, UNAPIGEN.P_TR_SEQ, L_EV_SEQ_NR);
         ELSE
            INSERT INTO UTRQICHS(RQ, IC, ICNODE, WHO, WHO_DESCRIPTION, WHAT, WHAT_DESCRIPTION, 
                                 LOGDATE, LOGDATE_TZ, WHY, TR_SEQ, EV_SEQ)
            VALUES(A_RQ(L_SEQ_NO), A_IC(L_SEQ_NO), A_ICNODE(L_SEQ_NO), UNAPIGEN.P_USER,
                   UNAPIGEN.P_USER_DESCRIPTION, L_EVENT_TP, 
                   'info field "'||A_II(L_SEQ_NO)||'" is updated.', 
                   L_CURRENT_TIMESTAMP, L_CURRENT_TIMESTAMP, A_MODIFY_REASON, UNAPIGEN.P_TR_SEQ, L_EV_SEQ_NR);
         END IF;
      END IF;

      
      
      
      IF L_II_RECORD_OK AND L_LOG_HS_DETAILS = '1' THEN
         IF L_EVENT_TP = 'RqInfoFieldDeleted' THEN
            L_HS_DETAILS_SEQ_NR := L_HS_DETAILS_SEQ_NR + 1;
            INSERT INTO UTRQICHSDETAILS(RQ, IC, ICNODE, TR_SEQ, EV_SEQ, SEQ, DETAILS)
            VALUES(A_RQ(L_SEQ_NO), A_IC(L_SEQ_NO), A_ICNODE(L_SEQ_NO), UNAPIGEN.P_TR_SEQ, L_EV_SEQ_NR, 
                   L_HS_DETAILS_SEQ_NR, 'info field "'||A_II(L_SEQ_NO)||'" is deleted.');
         ELSIF L_EVENT_TP = 'RqInfoFieldCreated' THEN
            L_HS_DETAILS_SEQ_NR := L_HS_DETAILS_SEQ_NR + 1;
            INSERT INTO UTRQICHSDETAILS(RQ, IC, ICNODE, TR_SEQ, EV_SEQ, SEQ, DETAILS)
            VALUES(A_RQ(L_SEQ_NO), A_IC(L_SEQ_NO), A_ICNODE(L_SEQ_NO), UNAPIGEN.P_TR_SEQ, L_EV_SEQ_NR, 
                   L_HS_DETAILS_SEQ_NR, 'info field "'||A_II(L_SEQ_NO)||'" is created.');
         ELSE
            L_HS_DETAILS_SEQ_NR := L_HS_DETAILS_SEQ_NR + 1;
            INSERT INTO UTRQICHSDETAILS(RQ, IC, ICNODE, TR_SEQ, EV_SEQ, SEQ, DETAILS)
            VALUES(A_RQ(L_SEQ_NO), A_IC(L_SEQ_NO), A_ICNODE(L_SEQ_NO), UNAPIGEN.P_TR_SEQ, L_EV_SEQ_NR, 
                   L_HS_DETAILS_SEQ_NR, 'info field "'||A_II(L_SEQ_NO)||'" is updated.');

            UNAPIHSDETAILS.ADDRQIIHSDETAILS(L_RQIIOLD_REC, L_RQIINEW_REC, UNAPIGEN.P_TR_SEQ, 
                                            L_EV_SEQ_NR, L_HS_DETAILS_SEQ_NR); 
         END IF;
      END IF;
   
   
   
   END LOOP;

   
   
   
   IF L_SVIIV_SEQ <> 0 THEN
      L_RET_CODE := UNAPIRQIC.SAVERQIIVALUE(L_SVIIV_RQ, L_SVIIV_IC, L_SVIIV_ICNODE,
                                 L_SVIIV_II, L_SVIIV_IINODE,
                                 L_SVIIV_IIVALUE, 
                                 L_SVIIV_MODIFY_FLAG,
                                 L_SVIIV_SEQ, A_MODIFY_REASON);
      IF L_RET_CODE = UNAPIGEN.DBERR_PARTIALSAVE THEN
         
         
         
         FOR L_ROW IN 1..L_SVIIV_SEQ LOOP
            IF L_SVIIV_MODIFY_FLAG(L_ROW) > UNAPIGEN.DBERR_SUCCESS THEN
               UNAPIGEN.P_TXN_ERROR := L_SVIIV_MODIFY_FLAG(L_ROW);
               L_ERRM := 'rq=' || L_SVIIV_RQ(L_ROW) || '#ic=' || L_SVIIV_IC(L_ROW) ||
                         '#icnode=' || TO_CHAR(L_SVIIV_ICNODE(L_ROW)) ||
                         '#ii=' || L_SVIIV_II(L_ROW)||
                         '#iinode=' || TO_CHAR(L_SVIIV_IINODE(L_ROW)) ||
                         '#SaveRqIiValue#ErrorCode=' || TO_CHAR(L_RET_CODE);
               RAISE STPERROR;
            END IF;
         END LOOP;
      ELSIF L_RET_CODE <> UNAPIGEN.DBERR_SUCCESS THEN
         UNAPIGEN.P_TXN_ERROR := L_RET_CODE;
         L_ERRM := 'rq=' || L_SVIIV_RQ(L_ROW) || '#ic=' || L_SVIIV_IC(L_ROW) ||
                   '#icnode=' || TO_CHAR(L_SVIIV_ICNODE(L_ROW)) ||
                   '#ii=' || L_SVIIV_II(L_ROW)||
                   '#iinode=' || TO_CHAR(L_SVIIV_IINODE(L_ROW)) ||
                   '#nr_of_rows=' || TO_CHAR(L_SVIIV_SEQ) ||
                   '#SaveRqIiValue#ErrorCode=' || TO_CHAR(L_RET_CODE);
         RAISE STPERROR;
      END IF;                           
   END IF;

   IF UNAPIGEN.ENDTXN <> UNAPIGEN.DBERR_SUCCESS THEN
      RAISE STPERROR;
   END IF;

   
   
   
   FOR L_SEQ_NO IN 1..A_NR_OF_ROWS LOOP
      IF A_MODIFY_FLAG(L_SEQ_NO) < UNAPIGEN.DBERR_SUCCESS THEN
         A_MODIFY_FLAG(L_SEQ_NO) := UNAPIGEN.DBERR_SUCCESS;
      END IF;
   END LOOP;

   IF L_COMPLETELY_SAVED THEN
      RETURN(UNAPIGEN.DBERR_SUCCESS);
   ELSE
      RETURN(UNAPIGEN.DBERR_PARTIALSAVE);
   END IF;

EXCEPTION
WHEN OTHERS THEN
   IF SQLCODE <> 1 THEN
      UNAPIGEN.LOGERROR('SaveRqInfoField',SQLERRM);
   ELSIF L_ERRM IS NOT NULL THEN
      UNAPIGEN.LOGERROR('SaveRqInfoField',L_ERRM);   
   END IF;
   IF L_RQIIOLD_CURSOR%ISOPEN THEN
      CLOSE L_RQIIOLD_CURSOR;
   END IF;
   IF L_SEQ_EVENT_CURSOR%ISOPEN THEN
      CLOSE L_SEQ_EVENT_CURSOR;
   END IF;
   RETURN(UNAPIGEN.ABORTTXN(UNAPIGEN.P_TXN_ERROR, 'SaveRqInfoField'));
END SAVERQINFOFIELD;

FUNCTION SAVERQIIVALUE                                    
(A_RQ               IN       UNAPIGEN.VC20_TABLE_TYPE,    
 A_IC               IN       UNAPIGEN.VC20_TABLE_TYPE,    
 A_ICNODE           IN OUT   UNAPIGEN.LONG_TABLE_TYPE,    
 A_II               IN       UNAPIGEN.VC20_TABLE_TYPE,    
 A_IINODE           IN OUT   UNAPIGEN.LONG_TABLE_TYPE,    
 A_IIVALUE          IN       UNAPIGEN.VC2000_TABLE_TYPE,  
 A_MODIFY_FLAG      IN OUT   UNAPIGEN.NUM_TABLE_TYPE,     
 A_NR_OF_ROWS       IN       NUMBER,                      
 A_MODIFY_REASON    IN       VARCHAR2)                    
RETURN NUMBER IS

L_LC                     VARCHAR2(2);
L_LC_VERSION             VARCHAR2(20);
L_SS                     VARCHAR2(2);
L_LOG_HS                 CHAR(1);
L_LOG_HS_DETAILS         CHAR(1);
L_ALLOW_MODIFY           CHAR(1);
L_ACTIVE                 CHAR(1);
L_CURRENT_TIMESTAMP                TIMESTAMP WITH TIME ZONE;
L_SEQ_NO                 INTEGER;
L_II_RECORD_OK           BOOLEAN;
L_II_OK                  BOOLEAN;
L_II_HANDLED             BOOLEAN;
L_COMPLETELY_SAVED       BOOLEAN;
L_SCAN_SEQ               INTEGER;
L_SCAN_PREV_RQ           VARCHAR2(20);
L_SCAN_PREV_IC           VARCHAR2(20);
L_SCAN_PREV_ICNODE       NUMBER;
L_IC_EVENT_INSERTED      BOOLEAN;
L_NEXT_II                VARCHAR2(20);
L_OLD_NEXT_II            VARCHAR2(20);
L_DATE_STRING            VARCHAR2(255);
L_DATE                   TIMESTAMP WITH TIME ZONE;
L_SQLERRM2               VARCHAR2(255);
L_HS_DETAILS_SEQ_NR      INTEGER;
L_IE_VERSION             VARCHAR2(20);
L_IP_VERSION             VARCHAR2(20);
L_IC_LOG_HS              CHAR(1);
L_IC_LOG_HS_DETAILS      CHAR(1);
L_LINKED_II              BOOLEAN;
L_POSITION               INTEGER;

CURSOR L_NEXT_II_CURSOR (C_RQ        VARCHAR2,
                         C_IC        VARCHAR2,
                         C_ICNODE    NUMBER) IS
   SELECT II
   FROM UTRQII
   WHERE RQ = C_RQ
     AND IC = C_IC
     AND ICNODE = C_ICNODE
     AND IIVALUE IS NULL
   ORDER BY IINODE;

CURSOR L_IEVALUE_CURSOR(A_IC VARCHAR2, A_IP_VERSION VARCHAR2, A_IE VARCHAR2, A_IE_VERSION VARCHAR2) IS
   SELECT B.IEVALUE, B.FORMAT
   FROM UTIE B
   WHERE B.DEF_VAL_TP = 'S'
     AND B.IE = A_IE
     AND NOT B.IEVALUE LIKE 'RT@%'
     AND B.VERSION = A_IE_VERSION
     AND (B.IE, B.VERSION) NOT IN (SELECT C.IE, UNAPIGEN.VALIDATEVERSION('ie',C.IE,C.IE_VERSION)
                                   FROM UTIPIE C
                                   WHERE C.IE = B.IE
                                     AND UNAPIGEN.VALIDATEVERSION('ie',C.IE,C.IE_VERSION) = B.VERSION
                                     AND C.IP = A_IC
                                     AND C.VERSION = A_IP_VERSION
                                     AND (C.DEF_VAL_TP<>'F' OR C.IEVALUE IS NOT NULL))
   UNION 
   SELECT B.IEVALUE, C.FORMAT
   FROM UTIPIE B, UTIE C
   WHERE B.IP = A_IC
     AND B.VERSION = A_IP_VERSION
     AND B.IE = C.IE
     AND NOT B.IEVALUE LIKE 'RT@%'
     AND UNAPIGEN.VALIDATEVERSION('ie',B.IE,B.IE_VERSION) = C.VERSION
     AND B.DEF_VAL_TP = 'S'
     AND C.IE = A_IE
     AND C.VERSION = A_IE_VERSION;
L_IEVALUE_REC       L_IEVALUE_CURSOR%ROWTYPE;

CURSOR L_RQIIOLD_CURSOR (A_RQ IN VARCHAR2,
                         A_IC IN VARCHAR2, A_ICNODE IN NUMBER,
                         A_II IN VARCHAR2, A_IINODE IN NUMBER) IS
   SELECT A.*
   FROM UDRQII A
   WHERE A.RQ = A_RQ
   AND A.IC = A_IC
   AND A.ICNODE = A_ICNODE
   AND A.II = A_II
   AND A.IINODE = A_IINODE;
L_RQIIOLD_REC UDRQII%ROWTYPE;
L_RQIINEW_REC UDRQII%ROWTYPE;

CURSOR L_RQ_CURSOR (A_RQ IN VARCHAR2) IS
   SELECT A.*
   FROM UDRQ A
   WHERE A.RQ = A_RQ;
L_RQOLD_REC UDRQ%ROWTYPE;
L_RQNEW_REC UDRQ%ROWTYPE;

CURSOR L_IPVERSION_CURSOR (C_RQ VARCHAR2, C_IC VARCHAR2, C_ICNODE NUMBER) IS
   SELECT IP_VERSION
   FROM UTRQIC
   WHERE RQ = C_RQ
     AND IC = C_IC
     AND ICNODE = C_ICNODE;

CURSOR L_RQIC_CURSOR (C_RQ VARCHAR2, C_IC VARCHAR2, C_ICNODE NUMBER) IS
   SELECT IP_VERSION, NEXT_II, LOG_HS, LOG_HS_DETAILS
   FROM UTRQIC
   WHERE RQ     = C_RQ
     AND IC     = C_IC
     AND ICNODE = C_ICNODE;

BEGIN
   IF UNAPIGEN.BEGINTXN(UNAPIGEN.P_SINGLE_API_TXN) <> UNAPIGEN.DBERR_SUCCESS THEN
      RAISE STPERROR;
   END IF;

   
   L_CURRENT_TIMESTAMP := CURRENT_TIMESTAMP;
   L_COMPLETELY_SAVED := TRUE;
   L_II_OK := FALSE;
   L_LINKED_II := FALSE;

   L_HS_DETAILS_SEQ_NR := 0;

   
   
   
   OPEN L_RQ_CURSOR(A_RQ(1));
   FETCH L_RQ_CURSOR
   INTO L_RQOLD_REC;
   CLOSE L_RQ_CURSOR;

   FOR L_SEQ_NO IN 1..A_NR_OF_ROWS LOOP
      L_II_RECORD_OK := TRUE;          
      L_II_HANDLED := TRUE;

      IF NVL(A_RQ(L_SEQ_NO), ' ') = ' ' OR
         NVL(A_IC(L_SEQ_NO), ' ') = ' ' OR
         NVL(A_ICNODE(L_SEQ_NO), 0) = 0 OR
         NVL(A_II(L_SEQ_NO), ' ') = ' ' OR
         NVL(A_IINODE(L_SEQ_NO), 0) = 0 THEN
         UNAPIGEN.P_TXN_ERROR := UNAPIGEN.DBERR_NOOBJID;
         RAISE STPERROR;

      ELSIF NVL(A_MODIFY_FLAG(L_SEQ_NO), UNAPIGEN.MOD_FLAG_INSERT)
            NOT IN (UNAPIGEN.DBERR_SUCCESS, UNAPIGEN.MOD_FLAG_UPDATE) THEN
         
         
         
         UNAPIGEN.P_TXN_ERROR := UNAPIGEN.DBERR_INVALMODFLAG;
         RAISE STPERROR;

      ELSIF A_MODIFY_FLAG(L_SEQ_NO) = UNAPIGEN.DBERR_SUCCESS THEN
         
         
         
         L_II_RECORD_OK := FALSE; 
         L_II_HANDLED := FALSE;   

      ELSE
         
         
         
         L_RET_CODE := UNAPIAUT.GETRQIIAUTHORISATION(A_RQ(L_SEQ_NO), A_IC(L_SEQ_NO), A_ICNODE(L_SEQ_NO),
                                                     A_II(L_SEQ_NO), A_IINODE(L_SEQ_NO), L_IE_VERSION, 
                                                     L_LC, L_LC_VERSION, L_SS, L_ALLOW_MODIFY,
                                                     L_ACTIVE, L_LOG_HS, L_LOG_HS_DETAILS);
        IF L_RET_CODE <> UNAPIGEN.DBERR_SUCCESS THEN
            A_MODIFY_FLAG(L_SEQ_NO) := L_RET_CODE;
            L_II_RECORD_OK := FALSE;
         END IF;
      END IF;

      IF L_II_RECORD_OK THEN
         
         
         L_II_OK := TRUE;
         
         
         
         
         OPEN L_RQIIOLD_CURSOR(A_RQ(L_SEQ_NO),
                               A_IC(L_SEQ_NO), A_ICNODE(L_SEQ_NO),
                               A_II(L_SEQ_NO), A_IINODE(L_SEQ_NO));
         FETCH L_RQIIOLD_CURSOR
         INTO L_RQIIOLD_REC;
         CLOSE L_RQIIOLD_CURSOR;
         L_RQIINEW_REC := L_RQIIOLD_REC;

         
         
         
         
         
         
         UPDATE UTRQII
         SET IIVALUE      = A_IIVALUE(L_SEQ_NO),
             ALLOW_MODIFY = '1'
         WHERE RQ = A_RQ(L_SEQ_NO)
           AND IC = A_IC(L_SEQ_NO)
           AND ICNODE = A_ICNODE(L_SEQ_NO)
           AND II = A_II(L_SEQ_NO)
           AND IINODE = A_IINODE(L_SEQ_NO)
         RETURNING IIVALUE, ALLOW_MODIFY
         INTO L_RQIINEW_REC.IIVALUE, L_RQIINEW_REC.ALLOW_MODIFY;

         OPEN L_IPVERSION_CURSOR(A_RQ(L_SEQ_NO), A_IC(L_SEQ_NO), A_ICNODE(L_SEQ_NO));
         FETCH L_IPVERSION_CURSOR INTO L_IP_VERSION;
         CLOSE L_IPVERSION_CURSOR;

         
         
         
         OPEN L_IEVALUE_CURSOR(A_IC(L_SEQ_NO), L_IP_VERSION, A_II(L_SEQ_NO), L_IE_VERSION);
         FETCH L_IEVALUE_CURSOR
         INTO L_IEVALUE_REC;
         IF L_IEVALUE_CURSOR%FOUND THEN

            L_POSITION := INSTR(L_IEVALUE_REC.IEVALUE,'@',1,1);
            IF L_POSITION > 0 THEN
               IF SUBSTR(L_IEVALUE_REC.IEVALUE,0,L_POSITION-1) = 'RT' THEN
                  
                  
                  
                  
                  
                  
                  L_SQLERRM := L_IEVALUE_REC.IEVALUE || ' is a request-type standard property (which can not be changed) for info field '||A_II(L_SEQ_NO)||
                               ' in info profile '||A_IC(L_SEQ_NO) ||' rq=' || A_RQ(L_SEQ_NO) || '#icnode=' ||
                               TO_CHAR(A_ICNODE(L_SEQ_NO)) || '#iinode=' || TO_CHAR(A_IINODE(L_SEQ_NO));
                  RAISE STPERROR;
               END IF;
               L_IEVALUE_REC.IEVALUE := SUBSTR(L_IEVALUE_REC.IEVALUE, L_POSITION+1);
            END IF;

            
            L_DATE := NULL;
            IF  L_IEVALUE_REC.IEVALUE IN ('sampling_date', 'creation_date', 'exec_start_date', 'exec_end_date', 
                                          'due_date', 'date1', 'date2', 'date3', 'date4', 'date5') THEN
                IF UNAPIGEN.P_RNDSUITESESSION = '1' THEN 
                   L_DATE_STRING := A_IIVALUE(L_SEQ_NO)||'@YYYY-MM-DD"T"HH24:MI:SS.FF1TZH:TZM';
                ELSE
                   L_DATE_STRING := A_IIVALUE(L_SEQ_NO)||'@'||SUBSTR(L_IEVALUE_REC.FORMAT,2);
                END IF;
                L_RET_CODE := UNAPIGEN.DATEVALID(L_DATE_STRING, L_DATE, L_SQLERRM);
                IF L_RET_CODE <> UNAPIGEN.DBERR_SUCCESS THEN
                   IF UNAPIGEN.P_RNDSUITESESSION = '1' THEN 
                      L_SQLERRM2 := 'UNAPIGEN.DateValid('''||A_IIVALUE(L_SEQ_NO)||'@YYYY-MM-DD"T"HH24:MI:SS.FF1TZH:TZM'') ret_code='||
                                    TO_CHAR(L_RET_CODE)||'#rq='||A_RQ(L_SEQ_NO)||'#ic='||A_IC(L_SEQ_NO)||'#icnode='||TO_CHAR(A_ICNODE(L_SEQ_NO))||
                                    '#ii='||A_II(L_SEQ_NO)||'#iinode='||TO_CHAR(A_IINODE(L_SEQ_NO));
                   ELSE
                      L_SQLERRM2 := 'UNAPIGEN.DateValid('''||A_IIVALUE(L_SEQ_NO)||'@'||SUBSTR(L_IEVALUE_REC.FORMAT,2)||''') ret_code='||
                                    TO_CHAR(L_RET_CODE)||'#rq='||A_RQ(L_SEQ_NO)||'#ic='||A_IC(L_SEQ_NO)||'#icnode='||TO_CHAR(A_ICNODE(L_SEQ_NO))||
                                    '#ii='||A_II(L_SEQ_NO)||'#iinode='||TO_CHAR(A_IINODE(L_SEQ_NO));
                   END IF;
                   RAISE STPERROR;
                END IF;
            END IF;

            
  
            IF L_IEVALUE_REC.IEVALUE = 'rq' THEN
               
               NULL;
               
            ELSIF L_IEVALUE_REC.IEVALUE = 'rt' THEN
               
               UPDATE UTRQ SET RT= A_IIVALUE(L_SEQ_NO) WHERE RQ=A_RQ(L_SEQ_NO);
               
            ELSIF L_IEVALUE_REC.IEVALUE = 'description' THEN
               UPDATE UTRQ SET DESCRIPTION= A_IIVALUE(L_SEQ_NO) WHERE RQ=A_RQ(L_SEQ_NO);
               
            ELSIF L_IEVALUE_REC.IEVALUE = 'descr_doc' THEN
               UPDATE UTRQ SET DESCR_DOC= A_IIVALUE(L_SEQ_NO) WHERE RQ=A_RQ(L_SEQ_NO);
               
            ELSIF L_IEVALUE_REC.IEVALUE = 'sampling_date' THEN
               UPDATE UTRQ SET SAMPLING_DATE= L_DATE, SAMPLING_DATE_TZ = L_DATE WHERE RQ=A_RQ(L_SEQ_NO);
               
            ELSIF L_IEVALUE_REC.IEVALUE = 'creation_date' THEN
               UPDATE UTRQ SET CREATION_DATE= L_DATE, CREATION_DATE_TZ= L_DATE WHERE RQ=A_RQ(L_SEQ_NO);
               
            ELSIF L_IEVALUE_REC.IEVALUE = 'created_by' THEN
               UPDATE UTRQ SET CREATED_BY= A_IIVALUE(L_SEQ_NO) WHERE RQ=A_RQ(L_SEQ_NO);
               
            ELSIF L_IEVALUE_REC.IEVALUE = 'exec_start_date' THEN
               UPDATE UTRQ SET EXEC_START_DATE= L_DATE, EXEC_START_DATE_TZ= L_DATE WHERE RQ=A_RQ(L_SEQ_NO);
               
            ELSIF L_IEVALUE_REC.IEVALUE = 'exec_end_date' THEN
               UPDATE UTRQ SET EXEC_END_DATE= L_DATE, EXEC_END_DATE_TZ= L_DATE  WHERE RQ=A_RQ(L_SEQ_NO);

            ELSIF L_IEVALUE_REC.IEVALUE = 'due_date' THEN
               UPDATE UTRQ SET DUE_DATE= L_DATE, DUE_DATE_TZ= L_DATE WHERE RQ=A_RQ(L_SEQ_NO);
               
            ELSIF L_IEVALUE_REC.IEVALUE = 'priority' THEN
               UPDATE UTRQ SET PRIORITY= A_IIVALUE(L_SEQ_NO) WHERE RQ=A_RQ(L_SEQ_NO);
               
            ELSIF L_IEVALUE_REC.IEVALUE = 'label_format' THEN
               UPDATE UTRQ SET LABEL_FORMAT= A_IIVALUE(L_SEQ_NO) WHERE RQ=A_RQ(L_SEQ_NO);
               
            ELSIF L_IEVALUE_REC.IEVALUE = 'date1' THEN
               UPDATE UTRQ SET DATE1= L_DATE, DATE1_TZ= L_DATE WHERE RQ=A_RQ(L_SEQ_NO);
               
            ELSIF L_IEVALUE_REC.IEVALUE = 'date2' THEN
               UPDATE UTRQ SET DATE2= L_DATE, DATE2_TZ= L_DATE WHERE RQ=A_RQ(L_SEQ_NO);
               
            ELSIF L_IEVALUE_REC.IEVALUE = 'date3' THEN
               UPDATE UTRQ SET DATE3= L_DATE, DATE3_TZ= L_DATE WHERE RQ=A_RQ(L_SEQ_NO);
               
            ELSIF L_IEVALUE_REC.IEVALUE = 'date4' THEN
               UPDATE UTRQ SET DATE4= L_DATE, DATE4_TZ= L_DATE WHERE RQ=A_RQ(L_SEQ_NO);
               
            ELSIF L_IEVALUE_REC.IEVALUE = 'date5' THEN
               UPDATE UTRQ SET DATE5= L_DATE, DATE5_TZ= L_DATE WHERE RQ=A_RQ(L_SEQ_NO);
               
            ELSIF L_IEVALUE_REC.IEVALUE = 'allow_any_st' THEN
               UPDATE UTRQ SET ALLOW_ANY_ST= A_IIVALUE(L_SEQ_NO) WHERE RQ=A_RQ(L_SEQ_NO);

            ELSIF L_IEVALUE_REC.IEVALUE = 'allow_new_sc' THEN
               UPDATE UTRQ SET ALLOW_NEW_SC= A_IIVALUE(L_SEQ_NO) WHERE RQ=A_RQ(L_SEQ_NO);

            ELSIF L_IEVALUE_REC.IEVALUE = 'responsible' THEN
               UPDATE UTRQ SET RESPONSIBLE= A_IIVALUE(L_SEQ_NO) WHERE RQ=A_RQ(L_SEQ_NO);
               
            ELSIF L_IEVALUE_REC.IEVALUE = 'rq_class' THEN
               UPDATE UTRQ SET RQ_CLASS= A_IIVALUE(L_SEQ_NO) WHERE RQ=A_RQ(L_SEQ_NO);
            
            ELSE
               L_SQLERRM := L_IEVALUE_REC.IEVALUE || ' is not a valid request standard property for info field '||A_II(L_SEQ_NO)||
                            ' in info profile '||A_IC(L_SEQ_NO) ||' rq=' || A_RQ(L_SEQ_NO) || '#icnode=' ||
                            TO_CHAR(A_ICNODE(L_SEQ_NO)) || '#iinode=' || TO_CHAR(A_IINODE(L_SEQ_NO));
               RAISE STPERROR;
            END IF;
            L_LINKED_II := TRUE;
         END IF;
         CLOSE L_IEVALUE_CURSOR;

         
         
         
         L_EV_SEQ_NR := -1;
         L_EVENT_TP  := 'RqInfoFieldValueChanged';
         L_EV_DETAILS := 'rq=' || A_RQ(L_SEQ_NO) || 
                         '#ic=' || A_IC(L_SEQ_NO) ||
                         '#icnode=' || TO_CHAR(A_ICNODE(L_SEQ_NO)) || 
                         '#iinode=' || TO_CHAR(A_IINODE(L_SEQ_NO)) || 
                         '#old_value=' || SUBSTR(L_RQIIOLD_REC.IIVALUE, 1,40) || 
                         '#new_value=' || SUBSTR(L_RQIINEW_REC.IIVALUE, 1,40)  ||
                         '#ie_version=' || L_IE_VERSION;
         L_RESULT := UNAPIEV.INSERTINFOFIELDEVENT('SaveRqIiValue', UNAPIGEN.P_EVMGR_NAME,
                                                  'rqii', A_II(L_SEQ_NO), L_LC, L_LC_VERSION, 
                                                  L_SS, L_EVENT_TP, L_EV_DETAILS, L_EV_SEQ_NR);
         IF L_RESULT <> UNAPIGEN.DBERR_SUCCESS THEN
            UNAPIGEN.P_TXN_ERROR := L_RESULT;
            RAISE STPERROR;
         END IF;

         
         IF L_EV_SEQ_NR = -1 THEN
            
            
            OPEN L_SEQ_EVENT_CURSOR;
            FETCH L_SEQ_EVENT_CURSOR INTO L_EV_SEQ_NR;
            CLOSE L_SEQ_EVENT_CURSOR;
         END IF;

         IF L_LOG_HS = '1' THEN
            IF NVL((L_RQIIOLD_REC.IIVALUE <> L_RQIINEW_REC.IIVALUE), TRUE) AND NOT(L_RQIIOLD_REC.IIVALUE IS NULL AND L_RQIINEW_REC.IIVALUE IS NULL)  THEN 
               INSERT INTO UTRQICHS(RQ, IC, ICNODE, WHO, WHO_DESCRIPTION, WHAT, WHAT_DESCRIPTION, 
                                    LOGDATE, LOGDATE_TZ, WHY, TR_SEQ, EV_SEQ)
               VALUES(A_RQ(L_SEQ_NO), A_IC(L_SEQ_NO), A_ICNODE(L_SEQ_NO), UNAPIGEN.P_USER, 
                      UNAPIGEN.P_USER_DESCRIPTION, L_EVENT_TP, 
                      'info field "'||A_II(L_SEQ_NO)||'" is updated: property <iivalue> changed value from "'||SUBSTR(L_RQIIOLD_REC.IIVALUE,1,40)||'" to "'||SUBSTR(L_RQIINEW_REC.IIVALUE,1,40)||'".',
                      CURRENT_TIMESTAMP, CURRENT_TIMESTAMP, A_MODIFY_REASON, UNAPIGEN.P_TR_SEQ, L_EV_SEQ_NR);
            END IF;
         END IF;

         IF L_LOG_HS_DETAILS = '1' THEN
            UNAPIHSDETAILS.ADDRQIIHSDETAILS(L_RQIIOLD_REC, L_RQIINEW_REC, UNAPIGEN.P_TR_SEQ, 
                                            L_EV_SEQ_NR, L_HS_DETAILS_SEQ_NR); 
         END IF;
      ELSE
         IF L_II_HANDLED THEN
            L_COMPLETELY_SAVED := FALSE;
         END IF;
      END IF;
   END LOOP;

   IF L_II_OK THEN
      
      
      
      OPEN L_RQ_CURSOR(A_RQ(1));
      FETCH L_RQ_CURSOR
      INTO L_RQNEW_REC;
      CLOSE L_RQ_CURSOR;

      
      
      
      IF (L_RQNEW_REC.LOG_HS = '1') AND L_LINKED_II THEN
         INSERT INTO UTRQHS(RQ, WHO, WHO_DESCRIPTION, WHAT, WHAT_DESCRIPTION, 
                            LOGDATE, LOGDATE_TZ, WHY, TR_SEQ, EV_SEQ)
         VALUES(A_RQ(1), UNAPIGEN.P_USER, UNAPIGEN.P_USER_DESCRIPTION, L_EVENT_TP, 
                'request "'||A_RQ(1)||'" is updated.',
                CURRENT_TIMESTAMP, CURRENT_TIMESTAMP, A_MODIFY_REASON, UNAPIGEN.P_TR_SEQ, L_EV_SEQ_NR);
      END IF;
      IF (L_RQNEW_REC.LOG_HS_DETAILS = '1') AND L_LINKED_II THEN
         L_HS_DETAILS_SEQ_NR := L_HS_DETAILS_SEQ_NR + 1;
         INSERT INTO UTRQHSDETAILS(RQ, TR_SEQ, EV_SEQ, SEQ, DETAILS)
         VALUES  (A_RQ(1), UNAPIGEN.P_TR_SEQ, L_EV_SEQ_NR, L_HS_DETAILS_SEQ_NR, 
                  'request "'||A_RQ(1)||'" is updated.');

         UNAPIHSDETAILS.ADDRQHSDETAILS(L_RQOLD_REC, L_RQNEW_REC, UNAPIGEN.P_TR_SEQ, 
                                       L_EV_SEQ_NR, L_HS_DETAILS_SEQ_NR); 
      END IF;
   END IF;

   
   
   
   
   
   
   L_SCAN_SEQ := 1;
   L_SCAN_PREV_RQ := NULL;
   L_SCAN_PREV_IC := NULL;
   L_SCAN_PREV_ICNODE := NULL;
   L_IC_EVENT_INSERTED := FALSE;
   WHILE L_SCAN_SEQ <= A_NR_OF_ROWS LOOP

      OPEN L_RQIC_CURSOR(A_RQ(L_SCAN_SEQ), A_IC(L_SCAN_SEQ), A_ICNODE(L_SCAN_SEQ));
      FETCH L_RQIC_CURSOR INTO L_IP_VERSION, L_OLD_NEXT_II, L_IC_LOG_HS, L_IC_LOG_HS_DETAILS;
      IF L_RQIC_CURSOR%NOTFOUND THEN
         UNAPIGEN.P_TXN_ERROR := UNAPIGEN.DBERR_NOOBJECT;
         RAISE STPERROR;
      END IF;
      CLOSE L_RQIC_CURSOR;

      IF NVL(L_SCAN_PREV_RQ, ' ') <> NVL(A_RQ(L_SCAN_SEQ), ' ') OR
         NVL(L_SCAN_PREV_IC, ' ') <> NVL(A_IC(L_SCAN_SEQ), ' ') OR
         NVL(L_SCAN_PREV_ICNODE, -1) <> NVL(A_ICNODE(L_SCAN_SEQ), -1) THEN

         
         
         OPEN L_NEXT_II_CURSOR(A_RQ(L_SCAN_SEQ), A_IC(L_SCAN_SEQ), A_ICNODE(L_SCAN_SEQ));
         FETCH L_NEXT_II_CURSOR
         INTO L_NEXT_II;
         IF L_NEXT_II_CURSOR%NOTFOUND THEN
            L_NEXT_II := NULL;   
         END IF;
         CLOSE L_NEXT_II_CURSOR;

         
         
         UPDATE UTRQIC
         SET NEXT_II = L_NEXT_II
         WHERE RQ = A_RQ(L_SCAN_SEQ)
           AND IC = A_IC(L_SCAN_SEQ)
           AND ICNODE = A_ICNODE(L_SCAN_SEQ);

         L_SCAN_PREV_RQ := A_RQ(L_SCAN_SEQ);
         L_SCAN_PREV_IC := A_IC(L_SCAN_SEQ);
         L_SCAN_PREV_ICNODE := A_ICNODE(L_SCAN_SEQ);
         L_IC_EVENT_INSERTED := FALSE;

      END IF;

      IF A_MODIFY_FLAG(L_SCAN_SEQ) <= UNAPIGEN.DBERR_SUCCESS AND NOT L_IC_EVENT_INSERTED THEN
         L_EV_SEQ_NR := -1;
         L_EVENT_TP  := 'RqInfoFieldValuesChanged';
         L_EV_DETAILS := 'rq=' || A_RQ(L_SCAN_SEQ) || 
                         '#icnode=' || TO_CHAR(A_ICNODE(L_SCAN_SEQ)) ||
                         '#ip_version=' || L_IP_VERSION;
         L_RESULT := UNAPIEV.INSERTEVENT('SaveRqIiValue', UNAPIGEN.P_EVMGR_NAME,
                                         'rqic', A_IC(L_SCAN_SEQ), L_LC, L_LC_VERSION, L_SS,
                                         L_EVENT_TP, L_EV_DETAILS, L_EV_SEQ_NR);
         IF L_RESULT <> UNAPIGEN.DBERR_SUCCESS THEN
            UNAPIGEN.P_TXN_ERROR := L_RESULT;
            RAISE STPERROR;
         END IF;

         IF L_IC_LOG_HS = '1' THEN
            INSERT INTO UTRQICHS(RQ, IC, ICNODE, WHO, WHO_DESCRIPTION, WHAT, 
                                 WHAT_DESCRIPTION, LOGDATE, LOGDATE_TZ, WHY, TR_SEQ, EV_SEQ)
            VALUES  (A_RQ(L_SCAN_SEQ), A_IC(L_SCAN_SEQ), A_ICNODE(L_SCAN_SEQ), UNAPIGEN.P_USER, 
                     UNAPIGEN.P_USER_DESCRIPTION, L_EVENT_TP, 
                     'info card "'||A_IC(L_SCAN_SEQ)||'" info field values are updated.',
                     CURRENT_TIMESTAMP, CURRENT_TIMESTAMP, A_MODIFY_REASON, UNAPIGEN.P_TR_SEQ, L_EV_SEQ_NR);
         END IF;

         IF L_IC_LOG_HS_DETAILS = '1' THEN
            L_HS_DETAILS_SEQ_NR := L_HS_DETAILS_SEQ_NR + 1;
            INSERT INTO UTRQICHSDETAILS(RQ, IC, ICNODE, TR_SEQ, EV_SEQ, SEQ, DETAILS)
            VALUES  (A_RQ(L_SCAN_SEQ), A_IC(L_SCAN_SEQ), A_ICNODE(L_SCAN_SEQ), 
                     UNAPIGEN.P_TR_SEQ, L_EV_SEQ_NR, L_HS_DETAILS_SEQ_NR, 
                     'info card "'||A_IC(L_SCAN_SEQ)||'" info field values are updated.');

            IF NVL((L_OLD_NEXT_II <> L_NEXT_II), TRUE) AND NOT(L_OLD_NEXT_II IS NULL AND L_NEXT_II IS NULL) THEN 
               L_HS_DETAILS_SEQ_NR := L_HS_DETAILS_SEQ_NR + 1;
               INSERT INTO UTRQICHSDETAILS(RQ, IC, ICNODE, TR_SEQ, EV_SEQ, SEQ, DETAILS)
               VALUES(A_RQ(L_SCAN_SEQ), A_IC(L_SCAN_SEQ), A_ICNODE(L_SCAN_SEQ), UNAPIGEN.P_TR_SEQ, L_EV_SEQ_NR, L_HS_DETAILS_SEQ_NR,
                      'info card "'||A_IC(L_SCAN_SEQ)||'" is updated: property <next_ii> changed value from "'||L_OLD_NEXT_II||'" to "'||L_NEXT_II||'".');
            END IF;
         END IF;

         L_IC_EVENT_INSERTED := TRUE;
      END IF;

      L_SCAN_SEQ := L_SCAN_SEQ + 1;

   END LOOP;

   IF UNAPIGEN.ENDTXN <> UNAPIGEN.DBERR_SUCCESS THEN
      RAISE STPERROR;
   END IF;

   
   
   
   FOR L_SEQ_NO IN 1..A_NR_OF_ROWS LOOP
      IF A_MODIFY_FLAG(L_SEQ_NO) < UNAPIGEN.DBERR_SUCCESS THEN
         A_MODIFY_FLAG(L_SEQ_NO) := UNAPIGEN.DBERR_SUCCESS;
      END IF;
   END LOOP;

   IF L_COMPLETELY_SAVED THEN
      RETURN(UNAPIGEN.DBERR_SUCCESS);
   ELSE
      RETURN(UNAPIGEN.DBERR_PARTIALSAVE);
   END IF;

EXCEPTION
WHEN OTHERS THEN
   IF SQLCODE <> 1 THEN
      UNAPIGEN.LOGERROR('SaveRqIiValue',SQLERRM);
   ELSIF L_SQLERRM IS NOT NULL THEN
      UNAPIGEN.LOGERROR('SaveRqIiValue',L_SQLERRM);
      IF L_SQLERRM2 IS NOT NULL THEN
         UNAPIGEN.LOGERROR('SaveRqIiValue',L_SQLERRM2);
      END IF;      
   END IF;
   IF L_NEXT_II_CURSOR%ISOPEN THEN
      CLOSE L_NEXT_II_CURSOR;
   END IF;
   IF L_IEVALUE_CURSOR%ISOPEN THEN
      CLOSE L_IEVALUE_CURSOR;
   END IF;
   IF L_RQIIOLD_CURSOR%ISOPEN THEN
      CLOSE L_RQIIOLD_CURSOR;
   END IF;
   IF L_RQ_CURSOR%ISOPEN THEN
      CLOSE L_RQ_CURSOR;
   END IF;
   IF L_SEQ_EVENT_CURSOR%ISOPEN THEN
      CLOSE L_SEQ_EVENT_CURSOR;
   END IF;
   IF L_IPVERSION_CURSOR%ISOPEN THEN
      CLOSE L_IPVERSION_CURSOR;
   END IF;
   IF L_RQIC_CURSOR%ISOPEN THEN
      CLOSE L_RQIC_CURSOR;
   END IF;
   RETURN(UNAPIGEN.ABORTTXN(UNAPIGEN.P_TXN_ERROR, 'SaveRqIiValue'));
END SAVERQIIVALUE;

FUNCTION CREATERQINFODETAILS
(A_RT             IN        VARCHAR2,                 
 A_RT_VERSION     IN OUT    VARCHAR2,                 
 A_RQ             IN        VARCHAR2,                 
 A_FILTER_FREQ    IN        CHAR,                     
 A_REF_DATE       IN        DATE,                     
 A_MODIFY_REASON  IN        VARCHAR2)                 
RETURN NUMBER IS

L_RQ                 UNAPIGEN.VC20_TABLE_TYPE;
L_IC                 UNAPIGEN.VC20_TABLE_TYPE;
L_IP_VERSION         UNAPIGEN.VC20_TABLE_TYPE;
L_ICNODE             UNAPIGEN.LONG_TABLE_TYPE;
L_DESCRIPTION        UNAPIGEN.VC40_TABLE_TYPE;
L_WINSIZE_X          UNAPIGEN.NUM_TABLE_TYPE;
L_WINSIZE_Y          UNAPIGEN.NUM_TABLE_TYPE;
L_IS_PROTECTED       UNAPIGEN.CHAR1_TABLE_TYPE;
L_HIDDEN             UNAPIGEN.CHAR1_TABLE_TYPE;
L_MANUALLY_ADDED     UNAPIGEN.CHAR1_TABLE_TYPE;
L_NEXT_II            UNAPIGEN.VC20_TABLE_TYPE;
L_IC_CLASS           UNAPIGEN.VC2_TABLE_TYPE;
L_LOG_HS             UNAPIGEN.CHAR1_TABLE_TYPE;
L_LOG_HS_DETAILS     UNAPIGEN.CHAR1_TABLE_TYPE;
L_LC                 UNAPIGEN.VC2_TABLE_TYPE;
L_LC_VERSION         UNAPIGEN.VC20_TABLE_TYPE;
L_MODIFY_FLAG        UNAPIGEN.NUM_TABLE_TYPE;
L_NR_OF_ROWS         NUMBER;
L_REF_DATE           TIMESTAMP WITH TIME ZONE;
L_FILTER_FREQ        CHAR(1);
L_ERRM               VARCHAR2(255);
L_SEQ                NUMBER(5);
L_LOG_HS_RQ          CHAR(1);
L_LOG_HS_DETAILS_RQ  CHAR(1);
L_HS_DETAILS_SEQ_NR  INTEGER;

BEGIN

   IF UNAPIGEN.BEGINTXN(UNAPIGEN.P_SINGLE_API_TXN) <> UNAPIGEN.DBERR_SUCCESS THEN
      RAISE STPERROR;
   END IF;

   IF NVL(A_RQ, ' ') = ' ' OR
      NVL(A_RT, ' ') = ' ' THEN
      UNAPIGEN.P_TXN_ERROR := UNAPIGEN.DBERR_NOOBJID;
      RAISE STPERROR;
   END IF;

   
   A_RT_VERSION := UNAPIGEN.VALIDATEVERSION('rt', A_RT, A_RT_VERSION);

   IF A_REF_DATE IS NULL THEN
      L_REF_DATE := CURRENT_TIMESTAMP;
   ELSE
      L_REF_DATE := A_REF_DATE;
   END IF;

   L_ERRM := NULL;
   L_FILTER_FREQ := NVL(A_FILTER_FREQ, '1');

   
   
   
   L_RET_CODE := UNAPIRQ.INITRQINFODETAILS(A_RT, A_RT_VERSION, A_RQ,
                                           L_FILTER_FREQ,
                                           L_REF_DATE, L_IC, L_IP_VERSION,
                                           L_DESCRIPTION, L_WINSIZE_X,
                                           L_WINSIZE_Y, L_IS_PROTECTED, L_HIDDEN,
                                           L_MANUALLY_ADDED, L_NEXT_II, L_IC_CLASS, L_LOG_HS,
                                           L_LOG_HS_DETAILS, L_LC, L_LC_VERSION, L_NR_OF_ROWS);


   IF L_RET_CODE <> UNAPIGEN.DBERR_SUCCESS THEN
      UNAPIGEN.P_TXN_ERROR := L_RET_CODE;
      L_ERRM := 'rq=' || A_RQ || '#rt='|| A_RT ||
                '#filter_freq=' || L_FILTER_FREQ ||
                '#ref_date=' || L_REF_DATE ||
                '#nr_of_rows=' || L_NR_OF_ROWS ||
                '#InitRqInfoDetails#ErrorCode=' || TO_CHAR(L_RET_CODE);
      RAISE STPERROR;
   END IF;

   
   
   
   FOR L_ROW IN 1..L_NR_OF_ROWS LOOP
      L_RQ(L_ROW) := A_RQ;
      L_MODIFY_FLAG(L_ROW) := UNAPIGEN.MOD_FLAG_INSERT;
      L_ICNODE(L_ROW) := 0;
   END LOOP;

   
   
   
   IF L_NR_OF_ROWS > 0 THEN
      L_RET_CODE := UNAPIRQIC.SAVERQINFOCARD(L_RQ, L_IC, L_ICNODE, L_IP_VERSION, L_DESCRIPTION,
                                           L_WINSIZE_X, L_WINSIZE_Y,
                                           L_IS_PROTECTED,
                                           L_HIDDEN, L_MANUALLY_ADDED, L_NEXT_II, 
                                           L_IC_CLASS, L_LOG_HS, L_LOG_HS_DETAILS, L_LC, L_LC_VERSION, L_MODIFY_FLAG,
                                           L_NR_OF_ROWS, A_MODIFY_REASON);
                  IF L_RET_CODE = UNAPIGEN.DBERR_PARTIALSAVE THEN
                           
                           
                           
                           FOR L_ROW IN 1..L_NR_OF_ROWS LOOP
                                    IF L_MODIFY_FLAG(L_ROW) > UNAPIGEN.DBERR_SUCCESS THEN
                                             UNAPIGEN.P_TXN_ERROR := L_MODIFY_FLAG(L_ROW);
                                             L_ERRM := 'rq=' || L_RQ(L_ROW) ||
                                                                              '#ic=' || L_IC(L_ROW) ||
                                                                              '#icnode=' || TO_CHAR(L_ICNODE(L_ROW)) ||
                                                                              '#SaveRqInfoCard#ErrorCode=' || TO_CHAR(L_RET_CODE) ||
                                                                              '#mod_flag=' || TO_CHAR(L_MODIFY_FLAG(L_ROW));
                                             RAISE STPERROR;
                                    END IF;
                           END LOOP;
                  ELSIF L_RET_CODE <> UNAPIGEN.DBERR_SUCCESS THEN
                           UNAPIGEN.P_TXN_ERROR := L_RET_CODE;
                           RAISE STPERROR;
                  END IF;
   END IF;

   
   
   
   FOR L_ROW IN 1..L_NR_OF_ROWS LOOP
      
      
      
      
      L_RET_CODE := UNAPIRQIC2.INITANDSAVERQICATTRIBUTES(A_RQ, L_IC(L_ROW), L_ICNODE(L_ROW));
      
      
      
      IF L_RET_CODE <> UNAPIGEN.DBERR_SUCCESS THEN
         L_ERRM := 'rq=' || A_RQ ||
                    '#ic=' || L_IC(L_ROW) ||
                    '#icnode=' || TO_CHAR(L_ICNODE(L_ROW)) ||
                    '#InitAndSaveRqIcattributes#ErrorCode=' || TO_CHAR(L_RET_CODE);
         UNAPIGEN.P_TXN_ERROR := L_RET_CODE;
         RAISE STPERROR;
      END IF;

      
      
      
      L_RET_CODE := UNAPIRQIC.CREATERQICDETAILS(A_RT, A_RT_VERSION, L_IC(L_ROW), L_IP_VERSION(L_ROW), L_SEQ,
                                                A_RQ, L_ICNODE(L_ROW),
                                                A_MODIFY_REASON);
      
      
      
      IF L_RET_CODE <> UNAPIGEN.DBERR_SUCCESS THEN
         L_ERRM := 'rq=' || A_RQ|| '#rt='|| A_RT ||
                    '#ic=' || L_IC(L_ROW) ||
                    '#icnode=' || TO_CHAR(L_ICNODE(L_ROW)) ||
                    '#CreateRqIcDetails#ErrorCode=' || TO_CHAR(L_RET_CODE);
         UNAPIGEN.P_TXN_ERROR := L_RET_CODE;
         RAISE STPERROR;
      END IF;
   END LOOP;

   
   
   
   L_EV_SEQ_NR := -1;
   L_EV_DETAILS := 'rt=' || A_RT || 
                   '#rt_version=' || A_RT_VERSION;
   L_EVENT_TP := 'RqInfoCreated';
   L_RESULT := UNAPIEV.INSERTEVENT('CreateRqInfoDetails', UNAPIGEN.P_EVMGR_NAME, 
                                   'rq', A_RQ, '', '', '', L_EVENT_TP, L_EV_DETAILS, L_EV_SEQ_NR);
   IF L_RESULT <> UNAPIGEN.DBERR_SUCCESS THEN
      UNAPIGEN.P_TXN_ERROR := L_RESULT;
      RAISE STPERROR;
   END IF;

   BEGIN
      SELECT LOG_HS, LOG_HS_DETAILS
      INTO L_LOG_HS_RQ, L_LOG_HS_DETAILS_RQ
      FROM UTRQ
      WHERE RQ = A_RQ;
   EXCEPTION
   WHEN NO_DATA_FOUND THEN
      UNAPIGEN.P_TXN_ERROR := UNAPIGEN.DBERR_NOOBJECT;
      RAISE STPERROR;
   END;

   IF NVL(L_LOG_HS_RQ, ' ') = '1' THEN
      INSERT INTO UTRQHS (RQ, WHO, WHO_DESCRIPTION, WHAT, WHAT_DESCRIPTION, LOGDATE, LOGDATE_TZ, WHY, TR_SEQ, EV_SEQ)
      VALUES (A_RQ, UNAPIGEN.P_USER, UNAPIGEN.P_USER_DESCRIPTION, L_EVENT_TP, 
              'Info cards for request "'||A_RQ||'" are created', 
              CURRENT_TIMESTAMP, CURRENT_TIMESTAMP, A_MODIFY_REASON, UNAPIGEN.P_TR_SEQ, L_EV_SEQ_NR);
   END IF;

   L_HS_DETAILS_SEQ_NR := 0;
   IF NVL(L_LOG_HS_DETAILS_RQ, ' ') = '1' THEN
      L_HS_DETAILS_SEQ_NR := L_HS_DETAILS_SEQ_NR + 1;
      INSERT INTO UTRQHSDETAILS(RQ, TR_SEQ, EV_SEQ, SEQ, DETAILS)
      VALUES  (A_RQ, UNAPIGEN.P_TR_SEQ, L_EV_SEQ_NR, L_HS_DETAILS_SEQ_NR, 
               'Info cards for request "'||A_RQ||'" are created');
   END IF;

   IF UNAPIGEN.ENDTXN <> UNAPIGEN.DBERR_SUCCESS THEN
      RAISE STPERROR;
   END IF;

   RETURN(UNAPIGEN.DBERR_SUCCESS);

EXCEPTION
WHEN OTHERS THEN
   IF SQLCODE <> 1 THEN
      UNAPIGEN.LOGERROR('CreateRqInfoDetails', SQLERRM);
   ELSIF L_ERRM IS NOT NULL THEN
      UNAPIGEN.LOGERROR('CreateRqInfoDetails', L_ERRM);
   END IF;
   RETURN(UNAPIGEN.ABORTTXN(UNAPIGEN.P_TXN_ERROR,'CreateRqInfoDetails'));
END CREATERQINFODETAILS;

FUNCTION ADDRQINFODETAILS
(A_RT             IN        VARCHAR2,                 
 A_RT_VERSION     IN        VARCHAR2,                 
 A_RQ             IN        VARCHAR2,                 
 A_IP             IN        VARCHAR2,                 
 A_IP_VERSION     IN        VARCHAR2,                 
 A_SEQ            IN        NUMBER,                   
 A_MODIFY_REASON  IN        VARCHAR2)                 
RETURN NUMBER IS

L_RQ                 UNAPIGEN.VC20_TABLE_TYPE;
L_IC                 UNAPIGEN.VC20_TABLE_TYPE;
L_ICNODE             UNAPIGEN.LONG_TABLE_TYPE;
L_IP_VERSION         UNAPIGEN.VC20_TABLE_TYPE;
L_DESCRIPTION        UNAPIGEN.VC40_TABLE_TYPE;
L_WINSIZE_X          UNAPIGEN.NUM_TABLE_TYPE;
L_WINSIZE_Y          UNAPIGEN.NUM_TABLE_TYPE;
L_IS_PROTECTED       UNAPIGEN.CHAR1_TABLE_TYPE;
L_HIDDEN             UNAPIGEN.CHAR1_TABLE_TYPE;
L_MANUALLY_ADDED     UNAPIGEN.CHAR1_TABLE_TYPE;
L_NEXT_II            UNAPIGEN.VC20_TABLE_TYPE;
L_IC_CLASS           UNAPIGEN.VC2_TABLE_TYPE;
L_LOG_HS             UNAPIGEN.CHAR1_TABLE_TYPE;
L_LOG_HS_DETAILS     UNAPIGEN.CHAR1_TABLE_TYPE;
L_LC                 UNAPIGEN.VC2_TABLE_TYPE;
L_LC_VERSION         UNAPIGEN.VC20_TABLE_TYPE;
L_MODIFY_FLAG        UNAPIGEN.NUM_TABLE_TYPE;
L_NR_OF_ROWS         NUMBER;
L_REF_DATE           TIMESTAMP WITH TIME ZONE;
L_FILTER_FREQ        CHAR(1);
L_ERRM               VARCHAR2(255);
L_SEQ                NUMBER(5);
L_LOG_HS_RQ          CHAR(1);
L_LOG_HS_DETAILS_RQ  CHAR(1);
L_RT_VERSION         VARCHAR2(20);
L_HS_DETAILS_SEQ_NR  INTEGER;

BEGIN

   IF UNAPIGEN.BEGINTXN(UNAPIGEN.P_SINGLE_API_TXN) <> UNAPIGEN.DBERR_SUCCESS THEN
      RAISE STPERROR;
   END IF;

   IF NVL(A_RQ, ' ') = ' ' OR
      NVL(A_RT, ' ') = ' ' OR
      NVL(A_IP, ' ') = ' ' OR
      A_SEQ IS NULL THEN
      UNAPIGEN.P_TXN_ERROR := UNAPIGEN.DBERR_NOOBJID;
      RAISE STPERROR;
   END IF;

   
   L_RT_VERSION := UNAPIGEN.VALIDATEVERSION('rt', A_RT, A_RT_VERSION);

   
   
   
   L_FILTER_FREQ := '0';
   L_REF_DATE := NULL; 
   L_ERRM := NULL;

   
   
   
   L_RET_CODE := UNAPIRQIC.INITRQINFOCARD(A_IP, A_IP_VERSION, A_SEQ, A_RT, L_RT_VERSION, A_RQ,
                                          L_IP_VERSION, L_DESCRIPTION, L_WINSIZE_X, L_WINSIZE_Y, 
                                          L_IS_PROTECTED, L_HIDDEN, L_MANUALLY_ADDED, L_NEXT_II, 
                                          L_IC_CLASS, L_LOG_HS, L_LOG_HS_DETAILS, L_LC, L_LC_VERSION, 
                                          L_NR_OF_ROWS);
   IF L_RET_CODE <> UNAPIGEN.DBERR_SUCCESS THEN
      UNAPIGEN.P_TXN_ERROR := L_RET_CODE;
      L_ERRM := 'ip=' || A_IP || '#seq='|| TO_CHAR(A_SEQ) ||
                '#rt=' || A_RT || '#rq='|| A_RQ ||
                '#nr_of_rows=' || L_NR_OF_ROWS ||
                '#InitRqInfoCard#ErrorCode=' || TO_CHAR(L_RET_CODE);
      RAISE STPERROR;
   END IF;

   
   
   
   FOR L_ROW IN 1..L_NR_OF_ROWS LOOP
      L_RQ(L_ROW) := A_RQ;
      L_MODIFY_FLAG(L_ROW) := UNAPIGEN.MOD_FLAG_INSERT;
      L_IC(L_ROW) := A_IP;
      L_ICNODE(L_ROW) := 0;
      L_IP_VERSION(L_ROW) := A_IP_VERSION;
   END LOOP;

   
   
   
   IF L_NR_OF_ROWS > 0 THEN
      L_RET_CODE := UNAPIRQIC.SAVERQINFOCARD(L_RQ, L_IC, L_ICNODE, L_IP_VERSION, L_DESCRIPTION,
                                             L_WINSIZE_X, L_WINSIZE_Y, L_IS_PROTECTED,
                                             L_HIDDEN, L_MANUALLY_ADDED, L_NEXT_II, 
                                             L_IC_CLASS, L_LOG_HS, L_LOG_HS_DETAILS, L_LC, L_LC_VERSION, 
                                             L_MODIFY_FLAG, L_NR_OF_ROWS, A_MODIFY_REASON);
                  IF L_RET_CODE = UNAPIGEN.DBERR_PARTIALSAVE THEN
                           
                           
                           
                           FOR L_ROW IN 1..L_NR_OF_ROWS LOOP
                                    IF L_MODIFY_FLAG(L_ROW) > UNAPIGEN.DBERR_SUCCESS THEN
                                             UNAPIGEN.P_TXN_ERROR := L_MODIFY_FLAG(L_ROW);
                                             L_ERRM := 'rq=' || L_RQ(L_ROW) ||
                                                                              '#ic=' || L_IC(L_ROW) ||
                                                                              '#icnode=' || TO_CHAR(L_ICNODE(L_ROW)) ||
                                                                              '#SaveRqInfoCard#ErrorCode=' || TO_CHAR(L_RET_CODE) ||
                                                                              '#mod_flag=' || TO_CHAR(L_MODIFY_FLAG(L_ROW));
                                             RAISE STPERROR;
                                    END IF;
                           END LOOP;
                  ELSIF L_RET_CODE <> UNAPIGEN.DBERR_SUCCESS THEN
                           UNAPIGEN.P_TXN_ERROR := L_RET_CODE;
                           RAISE STPERROR;
                  END IF;
   END IF;

   
   
   
   FOR L_ROW IN 1..L_NR_OF_ROWS LOOP
      
      
      
      L_RET_CODE := UNAPIRQIC2.INITANDSAVERQICATTRIBUTES(A_RQ, L_IC(L_ROW), L_ICNODE(L_ROW));
      
      
      
      IF L_RET_CODE <> UNAPIGEN.DBERR_SUCCESS THEN
         L_ERRM := 'rq=' || A_RQ ||
                    '#ic=' || L_IC(L_ROW) ||
                    '#icnode=' || TO_CHAR(L_ICNODE(L_ROW)) ||
                    '#InitAndSaveRqIcattributes#ErrorCode=' || TO_CHAR(L_RET_CODE);
         UNAPIGEN.P_TXN_ERROR := L_RET_CODE;
         RAISE STPERROR;
      END IF;

      
      
      
      L_RET_CODE := UNAPIRQIC.CREATERQICDETAILS(A_RT, L_RT_VERSION, L_IC(L_ROW), 
                                                L_IP_VERSION(L_ROW), L_SEQ, A_RQ, 
                                                L_ICNODE(L_ROW), A_MODIFY_REASON);
      
      
      
      IF L_RET_CODE <> UNAPIGEN.DBERR_SUCCESS THEN
         L_ERRM := 'rq=' || A_RQ|| '#rt='|| A_RT ||
                    '#ic=' || L_IC(L_ROW) ||
                    '#icnode=' || TO_CHAR(L_ICNODE(L_ROW)) ||
                    '#CreateRqIcDetails#ErrorCode=' || TO_CHAR(L_RET_CODE);
         UNAPIGEN.P_TXN_ERROR := L_RET_CODE;
         RAISE STPERROR;
      END IF;
   END LOOP;

   
   
   
   L_EV_SEQ_NR := -1;
   L_EV_DETAILS := 'rt=' || A_RT ||
                   '#rt_version=' || L_RT_VERSION;
   L_EVENT_TP := 'RqInfoCreated';
   L_RESULT := UNAPIEV.INSERTEVENT('AddRqInfoDetails', UNAPIGEN.P_EVMGR_NAME, 'rq', A_RQ, 
                                   '', '', '', L_EVENT_TP, L_EV_DETAILS, L_EV_SEQ_NR);
   IF L_RESULT <> UNAPIGEN.DBERR_SUCCESS THEN
      UNAPIGEN.P_TXN_ERROR := L_RESULT;
      RAISE STPERROR;
   END IF;

   BEGIN
      SELECT LOG_HS, LOG_HS_DETAILS
      INTO L_LOG_HS_RQ, L_LOG_HS_DETAILS_RQ
      FROM UTRQ
      WHERE RQ = A_RQ;
   EXCEPTION
   WHEN NO_DATA_FOUND THEN
      UNAPIGEN.P_TXN_ERROR := UNAPIGEN.DBERR_NOOBJECT;
      RAISE STPERROR;
   END;

   IF NVL(L_LOG_HS_RQ, ' ') = '1' THEN
      INSERT INTO UTRQHS (RQ, WHO, WHO_DESCRIPTION, WHAT, WHAT_DESCRIPTION, LOGDATE, LOGDATE_TZ, WHY, TR_SEQ, EV_SEQ)
      VALUES (A_RQ, UNAPIGEN.P_USER, UNAPIGEN.P_USER_DESCRIPTION, L_EVENT_TP, 
              'Info cards for request "'||A_RQ||'" are created', 
              CURRENT_TIMESTAMP, CURRENT_TIMESTAMP, A_MODIFY_REASON, UNAPIGEN.P_TR_SEQ, L_EV_SEQ_NR);
   END IF;

   L_HS_DETAILS_SEQ_NR := 0;
   IF NVL(L_LOG_HS_DETAILS_RQ, ' ') = '1' THEN
      L_HS_DETAILS_SEQ_NR := L_HS_DETAILS_SEQ_NR + 1;
      INSERT INTO UTRQHSDETAILS(RQ, TR_SEQ, EV_SEQ, SEQ, DETAILS)
      VALUES  (A_RQ, UNAPIGEN.P_TR_SEQ, L_EV_SEQ_NR, L_HS_DETAILS_SEQ_NR, 
               'Info cards for request "'||A_RQ||'" are created');
   END IF;

   IF UNAPIGEN.ENDTXN <> UNAPIGEN.DBERR_SUCCESS THEN
      RAISE STPERROR;
   END IF;

   RETURN(UNAPIGEN.DBERR_SUCCESS);

EXCEPTION
WHEN OTHERS THEN
   IF SQLCODE <> 1 THEN
      UNAPIGEN.LOGERROR('AddRqInfoDetails', SQLERRM);
   ELSIF L_ERRM IS NOT NULL THEN
      UNAPIGEN.LOGERROR('AddRqInfoDetails', L_ERRM);
   END IF;
   RETURN(UNAPIGEN.ABORTTXN(UNAPIGEN.P_TXN_ERROR,'AddRqInfoDetails'));
END ADDRQINFODETAILS;

FUNCTION CREATERQICDETAILS                            
(A_RT             IN        VARCHAR2,                 
 A_RT_VERSION     IN OUT    VARCHAR2,                 
 A_IP             IN        VARCHAR2,                 
 A_IP_VERSION     IN OUT    VARCHAR2,                 
 A_SEQ            IN        NUMBER,                   
 A_RQ             IN        VARCHAR2,                 
 A_ICNODE         IN        NUMBER,                   
 A_MODIFY_REASON  IN        VARCHAR2)                 
RETURN NUMBER IS

L_IE_REC                UTIE%ROWTYPE;
L_IPIE_REC              UTIPIE%ROWTYPE;
L_IIVALUE               VARCHAR2(2000);
L_ICNODE                NUMBER(9);
L_IINODE                NUMBER(9);
L_ERRM                  VARCHAR2(255);
L_NEXT_II               VARCHAR2(20);
L_LOG_HS                CHAR(1);
L_LOG_HS_DETAILS        CHAR(1);
L_DEF_LC                VARCHAR2(2);
L_HS_DETAILS_SEQ_NR     INTEGER;

CURSOR L_NEXT_II_CURSOR (C_RQ        VARCHAR2,
                         C_IC        VARCHAR2,
                         C_ICNODE    NUMBER) IS
   SELECT II
   FROM UTRQII
   WHERE RQ = C_RQ
     AND IC = C_IC
     AND ICNODE = C_ICNODE
     AND IIVALUE IS NULL
   ORDER BY IINODE;

CURSOR L_IE_CURSOR(C_IE VARCHAR2, C_IE_VERSION VARCHAR2) IS
   SELECT *
   FROM UTIE
   WHERE IE = C_IE
   AND VERSION = C_IE_VERSION;

CURSOR L_IPIE_CURSOR IS
   SELECT A.*
   FROM UTIPIE A
   WHERE A.IP = A_IP
   AND A.VERSION = A_IP_VERSION
   ORDER BY A.SEQ;

CURSOR L_OBJECTS_CURSOR (A_OBJECT_TYPE VARCHAR2) IS
   SELECT LOG_HS, LOG_HS_DETAILS, DEF_LC
   FROM UTOBJECTS
   WHERE OBJECT=A_OBJECT_TYPE;
   
BEGIN

   IF UNAPIGEN.BEGINTXN(UNAPIGEN.P_SINGLE_API_TXN) <> UNAPIGEN.DBERR_SUCCESS THEN
      RAISE STPERROR;
   END IF;

   IF NVL(A_RQ, ' ') = ' ' OR
      NVL(A_IP, ' ') = ' ' OR
      NVL(A_ICNODE, 0) = 0 THEN
      UNAPIGEN.P_TXN_ERROR := UNAPIGEN.DBERR_NOOBJID;
      RAISE STPERROR;
   END IF;

   
   A_RT_VERSION := UNAPIGEN.VALIDATEVERSION('rt', A_RT, A_RT_VERSION);
   A_IP_VERSION := UNAPIGEN.VALIDATEVERSION('ip', A_IP, A_IP_VERSION);
   
   
   L_LOG_HS := '0';
   L_LOG_HS_DETAILS := '0';
   OPEN L_OBJECTS_CURSOR('rqii');
   FETCH L_OBJECTS_CURSOR INTO L_LOG_HS, L_LOG_HS_DETAILS, L_DEF_LC;
   CLOSE L_OBJECTS_CURSOR;

   L_IINODE := 0;

   FOR L_IPIE_REC IN L_IPIE_CURSOR LOOP

      L_ERRM := '';
      L_IIVALUE := '';
      
      
      
      L_IPIE_REC.IE_VERSION := UNAPIGEN.VALIDATEVERSION('ie', L_IPIE_REC.IE, L_IPIE_REC.IE_VERSION);

      OPEN L_IE_CURSOR(L_IPIE_REC.IE, L_IPIE_REC.IE_VERSION);
      FETCH L_IE_CURSOR
      INTO L_IE_REC;
      IF L_IE_CURSOR%NOTFOUND THEN
         CLOSE L_IE_CURSOR;
         UNAPIGEN.P_TXN_ERROR := UNAPIGEN.DBERR_NOOBJECT;
         RAISE STPERROR;
      END IF;
      CLOSE L_IE_CURSOR;

      
      
      
      IF NVL(L_IPIE_REC.IS_PROTECTED, '2') <> '2' THEN
         L_IE_REC.IS_PROTECTED := L_IPIE_REC.IS_PROTECTED;
      END IF;

      IF NVL(L_IPIE_REC.MANDATORY, '2') <> '2' THEN
         L_IE_REC.MANDATORY := L_IPIE_REC.MANDATORY;
      END IF;

      IF NVL(L_IPIE_REC.HIDDEN, '2') <> '2' THEN
         L_IE_REC.HIDDEN := L_IPIE_REC.HIDDEN;
      END IF;
      
      IF NVL(L_IPIE_REC.DSP_TITLE_USE_IE, '1') <> '1' THEN
         L_IE_REC.DSP_TITLE := L_IPIE_REC.DSP_TITLE;
      END IF;
      IF L_IPIE_REC.DSP_LEN IS NOT NULL THEN
         L_IE_REC.DSP_LEN := L_IPIE_REC.DSP_LEN;
      END IF;
      IF L_IPIE_REC.DSP_ROWS IS NOT NULL THEN
         L_IE_REC.DSP_ROWS := L_IPIE_REC.DSP_ROWS;
      END IF;
      IF NVL(L_IPIE_REC.DSP_TP, ' ') <> ' ' THEN
         L_IE_REC.DSP_TP := L_IPIE_REC.DSP_TP;
      END IF;

      
      
      
      
      
      
      
      
      
      
      IF L_IE_REC.DSP_TP = 'B' THEN  
         IF NVL(L_IPIE_REC.IEVALUE, ' ') <> '2' THEN
            L_IE_REC.DEF_VAL_TP := L_IPIE_REC.DEF_VAL_TP;
            L_IE_REC.DEF_AU_LEVEL := L_IPIE_REC.DEF_AU_LEVEL;
            L_IE_REC.IEVALUE := L_IPIE_REC.IEVALUE;
         END IF;
      ELSE                           
         IF NVL(L_IPIE_REC.DEF_VAL_TP, 'F') <> 'F' OR
            NVL(L_IPIE_REC.IEVALUE, ' ') <> ' ' THEN
            L_IE_REC.DEF_VAL_TP := L_IPIE_REC.DEF_VAL_TP;
            L_IE_REC.DEF_AU_LEVEL := L_IPIE_REC.DEF_AU_LEVEL;
            L_IE_REC.IEVALUE := L_IPIE_REC.IEVALUE;
         END IF;
      END IF;

      BEGIN
      L_RET_CODE := FILLRQIIDEFAULTVALUE(A_RQ, L_IE_REC.DEF_VAL_TP,
                                               L_IE_REC.IEVALUE,
                                               L_IE_REC.DEF_AU_LEVEL,
                                               L_IE_REC.DATA_TP,
                                               L_IE_REC.FORMAT, L_IIVALUE);
      EXCEPTION
      WHEN OTHERS THEN
         L_ERRM := 'Warning#rq=' || A_RQ||
                   '#ic='|| A_IP ||
                   '#ii='|| L_IE_REC.IE ||
                   '#def_val_tp='|| L_IE_REC.DEF_VAL_TP ||
                   '#ievalue=' || SUBSTR(L_IE_REC.IEVALUE,1,40) ||
                   '#def_au_level=' || L_IE_REC.DEF_AU_LEVEL ||
                   '#data_tp=' || L_IE_REC.DATA_TP ||
                   '#format=' || L_IE_REC.FORMAT ||
                   '#FillRqiiDefaultValue#SqlCode=' || TO_CHAR(SQLCODE);
      END;

      IF L_RET_CODE = UNAPIGEN.DBERR_NODEFVALUE THEN
         L_IIVALUE := '';
      ELSIF L_RET_CODE <> UNAPIGEN.DBERR_SUCCESS AND L_ERRM IS NULL THEN
         L_ERRM := 'Warning#rq=' || A_RQ ||
                   '#ic='|| A_IP ||
                   '#ii='|| L_IE_REC.IE ||
                   '#def_val_tp='|| L_IE_REC.DEF_VAL_TP ||
                   '#ievalue=' || SUBSTR(L_IE_REC.IEVALUE,1,40) ||
                   '#def_au_level=' || L_IE_REC.DEF_AU_LEVEL ||
                   '#data_tp=' || L_IE_REC.DATA_TP ||
                   '#format=' || L_IE_REC.FORMAT ||
                   '#FillRqiiDefaultValue#ErrorCode=' || TO_CHAR(L_RET_CODE);
      END IF;
      IF L_ERRM IS NOT NULL THEN
         INSERT INTO UTERROR(CLIENT_ID, APPLIC, WHO, LOGDATE, LOGDATE_TZ, API_NAME, ERROR_MSG)
         VALUES(UNAPIGEN.P_CLIENT_ID, UNAPIGEN.P_APPLIC_NAME, UNAPIGEN.P_USER, CURRENT_TIMESTAMP, CURRENT_TIMESTAMP, 
                'CreateRqIcDetails', L_ERRM);
      END IF;

      
      
      IF L_IE_REC.SC_LC IS NOT NULL THEN
         L_IE_REC.SC_LC_VERSION := UNAPIGEN.VALIDATEVERSION('lc', L_IE_REC.SC_LC, L_IE_REC.SC_LC_VERSION);
      END IF;
      
      L_IINODE := L_IINODE + UNAPIGEN.DEFAULT_NODE_INTERVAL;
      INSERT INTO UTRQII(RQ, IC, ICNODE, II, IINODE, IE_VERSION, IIVALUE,
                         POS_X, POS_Y, IS_PROTECTED, MANDATORY,
                         HIDDEN, DSP_TITLE, DSP_LEN, DSP_TP,
                         DSP_ROWS, LOG_HS, LOG_HS_DETAILS, ALLOW_MODIFY, ACTIVE, LC, LC_VERSION, SS)
      VALUES(A_RQ, A_IP, A_ICNODE, L_IPIE_REC.IE, L_IINODE, L_IPIE_REC.IE_VERSION, L_IIVALUE,
             L_IPIE_REC.POS_X, L_IPIE_REC.POS_Y,
             L_IE_REC.IS_PROTECTED, L_IE_REC.MANDATORY,
             L_IE_REC.HIDDEN, L_IE_REC.DSP_TITLE,
             L_IE_REC.DSP_LEN, L_IE_REC.DSP_TP,
             L_IE_REC.DSP_ROWS, L_LOG_HS, L_LOG_HS_DETAILS, '1', '0', L_IE_REC.SC_LC, L_IE_REC.SC_LC_VERSION, '');

      L_EV_SEQ_NR := -1;
      L_EV_DETAILS := 'rq=' || A_RQ || 
                      '#ic=' || A_IP ||
                      '#icnode=' || TO_CHAR(A_ICNODE)||
                      '#iinode=' || TO_CHAR(L_IINODE) || 
                      '#ie_version=' || L_IPIE_REC.IE_VERSION;
      IF L_IIVALUE IS NOT NULL THEN
         L_EV_DETAILS := L_EV_DETAILS || '#new_value=' || SUBSTR(L_IIVALUE,1,40);
      END IF;                      
      L_EVENT_TP := 'RqInfoFieldCreated';
      L_RESULT := UNAPIEV.INSERTINFOFIELDEVENT('CreateRqIcDetails', UNAPIGEN.P_EVMGR_NAME, 'rqii', 
                                               L_IPIE_REC.IE, L_IE_REC.SC_LC, L_IE_REC.SC_LC_VERSION, 
                                               '', L_EVENT_TP, L_EV_DETAILS, L_EV_SEQ_NR);
      IF L_RESULT <> UNAPIGEN.DBERR_SUCCESS THEN
         UNAPIGEN.P_TXN_ERROR := L_RESULT;
         RAISE STPERROR;
      END IF;

      
      IF L_EV_SEQ_NR = -1 THEN
         
         
         OPEN L_SEQ_EVENT_CURSOR;
         FETCH L_SEQ_EVENT_CURSOR INTO L_EV_SEQ_NR;
         CLOSE L_SEQ_EVENT_CURSOR;
      END IF;
   END LOOP;

   IF L_LOG_HS = '1' THEN
      INSERT INTO UTRQICHS(RQ, IC, ICNODE, WHO, WHO_DESCRIPTION, WHAT, WHAT_DESCRIPTION, 
                           LOGDATE, LOGDATE_TZ, WHY, TR_SEQ, EV_SEQ)
      VALUES(A_RQ, A_IP, A_ICNODE, UNAPIGEN.P_USER, UNAPIGEN.P_USER_DESCRIPTION, L_EVENT_TP, 
             'info fields for info card "'||A_IP||'" are created',
             CURRENT_TIMESTAMP, CURRENT_TIMESTAMP, A_MODIFY_REASON, UNAPIGEN.P_TR_SEQ, L_EV_SEQ_NR);
   END IF;

   L_HS_DETAILS_SEQ_NR := 0;
   IF L_LOG_HS_DETAILS = '1' THEN
      L_HS_DETAILS_SEQ_NR := L_HS_DETAILS_SEQ_NR + 1;
      INSERT INTO UTRQICHSDETAILS(RQ, IC, ICNODE, TR_SEQ, EV_SEQ, SEQ, DETAILS)
      VALUES  (A_RQ, A_IP, A_ICNODE, UNAPIGEN.P_TR_SEQ, L_EV_SEQ_NR, L_HS_DETAILS_SEQ_NR, 
               'Info fields for info card "'||A_IP||'" are created');
   END IF;

   
   OPEN L_NEXT_II_CURSOR(A_RQ, A_IP, A_ICNODE);
   FETCH L_NEXT_II_CURSOR
   INTO L_NEXT_II;
   IF L_NEXT_II_CURSOR%NOTFOUND THEN
      L_NEXT_II := NULL;
   END IF;

   
   UPDATE UTRQIC
   SET NEXT_II = L_NEXT_II
   WHERE RQ = A_RQ
     AND IC = A_IP
     AND ICNODE = A_ICNODE;

   IF UNAPIGEN.ENDTXN <> UNAPIGEN.DBERR_SUCCESS THEN
      RAISE STPERROR;
   END IF;

   RETURN(UNAPIGEN.DBERR_SUCCESS);

EXCEPTION
WHEN OTHERS THEN
   IF SQLCODE <> 1 THEN
      UNAPIGEN.LOGERROR('CreateRqIcDetails',SQLERRM);
   END IF;
   IF L_IE_CURSOR%ISOPEN THEN
      CLOSE L_IE_CURSOR;
   END IF;
   IF L_OBJECTS_CURSOR%ISOPEN THEN
      CLOSE L_OBJECTS_CURSOR;
   END IF;
   IF L_SEQ_EVENT_CURSOR%ISOPEN THEN
      CLOSE L_SEQ_EVENT_CURSOR;
   END IF;
   RETURN(UNAPIGEN.ABORTTXN(UNAPIGEN.P_TXN_ERROR, 'CreateRqIcDetails'));
END CREATERQICDETAILS;

FUNCTION ADDRQICDETAILS                               
(A_RT             IN        VARCHAR2,                 
 A_RT_VERSION     IN        VARCHAR2,                 
 A_RQ             IN        VARCHAR2,                 
 A_IC             IN        VARCHAR2,                 
 A_ICNODE         IN        NUMBER,                   
 A_IE             IN        VARCHAR2,                 
 A_IE_VERSION     IN        VARCHAR2,                 
 A_SEQ            IN        NUMBER,                   
 A_MODIFY_REASON  IN        VARCHAR2)                 
RETURN NUMBER IS

L_IE_REC           UTIE%ROWTYPE;
L_IPIE_REC         UTIPIE%ROWTYPE;
L_IIVALUE          VARCHAR2(2000);
L_ICNODE           NUMBER(9);
L_IINODE           NUMBER(9);
L_ERRM             VARCHAR2(255);
L_NEXT_II          VARCHAR2(20);
L_LOG_HS           CHAR(1);
L_LOG_HS_DETAILS   CHAR(1);
L_DEF_LC           VARCHAR2(2);
L_IP_VERSION       VARCHAR2(20);
L_HS_DETAILS_SEQ_NR     INTEGER;

CURSOR L_NEXT_II_CURSOR (C_RQ        VARCHAR2,
                         C_IC        VARCHAR2,
                         C_ICNODE    NUMBER) IS
   SELECT II
   FROM UTRQII
   WHERE RQ = C_RQ
     AND IC = C_IC
     AND ICNODE = C_ICNODE
     AND IIVALUE IS NULL
   ORDER BY IINODE;

CURSOR L_IE_CURSOR(C_IE VARCHAR2, C_IE_VERSION VARCHAR2) IS
   SELECT *
   FROM UTIE
   WHERE IE = C_IE
     AND VERSION = UNAPIGEN.VALIDATEVERSION('ie',C_IE,C_IE_VERSION);

CURSOR L_IPVERSION_CURSOR(C_RQ VARCHAR2, C_IC VARCHAR2, C_ICNODE NUMBER) IS
   SELECT IP_VERSION
   FROM UTRQIC
   WHERE RQ = C_RQ
   AND IC = C_IC
   AND ICNODE = C_ICNODE;

CURSOR L_IPIE_CURSOR(C_IP_VERSION VARCHAR2) IS
   SELECT A.*
   FROM UTIPIE A, UTIE B
   WHERE A.IP = A_IC
   AND A.VERSION = C_IP_VERSION
   AND A.IE = B.IE
   AND UNAPIGEN.VALIDATEVERSION('ie',A.IE,A.IE_VERSION) = B.VERSION
   AND A.IE = A_IE
   AND UNAPIGEN.VALIDATEVERSION('ie',A.IE,A.IE_VERSION) = A_IE_VERSION
   AND A.SEQ = A_SEQ
   ORDER BY A.SEQ;

CURSOR L_IINODE_CURSOR (A_IC VARCHAR2, A_ICNODE NUMBER) IS
   SELECT NVL(MAX(IINODE),0)
   FROM UTRQII
   WHERE RQ = A_RQ
   AND IC = A_IC
   AND ICNODE = A_ICNODE;

CURSOR L_OBJECTS_CURSOR (A_OBJECT_TYPE VARCHAR2) IS
   SELECT LOG_HS, LOG_HS_DETAILS, DEF_LC
   FROM UTOBJECTS
   WHERE OBJECT=A_OBJECT_TYPE;
   
BEGIN

   IF UNAPIGEN.BEGINTXN(UNAPIGEN.P_SINGLE_API_TXN) <> UNAPIGEN.DBERR_SUCCESS THEN
      RAISE STPERROR;
   END IF;

   IF NVL(A_RQ, ' ') = ' ' OR
      NVL(A_IC, ' ') = ' ' OR
      NVL(A_ICNODE, 0) = 0 OR
      NVL(A_IE, ' ') = ' ' OR 
      A_SEQ IS NULL THEN
      UNAPIGEN.P_TXN_ERROR := UNAPIGEN.DBERR_NOOBJID;
      RAISE STPERROR;
   END IF;

   
   L_LOG_HS := '0';
   L_LOG_HS_DETAILS := '0';
   OPEN L_OBJECTS_CURSOR('rqii');
   FETCH L_OBJECTS_CURSOR INTO L_LOG_HS, L_LOG_HS_DETAILS, L_DEF_LC;
   CLOSE L_OBJECTS_CURSOR;

   OPEN L_IINODE_CURSOR(A_IC, A_ICNODE);
   FETCH L_IINODE_CURSOR INTO L_IINODE;
   CLOSE L_IINODE_CURSOR;
   
   OPEN L_IPVERSION_CURSOR(A_RQ, A_IC, A_ICNODE);
   FETCH L_IPVERSION_CURSOR INTO L_IP_VERSION;
   CLOSE L_IPVERSION_CURSOR;

   L_HS_DETAILS_SEQ_NR := 0;
   FOR L_IPIE_REC IN L_IPIE_CURSOR(L_IP_VERSION) LOOP

      L_ERRM := '';
      L_IIVALUE := '';
      L_IPIE_REC.IE_VERSION := UNAPIGEN.VALIDATEVERSION('ie', L_IPIE_REC.IE, L_IPIE_REC.IE_VERSION);
      
      
      
      
      OPEN L_IE_CURSOR(L_IPIE_REC.IE, L_IPIE_REC.IE_VERSION);
      FETCH L_IE_CURSOR
      INTO L_IE_REC;
      IF L_IE_CURSOR%NOTFOUND THEN
         CLOSE L_IE_CURSOR;
         UNAPIGEN.P_TXN_ERROR := UNAPIGEN.DBERR_NOOBJECT;
         RAISE STPERROR;
      END IF;
      CLOSE L_IE_CURSOR;

      
      
      
      IF NVL(L_IPIE_REC.IS_PROTECTED, '2') <> '2' THEN
         L_IE_REC.IS_PROTECTED := L_IPIE_REC.IS_PROTECTED;
      END IF;

      IF NVL(L_IPIE_REC.MANDATORY, '2') <> '2' THEN
         L_IE_REC.MANDATORY := L_IPIE_REC.MANDATORY;
      END IF;

      IF NVL(L_IPIE_REC.HIDDEN, '2') <> '2' THEN
         L_IE_REC.HIDDEN := L_IPIE_REC.HIDDEN;
      END IF;

      IF NVL(L_IPIE_REC.DSP_TITLE_USE_IE, '1') <> '1' THEN
         L_IE_REC.DSP_TITLE := L_IPIE_REC.DSP_TITLE;
      END IF;
      IF L_IPIE_REC.DSP_LEN IS NOT NULL THEN
         L_IE_REC.DSP_LEN := L_IPIE_REC.DSP_LEN;
      END IF;
      IF L_IPIE_REC.DSP_ROWS IS NOT NULL THEN
         L_IE_REC.DSP_ROWS := L_IPIE_REC.DSP_ROWS;
      END IF;
      IF NVL(L_IPIE_REC.DSP_TP, ' ') <> ' ' THEN
         L_IE_REC.DSP_TP := L_IPIE_REC.DSP_TP;
      END IF;
      
      
      IF L_IE_REC.DSP_TP = 'B' THEN  
         IF NVL(L_IPIE_REC.IEVALUE, ' ') <> '2' THEN
            L_IE_REC.DEF_VAL_TP := L_IPIE_REC.DEF_VAL_TP;
            L_IE_REC.DEF_AU_LEVEL := L_IPIE_REC.DEF_AU_LEVEL;
            L_IE_REC.IEVALUE := L_IPIE_REC.IEVALUE;
         END IF;
      ELSE                           
         IF NVL(L_IPIE_REC.DEF_VAL_TP, 'F') <> 'F' OR
            NVL(L_IPIE_REC.IEVALUE, ' ') <> ' ' THEN
            L_IE_REC.DEF_VAL_TP := L_IPIE_REC.DEF_VAL_TP;
            L_IE_REC.DEF_AU_LEVEL := L_IPIE_REC.DEF_AU_LEVEL;
            L_IE_REC.IEVALUE := L_IPIE_REC.IEVALUE;
         END IF;
      END IF;

      BEGIN
      L_RET_CODE := FILLRQIIDEFAULTVALUE(A_RQ, L_IE_REC.DEF_VAL_TP,
                                         L_IE_REC.IEVALUE,
                                         L_IE_REC.DEF_AU_LEVEL,
                                         L_IE_REC.DATA_TP,
                                         L_IE_REC.FORMAT, L_IIVALUE);
      EXCEPTION
      WHEN OTHERS THEN
         L_ERRM := 'Warning#rq=' || A_RQ||
                   '#ic='|| A_IC ||
                   '#ii='|| L_IE_REC.IE ||
                   '#def_val_tp='|| L_IE_REC.DEF_VAL_TP ||
                   '#ievalue=' || SUBSTR(L_IE_REC.IEVALUE,1,40) ||
                   '#def_au_level=' || L_IE_REC.DEF_AU_LEVEL ||
                   '#data_tp=' || L_IE_REC.DATA_TP ||
                   '#format=' || L_IE_REC.FORMAT ||
                   '#FillRqiiDefaultValue#SqlCode=' || TO_CHAR(SQLCODE);
      END;

      IF L_RET_CODE = UNAPIGEN.DBERR_NODEFVALUE THEN
         L_IIVALUE := '';
      ELSIF L_RET_CODE <> UNAPIGEN.DBERR_SUCCESS AND L_ERRM IS NULL THEN
         L_ERRM := 'Warning#rq=' || A_RQ ||
                   '#ic='|| A_IC ||
                   '#ii='|| L_IE_REC.IE ||
                   '#def_val_tp='|| L_IE_REC.DEF_VAL_TP ||
                   '#ievalue=' || SUBSTR(L_IE_REC.IEVALUE,1,40) ||
                   '#def_au_level=' || L_IE_REC.DEF_AU_LEVEL ||
                   '#data_tp=' || L_IE_REC.DATA_TP ||
                   '#format=' || L_IE_REC.FORMAT ||
                   '#FillRqiiDefaultValue#ErrorCode=' || TO_CHAR(L_RET_CODE);
      END IF;
      IF L_ERRM IS NOT NULL THEN
         INSERT INTO UTERROR(CLIENT_ID, APPLIC, WHO, LOGDATE, LOGDATE_TZ, API_NAME, ERROR_MSG)
         VALUES(UNAPIGEN.P_CLIENT_ID, UNAPIGEN.P_APPLIC_NAME, UNAPIGEN.P_USER, CURRENT_TIMESTAMP, CURRENT_TIMESTAMP, 
                'AddRqIcDetails', L_ERRM);
      END IF;

      
      
      IF L_IE_REC.SC_LC IS NOT NULL THEN
         L_IE_REC.SC_LC_VERSION := UNAPIGEN.VALIDATEVERSION('lc', L_IE_REC.SC_LC, L_IE_REC.SC_LC_VERSION);
      END IF;
      
      L_IINODE := L_IINODE + UNAPIGEN.DEFAULT_NODE_INTERVAL;
      INSERT INTO UTRQII(RQ, IC, ICNODE, II, IINODE, IE_VERSION, IIVALUE,
                         POS_X, POS_Y, IS_PROTECTED, MANDATORY,
                         HIDDEN, DSP_TITLE, DSP_LEN, DSP_TP,
                         DSP_ROWS, LOG_HS, LOG_HS_DETAILS, ALLOW_MODIFY, ACTIVE, LC, LC_VERSION, SS)
      VALUES(A_RQ, A_IC, A_ICNODE, L_IPIE_REC.IE, L_IINODE, L_IPIE_REC.IE_VERSION, L_IIVALUE,
             L_IPIE_REC.POS_X, L_IPIE_REC.POS_Y,
             L_IE_REC.IS_PROTECTED, L_IE_REC.MANDATORY,
             L_IE_REC.HIDDEN, L_IE_REC.DSP_TITLE,
             L_IE_REC.DSP_LEN, L_IE_REC.DSP_TP,
             L_IE_REC.DSP_ROWS, L_LOG_HS, L_LOG_HS_DETAILS, '1', '0', L_IE_REC.SC_LC, L_IE_REC.SC_LC_VERSION, '');

      L_EV_SEQ_NR := -1;
      L_EV_DETAILS := 'rq=' || A_RQ || 
                      '#ic=' || A_IC ||
                      '#icnode=' || TO_CHAR(A_ICNODE)||
                      '#iinode=' || TO_CHAR(L_IINODE) ||
                      '#ie_version=' || L_IPIE_REC.IE_VERSION;
      IF L_IIVALUE IS NOT NULL THEN
         L_EV_DETAILS := L_EV_DETAILS || '#new_value=' || SUBSTR(L_IIVALUE,1,40);
      END IF;                      
      L_EVENT_TP := 'RqInfoFieldCreated';

      L_RESULT := UNAPIEV.INSERTINFOFIELDEVENT('AddRqIcDetails', UNAPIGEN.P_EVMGR_NAME, 'rqii', 
                                               L_IPIE_REC.IE, L_IE_REC.SC_LC, L_IE_REC.SC_LC_VERSION, 
                                               '', L_EVENT_TP, L_EV_DETAILS, L_EV_SEQ_NR);
      IF L_RESULT <> UNAPIGEN.DBERR_SUCCESS THEN
         UNAPIGEN.P_TXN_ERROR := L_RESULT;
         RAISE STPERROR;
      END IF;

      
      IF L_EV_SEQ_NR = -1 THEN
         
         
         OPEN L_SEQ_EVENT_CURSOR;
         FETCH L_SEQ_EVENT_CURSOR INTO L_EV_SEQ_NR;
         CLOSE L_SEQ_EVENT_CURSOR;
      END IF;

      IF L_LOG_HS = '1' THEN
         INSERT INTO UTRQICHS(RQ, IC, ICNODE, WHO, WHO_DESCRIPTION, WHAT, WHAT_DESCRIPTION, 
                              LOGDATE, LOGDATE_TZ, WHY, TR_SEQ, EV_SEQ)
         VALUES(A_RQ, A_IC, A_ICNODE, UNAPIGEN.P_USER, UNAPIGEN.P_USER_DESCRIPTION, L_EVENT_TP, 
                'info fields for info card "'||A_IC||'" are created',
                CURRENT_TIMESTAMP, CURRENT_TIMESTAMP, A_MODIFY_REASON, UNAPIGEN.P_TR_SEQ, L_EV_SEQ_NR);
      END IF;

      IF L_LOG_HS_DETAILS = '1' THEN
         L_HS_DETAILS_SEQ_NR := L_HS_DETAILS_SEQ_NR + 1;
         INSERT INTO UTRQICHSDETAILS(RQ, IC, ICNODE, TR_SEQ, EV_SEQ, SEQ, DETAILS)
         VALUES  (A_RQ, A_IC, A_ICNODE, UNAPIGEN.P_TR_SEQ, L_EV_SEQ_NR, L_HS_DETAILS_SEQ_NR, 
                  'Info fields for info card "'||A_IC||'" are created');
      END IF;
   END LOOP;

   
   OPEN L_NEXT_II_CURSOR(A_RQ, A_IC, A_ICNODE);
   FETCH L_NEXT_II_CURSOR
   INTO L_NEXT_II;
   IF L_NEXT_II_CURSOR%NOTFOUND THEN
      L_NEXT_II := NULL;
   END IF;

   
   UPDATE UTRQIC
   SET NEXT_II = L_NEXT_II
   WHERE RQ = A_RQ
     AND IC = A_IC
     AND ICNODE = A_ICNODE;

   IF UNAPIGEN.ENDTXN <> UNAPIGEN.DBERR_SUCCESS THEN
      RAISE STPERROR;
   END IF;

   RETURN(UNAPIGEN.DBERR_SUCCESS);

EXCEPTION
WHEN OTHERS THEN
   IF SQLCODE <> 1 THEN
      UNAPIGEN.LOGERROR('AddRqIcDetails',SQLERRM);
   END IF;
   IF L_IE_CURSOR%ISOPEN THEN
      CLOSE L_IE_CURSOR;
   END IF;
   IF L_IPVERSION_CURSOR%ISOPEN THEN
      CLOSE L_IPVERSION_CURSOR;
   END IF;
   IF L_IINODE_CURSOR%ISOPEN THEN
      CLOSE L_IINODE_CURSOR;
   END IF;
   IF L_OBJECTS_CURSOR%ISOPEN THEN
      CLOSE L_OBJECTS_CURSOR;
   END IF;
   IF L_SEQ_EVENT_CURSOR%ISOPEN THEN
      CLOSE L_SEQ_EVENT_CURSOR;
   END IF;
   RETURN(UNAPIGEN.ABORTTXN(UNAPIGEN.P_TXN_ERROR, 'AddRqIcDetails'));
END ADDRQICDETAILS;

FUNCTION INITRQINFOCARD                              
(A_IP              IN     VARCHAR2,                  
 A_IP_VERSION_IN   IN     VARCHAR2,                  
 A_SEQ             IN     NUMBER,                    
 A_RT              IN     VARCHAR2,                  
 A_RT_VERSION      IN     VARCHAR2,                  
 A_RQ              IN     VARCHAR2,                  
 A_IP_VERSION      OUT    UNAPIGEN.VC20_TABLE_TYPE,  
 A_DESCRIPTION     OUT    UNAPIGEN.VC40_TABLE_TYPE,  
 A_WINSIZE_X       OUT    UNAPIGEN.NUM_TABLE_TYPE,   
 A_WINSIZE_Y       OUT    UNAPIGEN.NUM_TABLE_TYPE,   
 A_IS_PROTECTED    OUT    UNAPIGEN.CHAR1_TABLE_TYPE ,
 A_HIDDEN          OUT    UNAPIGEN.CHAR1_TABLE_TYPE, 
 A_MANUALLY_ADDED  OUT    UNAPIGEN.CHAR1_TABLE_TYPE, 
 A_NEXT_II         OUT    UNAPIGEN.VC20_TABLE_TYPE,  
 A_IC_CLASS        OUT    UNAPIGEN.VC2_TABLE_TYPE,   
 A_LOG_HS          OUT    UNAPIGEN.CHAR1_TABLE_TYPE, 
 A_LOG_HS_DETAILS  OUT    UNAPIGEN.CHAR1_TABLE_TYPE, 
 A_LC              OUT    UNAPIGEN.VC2_TABLE_TYPE,   
 A_LC_VERSION      OUT    UNAPIGEN.VC20_TABLE_TYPE,  
 A_NR_OF_ROWS      IN OUT NUMBER)                    
RETURN NUMBER IS

L_IP_VERSION        VARCHAR2(20);
L_RT_VERSION        VARCHAR2(20);
L_DESCRIPTION       VARCHAR2(40);
L_WINSIZE_X         NUMBER(4);
L_WINSIZE_Y         NUMBER(4);
L_IS_PROTECTED      CHAR(1);
L_HIDDEN            CHAR(1);
L_MANUALLY_ADDED    CHAR(1);
L_NEXT_II           VARCHAR2(20);
L_IC_CLASS          VARCHAR2(2);
L_LOG_HS            CHAR(1);
L_LOG_HS_DETAILS    CHAR(1);
L_LC                VARCHAR2(2);
L_LC_VERSION        VARCHAR2(20);
L_CUR_IS_PROTECTED  CHAR(1);
L_CUR_HIDDEN        CHAR(1);
L_RTIP_CURSOR       INTEGER;

CURSOR L_IP_CURSOR (C_IP VARCHAR2, C_IP_VERSION VARCHAR2) IS
   SELECT DESCRIPTION, WINSIZE_X, WINSIZE_Y, IS_PROTECTED, HIDDEN,
          IP_CLASS, SC_LC, UNAPIGEN.USEVERSION('lc', SC_LC, SC_LC_VERSION) SC_LC_VERSION
   FROM UTIP
   WHERE IP = C_IP
   AND VERSION = C_IP_VERSION;

CURSOR L_OBJECTS_CURSOR (A_OBJECT_TYPE VARCHAR2) IS
   SELECT LOG_HS, LOG_HS_DETAILS
   FROM UTOBJECTS
   WHERE OBJECT=A_OBJECT_TYPE;

BEGIN

   IF NVL(A_NR_OF_ROWS, 0) = 0 THEN
      A_NR_OF_ROWS := UNAPIGEN.P_DEFAULT_CHUNK_SIZE;
   ELSIF A_NR_OF_ROWS < 0 OR A_NR_OF_ROWS > UNAPIGEN.P_MAX_CHUNK_SIZE THEN
      RETURN(UNAPIGEN.DBERR_NROFROWS);
   END IF;

   IF NVL(A_IP, ' ') = ' ' THEN
      RETURN(UNAPIGEN.DBERR_NOOBJID);
   END IF;

   IF NVL(A_RQ, ' ') = ' ' THEN
      RETURN(UNAPIGEN.DBERR_NOOBJID);
   END IF;

   
   L_IP_VERSION := UNAPIGEN.VALIDATEVERSION('ip', A_IP, A_IP_VERSION_IN);
   
   L_RT_VERSION := UNAPIGEN.VALIDATEVERSION('rt', A_RT, A_RT_VERSION);

   L_WHERE_CLAUSE := 'WHERE rt = ''' || REPLACE(A_RT, '''', '''''') || 
                     ''' AND version = ''' || REPLACE(L_RT_VERSION, '''', '''''') || 
                     ''' AND ip = ''' || REPLACE(A_IP, '''', '''''') || 
                     ''' AND UNAPIGEN.ValidateVersion(''ip'', ip, ip_version) = ''' || REPLACE(L_IP_VERSION, '''', '''''') || ''''; 
   IF NVL(A_SEQ, 0) <> 0 THEN
      L_WHERE_CLAUSE := L_WHERE_CLAUSE || ' AND seq = ' || A_SEQ || 
                       ' ORDER BY seq';
   ELSE
      L_WHERE_CLAUSE := L_WHERE_CLAUSE || ' ORDER BY seq '; 
   END IF;

   
   L_MANUALLY_ADDED := '1';
   L_IC_CLASS := NULL;
   L_NEXT_II := NULL;

   
   
   
   OPEN L_IP_CURSOR(A_IP, L_IP_VERSION);
   FETCH L_IP_CURSOR
   INTO L_DESCRIPTION, L_WINSIZE_X, L_WINSIZE_Y, L_IS_PROTECTED,
        L_HIDDEN, L_IC_CLASS, L_LC, L_LC_VERSION;

   IF L_IP_CURSOR%NOTFOUND THEN
      CLOSE L_IP_CURSOR;
      RETURN(UNAPIGEN.DBERR_NOOBJECT);
   END IF;

   CLOSE L_IP_CURSOR;

   
   
   
   IF NVL(A_RT, ' ') <> ' ' THEN
      L_RTIP_CURSOR := DBMS_SQL.OPEN_CURSOR;
      L_SQL_STRING := 'SELECT is_protected, hidden FROM dd'||
                      UNAPIGEN.P_DD || '.uvrtip '|| L_WHERE_CLAUSE;

      DBMS_SQL.PARSE(L_RTIP_CURSOR, L_SQL_STRING, DBMS_SQL.V7); 
      DBMS_SQL.DEFINE_COLUMN_CHAR(L_RTIP_CURSOR, 1, L_CUR_IS_PROTECTED, 1);
      DBMS_SQL.DEFINE_COLUMN_CHAR(L_RTIP_CURSOR, 2, L_CUR_HIDDEN, 1);
      L_RESULT := DBMS_SQL.EXECUTE_AND_FETCH(L_RTIP_CURSOR);

      IF L_RESULT <> 0 THEN
         DBMS_SQL.COLUMN_VALUE_CHAR(L_RTIP_CURSOR, 1, L_CUR_IS_PROTECTED);
         DBMS_SQL.COLUMN_VALUE_CHAR(L_RTIP_CURSOR, 2, L_CUR_HIDDEN);

         IF NVL(L_CUR_IS_PROTECTED, '2') <> '2' THEN
            L_IS_PROTECTED := L_CUR_IS_PROTECTED;
         END IF;
         IF NVL(L_CUR_HIDDEN, '2') <> '2' THEN
            L_HIDDEN := L_CUR_HIDDEN;
         END IF;
      END IF;
      DBMS_SQL.CLOSE_CURSOR(L_RTIP_CURSOR);
   END IF;

   
   L_LOG_HS := '0';
   L_LOG_HS_DETAILS := '0';
   OPEN L_OBJECTS_CURSOR('rqic');
   FETCH L_OBJECTS_CURSOR INTO L_LOG_HS, L_LOG_HS_DETAILS;
   CLOSE L_OBJECTS_CURSOR;

   
   
   
   

   L_FETCHED_ROWS := 1;
   FOR L_ROWS IN 1..L_FETCHED_ROWS LOOP
      A_IP_VERSION(L_ROWS) := L_IP_VERSION;
      A_DESCRIPTION(L_ROWS) := L_DESCRIPTION;
      A_WINSIZE_X(L_ROWS) := L_WINSIZE_X;
      A_WINSIZE_Y(L_ROWS) := L_WINSIZE_Y;
      A_IS_PROTECTED(L_ROWS) := L_IS_PROTECTED;
      A_HIDDEN(L_ROWS) := L_HIDDEN;
      A_MANUALLY_ADDED(L_ROWS) := L_MANUALLY_ADDED;
      A_NEXT_II(L_ROWS) := L_NEXT_II;
      A_IC_CLASS(L_ROWS) := L_IC_CLASS;
      A_LOG_HS(L_ROWS) := L_LOG_HS;
      A_LOG_HS_DETAILS(L_ROWS) := L_LOG_HS_DETAILS;
      A_LC(L_ROWS) := L_LC;
      A_LC_VERSION(L_ROWS) := L_LC_VERSION;
   END LOOP;

   
   
   
   
   IF L_FETCHED_ROWS > A_NR_OF_ROWS THEN
      INSERT INTO UTERROR(CLIENT_ID, APPLIC, WHO, LOGDATE, LOGDATE_TZ, API_NAME, ERROR_MSG)
      VALUES(UNAPIGEN.P_CLIENT_ID, UNAPIGEN.P_APPLIC_NAME, UNAPIGEN.P_USER, CURRENT_TIMESTAMP, CURRENT_TIMESTAMP, 
             'InitScInfoCard','a_nr_of_rows ('||A_NR_OF_ROWS||
             ') too small for required InfoCard initialisation');
   END IF;

   A_NR_OF_ROWS := L_FETCHED_ROWS;
   RETURN(UNAPIGEN.DBERR_SUCCESS);

EXCEPTION
   WHEN OTHERS THEN
      L_SQLERRM := SQLERRM;
      UNAPIGEN.U4ROLLBACK;
      INSERT INTO UTERROR(CLIENT_ID, APPLIC, WHO, LOGDATE, LOGDATE_TZ, API_NAME, ERROR_MSG)
      VALUES(UNAPIGEN.P_CLIENT_ID, UNAPIGEN.P_APPLIC_NAME, UNAPIGEN.P_USER, CURRENT_TIMESTAMP, CURRENT_TIMESTAMP, 
              'InitRqInfoCard', L_SQLERRM);
      UNAPIGEN.U4COMMIT;
      IF L_OBJECTS_CURSOR%ISOPEN THEN
         CLOSE L_OBJECTS_CURSOR;
      END IF;      
      RETURN(UNAPIGEN.DBERR_GENFAIL);
END INITRQINFOCARD;







FUNCTION FILLRQIIDEFAULTVALUE               
(A_RQ                 IN      VARCHAR2,     
 A_DEF_VAL_TP         IN      CHAR,         
 A_IEVALUE            IN      VARCHAR2,     
 A_DEF_AU_LEVEL       IN      VARCHAR2,     
 A_DATA_TP            IN      CHAR,         
 A_FORMAT             IN      VARCHAR2,     
 A_IIVALUE            OUT     VARCHAR2)     
RETURN NUMBER IS

L_RETURN                   INTEGER;
L_FORMAT_TYPE              CHAR(1);
L_FORMAT_VALUE             VARCHAR2(40);
L_DIVIDE_POS               INTEGER;
L_DOT_POS                  INTEGER;
L_TO_CHAR_FORMAT           VARCHAR2(40);
L_IEVALUE                  VARCHAR2(2000);
L_IEVALUE_F                NUMBER;
L_IIVALUE                  VARCHAR2(2000);
L_ROUND_VALUE              NUMBER;
L_DATE_CURSOR              INTEGER;
L_DATE                     TIMESTAMP WITH TIME ZONE;
L_DATE_FORMAT              VARCHAR2(40);

L_NEXT_VAL                 VARCHAR2(160);
L_EDIT_ALLOWED             CHAR(1);
L_VALID_CF                 VARCHAR2(260);

L_IEVAL                    VARCHAR2(2000);
L_VERSION_IS_CURRENT       CHAR(1);
L_EFFECTIVE_FROM           TIMESTAMP WITH TIME ZONE;
L_EFFECTIVE_TILL           TIMESTAMP WITH TIME ZONE;
L_DESCRIPTION              VARCHAR2(80);
L_CREATION_DATE            TIMESTAMP WITH TIME ZONE;
L_CREATED_BY               VARCHAR2(20);
L_TOOLTIP                  VARCHAR2(255);
L_URL                      VARCHAR2(512);
L_DATA                     BLOB;
L_LAST_CHECKOUT_BY         VARCHAR2(20);
L_LAST_CHECKOUT_URL        VARCHAR2(512);
L_CHECKED_OUT              CHAR(1);
L_DC_CLASS                 VARCHAR2(2);
L_LOG_HS                   CHAR(1);
L_ALLOW_MODIFY             CHAR(1);
L_ACTIVE                   CHAR(1);
L_LC                       VARCHAR2(2);
L_LC_VERSION               VARCHAR2(20);
L_SS                       VARCHAR2(2);
L_DOC_VERSION              VARCHAR2(20);
L_DOC_VERSION_TEMP         VARCHAR2(20);
L_DOC_UC                   VARCHAR2(20);
L_DC                       VARCHAR2(20);
L_PROPERTY_RQ              BOOLEAN;
L_POSITION                 INTEGER;
L_EXTENTION                VARCHAR2(20);

CURSOR L_UCDC_CURSOR IS
   SELECT UC
   FROM UTUC
   WHERE DEF_MASK_FOR='dc';

CURSOR L_UC_CURSOR(L_UC VARCHAR2) IS
   SELECT UC
   FROM UTUC
   WHERE UC=L_UC;

CURSOR L_RQRT_CURSOR (C_RQ VARCHAR2) IS
   SELECT RQ, RT, RT_VERSION
   FROM UTRQ
   WHERE RQ     = C_RQ;
L_RQRT_REC L_RQRT_CURSOR%ROWTYPE; 
CURSOR L_RTAU_CURSOR IS
   SELECT VALUE
   FROM UTRTAU
   WHERE (RT,VERSION) = (SELECT RT,RT_VERSION 
                         FROM UTRQ
                         WHERE RQ = A_RQ)
     AND AU = A_IEVALUE
     AND ROWNUM = 1
   ORDER BY AUSEQ;

CURSOR L_RQAU_CURSOR IS
   SELECT VALUE
   FROM UTRQAU
   WHERE RQ = A_RQ
     AND AU = A_IEVALUE
     AND ROWNUM = 1
   ORDER BY AUSEQ;

CURSOR L_UCFW_CURSOR IS
   SELECT UC
   FROM UTUC
   WHERE DEF_MASK_FOR='fw';
   
L_FWB_UC                   VARCHAR2(40);
L_FWB                      VARCHAR2(40);
L_FWB_VERSION              VARCHAR2(20);

BEGIN
   A_IIVALUE := '';
   L_RET_CODE := UNAPIGEN.DBERR_SUCCESS;

   
   
   
   L_PROPERTY_RQ := TRUE;
   L_POSITION := INSTR(A_IEVALUE,'@',1,1);
   IF L_POSITION > 0 THEN
      IF SUBSTR(A_IEVALUE,0,L_POSITION-1) = 'RT' THEN
         L_PROPERTY_RQ := FALSE;
      END IF;
      L_IEVAL := SUBSTR(A_IEVALUE, L_POSITION+1);
   ELSE
      L_IEVAL := A_IEVALUE;
   END IF;

   L_POSITION := INSTR(A_IEVALUE,'#',1,1);
   IF L_POSITION > 0 THEN
      L_EXTENTION := SUBSTR(A_IEVALUE,L_POSITION +1);
   ELSE
      L_EXTENTION := ' '; 
   END IF;   
   
   IF UNAPIGEN.P_RNDSUITESESSION = '1' THEN
      L_DATE_FORMAT := 'YYYY-MM-DD"T"HH24:MI:SS.FF1"'||TO_CHAR(CURRENT_TIMESTAMP,'TZH:TZM')||'"';
   ELSIF NVL(LENGTH(A_FORMAT),0)>1 THEN   
      L_DATE_FORMAT  := SUBSTR(A_FORMAT,2);
   END IF;
   
   
   
   IF A_DEF_VAL_TP = 'S' THEN
      
      IF L_PROPERTY_RQ THEN
         IF L_IEVAL = 'rq' THEN                          
            SELECT RQ
            INTO L_IEVALUE
            FROM UTRQ
            WHERE RQ = A_RQ;
         ELSIF L_IEVAL = 'rt' THEN                       
            SELECT RT
            INTO L_IEVALUE
            FROM UTRQ
            WHERE RQ = A_RQ;
         ELSIF L_IEVAL = 'description' THEN              
            SELECT DESCRIPTION
            INTO L_IEVALUE
            FROM UTRQ
            WHERE RQ = A_RQ;
         ELSIF L_IEVAL = 'descr_doc' THEN               
            SELECT DESCR_DOC
            INTO L_IEVALUE
            FROM UTRQ
            WHERE RQ = A_RQ;
         ELSIF L_IEVAL = 'sampling_date' THEN            
            SELECT TO_CHAR(SAMPLING_DATE, L_DATE_FORMAT)
            INTO L_IEVALUE
            FROM UTRQ
            WHERE RQ = A_RQ;
         ELSIF L_IEVAL = 'creation_date' THEN            
            SELECT TO_CHAR(CREATION_DATE, L_DATE_FORMAT)
            INTO L_IEVALUE
            FROM UTRQ
            WHERE RQ = A_RQ;
         ELSIF L_IEVAL = 'created_by' THEN               
            SELECT CREATED_BY
            INTO L_IEVALUE
            FROM UTRQ
            WHERE RQ = A_RQ;
         ELSIF L_IEVAL = 'exec_start_date' THEN          
         SELECT TO_CHAR(EXEC_START_DATE, L_DATE_FORMAT)
            INTO L_IEVALUE
            FROM UTRQ
            WHERE RQ = A_RQ;
         ELSIF L_IEVAL = 'exec_end_date' THEN            
            SELECT TO_CHAR(EXEC_END_DATE, L_DATE_FORMAT)
            INTO L_IEVALUE
            FROM UTRQ
            WHERE RQ = A_RQ;
         ELSIF L_IEVAL = 'due_date' THEN                 
            SELECT TO_CHAR(DUE_DATE, L_DATE_FORMAT)
            INTO L_IEVALUE
            FROM UTRQ
            WHERE RQ = A_RQ;
         ELSIF L_IEVAL = 'priority' THEN                 
            SELECT PRIORITY
            INTO L_IEVALUE
            FROM UTRQ
            WHERE RQ = A_RQ;
         ELSIF L_IEVAL = 'label_format' THEN             
            SELECT LABEL_FORMAT
            INTO L_IEVALUE
            FROM UTRQ
            WHERE RQ = A_RQ;
         ELSIF L_IEVAL = 'date1' THEN                    
            SELECT TO_CHAR(DATE1, L_DATE_FORMAT)
            INTO L_IEVALUE
            FROM UTRQ
            WHERE RQ = A_RQ;
         ELSIF L_IEVAL = 'date2' THEN                    
            SELECT TO_CHAR(DATE2, L_DATE_FORMAT)
            INTO L_IEVALUE
            FROM UTRQ
            WHERE RQ = A_RQ;
         ELSIF L_IEVAL = 'date3' THEN                    
            SELECT TO_CHAR(DATE3, L_DATE_FORMAT)
            INTO L_IEVALUE
            FROM UTRQ
            WHERE RQ = A_RQ;
         ELSIF L_IEVAL = 'date4' THEN                    
            SELECT TO_CHAR(DATE4, L_DATE_FORMAT)
            INTO L_IEVALUE
            FROM UTRQ
            WHERE RQ = A_RQ;
         ELSIF L_IEVAL = 'date5' THEN                    
            SELECT TO_CHAR(DATE5, L_DATE_FORMAT)
            INTO L_IEVALUE
            FROM UTRQ
            WHERE RQ = A_RQ;
          ELSIF L_IEVAL = 'allow_any_st' THEN             
            SELECT ALLOW_ANY_ST
            INTO L_IEVALUE
            FROM UTRQ
            WHERE RQ = A_RQ;
         ELSIF L_IEVAL = 'allow_new_sc' THEN             
            SELECT ALLOW_NEW_SC
            INTO L_IEVALUE
            FROM UTRQ
            WHERE RQ = A_RQ;
         ELSIF L_IEVAL = 'responsible' THEN              
            SELECT RESPONSIBLE
            INTO L_IEVALUE
            FROM UTRQ
            WHERE RQ = A_RQ;
         ELSIF L_IEVAL = 'rq_class' THEN                 
            SELECT RQ_CLASS
            INTO L_IEVALUE
            FROM UTRQ
            WHERE RQ = A_RQ;
         ELSE
            RETURN(UNAPIGEN.DBERR_DEFVALUETP);
         END IF;
      ELSE
         OPEN L_RQRT_CURSOR(A_RQ);
         FETCH L_RQRT_CURSOR
         INTO L_RQRT_REC;
         CLOSE L_RQRT_CURSOR;        
          
         IF L_IEVAL = 'rt' THEN
            SELECT RT
            INTO L_IEVALUE
            FROM UTRT
            WHERE RT=L_RQRT_REC.RT AND VERSION=L_RQRT_REC.RT_VERSION;
                                
         ELSIF L_IEVAL = 'version' THEN
            SELECT VERSION
            INTO L_IEVALUE
            FROM UTRT
            WHERE RT=L_RQRT_REC.RT AND VERSION=L_RQRT_REC.RT_VERSION;
                
         ELSIF L_IEVAL = 'version_is_current' THEN
            SELECT VERSION_IS_CURRENT
            INTO L_IEVALUE
            FROM UTRT
            WHERE RT=L_RQRT_REC.RT AND VERSION=L_RQRT_REC.RT_VERSION;
                
         ELSIF L_IEVAL = 'effective_from' THEN
            SELECT TO_CHAR(EFFECTIVE_FROM, L_DATE_FORMAT)
            INTO L_IEVALUE
            FROM UTRT
            WHERE RT=L_RQRT_REC.RT AND VERSION=L_RQRT_REC.RT_VERSION;
         ELSIF L_IEVAL = 'effective_till' THEN
            SELECT TO_CHAR(EFFECTIVE_TILL, L_DATE_FORMAT)
            INTO L_IEVALUE
            FROM UTRT
            WHERE RT=L_RQRT_REC.RT AND VERSION=L_RQRT_REC.RT_VERSION;
         ELSIF L_IEVAL = 'description' THEN
            SELECT DESCRIPTION
            INTO L_IEVALUE
            FROM UTRT
            WHERE RT=L_RQRT_REC.RT AND VERSION=L_RQRT_REC.RT_VERSION;
             
         ELSIF L_IEVAL = 'description2' THEN
            SELECT DESCRIPTION2
            INTO L_IEVALUE
            FROM UTRT
            WHERE RT=L_RQRT_REC.RT AND VERSION=L_RQRT_REC.RT_VERSION;
                
         ELSIF L_IEVAL = 'descr_doc' THEN
            SELECT DESCR_DOC
            INTO L_IEVALUE
            FROM UTRT
            WHERE RT=L_RQRT_REC.RT AND VERSION=L_RQRT_REC.RT_VERSION;
              
         ELSIF L_IEVAL = 'descr_doc_version' THEN
            SELECT DESCR_DOC_VERSION
            INTO L_IEVALUE
            FROM UTRT
            WHERE RT=L_RQRT_REC.RT AND VERSION=L_RQRT_REC.RT_VERSION;
                
         ELSIF L_IEVAL = 'is_template' THEN
            SELECT IS_TEMPLATE
            INTO L_IEVALUE
            FROM UTRT
            WHERE RT=L_RQRT_REC.RT AND VERSION=L_RQRT_REC.RT_VERSION;
                
         ELSIF L_IEVAL = 'confirm_userid' THEN
            SELECT CONFIRM_USERID
            INTO L_IEVALUE
            FROM UTRT
            WHERE RT=L_RQRT_REC.RT AND VERSION=L_RQRT_REC.RT_VERSION;
                
         ELSIF L_IEVAL = 'nr_planned_rq' THEN
            SELECT NR_PLANNED_RQ
            INTO L_IEVALUE
            FROM UTRT
            WHERE RT=L_RQRT_REC.RT AND VERSION=L_RQRT_REC.RT_VERSION;
                
         ELSIF L_IEVAL = 'freq_tp' THEN
            SELECT FREQ_TP
            INTO L_IEVALUE
            FROM UTRT
            WHERE RT=L_RQRT_REC.RT AND VERSION=L_RQRT_REC.RT_VERSION;
                
         ELSIF L_IEVAL = 'freq_val' THEN
            SELECT FREQ_VAL
            INTO L_IEVALUE
            FROM UTRT
            WHERE RT=L_RQRT_REC.RT AND VERSION=L_RQRT_REC.RT_VERSION;
                
         ELSIF L_IEVAL = 'freq_unit' THEN
            SELECT FREQ_UNIT
            INTO L_IEVALUE
            FROM UTRT
            WHERE RT=L_RQRT_REC.RT AND VERSION=L_RQRT_REC.RT_VERSION;
                
         ELSIF L_IEVAL = 'invert_freq' THEN
            SELECT INVERT_FREQ
            INTO L_IEVALUE
            FROM UTRT
            WHERE RT=L_RQRT_REC.RT AND VERSION=L_RQRT_REC.RT_VERSION;
                
         ELSIF L_IEVAL = 'last_sched' THEN
            SELECT LAST_SCHED
            INTO L_IEVALUE
            FROM UTRT
            WHERE RT=L_RQRT_REC.RT AND VERSION=L_RQRT_REC.RT_VERSION;
                
         ELSIF L_IEVAL = 'last_cnt' THEN
            SELECT LAST_CNT
            INTO L_IEVALUE
            FROM UTRT
            WHERE RT=L_RQRT_REC.RT AND VERSION=L_RQRT_REC.RT_VERSION;
               
         ELSIF L_IEVAL = 'last_val' THEN
            SELECT LAST_VAL
            INTO L_IEVALUE
            FROM UTRT
            WHERE RT=L_RQRT_REC.RT AND VERSION=L_RQRT_REC.RT_VERSION;
                
         ELSIF L_IEVAL = 'priority' THEN
            SELECT PRIORITY
            INTO L_IEVALUE
            FROM UTRT
            WHERE RT=L_RQRT_REC.RT AND VERSION=L_RQRT_REC.RT_VERSION;
 
         ELSIF L_IEVAL = 'label_format' THEN
            SELECT LABEL_FORMAT
            INTO L_IEVALUE
            FROM UTRT
            WHERE RT=L_RQRT_REC.RT AND VERSION=L_RQRT_REC.RT_VERSION;
                
         ELSIF L_IEVAL = 'allow_any_st' THEN
            SELECT ALLOW_ANY_ST
            INTO L_IEVALUE
            FROM UTRT
            WHERE RT=L_RQRT_REC.RT AND VERSION=L_RQRT_REC.RT_VERSION;
                
         ELSIF L_IEVAL = 'allow_new_sc' THEN
            SELECT ALLOW_NEW_SC
            INTO L_IEVALUE
            FROM UTRT
            WHERE RT=L_RQRT_REC.RT AND VERSION=L_RQRT_REC.RT_VERSION;
                
         ELSIF L_IEVAL = 'add_stpp' THEN
            SELECT ADD_STPP
            INTO L_IEVALUE
            FROM UTRT
            WHERE RT=L_RQRT_REC.RT AND VERSION=L_RQRT_REC.RT_VERSION;
 
         ELSIF L_IEVAL = 'planned_responsible' THEN
            SELECT PLANNED_RESPONSIBLE
            INTO L_IEVALUE
            FROM UTRT
            WHERE RT=L_RQRT_REC.RT AND VERSION=L_RQRT_REC.RT_VERSION;
 
         ELSIF L_IEVAL = 'sc_uc' THEN
            SELECT SC_UC
            INTO L_IEVALUE
            FROM UTRT
            WHERE RT=L_RQRT_REC.RT AND VERSION=L_RQRT_REC.RT_VERSION;
 
         ELSIF L_IEVAL = 'sc_uc_version' THEN
            SELECT SC_UC_VERSION
            INTO L_IEVALUE
            FROM UTRT
            WHERE RT=L_RQRT_REC.RT AND VERSION=L_RQRT_REC.RT_VERSION;
 
         ELSIF L_IEVAL = 'rq_uc' THEN
            SELECT RQ_UC
            INTO L_IEVALUE
            FROM UTRT
            WHERE RT=L_RQRT_REC.RT AND VERSION=L_RQRT_REC.RT_VERSION;
 
         ELSIF L_IEVAL = 'rq_uc_version' THEN
            SELECT RQ_UC_VERSION
            INTO L_IEVALUE
            FROM UTRT
            WHERE RT=L_RQRT_REC.RT AND VERSION=L_RQRT_REC.RT_VERSION;
 
         ELSIF L_IEVAL = 'rq_lc' THEN
            SELECT RQ_LC
            INTO L_IEVALUE
            FROM UTRT
            WHERE RT=L_RQRT_REC.RT AND VERSION=L_RQRT_REC.RT_VERSION;
 
         ELSIF L_IEVAL = 'rq_lc_version' THEN
            SELECT RQ_LC_VERSION
            INTO L_IEVALUE
            FROM UTRT
            WHERE RT=L_RQRT_REC.RT AND VERSION=L_RQRT_REC.RT_VERSION;
 
         ELSIF L_IEVAL = 'inherit_au' THEN
            SELECT INHERIT_AU
            INTO L_IEVALUE
            FROM UTRT
            WHERE RT=L_RQRT_REC.RT AND VERSION=L_RQRT_REC.RT_VERSION;
 
         ELSIF L_IEVAL = 'inherit_gk' THEN
            SELECT INHERIT_GK
            INTO L_IEVALUE
            FROM UTRT
            WHERE RT=L_RQRT_REC.RT AND VERSION=L_RQRT_REC.RT_VERSION;
 
         ELSIF L_IEVAL = 'last_comment' THEN
            SELECT LAST_COMMENT
            INTO L_IEVALUE
            FROM UTRT
            WHERE RT=L_RQRT_REC.RT AND VERSION=L_RQRT_REC.RT_VERSION;
 
         ELSIF L_IEVAL = 'rt_class' THEN
            SELECT RT_CLASS
            INTO L_IEVALUE
            FROM UTRT
            WHERE RT=L_RQRT_REC.RT AND VERSION=L_RQRT_REC.RT_VERSION;
 
         ELSIF L_IEVAL = 'log_hs' THEN
            SELECT LOG_HS
            INTO L_IEVALUE
            FROM UTRT
            WHERE RT=L_RQRT_REC.RT AND VERSION=L_RQRT_REC.RT_VERSION;
 
         ELSIF L_IEVAL = 'log_hs_details' THEN
            SELECT LOG_HS_DETAILS
            INTO L_IEVALUE
            FROM UTRT
            WHERE RT=L_RQRT_REC.RT AND VERSION=L_RQRT_REC.RT_VERSION;
 
         ELSIF L_IEVAL = 'allow_modify' THEN
            SELECT ALLOW_MODIFY
            INTO L_IEVALUE
            FROM UTRT
            WHERE RT=L_RQRT_REC.RT AND VERSION=L_RQRT_REC.RT_VERSION;
 
         ELSIF L_IEVAL = 'active' THEN
            SELECT ACTIVE
            INTO L_IEVALUE
            FROM UTRT
            WHERE RT=L_RQRT_REC.RT AND VERSION=L_RQRT_REC.RT_VERSION;
                
         ELSIF L_IEVAL = 'lc' THEN
            SELECT LC
            INTO L_IEVALUE
            FROM UTRT
            WHERE RT=L_RQRT_REC.RT AND VERSION=L_RQRT_REC.RT_VERSION;
                
         ELSIF L_IEVAL = 'lc_version' THEN
            SELECT LC_VERSION
            INTO L_IEVALUE
            FROM UTRT
            WHERE RT=L_RQRT_REC.RT AND VERSION=L_RQRT_REC.RT_VERSION;
                
         ELSIF L_IEVAL = 'ss' THEN
            SELECT SS
            INTO L_IEVALUE
            FROM UTRT
            WHERE RT=L_RQRT_REC.RT AND VERSION=L_RQRT_REC.RT_VERSION;
                
         ELSE
            RETURN(UNAPIGEN.DBERR_DEFVALUETP);
         END IF;
      END IF;
      A_IIVALUE := L_IEVALUE;
   
   
   
   ELSIF A_DEF_VAL_TP = 'A' THEN
      IF A_DEF_AU_LEVEL = 'rt' THEN                     
         OPEN L_RTAU_CURSOR;
         FETCH L_RTAU_CURSOR
         INTO L_IEVALUE;
         IF L_RTAU_CURSOR%NOTFOUND THEN
            L_IEVALUE := '';
         END IF;
         CLOSE L_RTAU_CURSOR;

      ELSIF A_DEF_AU_LEVEL = 'rq' THEN                  
         OPEN L_RQAU_CURSOR;
         FETCH L_RQAU_CURSOR
         INTO L_IEVALUE;
         IF L_RQAU_CURSOR%NOTFOUND THEN
            L_IEVALUE := '';
         END IF;
         CLOSE L_RQAU_CURSOR;
      ELSIF A_DEF_AU_LEVEL IN ('sc','st') THEN                  
         
         NULL;
         L_IEVALUE := '';
      ELSE
         RETURN(UNAPIGEN.DBERR_AULEVEL);
      END IF;
      A_IIVALUE := L_IEVALUE;

   
   
   
   ELSIF NVL(A_DEF_VAL_TP, 'F') = 'F' THEN
      IF A_DATA_TP IN ('D', 'M') AND
         NVL(A_IEVALUE, ' ') <> ' ' THEN             

         BEGIN
            L_DATE_CURSOR := DBMS_SQL.OPEN_CURSOR;
            IF SUBSTR(A_IEVALUE, 1, 1) = '=' THEN
               L_SQL_STRING := 'BEGIN :l_date :' || A_IEVALUE || '; END;';
            ELSE
               L_SQL_STRING := 'BEGIN :l_date := ' || A_IEVALUE || '; END;';
            END IF;
            DBMS_SQL.PARSE(L_DATE_CURSOR, L_SQL_STRING, DBMS_SQL.V7); 
            DBMS_SQL.BIND_VARIABLE(L_DATE_CURSOR, ':l_date', L_DATE);
            L_RESULT := DBMS_SQL.EXECUTE(L_DATE_CURSOR);
            DBMS_SQL.VARIABLE_VALUE(L_DATE_CURSOR, ':l_date', L_DATE);
            DBMS_SQL.CLOSE_CURSOR(L_DATE_CURSOR);

            IF UNAPIGEN.P_RNDSUITESESSION = '1' THEN
               L_IEVALUE := TO_CHAR(L_DATE, 'YYYY-MM-DD"T"HH24:MI:SS.FF1TZH:TZM');
            ELSE
               L_IEVALUE := TO_CHAR(L_DATE, SUBSTR(A_FORMAT,2));
            END IF;
            
         EXCEPTION
         WHEN OTHERS THEN
            IF DBMS_SQL.IS_OPEN(L_DATE_CURSOR) THEN
               DBMS_SQL.CLOSE_CURSOR(L_DATE_CURSOR);
            END IF;
            RAISE;     
         END;
      ELSIF (A_DATA_TP = 'B') THEN 
      
         BEGIN
            OPEN L_UCDC_CURSOR;
            FETCH L_UCDC_CURSOR
            INTO L_DOC_UC;
            IF L_UCDC_CURSOR%NOTFOUND THEN
               L_DOC_UC := ' ';
            END IF;
            CLOSE L_UCDC_CURSOR;

            IF NVL(L_DOC_UC, ' ') =' ' THEN
               OPEN L_UC_CURSOR('doc_blob');
               FETCH L_UC_CURSOR
               INTO L_DOC_UC;
               IF L_UC_CURSOR%NOTFOUND THEN
                  L_DOC_UC := ' ';
               END IF;
              CLOSE L_UC_CURSOR;
            END IF;

            IF NVL(L_DOC_UC, ' ') =' ' THEN
               OPEN L_UC_CURSOR('doc_link');
               FETCH L_UC_CURSOR
               INTO L_DOC_UC;
               IF L_UC_CURSOR%NOTFOUND THEN
                  L_DOC_UC := ' ';
               END IF;
               CLOSE L_UC_CURSOR;
            END IF;

            IF NVL(L_DOC_UC, ' ') <> ' ' THEN
               
               L_RETURN := UNAPIUC.CREATENEXTUNIQUECODEVALUE(L_DOC_UC, ' ', ' ', ' ' ,' ', ' ', CURRENT_TIMESTAMP, L_NEXT_VAL, L_EDIT_ALLOWED, L_VALID_CF);
               IF (L_RETURN = 0) AND (NVL(L_NEXT_VAL, ' ') !=' ') THEN
                  IF INSTR(A_IEVALUE, '#', -1) = 0 THEN
                     L_DC := A_IEVALUE;
                     L_DOC_VERSION := '~Highest~'; 
                  ELSE
                     L_DC := SUBSTR(A_IEVALUE, 1, INSTR(A_IEVALUE, '#', -1)-1);
                     L_DOC_VERSION := SUBSTR(A_IEVALUE, INSTR(A_IEVALUE, '#', -1)+1); 
                  END IF;
                  BEGIN
                    
                    L_RETURN := UNAPIDC.GETDOCUMENT(L_DC,L_DOC_VERSION,L_VERSION_IS_CURRENT,L_EFFECTIVE_FROM,L_EFFECTIVE_TILL,L_DESCRIPTION,L_CREATION_DATE,L_CREATED_BY,L_TOOLTIP,L_URL,L_DATA,L_LAST_CHECKOUT_BY,L_LAST_CHECKOUT_URL,L_CHECKED_OUT,L_DC_CLASS,L_LOG_HS,L_ALLOW_MODIFY,L_ACTIVE,L_LC,L_LC_VERSION,L_SS);
                  EXCEPTION
                     WHEN OTHERS THEN
                     NULL;
                  END;
          
                  IF L_RETURN <> 0 THEN
                     IF INSTR(A_IEVALUE, '#', -1) <> 0 THEN
                      
                        L_DC := A_IEVALUE;
                        L_DOC_VERSION := '~Highest~';                 
                        L_RETURN := UNAPIDC.GETDOCUMENT(L_DC,L_DOC_VERSION,L_VERSION_IS_CURRENT,L_EFFECTIVE_FROM,L_EFFECTIVE_TILL,L_DESCRIPTION,L_CREATION_DATE,L_CREATED_BY,L_TOOLTIP,L_URL,L_DATA,L_LAST_CHECKOUT_BY,L_LAST_CHECKOUT_URL,L_CHECKED_OUT,L_DC_CLASS,L_LOG_HS,L_ALLOW_MODIFY,L_ACTIVE,L_LC,L_LC_VERSION,L_SS);
                     END IF;
                  END IF;
                  IF L_RETURN = 0 THEN
                     L_RETURN := UNAPIDC.SAVEDOCUMENT(L_NEXT_VAL,UNVERSION.P_INITIAL_VERSION,'0',NULL,NULL,L_DESCRIPTION, CURRENT_TIMESTAMP,'',L_TOOLTIP,L_URL,L_DATA,L_LAST_CHECKOUT_BY,L_LAST_CHECKOUT_URL,L_CHECKED_OUT,L_DC_CLASS,L_LOG_HS,L_LC,L_LC_VERSION,'');
                     IF L_RETURN = 0 THEN
                        IF UNAPIGEN.P_RNDSUITESESSION = '1' AND L_LC = '@D' THEN
                           L_IEVALUE := L_NEXT_VAL||'#~Highest~';
                        ELSE
                           L_IEVALUE := L_NEXT_VAL||'#'||UNVERSION.P_INITIAL_VERSION;
                        END IF;
                     END IF;
                  END IF;
               END IF;
            END IF;
         EXCEPTION
            WHEN OTHERS THEN
            NULL;
         END;
      ELSIF (A_DATA_TP = 'W') AND UNAPIGEN.P_RNDSUITESESSION = '1' THEN 
      
         BEGIN
            OPEN L_UCFW_CURSOR;
            FETCH L_UCFW_CURSOR
            INTO L_FWB_UC;
            IF L_UCFW_CURSOR%NOTFOUND THEN
               L_FWB_UC := ' ';
            END IF;
            CLOSE L_UCFW_CURSOR;

            IF NVL(L_FWB_UC, ' ') =' ' THEN
               OPEN L_UC_CURSOR('FwbId');
               FETCH L_UC_CURSOR
               INTO L_FWB_UC;
               IF L_UC_CURSOR%NOTFOUND THEN
                  L_FWB_UC := ' ';
               END IF;
               CLOSE L_UC_CURSOR;
            END IF;

            IF NVL(L_FWB_UC, ' ') <> ' ' THEN
               
               L_RETURN := UNAPIUC.CREATENEXTUNIQUECODEVALUE(L_FWB_UC, ' ', ' ', ' ' ,' ', ' ', CURRENT_TIMESTAMP, L_NEXT_VAL, L_EDIT_ALLOWED, L_VALID_CF);
               IF (L_RETURN = 0) AND (NVL(L_NEXT_VAL, ' ') !=' ') THEN
                  L_FWB := SUBSTR(A_IEVALUE, 1, INSTR(A_IEVALUE, '#', -1)-1);
                  L_FWB_VERSION := SUBSTR(A_IEVALUE, INSTR(A_IEVALUE, '#', -1)+1); 
                  
                  EXECUTE IMMEDIATE 'BEGIN :l_return := FWBAPIFWB.COPYFWB(:l_fwb, :l_fwb_version, :l_next_val, :UNVERSION.P_INITIAL_VERSION);END;' USING OUT L_RETURN, L_FWB, L_FWB_VERSION, L_NEXT_VAL, UNVERSION.P_INITIAL_VERSION;
                  IF L_RETURN = UNAPIGEN.DBERR_SUCCESS THEN
                     L_IEVALUE := L_NEXT_VAL||'#'||UNVERSION.P_INITIAL_VERSION;
                  END IF;
               END IF;
            END IF;
         EXCEPTION
            WHEN OTHERS THEN
            NULL;
         END;
      ELSE
         L_IEVALUE := A_IEVALUE;
      END IF;
      A_IIVALUE := L_IEVALUE;

   
   
   
   ELSE
      RETURN(UNAPIGEN.DBERR_DEFVALUETP);
   END IF;


   
   
   
   IF A_DATA_TP = 'A' THEN     
      
      IF SUBSTR(NVL(A_FORMAT,'C'),1,1)='C' THEN
         IF LENGTH(A_FORMAT)>1 THEN
            A_IIVALUE := SUBSTR(L_IEVALUE,1,SUBSTR(A_FORMAT,2));
         ELSE
            A_IIVALUE := L_IEVALUE;
         END IF;
      ELSIF SUBSTR(A_FORMAT,1,1)='D' THEN
         IF UNAPIGEN.P_RNDSUITESESSION = '1' THEN   
            L_RET_CODE := UNAPIGEN.DBERR_SUCCESS;
         ELSE
            L_IIVALUE := L_IEVALUE||'@'||SUBSTR(A_FORMAT,2);
            L_RET_CODE := UNAPIGEN.DATEVALID(L_IIVALUE, L_ERRM);
         END IF;
         IF L_RET_CODE <> UNAPIGEN.DBERR_SUCCESS THEN
            RETURN(L_RET_CODE);
         END IF;         
      ELSE
         L_IEVALUE_F := TO_NUMBER(L_IEVALUE);
         L_IIVALUE := '';
         L_RET_CODE := UNAPIGEN.FORMATRESULT(L_IEVALUE_F, A_FORMAT, L_IIVALUE);
         IF L_RET_CODE <> UNAPIGEN.DBERR_SUCCESS THEN
            RETURN(L_RET_CODE);
         END IF;
         A_IIVALUE := L_IIVALUE;         
      END IF;
   ELSIF A_DATA_TP IN ('D','M') THEN            
      IF UNAPIGEN.P_RNDSUITESESSION = '1' THEN   
         L_RET_CODE := UNAPIGEN.DBERR_SUCCESS;
      ELSE
         L_IIVALUE := L_IEVALUE||'@'||SUBSTR(A_FORMAT,2);
         L_RET_CODE := UNAPIGEN.DATEVALID(L_IIVALUE, L_ERRM);
      END IF;
      IF L_RET_CODE <> UNAPIGEN.DBERR_SUCCESS THEN
         RETURN(L_RET_CODE);
      END IF;         
   ELSIF A_DATA_TP IN ('I','F') THEN            
      L_IEVALUE_F := TO_NUMBER(L_IEVALUE);
      L_IIVALUE := '';
      L_RET_CODE := UNAPIGEN.FORMATRESULT(L_IEVALUE_F, A_FORMAT, L_IIVALUE);
      IF L_RET_CODE <> UNAPIGEN.DBERR_SUCCESS THEN
         RETURN(L_RET_CODE);
      END IF;
      A_IIVALUE := L_IIVALUE;
   END IF;
   RETURN(L_RET_CODE);

EXCEPTION
  WHEN NO_DATA_FOUND THEN
     A_IIVALUE := '';
     RETURN(UNAPIGEN.DBERR_NODEFVALUE);
END FILLRQIIDEFAULTVALUE;

FUNCTION INITRQICATTRIBUTE                            
(A_RQ               IN     VARCHAR2,                  
 A_RT               IN     VARCHAR2,                  
 A_RT_VERSION       IN     VARCHAR2,                  
 A_IP               IN     VARCHAR2,                  
 A_IP_VERSION       IN     VARCHAR2,                  
 A_AU               OUT    UNAPIGEN.VC20_TABLE_TYPE,  
 A_AU_VERSION       OUT    UNAPIGEN.VC20_TABLE_TYPE,  
 A_VALUE            OUT    UNAPIGEN.VC40_TABLE_TYPE,  
 A_DESCRIPTION      OUT    UNAPIGEN.VC40_TABLE_TYPE,  
 A_IS_PROTECTED     OUT    UNAPIGEN.CHAR1_TABLE_TYPE, 
 A_SINGLE_VALUED    OUT    UNAPIGEN.CHAR1_TABLE_TYPE, 
 A_NEW_VAL_ALLOWED  OUT    UNAPIGEN.CHAR1_TABLE_TYPE, 
 A_STORE_DB         OUT    UNAPIGEN.CHAR1_TABLE_TYPE, 
 A_VALUE_LIST_TP    OUT    UNAPIGEN.CHAR1_TABLE_TYPE, 
 A_RUN_MODE         OUT    UNAPIGEN.CHAR1_TABLE_TYPE, 
 A_SERVICE          OUT    UNAPIGEN.VC255_TABLE_TYPE, 
 A_CF_VALUE         OUT    UNAPIGEN.VC20_TABLE_TYPE,  
 A_NR_OF_ROWS       IN OUT NUMBER)                    
RETURN NUMBER IS

L_RT             VARCHAR2(20);
L_RT_VERSION     VARCHAR2(20);

CURSOR L_RTIP_CURSOR(C_RT VARCHAR2, C_RT_VERSION VARCHAR2, C_IP VARCHAR2, C_IP_VERSION VARCHAR2) IS
   SELECT RT
   FROM UTRTIP
   WHERE RT = C_RT
   AND VERSION = C_RT_VERSION
   AND IP = C_IP
   AND UNAPIGEN.VALIDATEVERSION('ip', IP, IP_VERSION) = C_IP_VERSION;
L_RTIP_REC L_RTIP_CURSOR%ROWTYPE;

CURSOR L_RTIPAU_CURSOR (C_RT VARCHAR2, C_RT_VERSION VARCHAR2, C_IP VARCHAR2, C_IP_VERSION VARCHAR2) IS
   
   
   
   
   SELECT A.AU, A.AUSEQ, D.VERSION, A.VALUE, D.DESCRIPTION, D.IS_PROTECTED, D.SINGLE_VALUED,
       D.NEW_VAL_ALLOWED, D.STORE_DB, D.VALUE_LIST_TP,
       D.RUN_MODE, D.SERVICE, D.CF_VALUE
   FROM UTIPAU A, UTIP B, UTRTIP C, UTAU D
   WHERE A.IP = C_IP
     AND A.VERSION = C_IP_VERSION
     AND A.IP = B.IP
     AND A.VERSION = B.VERSION     
     AND A.AU = D.AU
     AND UNAPIGEN.VALIDATEVERSION('au', A.AU, A.AU_VERSION) = D.VERSION     
     AND C.RT = C_RT
     AND C.VERSION = C_RT_VERSION     
     AND C.IP = B.IP
     AND UNAPIGEN.VALIDATEVERSION('ip', C.IP, C.IP_VERSION) = B.VERSION     
     AND DECODE(D.INHERIT_AU,'0',DECODE(C.INHERIT_AU,'2',B.INHERIT_AU,C.INHERIT_AU),D.INHERIT_AU) = '1'
     AND A.AU NOT IN (SELECT DISTINCT J.AU
                      FROM UTRTIPAU J, UTRTIP K, UTIP L, UTAU M
                      WHERE J.IP = L.IP
                        AND UNAPIGEN.VALIDATEVERSION('ip', J.IP, J.IP_VERSION) = L.VERSION                      
                        AND J.RT = C_RT
                        AND J.VERSION = C_RT_VERSION                        
                        AND J.IP = C_IP
                        AND UNAPIGEN.VALIDATEVERSION('ip', J.IP, J.IP_VERSION) = C_IP_VERSION
                        AND J.IP = K.IP
                        AND UNAPIGEN.VALIDATEVERSION('ip', J.IP, J.IP_VERSION) = UNAPIGEN.VALIDATEVERSION('ip', K.IP, K.IP_VERSION)                        
                        AND J.RT = K.RT
                        AND J.VERSION = K.VERSION
                        AND DECODE(M.INHERIT_AU,'0',DECODE(K.INHERIT_AU,'2',L.INHERIT_AU,K.INHERIT_AU),M.INHERIT_AU) = '1'
                        AND M.AU = J.AU
                        AND M.VERSION = UNAPIGEN.VALIDATEVERSION('au', J.AU, J.AU_VERSION))
   
   
   
   
   UNION
   SELECT V.AU, V.AUSEQ+500, Y.VERSION, V.VALUE, Y.DESCRIPTION, Y.IS_PROTECTED, Y.SINGLE_VALUED,
       Y.NEW_VAL_ALLOWED, Y.STORE_DB, Y.VALUE_LIST_TP,
       Y.RUN_MODE, Y.SERVICE, Y.CF_VALUE
   FROM UTRTIPAU V, UTRTIP W, UTIP X, UTAU Y
   WHERE V.IP = X.IP
     AND UNAPIGEN.VALIDATEVERSION('ip', V.IP, V.IP_VERSION) = X.VERSION   
     AND V.RT = C_RT
     AND V.VERSION = C_RT_VERSION
     AND V.IP = C_IP
     AND UNAPIGEN.VALIDATEVERSION('ip', V.IP, V.IP_VERSION) = C_IP_VERSION
     AND V.RT = W.RT
     AND V.VERSION = W.VERSION     
     AND V.IP = W.IP
     AND UNAPIGEN.VALIDATEVERSION('ip', V.IP, V.IP_VERSION) = UNAPIGEN.VALIDATEVERSION('ip', W.IP, W.IP_VERSION)
     AND DECODE(Y.INHERIT_AU,'0',DECODE(W.INHERIT_AU,'2',X.INHERIT_AU,W.INHERIT_AU),Y.INHERIT_AU) = '1' 
     AND V.AU = Y.AU
     AND UNAPIGEN.VALIDATEVERSION('au', V.AU, V.AU_VERSION) = Y.VERSION
   ORDER BY 2;

CURSOR L_IPAU_CURSOR (C_IP VARCHAR2, C_IP_VERSION VARCHAR2) IS
   
   
   
   
   SELECT A.AU, A.AUSEQ, D.VERSION, A.VALUE, D.DESCRIPTION, D.IS_PROTECTED, D.SINGLE_VALUED,
       D.NEW_VAL_ALLOWED, D.STORE_DB, D.VALUE_LIST_TP,
       D.RUN_MODE, D.SERVICE, D.CF_VALUE
   FROM UTIPAU A, UTIP B, UTAU D
   WHERE A.IP = C_IP
     AND A.VERSION = C_IP_VERSION
     AND A.IP = B.IP
     AND A.VERSION = B.VERSION
     AND A.AU = D.AU
     AND UNAPIGEN.VALIDATEVERSION('au', A.AU, A.AU_VERSION) = D.VERSION     
     AND DECODE(D.INHERIT_AU, '0', B.INHERIT_AU, D.INHERIT_AU) = '1'
     ORDER BY 2;

BEGIN

   IF NVL(A_NR_OF_ROWS,0) = 0 THEN
      A_NR_OF_ROWS := UNAPIGEN.P_DEFAULT_CHUNK_SIZE;
   ELSIF A_NR_OF_ROWS < 0 OR A_NR_OF_ROWS > UNAPIGEN.P_MAX_CHUNK_SIZE THEN
      RETURN(UNAPIGEN.DBERR_NROFROWS);
   END IF;

   IF NVL(A_IP, ' ') = ' ' THEN
      RETURN(UNAPIGEN.DBERR_NOOBJID);
   END IF;
   IF NVL(A_IP_VERSION, ' ') = ' ' THEN
      RETURN(UNAPIGEN.DBERR_IPVERSION);
   END IF;
   
   IF NVL(A_RT, ' ') = ' ' THEN
      SELECT RT, RT_VERSION
      INTO L_RT, L_RT_VERSION
      FROM UTRQ
      WHERE RQ = A_RQ;
   ELSE
      L_RT := A_RT;
      IF NVL(A_RT_VERSION, ' ') = ' ' THEN
         RETURN(UNAPIGEN.DBERR_RTVERSION);
      END IF;      
      L_RT_VERSION := A_RT_VERSION;      
   END IF;

   IF L_RT IS NULL AND
      A_RQ IS NULL THEN
      RETURN(UNAPIGEN.DBERR_NOOBJECT);
   END IF;

   IF L_RT IS NOT NULL THEN
      
      
      
      
      OPEN L_RTIP_CURSOR(L_RT, L_RT_VERSION, A_IP, A_IP_VERSION);
      FETCH L_RTIP_CURSOR
      INTO L_RTIP_REC;
      IF L_RTIP_CURSOR%NOTFOUND THEN
         L_RT := NULL;
      END IF;
      CLOSE L_RTIP_CURSOR;
   END IF;
   
   L_FETCHED_ROWS := 0;

   IF L_RT IS NOT NULL THEN   
   
      FOR L_RTIPAU_REC IN L_RTIPAU_CURSOR(L_RT, L_RT_VERSION, A_IP, A_IP_VERSION) LOOP
         L_FETCHED_ROWS := L_FETCHED_ROWS + 1;
         A_AU(L_FETCHED_ROWS) := L_RTIPAU_REC.AU;
         A_AU_VERSION(L_FETCHED_ROWS) := L_RTIPAU_REC.VERSION;
         A_VALUE(L_FETCHED_ROWS) := L_RTIPAU_REC.VALUE;
         A_DESCRIPTION(L_FETCHED_ROWS) := L_RTIPAU_REC.DESCRIPTION;
         A_IS_PROTECTED(L_FETCHED_ROWS) := L_RTIPAU_REC.IS_PROTECTED;
         A_SINGLE_VALUED(L_FETCHED_ROWS) := L_RTIPAU_REC.SINGLE_VALUED;
         A_NEW_VAL_ALLOWED(L_FETCHED_ROWS) := L_RTIPAU_REC.NEW_VAL_ALLOWED;
         A_STORE_DB(L_FETCHED_ROWS) := L_RTIPAU_REC.STORE_DB;
         A_VALUE_LIST_TP(L_FETCHED_ROWS) := L_RTIPAU_REC.VALUE_LIST_TP;
         A_RUN_MODE(L_FETCHED_ROWS) := L_RTIPAU_REC.RUN_MODE;
         A_SERVICE(L_FETCHED_ROWS) := L_RTIPAU_REC.SERVICE;
         A_CF_VALUE(L_FETCHED_ROWS) := L_RTIPAU_REC.CF_VALUE;
         EXIT WHEN L_FETCHED_ROWS >= A_NR_OF_ROWS;
      END LOOP;
      
   ELSE

      FOR L_IPAU_REC IN L_IPAU_CURSOR(A_IP, A_IP_VERSION) LOOP
         L_FETCHED_ROWS := L_FETCHED_ROWS + 1;
         A_AU(L_FETCHED_ROWS) := L_IPAU_REC.AU;
         A_AU_VERSION(L_FETCHED_ROWS) := L_IPAU_REC.VERSION;
         A_VALUE(L_FETCHED_ROWS) := L_IPAU_REC.VALUE;
         A_DESCRIPTION(L_FETCHED_ROWS) := L_IPAU_REC.DESCRIPTION;
         A_IS_PROTECTED(L_FETCHED_ROWS) := L_IPAU_REC.IS_PROTECTED;
         A_SINGLE_VALUED(L_FETCHED_ROWS) := L_IPAU_REC.SINGLE_VALUED;
         A_NEW_VAL_ALLOWED(L_FETCHED_ROWS) := L_IPAU_REC.NEW_VAL_ALLOWED;
         A_STORE_DB(L_FETCHED_ROWS) := L_IPAU_REC.STORE_DB;
         A_VALUE_LIST_TP(L_FETCHED_ROWS) := L_IPAU_REC.VALUE_LIST_TP;
         A_RUN_MODE(L_FETCHED_ROWS) := L_IPAU_REC.RUN_MODE;
         A_SERVICE(L_FETCHED_ROWS) := L_IPAU_REC.SERVICE;
         A_CF_VALUE(L_FETCHED_ROWS) := L_IPAU_REC.CF_VALUE;
         EXIT WHEN L_FETCHED_ROWS >= A_NR_OF_ROWS;
      END LOOP;
   
   END IF;

   IF L_FETCHED_ROWS = 0 THEN
      A_NR_OF_ROWS := 0;
      L_RET_CODE := UNAPIGEN.DBERR_NORECORDS;
   ELSE
      L_RET_CODE := UNAPIGEN.DBERR_SUCCESS;
      A_NR_OF_ROWS := L_FETCHED_ROWS;
   END IF;

   RETURN(L_RET_CODE);

EXCEPTION
WHEN NO_DATA_FOUND THEN
   RETURN(UNAPIGEN.DBERR_NOOBJECT);
WHEN OTHERS THEN
   L_SQLERRM := SQLERRM;
   UNAPIGEN.U4ROLLBACK;
   INSERT INTO UTERROR(CLIENT_ID, APPLIC, WHO, LOGDATE, LOGDATE_TZ, API_NAME, ERROR_MSG)
   VALUES(UNAPIGEN.P_CLIENT_ID, UNAPIGEN.P_APPLIC_NAME, UNAPIGEN.P_USER, CURRENT_TIMESTAMP, CURRENT_TIMESTAMP, 
          'InitRqIcAttribute', L_SQLERRM);
   UNAPIGEN.U4COMMIT;
   RETURN(UNAPIGEN.DBERR_GENFAIL);
END INITRQICATTRIBUTE;

FUNCTION INITRQICDETAILS                              
(A_IP              IN     VARCHAR2,                   
 A_IP_VERSION      IN OUT VARCHAR2,                   
 A_RQ              IN     VARCHAR2,                   
 A_II              OUT    UNAPIGEN.VC20_TABLE_TYPE,   
 A_IE_VERSION      OUT    UNAPIGEN.VC20_TABLE_TYPE,   
 A_IIVALUE         OUT    UNAPIGEN.VC2000_TABLE_TYPE, 
 A_POS_X           OUT    UNAPIGEN.NUM_TABLE_TYPE,    
 A_POS_Y           OUT    UNAPIGEN.NUM_TABLE_TYPE,    
 A_IS_PROTECTED    OUT    UNAPIGEN.CHAR1_TABLE_TYPE,  
 A_MANDATORY       OUT    UNAPIGEN.CHAR1_TABLE_TYPE,  
 A_HIDDEN          OUT    UNAPIGEN.CHAR1_TABLE_TYPE,  
 A_DSP_TITLE       OUT    UNAPIGEN.VC40_TABLE_TYPE,   
 A_DSP_LEN         OUT    UNAPIGEN.NUM_TABLE_TYPE,    
 A_DSP_TP          OUT    UNAPIGEN.CHAR1_TABLE_TYPE,  
 A_DSP_ROWS        OUT    UNAPIGEN.NUM_TABLE_TYPE,    
 A_II_CLASS        OUT    UNAPIGEN.VC2_TABLE_TYPE,    
 A_LOG_HS          OUT    UNAPIGEN.CHAR1_TABLE_TYPE,  
 A_LOG_HS_DETAILS  OUT    UNAPIGEN.CHAR1_TABLE_TYPE,  
 A_LC              OUT    UNAPIGEN.VC2_TABLE_TYPE,    
 A_LC_VERSION      OUT    UNAPIGEN.VC20_TABLE_TYPE,   
 A_NR_OF_ROWS      IN OUT NUMBER,                     
 A_NEXT_ROWS       IN     NUMBER)                     
RETURN NUMBER IS

L_IE_REC           UTIE%ROWTYPE;
L_IPIE_REC         UTIPIE%ROWTYPE;
L_IIVALUE          VARCHAR2(2000);
L_ERRM             VARCHAR2(255);
L_LOG_HS           CHAR(1);
L_LOG_HS_DETAILS   CHAR(1);

CURSOR L_IE_CURSOR(C_IE VARCHAR2, C_IE_VERSION VARCHAR2) IS
   SELECT *
   FROM UTIE
   WHERE IE = C_IE
     AND VERSION = C_IE_VERSION;

CURSOR L_OBJECTS_CURSOR (A_OBJECT_TYPE VARCHAR2) IS
   SELECT LOG_HS, LOG_HS_DETAILS
   FROM UTOBJECTS
   WHERE OBJECT=A_OBJECT_TYPE;

BEGIN

   IF NVL(A_IP, ' ') = ' ' OR
      NVL(A_RQ, ' ') = ' ' THEN
      RETURN(UNAPIGEN.DBERR_NOOBJID);
   END IF;

   IF NVL(A_NEXT_ROWS, 0) NOT IN (-1, 0, 1) THEN
      RETURN(UNAPIGEN.DBERR_NEXTROWS);
   END IF;

   
   IF A_NEXT_ROWS = -1 THEN
      IF P_IPIE_CURSOR%ISOPEN THEN
         CLOSE P_IPIE_CURSOR;
      END IF;
      RETURN (UNAPIGEN.DBERR_SUCCESS);
   END IF;

   
   IF A_NEXT_ROWS = 1 THEN
      IF NOT P_IPIE_CURSOR%ISOPEN THEN
         RETURN(UNAPIGEN.DBERR_NOCURSOR);
      END IF;
   END IF;

   IF NVL(A_NR_OF_ROWS, 0) = 0 THEN
      A_NR_OF_ROWS := UNAPIGEN.P_DEFAULT_CHUNK_SIZE;
   ELSIF A_NR_OF_ROWS < 0 OR A_NR_OF_ROWS > UNAPIGEN.P_MAX_CHUNK_SIZE THEN
      RETURN(UNAPIGEN.DBERR_NROFROWS);
   END IF;

   
   A_IP_VERSION := UNAPIGEN.VALIDATEVERSION('ip', A_IP, A_IP_VERSION);

   
   L_LOG_HS := '0';
   L_LOG_HS_DETAILS := '0';
   OPEN L_OBJECTS_CURSOR('rqii');
   FETCH L_OBJECTS_CURSOR INTO L_LOG_HS, L_LOG_HS_DETAILS;
   CLOSE L_OBJECTS_CURSOR;

   
   
   
   
   IF NVL(A_NEXT_ROWS,0) = 0 THEN
      IF P_IPIE_CURSOR%ISOPEN THEN
         CLOSE P_IPIE_CURSOR;
      END IF;
      OPEN P_IPIE_CURSOR(A_IP, A_IP_VERSION);
   END IF;

   
   
   
   
   FETCH P_IPIE_CURSOR
   INTO L_IPIE_REC;
   L_FETCHED_ROWS := 0;
   
   LOOP

      EXIT WHEN P_IPIE_CURSOR%NOTFOUND OR L_FETCHED_ROWS >= A_NR_OF_ROWS;

      L_ERRM := '';
      L_IIVALUE := '';
      
      
      
      L_IPIE_REC.IE_VERSION := UNAPIGEN.VALIDATEVERSION('ie', L_IPIE_REC.IE, L_IPIE_REC.IE_VERSION);

      OPEN L_IE_CURSOR(L_IPIE_REC.IE, L_IPIE_REC.IE_VERSION);
      FETCH L_IE_CURSOR
      INTO L_IE_REC;
      
      
      
      
      
      
      

      IF L_IE_CURSOR%FOUND THEN
         
         
         
         IF NVL(L_IPIE_REC.IS_PROTECTED, '2') <> '2' THEN
            L_IE_REC.IS_PROTECTED := L_IPIE_REC.IS_PROTECTED;
         END IF;

         IF NVL(L_IPIE_REC.MANDATORY, '2') <> '2' THEN
            L_IE_REC.MANDATORY := L_IPIE_REC.MANDATORY;
         END IF;

         IF NVL(L_IPIE_REC.HIDDEN, '2') <> '2' THEN
            L_IE_REC.HIDDEN := L_IPIE_REC.HIDDEN;
         END IF;

         IF NVL(L_IPIE_REC.DSP_TITLE_USE_IE, '1') <> '1' THEN
            L_IE_REC.DSP_TITLE := L_IPIE_REC.DSP_TITLE;
         END IF;
         IF L_IPIE_REC.DSP_LEN IS NOT NULL THEN
            L_IE_REC.DSP_LEN := L_IPIE_REC.DSP_LEN;
         END IF;
         IF L_IPIE_REC.DSP_ROWS IS NOT NULL THEN
            L_IE_REC.DSP_ROWS := L_IPIE_REC.DSP_ROWS;
         END IF;
         IF NVL(L_IPIE_REC.DSP_TP, ' ') <> ' ' THEN
            L_IE_REC.DSP_TP := L_IPIE_REC.DSP_TP;
         END IF;
          
         
         
         
         IF L_IE_REC.DSP_TP = 'B' THEN  
            IF NVL(L_IPIE_REC.IEVALUE, ' ') <> '2' THEN
               L_IE_REC.DEF_VAL_TP := L_IPIE_REC.DEF_VAL_TP;
               L_IE_REC.DEF_AU_LEVEL := L_IPIE_REC.DEF_AU_LEVEL;
               L_IE_REC.IEVALUE := L_IPIE_REC.IEVALUE;
            END IF;
         ELSE                           
            IF NVL(L_IPIE_REC.DEF_VAL_TP, 'F') <> 'F' OR
               NVL(L_IPIE_REC.IEVALUE, ' ') <> ' ' THEN
               L_IE_REC.DEF_VAL_TP := L_IPIE_REC.DEF_VAL_TP;
               L_IE_REC.DEF_AU_LEVEL := L_IPIE_REC.DEF_AU_LEVEL;
               L_IE_REC.IEVALUE := L_IPIE_REC.IEVALUE;
            END IF;
         END IF;

         BEGIN
            L_RET_CODE := FILLRQIIDEFAULTVALUE(A_RQ, L_IE_REC.DEF_VAL_TP,
                                             L_IE_REC.IEVALUE,
                                             L_IE_REC.DEF_AU_LEVEL, L_IE_REC.DATA_TP,
                                             L_IE_REC.FORMAT, L_IIVALUE);
         EXCEPTION
         WHEN OTHERS THEN
            L_ERRM := 'Warning#rq=' || A_RQ||
                      '#ic='|| A_IP ||
                      '#ii='|| L_IE_REC.IE ||
                      '#def_val_tp='|| L_IE_REC.DEF_VAL_TP ||
                      '#ievalue=' || SUBSTR(L_IE_REC.IEVALUE,1,40) ||
                      '#def_au_level=' || L_IE_REC.DEF_AU_LEVEL ||
                      '#data_tp=' || L_IE_REC.DATA_TP ||
                      '#format=' || L_IE_REC.FORMAT ||
                      '#FillRqiiDefaultValue#SqlCode=' || TO_CHAR(SQLCODE);
         END;

         IF L_RET_CODE = UNAPIGEN.DBERR_NODEFVALUE THEN
            L_IIVALUE := '';
         ELSIF L_RET_CODE <> UNAPIGEN.DBERR_SUCCESS THEN
            L_ERRM := 'Warning#rq=' || A_RQ ||
                      '#ic='|| A_IP ||
                      '#ii='|| L_IE_REC.IE ||
                      '#def_val_tp='|| L_IE_REC.DEF_VAL_TP ||
                      '#ievalue=' || SUBSTR(L_IE_REC.IEVALUE,1,40) ||
                      '#def_au_level=' || L_IE_REC.DEF_AU_LEVEL ||
                      '#data_tp=' || L_IE_REC.DATA_TP ||
                      '#format=' || L_IE_REC.FORMAT ||
                      '#FillRqiiDefaultValue#ErrorCode=' || TO_CHAR(L_RET_CODE);
         END IF;

         IF L_ERRM IS NOT NULL THEN
            INSERT INTO UTERROR(CLIENT_ID, APPLIC, WHO, LOGDATE, LOGDATE_TZ, API_NAME, ERROR_MSG)
            VALUES(UNAPIGEN.P_CLIENT_ID, UNAPIGEN.P_APPLIC_NAME, UNAPIGEN.P_USER, CURRENT_TIMESTAMP, CURRENT_TIMESTAMP, 
                   'CreateRqIcDetails', L_ERRM);
         END IF;

         
         
         
         L_FETCHED_ROWS := L_FETCHED_ROWS + 1;
         A_II(L_FETCHED_ROWS) := L_IPIE_REC.IE;
         A_IE_VERSION(L_FETCHED_ROWS) := L_IPIE_REC.IE_VERSION;      
         A_IIVALUE(L_FETCHED_ROWS) := L_IIVALUE;
         A_POS_X(L_FETCHED_ROWS) := L_IPIE_REC.POS_X;
         A_POS_Y(L_FETCHED_ROWS) := L_IPIE_REC.POS_Y;
         A_IS_PROTECTED(L_FETCHED_ROWS) := L_IE_REC.IS_PROTECTED;
         A_MANDATORY(L_FETCHED_ROWS) := L_IE_REC.MANDATORY;
         A_HIDDEN(L_FETCHED_ROWS) := L_IE_REC.HIDDEN;
         A_DSP_TITLE(L_FETCHED_ROWS) := L_IE_REC.DSP_TITLE;
         A_DSP_LEN(L_FETCHED_ROWS) := L_IE_REC.DSP_LEN;
         A_DSP_TP(L_FETCHED_ROWS) := L_IE_REC.DSP_TP;
         A_DSP_ROWS(L_FETCHED_ROWS) := L_IE_REC.DSP_ROWS;
         A_II_CLASS(L_FETCHED_ROWS) := L_IE_REC.IE_CLASS;
         A_LOG_HS(L_FETCHED_ROWS) := L_LOG_HS;
         A_LOG_HS_DETAILS(L_FETCHED_ROWS) := L_LOG_HS_DETAILS;
         A_LC(L_FETCHED_ROWS) := L_IE_REC.SC_LC;
         A_LC_VERSION(L_FETCHED_ROWS) := L_IE_REC.SC_LC_VERSION;
      
      END IF;
      CLOSE L_IE_CURSOR;

      IF L_FETCHED_ROWS < A_NR_OF_ROWS THEN
         FETCH P_IPIE_CURSOR
         INTO L_IPIE_REC;
      END IF;
   END LOOP;

   
   IF (L_FETCHED_ROWS = 0) THEN
      CLOSE P_IPIE_CURSOR;
      RETURN(UNAPIGEN.DBERR_NORECORDS);
   ELSIF L_FETCHED_ROWS < A_NR_OF_ROWS THEN
      CLOSE P_IPIE_CURSOR;
      A_NR_OF_ROWS := L_FETCHED_ROWS;
   END IF;
   RETURN(UNAPIGEN.DBERR_SUCCESS);

EXCEPTION
   WHEN OTHERS THEN
      L_SQLERRM := SQLERRM;
      UNAPIGEN.U4ROLLBACK;
      INSERT INTO UTERROR(CLIENT_ID, APPLIC, WHO, LOGDATE, LOGDATE_TZ, API_NAME, ERROR_MSG)
      VALUES(UNAPIGEN.P_CLIENT_ID, UNAPIGEN.P_APPLIC_NAME, UNAPIGEN.P_USER, CURRENT_TIMESTAMP, CURRENT_TIMESTAMP, 
             'InitRqIcDetails', L_SQLERRM);
      UNAPIGEN.U4COMMIT;
      IF P_IPIE_CURSOR%ISOPEN THEN
         CLOSE P_IPIE_CURSOR;
      END IF;
      IF L_IE_CURSOR%ISOPEN THEN
         CLOSE L_IE_CURSOR;
      END IF;
      IF L_OBJECTS_CURSOR%ISOPEN THEN
         CLOSE L_OBJECTS_CURSOR;
      END IF;      
      RETURN(UNAPIGEN.DBERR_GENFAIL);
END INITRQICDETAILS;

FUNCTION INITRQINFODETAILS
(A_RT             IN        VARCHAR2,                  
 A_RT_VERSION     IN OUT    VARCHAR2,                 
 A_RQ             IN        VARCHAR2,                  
 A_FILTER_FREQ    IN        CHAR,                      
 A_REF_DATE       IN        DATE,                      
 A_IC             OUT       UNAPIGEN.VC20_TABLE_TYPE,  
 A_IP_VERSION     OUT       UNAPIGEN.VC20_TABLE_TYPE,  
 A_DESCRIPTION    OUT       UNAPIGEN.VC40_TABLE_TYPE,  
 A_WINSIZE_X      OUT       UNAPIGEN.NUM_TABLE_TYPE,   
 A_WINSIZE_Y      OUT       UNAPIGEN.NUM_TABLE_TYPE,   
 A_IS_PROTECTED   OUT       UNAPIGEN.CHAR1_TABLE_TYPE, 
 A_HIDDEN         OUT       UNAPIGEN.CHAR1_TABLE_TYPE, 
 A_MANUALLY_ADDED OUT       UNAPIGEN.CHAR1_TABLE_TYPE, 
 A_NEXT_II        OUT       UNAPIGEN.VC20_TABLE_TYPE,  
 A_IC_CLASS       OUT       UNAPIGEN.VC2_TABLE_TYPE,   
 A_LOG_HS         OUT       UNAPIGEN.CHAR1_TABLE_TYPE, 
 A_LOG_HS_DETAILS OUT       UNAPIGEN.CHAR1_TABLE_TYPE, 
 A_LC             OUT       UNAPIGEN.VC2_TABLE_TYPE,   
 A_LC_VERSION     OUT       UNAPIGEN.VC20_TABLE_TYPE,  
 A_NR_OF_ROWS     IN OUT    NUMBER)                    
RETURN NUMBER IS

L_IC               UNAPIGEN.VC20_TABLE_TYPE;
L_IP_VERSION       UNAPIGEN.VC20_TABLE_TYPE;
L_DESCRIPTION      UNAPIGEN.VC40_TABLE_TYPE;
L_WINSIZE_X        UNAPIGEN.NUM_TABLE_TYPE;
L_WINSIZE_Y        UNAPIGEN.NUM_TABLE_TYPE;
L_IS_PROTECTED     UNAPIGEN.CHAR1_TABLE_TYPE;
L_HIDDEN           UNAPIGEN.CHAR1_TABLE_TYPE;
L_MANUALLY_ADDED   UNAPIGEN.CHAR1_TABLE_TYPE;
L_NEXT_II          UNAPIGEN.VC20_TABLE_TYPE;
L_IC_CLASS         UNAPIGEN.VC2_TABLE_TYPE;
L_LOG_HS           UNAPIGEN.CHAR1_TABLE_TYPE;
L_LOG_HS_DETAILS   UNAPIGEN.CHAR1_TABLE_TYPE;
L_LC               UNAPIGEN.VC2_TABLE_TYPE;
L_LC_VERSION       UNAPIGEN.VC20_TABLE_TYPE;
L_REF_DATE         TIMESTAMP WITH TIME ZONE;
L_NR_OF_ROWS       NUMBER;
L_TOT_NR_ROWS      NUMBER;
L_ASSIGN           BOOLEAN;
L_FILTER_FREQ      CHAR(1);
L_DYN_CURSOR       INTEGER;

CURSOR L_RTIP_CURSOR(C_RT VARCHAR2, C_RT_VERSION VARCHAR2) IS
   SELECT RT, VERSION RT_VERSION,  IP,  IP_VERSION,  SEQ,  IS_PROTECTED,  HIDDEN,
         FREQ_TP,  FREQ_VAL,  FREQ_UNIT,  INVERT_FREQ,  
         LAST_SCHED,  LAST_CNT,  LAST_VAL,  INHERIT_AU
   FROM UTRTIP
   WHERE RT = C_RT
   AND VERSION = C_RT_VERSION
   ORDER BY SEQ;

   
   PROCEDURE ASSIGN(A_IP_TO_ASSIGN IN VARCHAR2) IS

   L_ROW      INTEGER;

   BEGIN
      FOR L_ROW IN 1..L_NR_OF_ROWS LOOP
         
         
         
         A_IC(L_TOT_NR_ROWS + L_ROW) := A_IP_TO_ASSIGN;
         A_IP_VERSION(L_TOT_NR_ROWS + L_ROW) := L_IP_VERSION(L_ROW);
         A_DESCRIPTION(L_TOT_NR_ROWS + L_ROW) := L_DESCRIPTION(L_ROW);
         A_WINSIZE_X(L_TOT_NR_ROWS + L_ROW) := L_WINSIZE_X(L_ROW);
         A_WINSIZE_Y(L_TOT_NR_ROWS + L_ROW) := L_WINSIZE_Y(L_ROW);
         A_IS_PROTECTED(L_TOT_NR_ROWS + L_ROW) := L_IS_PROTECTED(L_ROW);
         A_HIDDEN(L_TOT_NR_ROWS + L_ROW) := L_HIDDEN(L_ROW);
         A_MANUALLY_ADDED(L_TOT_NR_ROWS + L_ROW) := L_MANUALLY_ADDED(L_ROW);
         A_NEXT_II(L_TOT_NR_ROWS + L_ROW) := L_NEXT_II(L_ROW);
         A_IC_CLASS(L_TOT_NR_ROWS + L_ROW) := L_IC_CLASS(L_ROW);
         A_LOG_HS(L_TOT_NR_ROWS + L_ROW) := L_LOG_HS(L_ROW);
         A_LOG_HS_DETAILS(L_TOT_NR_ROWS + L_ROW) := L_LOG_HS_DETAILS(L_ROW);
         A_LC(L_TOT_NR_ROWS + L_ROW) := L_LC(L_ROW);
         A_LC_VERSION(L_TOT_NR_ROWS + L_ROW) := L_LC_VERSION(L_ROW);
      END LOOP;
   END ASSIGN;

BEGIN

   IF UNAPIGEN.BEGINTXN(UNAPIGEN.P_SINGLE_API_TXN) <> UNAPIGEN.DBERR_SUCCESS THEN
      RAISE STPERROR;
   END IF;

   IF NVL(A_NR_OF_ROWS, 0) = 0 THEN
      A_NR_OF_ROWS := UNAPIGEN.P_DEFAULT_CHUNK_SIZE;
   ELSIF A_NR_OF_ROWS < 0 OR A_NR_OF_ROWS > UNAPIGEN.P_MAX_CHUNK_SIZE THEN
      UNAPIGEN.P_TXN_ERROR := UNAPIGEN.DBERR_NROFROWS;
      RAISE STPERROR;
   END IF;

   IF NVL(A_RQ, ' ') = ' ' OR
      NVL(A_RT, ' ') = ' ' OR
      NVL(A_RT_VERSION, ' ') = ' ' THEN
      UNAPIGEN.P_TXN_ERROR := UNAPIGEN.DBERR_NOOBJID;
      RAISE STPERROR;
   END IF;

   
   
   
   L_FILTER_FREQ := NVL(A_FILTER_FREQ, '1');
   L_REF_DATE := NVL(A_REF_DATE, CURRENT_TIMESTAMP);
   L_NR_OF_ROWS := 0;
   L_TOT_NR_ROWS := 0;

   
   A_RT_VERSION := UNAPIGEN.VALIDATEVERSION('rt', A_RT, A_RT_VERSION);

   
   
   
   FOR L_RTIP_REC IN L_RTIP_CURSOR(A_RT, A_RT_VERSION) LOOP

      L_RTIP_REC.IP_VERSION := UNAPIGEN.VALIDATEVERSION('ip', L_RTIP_REC.IP, L_RTIP_REC.IP_VERSION);
      L_ASSIGN := FALSE;
      
      
      
      IF L_FILTER_FREQ = '0' THEN
         L_ASSIGN := TRUE;
      ELSIF L_FILTER_FREQ = '1' THEN
         L_ASSIGN := TRUE;
         IF L_RTIP_REC.FREQ_TP = 'C' THEN
            
            
            
            L_SQL_STRING := 'BEGIN :l_ret_code := UNFREQ.'|| L_RTIP_REC.FREQ_UNIT ||
                '(:a_rq, :a_rt, :a_rt_version, :a_ip, :a_ip_version, :a_freq_val, :a_invert_freq, :a_ref_date, ' ||
                ':a_last_sched, :a_last_cnt, :a_last_val); END;';

            L_DYN_CURSOR := DBMS_SQL.OPEN_CURSOR;

            DBMS_SQL.PARSE(L_DYN_CURSOR, L_SQL_STRING, DBMS_SQL.V7); 
            DBMS_SQL.BIND_VARIABLE(L_DYN_CURSOR, ':l_ret_code', L_RET_CODE);
            DBMS_SQL.BIND_VARIABLE(L_DYN_CURSOR, ':a_rq', A_RQ, 20);
            DBMS_SQL.BIND_VARIABLE(L_DYN_CURSOR, ':a_rt', A_RT, 20);
            DBMS_SQL.BIND_VARIABLE(L_DYN_CURSOR, ':a_rt_version', A_RT_VERSION, 20);
            DBMS_SQL.BIND_VARIABLE(L_DYN_CURSOR, ':a_ip', L_RTIP_REC.IP, 20);
            DBMS_SQL.BIND_VARIABLE(L_DYN_CURSOR, ':a_ip_version', L_RTIP_REC.IP_VERSION, 20);
            DBMS_SQL.BIND_VARIABLE(L_DYN_CURSOR, ':a_freq_val', L_RTIP_REC.FREQ_VAL);
            DBMS_SQL.BIND_VARIABLE_CHAR(L_DYN_CURSOR, ':a_invert_freq', L_RTIP_REC.INVERT_FREQ, 1);
            DBMS_SQL.BIND_VARIABLE(L_DYN_CURSOR, ':a_ref_date', L_REF_DATE);
            DBMS_SQL.BIND_VARIABLE(L_DYN_CURSOR, ':a_last_sched', L_RTIP_REC.LAST_SCHED);
            DBMS_SQL.BIND_VARIABLE(L_DYN_CURSOR, ':a_last_cnt', L_RTIP_REC.LAST_CNT);
            DBMS_SQL.BIND_VARIABLE(L_DYN_CURSOR, ':a_last_val', L_RTIP_REC.LAST_VAL, 40);

            L_RESULT := DBMS_SQL.EXECUTE(L_DYN_CURSOR);
            DBMS_SQL.VARIABLE_VALUE(L_DYN_CURSOR, ':l_ret_code', L_RET_CODE);
            DBMS_SQL.VARIABLE_VALUE(L_DYN_CURSOR, ':a_last_sched', L_RTIP_REC.LAST_SCHED);
            DBMS_SQL.VARIABLE_VALUE(L_DYN_CURSOR, ':a_last_cnt', L_RTIP_REC.LAST_CNT);
            DBMS_SQL.VARIABLE_VALUE(L_DYN_CURSOR, ':a_last_val', L_RTIP_REC.LAST_VAL);
               
            DBMS_SQL.CLOSE_CURSOR(L_DYN_CURSOR);
   
            IF L_RET_CODE <> UNAPIGEN.DBERR_SUCCESS THEN
               L_ASSIGN := FALSE;
            END IF;
         ELSE
            IF NOT UNAPIAUT.EVALASSIGNMENTFREQ('rt', A_RT, A_RT_VERSION, 
                              'ic', L_RTIP_REC.IP, L_RTIP_REC.IP_VERSION,
                                               L_RTIP_REC.FREQ_TP,
                                               L_RTIP_REC.FREQ_VAL,
                                               L_RTIP_REC.FREQ_UNIT,
                                               L_RTIP_REC.INVERT_FREQ,
                                               L_REF_DATE,
                                               L_RTIP_REC.LAST_SCHED,
                                               L_RTIP_REC.LAST_CNT,
                                               L_RTIP_REC.LAST_VAL) THEN
               L_ASSIGN := FALSE;
            END IF;
         END IF;
         
         
         
         
         UPDATE UTRTIP
         SET LAST_SCHED = L_RTIP_REC.LAST_SCHED,
        LAST_SCHED_TZ =  DECODE(L_RTIP_REC.LAST_SCHED, LAST_SCHED_TZ, LAST_SCHED_TZ, L_RTIP_REC.LAST_SCHED),
             LAST_CNT = L_RTIP_REC.LAST_CNT,
             LAST_VAL = L_RTIP_REC.LAST_VAL
         WHERE RT = L_RTIP_REC.RT
           AND VERSION = L_RTIP_REC.RT_VERSION
           AND IP = L_RTIP_REC.IP
           AND UNAPIGEN.VALIDATEVERSION('ip',IP,IP_VERSION) = UNAPIGEN.VALIDATEVERSION('ip',L_RTIP_REC.IP,L_RTIP_REC.IP_VERSION)
           AND SEQ = L_RTIP_REC.SEQ;
      END IF;

      IF L_ASSIGN THEN
         
         
         
         L_NR_OF_ROWS := 0;
         L_RET_CODE := UNAPIRQIC.INITRQINFOCARD(L_RTIP_REC.IP, L_RTIP_REC.IP_VERSION,
                                   L_RTIP_REC.SEQ, A_RT, A_RT_VERSION, A_RQ,
                                   L_IP_VERSION, L_DESCRIPTION, L_WINSIZE_X, L_WINSIZE_Y,
                                   L_IS_PROTECTED, L_HIDDEN, L_MANUALLY_ADDED,
                                   L_NEXT_II, L_IC_CLASS, L_LOG_HS, L_LOG_HS_DETAILS, L_LC, L_LC_VERSION, L_NR_OF_ROWS);
         IF L_RET_CODE NOT IN (UNAPIGEN.DBERR_SUCCESS, UNAPIGEN.DBERR_NORECORDS) THEN
            
            UNAPIGEN.P_TXN_ERROR := L_RET_CODE;
            RAISE STPERROR;
         END IF;

        
        
        
        IF L_NR_OF_ROWS > 0 THEN
           ASSIGN(L_RTIP_REC.IP);
           L_TOT_NR_ROWS := L_TOT_NR_ROWS + L_NR_OF_ROWS;
        END IF;
      END IF;
   END LOOP;

   IF UNAPIGEN.ENDTXN <> UNAPIGEN.DBERR_SUCCESS THEN
      RAISE STPERROR;
   END IF;

   
   
   
   IF L_TOT_NR_ROWS > A_NR_OF_ROWS THEN
      INSERT INTO UTERROR(CLIENT_ID, APPLIC, WHO, LOGDATE, LOGDATE_TZ, API_NAME, ERROR_MSG)
      VALUES(UNAPIGEN.P_CLIENT_ID, UNAPIGEN.P_APPLIC_NAME, UNAPIGEN.P_USER, CURRENT_TIMESTAMP, CURRENT_TIMESTAMP, 
             'InitRqInfoDetails','a_nr_of_rows ('||A_NR_OF_ROWS||
             ') too small for required InfoCard initialisation');
   END IF;

   A_NR_OF_ROWS := L_TOT_NR_ROWS;
   RETURN(UNAPIGEN.DBERR_SUCCESS);

EXCEPTION
WHEN OTHERS THEN
   IF SQLCODE <> 1 THEN
      UNAPIGEN.LOGERROR('InitRqInfoDetails', SQLERRM);
   ELSIF L_SQLERRM IS NOT NULL THEN
      UNAPIGEN.LOGERROR('InitRqInfoDetails', L_SQLERRM);
   END IF;
   IF DBMS_SQL.IS_OPEN(L_DYN_CURSOR) THEN
      DBMS_SQL.CLOSE_CURSOR(L_DYN_CURSOR);
   END IF;   
   RETURN(UNAPIGEN.ABORTTXN(UNAPIGEN.P_TXN_ERROR, 'InitRqInfoDetails'));
END INITRQINFODETAILS;

FUNCTION COPYRQINFODETAILS
(A_RQ_FROM        IN        VARCHAR2,                 
 A_RT_TO          IN        VARCHAR2,                 
 A_RT_TO_VERSION  IN        VARCHAR2,                 
 A_RQ_TO          IN        VARCHAR2,                 
 A_MODIFY_REASON  IN        VARCHAR2)                 
RETURN NUMBER IS

L_ERRM               VARCHAR2(255);
L_NR_OF_ROWS         NUMBER;
L_RQ                 UNAPIGEN.VC20_TABLE_TYPE;
L_IC                 UNAPIGEN.VC20_TABLE_TYPE;
L_ICNODE             UNAPIGEN.LONG_TABLE_TYPE;
L_IP_VERSION         UNAPIGEN.VC20_TABLE_TYPE;
L_DESCRIPTION        UNAPIGEN.VC40_TABLE_TYPE;
L_WINSIZE_X          UNAPIGEN.NUM_TABLE_TYPE;
L_WINSIZE_Y          UNAPIGEN.NUM_TABLE_TYPE;
L_IS_PROTECTED       UNAPIGEN.CHAR1_TABLE_TYPE;
L_HIDDEN             UNAPIGEN.CHAR1_TABLE_TYPE;
L_MANUALLY_ADDED     UNAPIGEN.CHAR1_TABLE_TYPE;
L_NEXT_II            UNAPIGEN.VC20_TABLE_TYPE;
L_IC_CLASS           UNAPIGEN.VC2_TABLE_TYPE;
L_LOG_HS             UNAPIGEN.CHAR1_TABLE_TYPE;
L_LOG_HS_DETAILS     UNAPIGEN.CHAR1_TABLE_TYPE;
L_LC                 UNAPIGEN.VC2_TABLE_TYPE;
L_LC_VERSION         UNAPIGEN.VC20_TABLE_TYPE;
L_MODIFY_FLAG        UNAPIGEN.NUM_TABLE_TYPE;
L_LOG_HS_RQ          CHAR(1);
L_LOG_HS_DETAILS_RQ  CHAR(1);
L_HS_DETAILS_SEQ_NR  INTEGER;

CURSOR L_RQIC_CURSOR(A_RQ VARCHAR2) IS
   SELECT *
   FROM UTRQIC
   WHERE RQ = A_RQ
   ORDER BY ICNODE;

BEGIN

   IF UNAPIGEN.BEGINTXN(UNAPIGEN.P_SINGLE_API_TXN) <> UNAPIGEN.DBERR_SUCCESS THEN
      RAISE STPERROR;
   END IF;

   IF (NVL(A_RQ_FROM, ' ') = ' ') OR (NVL(A_RQ_TO, ' ') = ' ') THEN
      UNAPIGEN.P_TXN_ERROR := UNAPIGEN.DBERR_NOOBJID;
      RAISE STPERROR;
   END IF;

   L_ERRM := NULL;

   
   L_NR_OF_ROWS := 0;
   FOR L_RQIC_REC IN L_RQIC_CURSOR(A_RQ_FROM) LOOP
      L_NR_OF_ROWS := L_NR_OF_ROWS + 1;
      L_RQ(L_NR_OF_ROWS)             := A_RQ_TO;
      L_IC(L_NR_OF_ROWS)             := L_RQIC_REC.IC;
      L_ICNODE(L_NR_OF_ROWS)         := L_RQIC_REC.ICNODE;
      L_IP_VERSION(L_NR_OF_ROWS)     := L_RQIC_REC.IP_VERSION;
      L_DESCRIPTION(L_NR_OF_ROWS)    := L_RQIC_REC.DESCRIPTION;
      L_WINSIZE_X(L_NR_OF_ROWS)      := L_RQIC_REC.WINSIZE_X;
      L_WINSIZE_Y(L_NR_OF_ROWS)      := L_RQIC_REC.WINSIZE_Y;
      L_IS_PROTECTED(L_NR_OF_ROWS)   := L_RQIC_REC.IS_PROTECTED;
      L_HIDDEN(L_NR_OF_ROWS)         := L_RQIC_REC.HIDDEN;
      L_NEXT_II(L_NR_OF_ROWS)        := L_RQIC_REC.NEXT_II;
      L_IC_CLASS(L_NR_OF_ROWS)       := L_RQIC_REC.IC_CLASS;
      L_LOG_HS(L_NR_OF_ROWS)         := L_RQIC_REC.LOG_HS;
      L_LOG_HS_DETAILS(L_NR_OF_ROWS) := L_RQIC_REC.LOG_HS_DETAILS;      
      L_LC(L_NR_OF_ROWS)             := L_RQIC_REC.LC;
      L_LC_VERSION(L_NR_OF_ROWS)     := L_RQIC_REC.LC_VERSION;
      
      L_MANUALLY_ADDED(L_NR_OF_ROWS) := '0';
      L_MODIFY_FLAG(L_NR_OF_ROWS)    := UNAPIGEN.MOD_FLAG_INSERT_WITH_NODES;
   END LOOP;
   
   
   IF L_NR_OF_ROWS > 0 THEN
      L_RET_CODE := UNAPIRQIC.SAVERQINFOCARD(L_RQ, L_IC, L_ICNODE, L_IP_VERSION, L_DESCRIPTION,
                                             L_WINSIZE_X, L_WINSIZE_Y, L_IS_PROTECTED,
                                             L_HIDDEN, L_MANUALLY_ADDED, L_NEXT_II, 
                                             L_IC_CLASS, L_LOG_HS, L_LOG_HS_DETAILS, L_LC, L_LC_VERSION,
                                             L_MODIFY_FLAG, L_NR_OF_ROWS, A_MODIFY_REASON);
      IF L_RET_CODE = UNAPIGEN.DBERR_PARTIALSAVE THEN
         
         FOR L_ROW IN 1..L_NR_OF_ROWS LOOP
            IF L_MODIFY_FLAG(L_ROW) > UNAPIGEN.DBERR_SUCCESS THEN
               UNAPIGEN.P_TXN_ERROR := L_MODIFY_FLAG(L_ROW);
               L_ERRM := 'rq=' || L_RQ(L_ROW) ||
                         '#ic=' || L_IC(L_ROW) ||
                         '#icnode=' || TO_CHAR(L_ICNODE(L_ROW)) ||
                         '#SaveRqInfoCard#ErrorCode='||TO_CHAR(L_RET_CODE);
               RAISE STPERROR;
            END IF;
         END LOOP;
      ELSIF L_RET_CODE <> UNAPIGEN.DBERR_SUCCESS THEN
         UNAPIGEN.P_TXN_ERROR := L_RET_CODE;
         RAISE STPERROR;
      END IF;
   END IF;

   
   INSERT INTO UTRQICAU(RQ, IC, ICNODE, AU, AUSEQ, VALUE)
   SELECT A_RQ_TO, IC, ICNODE, AU, AUSEQ, VALUE
   FROM UTRQICAU
   WHERE RQ = A_RQ_FROM;
   
   
   FOR L_ROW IN 1..L_NR_OF_ROWS LOOP
      
      
      
      L_RET_CODE := UNAPIRQIC.COPYRQICDETAILS(A_RQ_FROM, L_IC(L_ROW), L_ICNODE(L_ROW), A_RT_TO, 
                                              A_RT_TO_VERSION, L_RQ(L_ROW), L_IC(L_ROW), 
                                              L_ICNODE(L_ROW), A_MODIFY_REASON);
      
      IF L_RET_CODE <> UNAPIGEN.DBERR_SUCCESS THEN
         L_ERRM := 'rq_from=' || A_RQ_FROM|| 
                   '#ic_from=' || L_IC(L_ROW) ||
                   '#icnode_from=' || TO_CHAR(L_ICNODE(L_ROW)) ||
                   '#row='|| TO_CHAR(L_ROW) ||
                   '#rq_to='|| L_RQ(L_ROW) ||
                   '#ic=' || L_IC(L_ROW) ||
                   '#icnode=' || TO_CHAR(L_ICNODE(L_ROW)) ||
                   '#CopyRqIcDetails#ErrorCode=' || TO_CHAR(L_RET_CODE);
         UNAPIGEN.P_TXN_ERROR := L_RET_CODE;
         RAISE STPERROR;
      END IF;
   END LOOP;

   
   L_EV_SEQ_NR := -1;
   L_EV_DETAILS := 'rt='||A_RT_TO||'#rt_version='||A_RT_TO_VERSION;
   L_EVENT_TP := 'RqInfoCreated';
   L_RESULT := UNAPIEV.INSERTEVENT('CopyRqInfoDetails', UNAPIGEN.P_EVMGR_NAME, 'rq', A_RQ_TO, 
                                   '', '', '', L_EVENT_TP, L_EV_DETAILS, L_EV_SEQ_NR);
   IF L_RESULT <> UNAPIGEN.DBERR_SUCCESS THEN
      UNAPIGEN.P_TXN_ERROR := L_RESULT;
      RAISE STPERROR;
   END IF;

   BEGIN
      SELECT LOG_HS, LOG_HS_DETAILS
      INTO L_LOG_HS_RQ, L_LOG_HS_DETAILS_RQ
      FROM UTRQ
      WHERE RQ = A_RQ_TO;
   EXCEPTION
   WHEN NO_DATA_FOUND THEN
      UNAPIGEN.P_TXN_ERROR := UNAPIGEN.DBERR_NOOBJECT;
      RAISE STPERROR;
   END;

   IF L_LOG_HS_RQ = '1' THEN
      INSERT INTO UTRQHS(RQ, WHO, WHO_DESCRIPTION, WHAT, 
                         WHAT_DESCRIPTION, LOGDATE, LOGDATE_TZ, WHY, TR_SEQ, EV_SEQ)
      VALUES(A_RQ_TO, UNAPIGEN.P_USER, UNAPIGEN.P_USER_DESCRIPTION, L_EVENT_TP, 
             'Info cards for request "'||A_RQ_TO||'" are created',
               CURRENT_TIMESTAMP, CURRENT_TIMESTAMP, A_MODIFY_REASON, UNAPIGEN.P_TR_SEQ, L_EV_SEQ_NR);
   END IF;

   L_HS_DETAILS_SEQ_NR := 0;
   IF L_LOG_HS_DETAILS_RQ = '1' THEN
      L_HS_DETAILS_SEQ_NR := L_HS_DETAILS_SEQ_NR + 1;
      INSERT INTO UTRQHSDETAILS(RQ, TR_SEQ, EV_SEQ, SEQ, DETAILS)
      VALUES(A_RQ_TO, UNAPIGEN.P_TR_SEQ, L_EV_SEQ_NR, L_HS_DETAILS_SEQ_NR, 
             'Info cards for request "'||A_RQ_TO||'" are created');
   END IF;

   IF UNAPIGEN.ENDTXN <> UNAPIGEN.DBERR_SUCCESS THEN
      RAISE STPERROR;
   END IF;

   RETURN(UNAPIGEN.DBERR_SUCCESS);

EXCEPTION
WHEN OTHERS THEN
   IF SQLCODE <> 1 THEN
      UNAPIGEN.LOGERROR('CopyRqInfoDetails', SQLERRM);
   ELSIF L_ERRM IS NOT NULL THEN
      UNAPIGEN.LOGERROR('CopyRqInfoDetails', L_ERRM);
   END IF;
   RETURN(UNAPIGEN.ABORTTXN(UNAPIGEN.P_TXN_ERROR,'CopyRqInfoDetails'));
END COPYRQINFODETAILS;

FUNCTION COPYRQICDETAILS
(A_RQ_FROM        IN        VARCHAR2,                 
 A_IC_FROM        IN        VARCHAR2,                 
 A_ICNODE_FROM    IN        NUMBER,                   
 A_RT_TO          IN        VARCHAR2,                 
 A_RT_TO_VERSION  IN        VARCHAR2,                 
 A_RQ_TO          IN        VARCHAR2,                 
 A_IC_TO          IN        VARCHAR2,                 
 A_ICNODE_TO      IN        NUMBER,                   
 A_MODIFY_REASON  IN        VARCHAR2)                 
RETURN NUMBER IS

L_IP_VERSION         VARCHAR2(20);
L_HS_DETAILS_SEQ_NR  INTEGER;
L_IIVALUE            VARCHAR2(2000);
L_ERRM               VARCHAR2(255);
L_NEXT_II            VARCHAR2(20);

CURSOR L_IP_VERSION_CURSOR(C_RQ VARCHAR2, C_IC VARCHAR2, C_ICNODE NUMBER) IS
   SELECT IP_VERSION
     FROM UTRQIC
    WHERE RQ = C_RQ
      AND IC = C_IC
      AND ICNODE = C_ICNODE;

CURSOR L_RQII_CURSOR(C_RQ VARCHAR2, C_IC VARCHAR2, C_ICNODE NUMBER) IS
   SELECT *
     FROM UTRQII
    WHERE RQ = C_RQ
      AND IC = C_IC
      AND ICNODE = C_ICNODE
    ORDER BY IINODE;

CURSOR L_IPIE_CURSOR(C_IP VARCHAR2, C_IP_VERSION VARCHAR2, C_IE VARCHAR2, C_IE_VERSION VARCHAR2) IS
   SELECT *
     FROM UTIPIE
    WHERE IP = C_IP
      AND VERSION = C_IP_VERSION
      AND IE = C_IE
      AND UNAPIGEN.VALIDATEVERSION('ie',IE,IE_VERSION) = C_IE_VERSION;
L_IPIE_REC     L_IPIE_CURSOR%ROWTYPE;
L_IPIE_FOUND   BOOLEAN;

CURSOR L_IE_CURSOR(C_IE VARCHAR2, C_IE_VERSION VARCHAR2) IS
   SELECT *
     FROM UTIE
    WHERE IE = C_IE
      AND VERSION = C_IE_VERSION;
L_IE_REC     L_IE_CURSOR%ROWTYPE;
L_IE_FOUND   BOOLEAN;

CURSOR L_NEXT_II_CURSOR(C_RQ VARCHAR2, C_IC VARCHAR2, C_ICNODE NUMBER) IS
   SELECT II
     FROM UTRQII
    WHERE RQ = C_RQ
      AND IC = C_IC
      AND ICNODE = C_ICNODE
      AND IIVALUE IS NULL
    ORDER BY IINODE;

BEGIN

   IF UNAPIGEN.BEGINTXN(UNAPIGEN.P_SINGLE_API_TXN) <> UNAPIGEN.DBERR_SUCCESS THEN
      RAISE STPERROR;
   END IF;

   IF NVL(A_RQ_FROM, ' ') = ' ' OR
      NVL(A_IC_FROM, ' ') = ' ' OR
      NVL(A_ICNODE_FROM, 0) = 0 OR
      NVL(A_RQ_TO, ' ') = ' ' OR
      NVL(A_IC_TO, ' ') = ' ' OR
      NVL(A_ICNODE_TO, 0) = 0 THEN
      UNAPIGEN.P_TXN_ERROR := UNAPIGEN.DBERR_NOOBJID;
      RAISE STPERROR;
   END IF;

   OPEN L_IP_VERSION_CURSOR(A_RQ_FROM, A_IC_FROM, A_ICNODE_FROM);
   FETCH L_IP_VERSION_CURSOR INTO L_IP_VERSION;
   CLOSE L_IP_VERSION_CURSOR;

   L_HS_DETAILS_SEQ_NR := 0;
   FOR L_RQII_REC IN L_RQII_CURSOR(A_RQ_FROM, A_IC_FROM, A_ICNODE_FROM) LOOP
      
      L_IIVALUE := '';
      L_ERRM := '';

      
      
      OPEN L_IPIE_CURSOR(A_IC_TO, L_IP_VERSION, L_RQII_REC.II, L_RQII_REC.IE_VERSION);
      FETCH L_IPIE_CURSOR
      INTO L_IPIE_REC;
      L_IPIE_FOUND := L_IPIE_CURSOR%FOUND;
      CLOSE L_IPIE_CURSOR;

      OPEN L_IE_CURSOR(L_RQII_REC.II, L_RQII_REC.IE_VERSION);
      FETCH L_IE_CURSOR
       INTO L_IE_REC;
      L_IE_FOUND := L_IE_CURSOR%FOUND;
      CLOSE L_IE_CURSOR;

      
      IF L_IPIE_FOUND THEN
         IF NVL(L_IPIE_REC.DEF_VAL_TP, 'F') <> 'F' OR
            NVL(L_IPIE_REC.IEVALUE, ' ') <> ' ' THEN
            L_IE_REC.DEF_VAL_TP := L_IPIE_REC.DEF_VAL_TP;
            L_IE_REC.DEF_AU_LEVEL := L_IPIE_REC.DEF_AU_LEVEL;
            L_IE_REC.IEVALUE := L_IPIE_REC.IEVALUE;
         END IF;
      END IF;
      
      IF L_IE_FOUND THEN
         BEGIN
            L_RET_CODE := UNAPIRQIC.FILLRQIIDEFAULTVALUE(A_RQ_TO, L_IE_REC.DEF_VAL_TP,
                             L_IE_REC.IEVALUE, L_IE_REC.DEF_AU_LEVEL, L_IE_REC.DATA_TP,
                             L_IE_REC.FORMAT, L_IIVALUE);
         EXCEPTION
         WHEN OTHERS THEN
            L_ERRM := 'Warning#rq=' || A_RQ_TO ||
                      '#ic='|| A_IC_TO ||
                      '#ii='|| L_IE_REC.IE ||
                      '#def_val_tp='|| L_IE_REC.DEF_VAL_TP ||
                      '#ievalue=' || SUBSTR(L_IE_REC.IEVALUE,1,40) ||
                      '#def_au_level=' || L_IE_REC.DEF_AU_LEVEL ||
                      '#data_tp=' || L_IE_REC.DATA_TP ||
                      '#format=' || L_IE_REC.FORMAT ||
                      '#FillRqiiDefaultValue#SqlCode=' || TO_CHAR(SQLCODE);
         END;

         IF L_RET_CODE = UNAPIGEN.DBERR_NODEFVALUE THEN
            L_IIVALUE := '';
         ELSIF L_RET_CODE <> UNAPIGEN.DBERR_SUCCESS AND L_ERRM IS NULL THEN
            L_ERRM := 'Warning#rq=' || A_RQ_TO ||
                      '#ic='|| A_IC_TO ||
                      '#ii='|| L_IE_REC.IE ||
                      '#def_val_tp='|| L_IE_REC.DEF_VAL_TP ||
                      '#ievalue=' || SUBSTR(L_IE_REC.IEVALUE,1,40) ||
                      '#def_au_level=' || L_IE_REC.DEF_AU_LEVEL ||
                      '#data_tp=' || L_IE_REC.DATA_TP ||
                      '#format=' || L_IE_REC.FORMAT ||
                      '#FillRqiiDefaultValue#ErrorCode=' || TO_CHAR(L_RET_CODE);
         END IF;
         IF L_ERRM IS NOT NULL THEN
            INSERT INTO UTERROR(CLIENT_ID, APPLIC, WHO, LOGDATE, LOGDATE_TZ, API_NAME, ERROR_MSG)
            VALUES(UNAPIGEN.P_CLIENT_ID, UNAPIGEN.P_APPLIC_NAME, UNAPIGEN.P_USER, CURRENT_TIMESTAMP, CURRENT_TIMESTAMP, 
                   'CopyRqIcDetails', L_ERRM);
         END IF;
      END IF;
      
      INSERT INTO UTRQII(RQ, IC, ICNODE, II, IINODE, IE_VERSION, IIVALUE, POS_X, POS_Y, 
                         IS_PROTECTED, MANDATORY, HIDDEN, DSP_TITLE, DSP_LEN, DSP_TP,
                         DSP_ROWS, LOG_HS, LOG_HS_DETAILS, ALLOW_MODIFY, ACTIVE, LC, LC_VERSION, SS)
      VALUES(A_RQ_TO, A_IC_TO, A_ICNODE_TO, L_RQII_REC.II, L_RQII_REC.IINODE, L_RQII_REC.IE_VERSION,
             L_IIVALUE, L_RQII_REC.POS_X, L_RQII_REC.POS_Y, L_RQII_REC.IS_PROTECTED, 
             L_RQII_REC.MANDATORY, L_RQII_REC.HIDDEN, L_RQII_REC.DSP_TITLE, L_RQII_REC.DSP_LEN, 
             L_RQII_REC.DSP_TP, L_RQII_REC.DSP_ROWS, L_RQII_REC.LOG_HS, L_RQII_REC.LOG_HS_DETAILS, 
             '1', '0', L_RQII_REC.LC, L_RQII_REC.LC_VERSION, '');

      L_EV_SEQ_NR := -1;
      L_EV_DETAILS := 'rq=' || A_RQ_TO || 
                      '#ic=' || A_IC_TO ||
                      '#icnode=' || TO_CHAR(A_ICNODE_TO) ||
                      '#iinode=' || TO_CHAR(L_RQII_REC.IINODE) ||
                      '#ie_version=' || L_RQII_REC.IE_VERSION;
      IF L_IIVALUE IS NOT NULL THEN
         L_EV_DETAILS := L_EV_DETAILS || '#new_value=' || SUBSTR(L_IIVALUE,1,40);
      END IF;                      
      L_EVENT_TP := 'RqInfoFieldCreated';
      L_RESULT := UNAPIEV.INSERTINFOFIELDEVENT('CopyRqIcDetails', UNAPIGEN.P_EVMGR_NAME, 'rqii', 
                                               L_RQII_REC.II, L_RQII_REC.LC, 
                                               L_RQII_REC.LC_VERSION, '', L_EVENT_TP, 
                                               L_EV_DETAILS, L_EV_SEQ_NR);
      IF L_RESULT <> UNAPIGEN.DBERR_SUCCESS THEN
         UNAPIGEN.P_TXN_ERROR := L_RESULT;
         RAISE STPERROR;
      END IF;

      
      IF L_EV_SEQ_NR = -1 THEN
         
         
         OPEN L_SEQ_EVENT_CURSOR;
         FETCH L_SEQ_EVENT_CURSOR INTO L_EV_SEQ_NR;
         CLOSE L_SEQ_EVENT_CURSOR;
      END IF;

      IF L_RQII_REC.LOG_HS = '1' THEN
         INSERT INTO UTRQICHS(RQ, IC, ICNODE, WHO, WHO_DESCRIPTION, WHAT, 
                              WHAT_DESCRIPTION, LOGDATE, LOGDATE_TZ, WHY, TR_SEQ, EV_SEQ)
         VALUES(A_RQ_TO, A_IC_TO, A_ICNODE_TO, UNAPIGEN.P_USER, UNAPIGEN.P_USER_DESCRIPTION, L_EVENT_TP, 
                'Info fields for info card "'||A_IC_TO||'" are created',
                CURRENT_TIMESTAMP, CURRENT_TIMESTAMP, A_MODIFY_REASON, UNAPIGEN.P_TR_SEQ, L_EV_SEQ_NR);
      END IF;

      IF L_RQII_REC.LOG_HS_DETAILS = '1' THEN
         L_HS_DETAILS_SEQ_NR := L_HS_DETAILS_SEQ_NR + 1;
         INSERT INTO UTRQICHSDETAILS(RQ, IC, ICNODE, TR_SEQ, EV_SEQ, SEQ, DETAILS)
         VALUES  (A_RQ_TO, A_IC_TO, A_ICNODE_TO, UNAPIGEN.P_TR_SEQ, L_EV_SEQ_NR, L_HS_DETAILS_SEQ_NR, 
                  'Info fields for info card "'||A_IC_TO||'" are created');
      END IF;
   END LOOP;

   
   OPEN L_NEXT_II_CURSOR(A_RQ_TO, A_IC_TO, A_ICNODE_TO);
   FETCH L_NEXT_II_CURSOR
    INTO L_NEXT_II;
   IF L_NEXT_II_CURSOR%NOTFOUND THEN
      L_NEXT_II := NULL;
   END IF;

   
   UPDATE UTRQIC
   SET NEXT_II = L_NEXT_II
   WHERE RQ = A_RQ_TO
     AND IC = A_IC_TO
     AND ICNODE = A_ICNODE_TO;

   IF UNAPIGEN.ENDTXN <> UNAPIGEN.DBERR_SUCCESS THEN
      RAISE STPERROR;
   END IF;

   RETURN(UNAPIGEN.DBERR_SUCCESS);

EXCEPTION
WHEN OTHERS THEN
   IF SQLCODE <> 1 THEN
      UNAPIGEN.LOGERROR('CopyRqIcDetails',SQLERRM);
   END IF;
   IF L_IP_VERSION_CURSOR%ISOPEN THEN
      CLOSE L_IP_VERSION_CURSOR;
   END IF;
   IF L_IPIE_CURSOR%ISOPEN THEN
      CLOSE L_IPIE_CURSOR;
   END IF;
   IF L_IE_CURSOR%ISOPEN THEN
      CLOSE L_IE_CURSOR;
   END IF;
   IF L_SEQ_EVENT_CURSOR%ISOPEN THEN
      CLOSE L_SEQ_EVENT_CURSOR;
   END IF;
   IF L_NEXT_II_CURSOR%ISOPEN THEN
      CLOSE L_NEXT_II_CURSOR;
   END IF;
   RETURN(UNAPIGEN.ABORTTXN(UNAPIGEN.P_TXN_ERROR, 'CopyRqIcDetails'));
END COPYRQICDETAILS;

FUNCTION COPYRQINFOVALUES
(A_RQ_FROM        IN        VARCHAR2,                 
 A_IC_FROM        IN        VARCHAR2,                 
 A_ICNODE_FROM    IN        NUMBER,                   
 A_RT_TO          IN        VARCHAR2,                 
 A_RT_TO_VERSION  IN        VARCHAR2,                 
 A_RQ_TO          IN        VARCHAR2,                 
 A_IC_TO          IN        VARCHAR2,                 
 A_ICNODE_TO      IN        NUMBER,                   
 A_MODIFY_REASON  IN        VARCHAR2)                 
RETURN NUMBER IS

L_SQLERRM2               VARCHAR2(255);
L_IP_VERSION             VARCHAR2(20);
L_ROW                    INTEGER;


L_SVIIV_SEQ              INTEGER;
L_SVIIV_RQ               UNAPIGEN.VC20_TABLE_TYPE;
L_SVIIV_IC               UNAPIGEN.VC20_TABLE_TYPE;
L_SVIIV_ICNODE           UNAPIGEN.LONG_TABLE_TYPE;
L_SVIIV_II               UNAPIGEN.VC20_TABLE_TYPE;
L_SVIIV_IINODE           UNAPIGEN.LONG_TABLE_TYPE;
L_SVIIV_IIVALUE          UNAPIGEN.VC2000_TABLE_TYPE;
L_SVIIV_MODIFY_FLAG      UNAPIGEN.NUM_TABLE_TYPE;

CURSOR L_IP_VERSION_CURSOR(A_RQ VARCHAR2, A_IC VARCHAR2, A_ICNODE NUMBER) IS
   SELECT IP_VERSION
   FROM UTRQIC
   WHERE RQ = A_RQ
   AND IC = A_IC
   AND ICNODE = A_ICNODE;

CURSOR L_ALLRQII_CURSOR(A_RQ VARCHAR2, A_IC VARCHAR2, A_ICNODE NUMBER) IS
   SELECT II, IINODE, IIVALUE
   FROM UTRQII
   WHERE RQ = A_RQ
   AND IC = A_IC
   AND ICNODE = A_ICNODE
   ORDER BY IINODE;









CURSOR L_RQII_POS_CURSOR(A_RQ_FROM VARCHAR2, A_IC_FROM VARCHAR2, A_ICNODE_FROM NUMBER,
                         A_RQ_TO   VARCHAR2, A_IC_TO   VARCHAR2, A_ICNODE_TO   NUMBER, 
                         A_II VARCHAR2, A_IP_VERSION VARCHAR2) IS
SELECT A.II_TO, A.IINODE_TO, B.II_FROM, B.IINODE_FROM, B.IIVALUE 
FROM (SELECT II II_TO, IINODE IINODE_TO, ROW_NUMBER() OVER  (ORDER BY II, IINODE)  POSITION
      FROM (SELECT II, IINODE FROM UTRQII
            WHERE RQ = A_RQ_TO 
            AND IC = A_IC_TO
            AND ICNODE = A_ICNODE_TO
            AND II = A_II )) A,
     (SELECT II II_FROM, IINODE IINODE_FROM, ROW_NUMBER() OVER  (ORDER BY II, IINODE, IIVALUE) POSITION , IIVALUE
      FROM (SELECT II, IINODE, IIVALUE FROM UTRQII
            WHERE RQ = A_RQ_FROM 
            AND IC = A_IC_FROM
            AND ICNODE = A_ICNODE_FROM
            AND II = A_II )) B
WHERE A.POSITION = B.POSITION(+)
AND A.II_TO NOT IN (SELECT A.II
                    FROM UTRQII A, UTIE B
                    WHERE A.RQ = A_RQ_TO
                      AND A.IC = A_IC_TO
                      AND A.ICNODE = A_ICNODE_TO
                      AND A.II = B.IE
                      AND A.IE_VERSION = B.VERSION
                      AND B.DEF_VAL_TP = 'S'
                      AND B.IEVALUE IS NOT NULL
                      AND (B.IE,B.VERSION) NOT IN 
                         (SELECT C.IE, UNAPIGEN.VALIDATEVERSION('ie',C.IE,C.IE_VERSION)
                          FROM UTIPIE C
                          WHERE C.IP = A.IC
                            AND C.VERSION = A_IP_VERSION
                            AND C.IE = A.II
                            AND UNAPIGEN.VALIDATEVERSION('ie',C.IE,C.IE_VERSION) = A.IE_VERSION
                            AND (C.DEF_VAL_TP<>'F' OR C.IEVALUE IS NOT NULL))
                    UNION 
                    SELECT A.II
                    FROM UTRQII A, UTIPIE B
                    WHERE A.RQ = A_RQ_TO
                      AND A.IC = A_IC_TO
                      AND A.ICNODE = A_ICNODE_TO
                      AND A.IC = B.IP
                      AND A_IP_VERSION = B.VERSION
                      AND A.II = B.IE
                      AND A.IE_VERSION = UNAPIGEN.VALIDATEVERSION('ie',B.IE,B.IE_VERSION)
                      AND B.DEF_VAL_TP = 'S'
                      AND B.IEVALUE IS NOT NULL);
                      



L_RQII_POS_REC     L_RQII_POS_CURSOR%ROWTYPE;    
L_RQII_POS_FOUND   BOOLEAN;    

BEGIN

   IF UNAPIGEN.BEGINTXN(UNAPIGEN.P_SINGLE_API_TXN) <> UNAPIGEN.DBERR_SUCCESS THEN
      RAISE STPERROR;
   END IF;

   
   L_SQLERRM := NULL;
   L_SQLERRM2 := NULL;
   L_SVIIV_SEQ := 0;
   
   
   OPEN L_IP_VERSION_CURSOR(A_RQ_TO, A_IC_TO, A_ICNODE_TO);
   FETCH L_IP_VERSION_CURSOR INTO L_IP_VERSION;
   CLOSE L_IP_VERSION_CURSOR;

   
   FOR L_ALLRQII_REC IN L_ALLRQII_CURSOR(A_RQ_TO, A_IC_TO, A_ICNODE_TO) LOOP
      
      
      L_RQII_POS_FOUND := FALSE;
      OPEN L_RQII_POS_CURSOR(A_RQ_FROM, A_IC_FROM, A_ICNODE_FROM, A_RQ_TO, A_IC_TO, A_ICNODE_TO, 
                             L_ALLRQII_REC.II, L_IP_VERSION);
      LOOP
         FETCH L_RQII_POS_CURSOR
         INTO L_RQII_POS_REC;
         IF L_RQII_POS_REC.II_TO = L_ALLRQII_REC.II AND
            L_RQII_POS_REC.IINODE_TO = L_ALLRQII_REC.IINODE THEN
            L_RQII_POS_FOUND := TRUE;
            EXIT;
         END IF;
         EXIT WHEN L_RQII_POS_CURSOR%NOTFOUND;
      END LOOP;
      CLOSE L_RQII_POS_CURSOR;

      IF L_RQII_POS_FOUND AND 
         L_RQII_POS_REC.II_FROM IS NOT NULL AND
         NVL(L_RQII_POS_REC.IIVALUE,' ') <> NVL(L_ALLRQII_REC.IIVALUE,' ') THEN
         
         
         L_SVIIV_SEQ := L_SVIIV_SEQ + 1;
         L_SVIIV_RQ(L_SVIIV_SEQ) := A_RQ_TO;
         L_SVIIV_IC(L_SVIIV_SEQ) := A_IC_TO;
         L_SVIIV_ICNODE(L_SVIIV_SEQ) := A_ICNODE_TO;
         L_SVIIV_II(L_SVIIV_SEQ) := L_RQII_POS_REC.II_TO;
         L_SVIIV_IINODE(L_SVIIV_SEQ) := L_RQII_POS_REC.IINODE_TO;
         L_SVIIV_IIVALUE(L_SVIIV_SEQ) := L_RQII_POS_REC.IIVALUE;
         L_SVIIV_MODIFY_FLAG(L_SVIIV_SEQ) := UNAPIGEN.MOD_FLAG_UPDATE;
      END IF;
   END LOOP;         
   
   
   IF L_SVIIV_SEQ <> 0 THEN
      L_RET_CODE := UNAPIRQIC.SAVERQIIVALUE(L_SVIIV_RQ, L_SVIIV_IC, L_SVIIV_ICNODE, L_SVIIV_II, 
                                            L_SVIIV_IINODE, L_SVIIV_IIVALUE, L_SVIIV_MODIFY_FLAG,
                                            L_SVIIV_SEQ, A_MODIFY_REASON);
      IF L_RET_CODE = UNAPIGEN.DBERR_PARTIALSAVE THEN
         
         FOR L_ROW IN 1..L_SVIIV_SEQ LOOP
            IF L_SVIIV_MODIFY_FLAG(L_ROW) > UNAPIGEN.DBERR_SUCCESS THEN
               UNAPIGEN.P_TXN_ERROR := L_SVIIV_MODIFY_FLAG(L_ROW);
               L_SQLERRM := 'rq=' || L_SVIIV_RQ(L_ROW) || 
                            '#ic=' || L_SVIIV_IC(L_ROW) ||
                            '#icnode=' || TO_CHAR(L_SVIIV_ICNODE(L_ROW)) ||
                            '#ii=' || L_SVIIV_II(L_ROW)||
                            '#iinode=' || TO_CHAR(L_SVIIV_IINODE(L_ROW)) ||
                            '#SaveRqIiValue#ErrorCode=' || TO_CHAR(L_RET_CODE);
               IF UNAPIAUT.P_NOT_AUTHORISED IS NOT NULL THEN
                  L_SQLERRM2 := UNAPIAUT.P_NOT_AUTHORISED;
               END IF;
               RAISE STPERROR;
            END IF;
         END LOOP;
      ELSIF L_RET_CODE <> UNAPIGEN.DBERR_SUCCESS THEN
         UNAPIGEN.P_TXN_ERROR := L_RET_CODE;
         L_SQLERRM := 'rq(1)=' || L_SVIIV_RQ(1) || '#ic(1)=' || L_SVIIV_IC(1) ||
                      '#icnode(1)=' || TO_CHAR(L_SVIIV_ICNODE(1)) ||
                      '#ii(1)=' || L_SVIIV_II(1)||
                      '#iinode(1)=' || TO_CHAR(L_SVIIV_IINODE(1)) ||
                      '#nr_of_rows=' || TO_CHAR(L_SVIIV_SEQ) ||
                      '#SaveRqIiValue#ErrorCode=' || TO_CHAR(L_RET_CODE);
         RAISE STPERROR;
      END IF;                           
   END IF;

   IF UNAPIGEN.ENDTXN <> UNAPIGEN.DBERR_SUCCESS THEN
      RAISE STPERROR;
   END IF;

   RETURN(UNAPIGEN.DBERR_SUCCESS);

EXCEPTION
WHEN OTHERS THEN
   IF SQLCODE <> 1 THEN
      UNAPIGEN.LOGERROR('CopyRqInfoValues',SQLERRM);
   ELSIF L_SQLERRM IS NOT NULL THEN
      UNAPIGEN.LOGERROR('CopyRqInfoValues',L_SQLERRM);
      IF L_SQLERRM2 IS NOT NULL THEN
         UNAPIGEN.LOGERROR('CopyRqInfoValues',L_SQLERRM2);
      END IF;      
   END IF;
   IF L_IP_VERSION_CURSOR%ISOPEN THEN
      CLOSE L_IP_VERSION_CURSOR;
   END IF;   
   IF L_RQII_POS_CURSOR%ISOPEN THEN
      CLOSE L_RQII_POS_CURSOR;
   END IF;   
   RETURN(UNAPIGEN.ABORTTXN(UNAPIGEN.P_TXN_ERROR, 'CopyRqInfoValues'));
END COPYRQINFOVALUES;

END UNAPIRQIC;