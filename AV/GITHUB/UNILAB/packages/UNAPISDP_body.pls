PACKAGE BODY unapisdp AS

TYPE BOOLEAN_TABLE_TYPE IS TABLE OF BOOLEAN INDEX BY BINARY_INTEGER;
L_SQLERRM         VARCHAR2(255);
L_SQL_STRING      VARCHAR2(4000);
L_WHERE_CLAUSE    VARCHAR2(1000);
L_EVENT_TP        UTEV.EV_TP%TYPE;
L_RET_CODE        NUMBER;
L_RESULT          NUMBER;
L_FETCHED_ROWS    NUMBER;
L_EV_SEQ_NR       NUMBER;
STPERROR          EXCEPTION;
L_EV_DETAILS      VARCHAR2(255);
L_AU_CURSOR       INTEGER;
L_SDGK_CURSOR     INTEGER;


P_GETSDCOMMENT_CURSOR       INTEGER;
P_HS_DETAILS_CURSOR         INTEGER;
P_HS_CURSOR                 INTEGER;

FUNCTION GETVERSION
   RETURN VARCHAR2
IS
BEGIN
   RETURN('06.07.00.00_00.13');
EXCEPTION
   WHEN OTHERS THEN
      RETURN (NULL);
END GETVERSION;

FUNCTION INITSDATTRIBUTE
(A_SD               IN     VARCHAR2,                  
 A_PT               IN     VARCHAR2,                  
 A_PT_VERSION       IN     VARCHAR2,                  
 A_AU               OUT    UNAPIGEN.VC20_TABLE_TYPE,  
 A_AU_VERSION       OUT    UNAPIGEN.VC20_TABLE_TYPE,  
 A_VALUE            OUT    UNAPIGEN.VC40_TABLE_TYPE,  
 A_DESCRIPTION      OUT    UNAPIGEN.VC40_TABLE_TYPE,  
 A_IS_PROTECTED     OUT    UNAPIGEN.CHAR1_TABLE_TYPE, 
 A_SINGLE_VALUED    OUT    UNAPIGEN.CHAR1_TABLE_TYPE, 
 A_NEW_VAL_ALLOWED  OUT    UNAPIGEN.CHAR1_TABLE_TYPE, 
 A_STORE_DB         OUT    UNAPIGEN.CHAR1_TABLE_TYPE, 
 A_VALUE_LIST_TP    OUT    UNAPIGEN.CHAR1_TABLE_TYPE, 
 A_RUN_MODE         OUT    UNAPIGEN.CHAR1_TABLE_TYPE, 
 A_SERVICE          OUT    UNAPIGEN.VC255_TABLE_TYPE, 
 A_CF_VALUE         OUT    UNAPIGEN.VC20_TABLE_TYPE,  
 A_NR_OF_ROWS       IN OUT NUMBER)                    
RETURN NUMBER IS

L_PT              VARCHAR2(20);
L_PT_VERSION      VARCHAR2(20);
L_INHERIT_AU      CHAR(1);
L_DESCRIPTION     VARCHAR2(40);
L_IS_PROTECTED    CHAR(1);
L_SINGLE_VALUED   CHAR(1);
L_NEW_VAL_ALLOWED CHAR(1);
L_STORE_DB        CHAR(1);
L_VALUE_LIST_TP   CHAR(1);
L_RUN_MODE        CHAR(1);
L_SERVICE         VARCHAR2(255);
L_CF_VALUE        VARCHAR2(20);

CURSOR L_PTAU_CURSOR (C_PT VARCHAR2, C_PT_VERSION VARCHAR2, C_PT_INHERIT_AU CHAR) IS
   SELECT A.AU, A.AU_VERSION, A.VALUE, AU.DESCRIPTION, AU.IS_PROTECTED, AU.SINGLE_VALUED,
          AU.NEW_VAL_ALLOWED, AU.STORE_DB, AU.VALUE_LIST_TP,
          AU.RUN_MODE, AU.SERVICE, AU.CF_VALUE
   FROM UTAU AU, UTPTAU A
   WHERE A.PT = C_PT
   AND A.VERSION = C_PT_VERSION
   AND A.AU = AU.AU
   AND UNAPIGEN.VALIDATEVERSION('au', A.AU, A.AU_VERSION) = AU.AU
   AND AU.INHERIT_AU = DECODE(C_PT_INHERIT_AU,'0','1',AU.INHERIT_AU)
   ORDER BY A.AUSEQ;
   
   
   
   
   
                          
BEGIN

   IF NVL(A_NR_OF_ROWS,0) = 0 THEN
      A_NR_OF_ROWS := UNAPIGEN.P_DEFAULT_CHUNK_SIZE;
   ELSIF A_NR_OF_ROWS < 0 OR A_NR_OF_ROWS > UNAPIGEN.P_MAX_CHUNK_SIZE THEN
      RETURN(UNAPIGEN.DBERR_NROFROWS);
   END IF;

   IF NVL(A_SD, ' ') <> ' ' THEN
      SELECT PT, PT_VERSION
      INTO L_PT, L_PT_VERSION
      FROM UTSD
      WHERE SD = A_SD;
   ELSE
      L_PT := A_PT;
      IF A_PT_VERSION IS NULL THEN
         RETURN(UNAPIGEN.DBERR_PTVERSION);
      END IF;
      L_PT_VERSION := A_PT_VERSION;
   END IF;

   SELECT INHERIT_AU
   INTO L_INHERIT_AU
   FROM UTPT
   WHERE PT = L_PT
   AND VERSION = L_PT_VERSION;

   L_FETCHED_ROWS := 0;

   FOR L_PTAU IN L_PTAU_CURSOR(L_PT, L_PT_VERSION, NVL(L_INHERIT_AU, '0')) LOOP
      L_FETCHED_ROWS := L_FETCHED_ROWS + 1;
      A_AU(L_FETCHED_ROWS) := L_PTAU.AU;
      A_AU_VERSION(L_FETCHED_ROWS) := L_PTAU.AU_VERSION;
      A_VALUE(L_FETCHED_ROWS) := L_PTAU.VALUE;
      A_DESCRIPTION(L_FETCHED_ROWS) := L_PTAU.DESCRIPTION;
      A_IS_PROTECTED(L_FETCHED_ROWS) := L_PTAU.IS_PROTECTED;
      A_SINGLE_VALUED(L_FETCHED_ROWS) := L_PTAU.SINGLE_VALUED;
      A_NEW_VAL_ALLOWED(L_FETCHED_ROWS) := L_PTAU.NEW_VAL_ALLOWED;
      A_STORE_DB(L_FETCHED_ROWS) := L_PTAU.STORE_DB;
      A_VALUE_LIST_TP(L_FETCHED_ROWS) := L_PTAU.VALUE_LIST_TP;
      A_RUN_MODE(L_FETCHED_ROWS) := L_PTAU.RUN_MODE;
      A_SERVICE(L_FETCHED_ROWS) := L_PTAU.SERVICE;
      A_CF_VALUE(L_FETCHED_ROWS) := L_PTAU.CF_VALUE;

      EXIT WHEN L_FETCHED_ROWS >= A_NR_OF_ROWS;
   END LOOP;

   IF L_FETCHED_ROWS = 0 THEN
      A_NR_OF_ROWS := 0;
      L_RET_CODE := UNAPIGEN.DBERR_NORECORDS;
   ELSE
      L_RET_CODE := UNAPIGEN.DBERR_SUCCESS;
      A_NR_OF_ROWS := L_FETCHED_ROWS;
   END IF;

   RETURN(L_RET_CODE);

EXCEPTION
   WHEN NO_DATA_FOUND THEN
      RETURN(UNAPIGEN.DBERR_NOOBJECT);
   WHEN OTHERS THEN
      L_SQLERRM := SQLERRM;
      UNAPIGEN.U4ROLLBACK;
      INSERT INTO UTERROR(CLIENT_ID, APPLIC, WHO, LOGDATE, LOGDATE_TZ, API_NAME, ERROR_MSG)
      VALUES(UNAPIGEN.P_CLIENT_ID, UNAPIGEN.P_APPLIC_NAME, UNAPIGEN.P_USER, CURRENT_TIMESTAMP, CURRENT_TIMESTAMP,
             'InitSdAttribute', L_SQLERRM);
      UNAPIGEN.U4COMMIT;
      RETURN(UNAPIGEN.DBERR_GENFAIL);
END INITSDATTRIBUTE;

FUNCTION GETSDATTRIBUTE
(A_SD              OUT    UNAPIGEN.VC20_TABLE_TYPE,  
 A_AU               OUT    UNAPIGEN.VC20_TABLE_TYPE,  
 A_AU_VERSION       OUT    UNAPIGEN.VC20_TABLE_TYPE,  
 A_VALUE            OUT    UNAPIGEN.VC40_TABLE_TYPE,  
 A_DESCRIPTION      OUT    UNAPIGEN.VC40_TABLE_TYPE,  
 A_IS_PROTECTED     OUT    UNAPIGEN.CHAR1_TABLE_TYPE, 
 A_SINGLE_VALUED    OUT    UNAPIGEN.CHAR1_TABLE_TYPE, 
 A_NEW_VAL_ALLOWED  OUT    UNAPIGEN.CHAR1_TABLE_TYPE, 
 A_STORE_DB         OUT    UNAPIGEN.CHAR1_TABLE_TYPE, 
 A_VALUE_LIST_TP    OUT    UNAPIGEN.CHAR1_TABLE_TYPE, 
 A_RUN_MODE         OUT    UNAPIGEN.CHAR1_TABLE_TYPE, 
 A_SERVICE          OUT    UNAPIGEN.VC255_TABLE_TYPE, 
 A_CF_VALUE         OUT    UNAPIGEN.VC20_TABLE_TYPE,  
 A_NR_OF_ROWS       IN OUT NUMBER,                    
 A_WHERE_CLAUSE    IN     VARCHAR2)                  
RETURN NUMBER IS

L_SD                             VARCHAR2(20);
L_AU                             VARCHAR2(20);
L_AU_VERSION                     VARCHAR2(20);
L_VALUE                          VARCHAR2(40);
L_DESCRIPTION                    VARCHAR2(40);
L_IS_PROTECTED                   CHAR(1);
L_SINGLE_VALUED                  CHAR(1);
L_NEW_VAL_ALLOWED                CHAR(1);
L_STORE_DB                       CHAR(1);
L_VALUE_LIST_TP                  CHAR(1);
L_RUN_MODE                       CHAR(1);
L_SERVICE                        VARCHAR2(255);
L_CF_VALUE                       VARCHAR2(20);
L_BIND_SD_SELECTION              BOOLEAN;
L_BIND_FIXED_SD_FLAG             BOOLEAN;

BEGIN

   L_BIND_SD_SELECTION := FALSE;
   L_BIND_FIXED_SD_FLAG := FALSE;
   IF NVL(A_NR_OF_ROWS,0) = 0 THEN
      A_NR_OF_ROWS := UNAPIGEN.P_DEFAULT_CHUNK_SIZE;
   ELSIF A_NR_OF_ROWS < 0 OR A_NR_OF_ROWS > UNAPIGEN.P_MAX_CHUNK_SIZE THEN
      RETURN(UNAPIGEN.DBERR_NROFROWS);
   END IF;

   IF NVL(A_WHERE_CLAUSE, ' ') = ' ' THEN
      RETURN(UNAPIGEN.DBERR_WHERECLAUSE);
   ELSIF A_WHERE_CLAUSE = 'SELECTION' THEN
      IF UNAPISD.P_SELECTION_CLAUSE IS NOT NULL THEN
         IF INSTR(UPPER(UNAPISD.P_SELECTION_CLAUSE), ' WHERE ') <> 0 THEN       
            L_WHERE_CLAUSE := ','||UNAPISD.P_SELECTION_CLAUSE|| ' AND a.sd = au.sd ORDER BY au.sd,au.auseq'; 
         ELSE
            L_WHERE_CLAUSE := ','||UNAPISD.P_SELECTION_CLAUSE|| ' WHERE a.sd = au.sd ORDER BY au.sd,au.auseq'; 
         END IF;
         L_BIND_SD_SELECTION := TRUE;
      ELSE
         L_WHERE_CLAUSE := 'ORDER BY au.sd,au.auseq'; 
      END IF;
   ELSIF
      UPPER(SUBSTR(A_WHERE_CLAUSE,1,6)) <> 'WHERE ' THEN
      L_BIND_FIXED_SD_FLAG := TRUE;
      L_WHERE_CLAUSE := 'WHERE au.sd = :sd_val ORDER BY au.auseq';
   ELSE
      L_WHERE_CLAUSE := A_WHERE_CLAUSE; 
   END IF;

   L_SQL_STRING := 'SELECT au.sd, au.au, au.au_version, au.value FROM dd' ||
                   UNAPIGEN.P_DD || '.uvsdau au ' || L_WHERE_CLAUSE;

   IF NOT DBMS_SQL.IS_OPEN(L_AU_CURSOR) THEN
      L_AU_CURSOR := DBMS_SQL.OPEN_CURSOR;
      DBMS_SQL.PARSE(L_AU_CURSOR, L_SQL_STRING, DBMS_SQL.V7); 
      IF L_BIND_SD_SELECTION THEN
         FOR L_X IN 1..UNAPISD.P_SELECTION_VAL_TAB.COUNT() LOOP
            DBMS_SQL.BIND_VARIABLE(L_AU_CURSOR, ':col_val'||L_X , UNAPISD.P_SELECTION_VAL_TAB(L_X)); 
         END LOOP;
      ELSIF L_BIND_FIXED_SD_FLAG THEN
         DBMS_SQL.BIND_VARIABLE(L_AU_CURSOR, ':sd_val' , A_WHERE_CLAUSE); 
      END IF;
 
      DBMS_SQL.DEFINE_COLUMN(L_AU_CURSOR, 1, L_SD, 20);
      DBMS_SQL.DEFINE_COLUMN(L_AU_CURSOR, 2, L_AU, 20);
      DBMS_SQL.DEFINE_COLUMN(L_AU_CURSOR, 3, L_AU_VERSION, 20);
      DBMS_SQL.DEFINE_COLUMN(L_AU_CURSOR, 4, L_VALUE, 40);
      L_RESULT := DBMS_SQL.EXECUTE(L_AU_CURSOR);
   END IF;
   L_RESULT := DBMS_SQL.FETCH_ROWS(L_AU_CURSOR);
   L_FETCHED_ROWS := 0;

   LOOP
      EXIT WHEN L_RESULT = 0 OR L_FETCHED_ROWS >= A_NR_OF_ROWS;

      DBMS_SQL.COLUMN_VALUE(L_AU_CURSOR, 1, L_SD);
      DBMS_SQL.COLUMN_VALUE(L_AU_CURSOR, 2, L_AU);
      DBMS_SQL.COLUMN_VALUE(L_AU_CURSOR, 3, L_AU_VERSION);
      DBMS_SQL.COLUMN_VALUE(L_AU_CURSOR, 4, L_VALUE);

      L_FETCHED_ROWS := L_FETCHED_ROWS + 1;
      A_SD(L_FETCHED_ROWS) := L_SD;
      A_AU(L_FETCHED_ROWS) := L_AU;
      A_AU_VERSION(L_FETCHED_ROWS) := L_AU_VERSION;
      A_VALUE(L_FETCHED_ROWS) := L_VALUE;

      OPEN UNAPIGEN.L_AUDET_CURSOR(L_AU, L_AU_VERSION);
      FETCH UNAPIGEN.L_AUDET_CURSOR
      INTO L_DESCRIPTION, L_IS_PROTECTED, L_SINGLE_VALUED,
           L_NEW_VAL_ALLOWED, L_STORE_DB, L_VALUE_LIST_TP, L_RUN_MODE,
           L_SERVICE, L_CF_VALUE;
      IF UNAPIGEN.L_AUDET_CURSOR%NOTFOUND THEN
         
         
         
         A_DESCRIPTION(L_FETCHED_ROWS)     := L_AU;
         A_IS_PROTECTED(L_FETCHED_ROWS)    := '1';
         A_SINGLE_VALUED(L_FETCHED_ROWS)   := '1';
         A_NEW_VAL_ALLOWED(L_FETCHED_ROWS) := '0';
         A_STORE_DB(L_FETCHED_ROWS)        := '0';
         A_VALUE_LIST_TP(L_FETCHED_ROWS)   := 'E';
         A_RUN_MODE(L_FETCHED_ROWS)        := 'H';
         A_SERVICE(L_FETCHED_ROWS)         := NULL;
         A_CF_VALUE(L_FETCHED_ROWS)        := NULL;
      ELSE
         A_DESCRIPTION(L_FETCHED_ROWS) := L_DESCRIPTION;
         A_IS_PROTECTED(L_FETCHED_ROWS) := L_IS_PROTECTED;
         A_SINGLE_VALUED(L_FETCHED_ROWS) := L_SINGLE_VALUED;
         A_NEW_VAL_ALLOWED(L_FETCHED_ROWS) := L_NEW_VAL_ALLOWED;
         A_STORE_DB(L_FETCHED_ROWS) := L_STORE_DB;
         A_VALUE_LIST_TP(L_FETCHED_ROWS) := L_VALUE_LIST_TP;
         A_RUN_MODE(L_FETCHED_ROWS) := L_RUN_MODE;
         A_SERVICE(L_FETCHED_ROWS) := L_SERVICE;
         A_CF_VALUE(L_FETCHED_ROWS) := L_CF_VALUE;
      END IF;
      CLOSE UNAPIGEN.L_AUDET_CURSOR;

      IF L_FETCHED_ROWS < A_NR_OF_ROWS THEN
         L_RESULT := DBMS_SQL.FETCH_ROWS(L_AU_CURSOR);
      END IF;
   END LOOP;

   IF L_FETCHED_ROWS = 0 THEN
      L_RET_CODE := UNAPIGEN.DBERR_NORECORDS;
      DBMS_SQL.CLOSE_CURSOR(L_AU_CURSOR);
   ELSIF L_FETCHED_ROWS < A_NR_OF_ROWS THEN
      L_RET_CODE := UNAPIGEN.DBERR_SUCCESS;
      A_NR_OF_ROWS := L_FETCHED_ROWS;
      DBMS_SQL.CLOSE_CURSOR(L_AU_CURSOR);
   ELSE   
      L_RET_CODE := UNAPIGEN.DBERR_SUCCESS;
      A_NR_OF_ROWS := L_FETCHED_ROWS;
   END IF;

   IF A_WHERE_CLAUSE <> 'SELECTION' AND
      DBMS_SQL.IS_OPEN(L_AU_CURSOR) THEN
      DBMS_SQL.CLOSE_CURSOR(L_AU_CURSOR);
   END IF;

   RETURN(L_RET_CODE);

EXCEPTION
   WHEN OTHERS THEN
      L_SQLERRM := SQLERRM;
      UNAPIGEN.U4ROLLBACK;
      INSERT INTO UTERROR(CLIENT_ID, APPLIC, WHO, LOGDATE, LOGDATE_TZ, API_NAME, ERROR_MSG)
      VALUES(UNAPIGEN.P_CLIENT_ID, UNAPIGEN.P_APPLIC_NAME, UNAPIGEN.P_USER, CURRENT_TIMESTAMP, CURRENT_TIMESTAMP,
             'GetSdAttribute', L_SQLERRM);
      UNAPIGEN.U4COMMIT;
      IF UNAPIGEN.L_AUDET_CURSOR%ISOPEN THEN
         CLOSE UNAPIGEN.L_AUDET_CURSOR;
      END IF;
      IF DBMS_SQL.IS_OPEN(L_AU_CURSOR) THEN
         DBMS_SQL.CLOSE_CURSOR(L_AU_CURSOR);
      END IF;
      RETURN(UNAPIGEN.DBERR_GENFAIL);
END GETSDATTRIBUTE;

FUNCTION SAVESDATTRIBUTE 
(A_SD             IN        VARCHAR2,                 
 A_AU             IN        UNAPIGEN.VC20_TABLE_TYPE, 
 A_AU_VERSION     IN OUT    UNAPIGEN.VC20_TABLE_TYPE, 
 A_VALUE          IN        UNAPIGEN.VC40_TABLE_TYPE, 
 A_NR_OF_ROWS     IN        NUMBER,                   
 A_MODIFY_REASON  IN        VARCHAR2)                 
RETURN NUMBER IS

L_ALLOW_MODIFY      CHAR(1);
L_LOG_HS            CHAR(1);
L_LOG_HS_DETAILS    CHAR(1);
L_ACTIVE            CHAR(1);
L_LC                VARCHAR2(2);
L_LC_VERSION        VARCHAR2(20);
L_SS                VARCHAR2(2);
L_WHAT_DESCRIPTION  VARCHAR2(255);
L_HS_SEQ            INTEGER;
L_PT_VERSION        VARCHAR2(20);

CURSOR L_MODIFIEDAU_CURSOR IS
   
   (SELECT A.AU AU, A.AU_VERSION AU_VERSION, A.VALUE VALUE, 'DELETE' ACTION
    FROM UTSDAU A
    WHERE A.SD = A_SD
    AND AUSEQ < 0
   MINUS
    SELECT A.AU AU, A.AU_VERSION AU_VERSION, A.VALUE VALUE, 'DELETE' ACTION
    FROM UTSDAU A
    WHERE A.SD = A_SD
    AND AUSEQ > 0)
   UNION ALL
   
   (SELECT A.AU AU, A.AU_VERSION AU_VERSION, A.VALUE VALUE, 'NEW' ACTION
    FROM UTSDAU A
    WHERE A.SD = A_SD
    AND AUSEQ > 0
   MINUS
    SELECT A.AU AU, A.AU_VERSION AU_VERSION, A.VALUE VALUE, 'NEW' ACTION
    FROM UTSDAU A
    WHERE A.SD = A_SD
    AND AUSEQ < 0);
   
BEGIN

   IF UNAPIGEN.BEGINTXN(UNAPIGEN.P_SINGLE_API_TXN) <>
      UNAPIGEN.DBERR_SUCCESS THEN
      RAISE STPERROR;
   END IF;

   IF NVL(A_NR_OF_ROWS, -1) < 0 THEN
      UNAPIGEN.P_TXN_ERROR := UNAPIGEN.DBERR_NROFROWS;
      RAISE STPERROR;
   END IF;

   IF NVL(A_SD, ' ') = ' ' THEN
      UNAPIGEN.P_TXN_ERROR := UNAPIGEN.DBERR_NOOBJID;
      RAISE STPERROR;
   END IF;

   L_RET_CODE := UNAPIAUT.GETSDAUTHORISATION(A_SD, L_PT_VERSION, L_LC, L_LC_VERSION, L_SS, L_ALLOW_MODIFY,
                                             L_ACTIVE, L_LOG_HS, L_LOG_HS_DETAILS);
   IF L_RET_CODE <> UNAPIGEN.DBERR_SUCCESS THEN
      UNAPIGEN.P_TXN_ERROR := L_RET_CODE;
      RAISE STPERROR;
   END IF;

   UPDATE UTSD
   SET ALLOW_MODIFY = '#'
   WHERE SD = A_SD;

   IF SQL%ROWCOUNT < 1 THEN
      UNAPIGEN.P_TXN_ERROR := UNAPIGEN.DBERR_NOOBJECT;
      RAISE STPERROR ;
   END IF;




   L_EVENT_TP := 'SdAttributesUpdated';
   L_EV_SEQ_NR := -1;
   L_EV_DETAILS := 'pt_version=' || L_PT_VERSION;
   L_RESULT := UNAPIEV.INSERTEVENT('SaveSdAttribute', UNAPIGEN.P_EVMGR_NAME, 'sd', A_SD,
                                   L_LC, L_LC_VERSION, L_SS, L_EVENT_TP, L_EV_DETAILS, L_EV_SEQ_NR);
   IF L_RESULT <> UNAPIGEN.DBERR_SUCCESS THEN
      UNAPIGEN.P_TXN_ERROR := L_RESULT;
      RAISE STPERROR;
   END IF;

   IF (L_LOG_HS = '1') THEN
      INSERT INTO UTSDHS(SD, WHO, WHO_DESCRIPTION, WHAT, WHAT_DESCRIPTION, LOGDATE, LOGDATE_TZ, WHY, TR_SEQ, EV_SEQ)
      VALUES(A_SD, UNAPIGEN.P_USER, UNAPIGEN.P_USER_DESCRIPTION, L_EVENT_TP, 
             'study "'||A_SD||'" attributes are updated.', 
             CURRENT_TIMESTAMP, CURRENT_TIMESTAMP, A_MODIFY_REASON, UNAPIGEN.P_TR_SEQ, L_EV_SEQ_NR);
   END IF;

   L_HS_SEQ := 0;
   IF (L_LOG_HS_DETAILS = '1') THEN
      L_HS_SEQ := L_HS_SEQ + 1;
      INSERT INTO UTSDHSDETAILS(SD, TR_SEQ, EV_SEQ, SEQ, DETAILS)
      VALUES(A_SD, UNAPIGEN.P_TR_SEQ, L_EV_SEQ_NR, L_HS_SEQ, 
             'study "'||A_SD||'" attributes are updated.');
   END IF;
   
   
   
   
   UPDATE UTSDAU
   SET AUSEQ = -AUSEQ
   WHERE SD = A_SD;

   FOR L_SEQ_NO IN 1..A_NR_OF_ROWS LOOP
      INSERT INTO UTSDAU(SD, AU, AUSEQ, VALUE)
      VALUES(A_SD, A_AU(L_SEQ_NO), L_SEQ_NO, A_VALUE(L_SEQ_NO));
   END LOOP;

   
   
      
   
   
   
   IF (L_LOG_HS_DETAILS = '1') THEN
      FOR L_AU_REC IN L_MODIFIEDAU_CURSOR LOOP
         IF L_AU_REC.ACTION = 'DELETE' THEN
            L_WHAT_DESCRIPTION := 'Attribute "'||L_AU_REC.AU||'" with value "'||L_AU_REC.VALUE||'" is removed from study "'||A_SD||'".';
         ELSE
            L_WHAT_DESCRIPTION := 'Attribute "'||L_AU_REC.AU||'" is added to study "'||A_SD||'", value is "'||L_AU_REC.VALUE||'".';
         END IF;
         L_HS_SEQ := L_HS_SEQ + 1;
         INSERT INTO UTSDHSDETAILS(SD, TR_SEQ, EV_SEQ, SEQ, DETAILS)
         VALUES(A_SD, UNAPIGEN.P_TR_SEQ, L_EV_SEQ_NR,L_HS_SEQ, L_WHAT_DESCRIPTION);
      END LOOP;
   END IF;
  
   
   
   
   DELETE FROM UTSDAU
   WHERE SD = A_SD
   AND AUSEQ < 0;
   
   IF UNAPIGEN.ENDTXN <> UNAPIGEN.DBERR_SUCCESS THEN
      RAISE STPERROR;
   END IF;

   RETURN(UNAPIGEN.DBERR_SUCCESS);

EXCEPTION
WHEN OTHERS THEN
   IF SQLCODE <> 1 THEN
      UNAPIGEN.LOGERROR('SaveSdAttribute', SQLERRM);
   END IF;
   IF L_MODIFIEDAU_CURSOR%ISOPEN THEN
      CLOSE L_MODIFIEDAU_CURSOR;
   END IF;
   RETURN(UNAPIGEN.ABORTTXN(UNAPIGEN.P_TXN_ERROR, 'SaveSdAttribute'));
END SAVESDATTRIBUTE;

FUNCTION SAVE1SDATTRIBUTE 
(A_SD             IN        VARCHAR2,                 
 A_AU             IN        VARCHAR2,                 
 A_AU_VERSION     IN OUT    VARCHAR2,                 
 A_VALUE          IN        UNAPIGEN.VC40_TABLE_TYPE, 
 A_NR_OF_ROWS     IN        NUMBER,                   
 A_MODIFY_REASON  IN        VARCHAR2)                 
RETURN NUMBER IS

L_LC                   VARCHAR2(2);
L_LC_VERSION           VARCHAR2(20);
L_SS                   VARCHAR2(2);
L_LOG_HS               CHAR(1);
L_LOG_HS_DETAILS       CHAR(1);
L_ALLOW_MODIFY         CHAR(1);
L_ACTIVE               CHAR(1);
L_NEW_SEQ              NUMBER;
L_AU_HANDLE            BOOLEAN_TABLE_TYPE;
L_AU_FOUND             BOOLEAN;
L_INSERT_EVENT         BOOLEAN;
L_LAST_SEQ             INTEGER;
L_WHAT_DESCRIPTION     VARCHAR2(255);
L_HS_SEQ               INTEGER;
L_PT_VERSION           VARCHAR2(20);

CURSOR L_AU_CURSOR IS
SELECT VALUE, AUSEQ
FROM UTSDAU
WHERE SD = A_SD
AND AU = A_AU
ORDER BY AUSEQ;

BEGIN

   IF UNAPIGEN.BEGINTXN(UNAPIGEN.P_SINGLE_API_TXN) <>
      UNAPIGEN.DBERR_SUCCESS THEN
      RAISE STPERROR;
   END IF;

   IF NVL(A_NR_OF_ROWS, -1) < 0 THEN
      UNAPIGEN.P_TXN_ERROR := UNAPIGEN.DBERR_NROFROWS;
      RAISE STPERROR;
   END IF;

   IF NVL(A_SD, ' ') = ' ' OR NVL(A_AU, ' ') = ' ' THEN
      UNAPIGEN.P_TXN_ERROR := UNAPIGEN.DBERR_NOOBJID;
      RAISE STPERROR;
   END IF;

   FOR L_SEQ_NO IN 1..A_NR_OF_ROWS LOOP
      L_AU_HANDLE(L_SEQ_NO) := TRUE;
   END LOOP;

   L_RET_CODE := UNAPIAUT.GETSDAUTHORISATION(A_SD, L_PT_VERSION, L_LC, L_LC_VERSION, L_SS,
                                             L_ALLOW_MODIFY, L_ACTIVE, L_LOG_HS, L_LOG_HS_DETAILS);

   IF L_RET_CODE <> UNAPIGEN.DBERR_SUCCESS THEN
      UNAPIGEN.P_TXN_ERROR := L_RET_CODE;
      RAISE STPERROR;
   END IF;

   UPDATE UTSD
   SET ALLOW_MODIFY = '#'
   WHERE SD = A_SD;

   IF SQL%ROWCOUNT < 1 THEN
      UNAPIGEN.P_TXN_ERROR := UNAPIGEN.DBERR_NOOBJECT;
      RAISE STPERROR ;
   END IF;

   L_EVENT_TP := 'SdAttributesUpdated';
   L_EV_SEQ_NR := -1;
   L_EV_DETAILS := 'pt_version=' || L_PT_VERSION;
   L_RESULT := UNAPIEV.INSERTEVENT('Save1SdAttribute', UNAPIGEN.P_EVMGR_NAME, 'sd', A_SD, L_LC, 
                                   L_LC_VERSION, L_SS, L_EVENT_TP, L_EV_DETAILS, L_EV_SEQ_NR);
   IF L_RESULT <> UNAPIGEN.DBERR_SUCCESS THEN
      UNAPIGEN.P_TXN_ERROR := L_RESULT;
      RAISE STPERROR;
   END IF;
   
   IF L_LOG_HS = '1' THEN
      INSERT INTO UTSDHS(SD, WHO, WHO_DESCRIPTION, WHAT, WHAT_DESCRIPTION, LOGDATE, LOGDATE_TZ, WHY, TR_SEQ, EV_SEQ)
      VALUES(A_SD, UNAPIGEN.P_USER, UNAPIGEN.P_USER_DESCRIPTION, L_EVENT_TP, 
             'study "'||A_SD||'" attribute "'||A_AU||'" is created/updated.', 
             CURRENT_TIMESTAMP, CURRENT_TIMESTAMP, A_MODIFY_REASON, UNAPIGEN.P_TR_SEQ, L_EV_SEQ_NR);
   END IF;

   L_HS_SEQ := 0;
   IF L_LOG_HS_DETAILS = '1' THEN
      L_HS_SEQ := L_HS_SEQ + 1;
      INSERT INTO UTSDHSDETAILS(SD, TR_SEQ, EV_SEQ, SEQ ,DETAILS)
      VALUES(A_SD, UNAPIGEN.P_TR_SEQ, L_EV_SEQ_NR, L_HS_SEQ, 
             'study "'||A_SD||'" attribute "'||A_AU||'" is created/updated.');
   END IF;

   L_LAST_SEQ := 499;
   FOR L_SDAU IN L_AU_CURSOR LOOP
      L_AU_FOUND := FALSE;
      FOR L_SEQ_NO IN 1..A_NR_OF_ROWS LOOP
         
         IF L_SDAU.VALUE = A_VALUE(L_SEQ_NO) OR
            (L_SDAU.VALUE IS NULL AND A_VALUE(L_SEQ_NO) IS NULL) THEN
            L_AU_HANDLE(L_SEQ_NO) := FALSE;
            L_AU_FOUND := TRUE;
            EXIT;
         END IF;
      END LOOP;

      IF NOT L_AU_FOUND THEN
         
         DELETE FROM UTSDAU
         WHERE SD = A_SD
            AND AU = A_AU
            AND VALUE = L_SDAU.VALUE;

         
         IF L_SDAU.VALUE IS NULL THEN
            DELETE FROM UTSDAU
            WHERE SD = A_SD
              AND AU = A_AU
              AND VALUE IS NULL;
         END IF;            

         IF (L_LOG_HS_DETAILS = '1') THEN
            L_WHAT_DESCRIPTION := 'Attribute "'||A_AU||'" with value "'||L_SDAU.VALUE||'" is removed from study "'||A_SD||'".';
            L_HS_SEQ := L_HS_SEQ + 1;
            INSERT INTO UTSDHSDETAILS(SD, TR_SEQ, EV_SEQ, SEQ, DETAILS)
            VALUES(A_SD, UNAPIGEN.P_TR_SEQ, L_EV_SEQ_NR, L_HS_SEQ, L_WHAT_DESCRIPTION);
         END IF;   
      ELSE      
         L_LAST_SEQ := L_SDAU.AUSEQ;
      END IF;
   END LOOP;

   FOR L_SEQ_NO IN 1..A_NR_OF_ROWS LOOP
      IF L_AU_HANDLE(L_SEQ_NO) THEN
         L_LAST_SEQ := L_LAST_SEQ+1;

         
         INSERT INTO UTSDAU(SD, AU, AUSEQ, VALUE)
         VALUES
         (A_SD, A_AU, L_LAST_SEQ, A_VALUE(L_SEQ_NO));

         IF (L_LOG_HS_DETAILS = '1') THEN
            L_WHAT_DESCRIPTION := 'Attribute "'||A_AU||'" is added to study "'||A_SD||'", value is "'||A_VALUE(L_SEQ_NO)||'".';
            L_HS_SEQ := L_HS_SEQ + 1;
            INSERT INTO UTSDHSDETAILS(SD, TR_SEQ, EV_SEQ, SEQ, DETAILS)
            VALUES(A_SD, UNAPIGEN.P_TR_SEQ, L_EV_SEQ_NR, L_HS_SEQ, L_WHAT_DESCRIPTION);
         END IF; 
      END IF;
   END LOOP;

   IF UNAPIGEN.ENDTXN <> UNAPIGEN.DBERR_SUCCESS THEN
      RAISE STPERROR;
   END IF;

   RETURN(UNAPIGEN.DBERR_SUCCESS);

EXCEPTION
WHEN OTHERS THEN
   IF SQLCODE <> 1 THEN
      UNAPIGEN.LOGERROR('Save1SdAttribute', SQLERRM);
   ELSIF L_SQLERRM IS NOT NULL THEN
      UNAPIGEN.LOGERROR('Save1SdAttribute', L_SQLERRM);
   END IF;
   RETURN(UNAPIGEN.ABORTTXN(UNAPIGEN.P_TXN_ERROR, 'Save1SdAttribute'));
END SAVE1SDATTRIBUTE;

FUNCTION GETSDHISTORY
(A_SD                OUT     UNAPIGEN.VC20_TABLE_TYPE,  
 A_WHO               OUT     UNAPIGEN.VC20_TABLE_TYPE,  
 A_WHO_DESCRIPTION   OUT     UNAPIGEN.VC40_TABLE_TYPE,  
 A_WHAT              OUT     UNAPIGEN.VC60_TABLE_TYPE,  
 A_WHAT_DESCRIPTION  OUT     UNAPIGEN.VC255_TABLE_TYPE, 
 A_LOGDATE           OUT     UNAPIGEN.DATE_TABLE_TYPE,  
 A_WHY               OUT     UNAPIGEN.VC255_TABLE_TYPE, 
 A_TR_SEQ            OUT     UNAPIGEN.NUM_TABLE_TYPE,   
 A_EV_SEQ            OUT     UNAPIGEN.NUM_TABLE_TYPE,   
 A_NR_OF_ROWS        IN OUT  NUMBER,                    
 A_WHERE_CLAUSE      IN      VARCHAR2)                  
RETURN NUMBER IS

L_NR_OF_ROWS_IN               INTEGER;
L_NR_OF_ROWS_OUT              INTEGER;


L_SD_TAB                      UNAPIGEN.VC20_TABLE_TYPE;
L_WHO_TAB                     UNAPIGEN.VC20_TABLE_TYPE;
L_WHO_DESCRIPTION_TAB         UNAPIGEN.VC40_TABLE_TYPE;
L_WHAT_TAB                    UNAPIGEN.VC60_TABLE_TYPE;
L_WHAT_DESCRIPTION_TAB        UNAPIGEN.VC255_TABLE_TYPE;
L_LOGDATE_TAB                 UNAPIGEN.DATE_TABLE_TYPE;
L_WHY_TAB                     UNAPIGEN.VC255_TABLE_TYPE;
L_TR_SEQ_TAB                  UNAPIGEN.NUM_TABLE_TYPE;
L_EV_SEQ_TAB                  UNAPIGEN.NUM_TABLE_TYPE;

BEGIN
   L_NR_OF_ROWS_IN := A_NR_OF_ROWS;
   L_NR_OF_ROWS_OUT := L_NR_OF_ROWS_IN;
   L_RET_CODE := GETSDHISTORY(A_SD,
                              A_WHO,             
                              A_WHO_DESCRIPTION,
                              A_WHAT,            
                              A_WHAT_DESCRIPTION,
                              A_LOGDATE,         
                              A_WHY,             
                              A_TR_SEQ,          
                              A_EV_SEQ,          
                              L_NR_OF_ROWS_OUT,      
                              A_WHERE_CLAUSE,
                              0);
    IF L_RET_CODE <> UNAPIGEN.DBERR_SUCCESS THEN
       RETURN(L_RET_CODE);
    ELSE
       A_NR_OF_ROWS := L_NR_OF_ROWS_OUT;
       IF L_NR_OF_ROWS_OUT = L_NR_OF_ROWS_IN THEN
          
          L_RET_CODE := GETSDHISTORY(L_SD_TAB,             
                                     L_WHO_TAB,
                                     L_WHO_DESCRIPTION_TAB,
                                     L_WHAT_TAB,
                                     L_WHAT_DESCRIPTION_TAB,
                                     L_LOGDATE_TAB,
                                     L_WHY_TAB,
                                     L_TR_SEQ_TAB,
                                     L_EV_SEQ_TAB,
                                     L_NR_OF_ROWS_OUT,      
                                     A_WHERE_CLAUSE,
                                     -1);
          IF L_RET_CODE <> UNAPIGEN.DBERR_SUCCESS THEN
             RETURN(L_RET_CODE);
          END IF;
       END IF;
    END IF;
    RETURN(L_RET_CODE);
                              
EXCEPTION
   WHEN OTHERS THEN
      L_SQLERRM := SQLERRM;
      UNAPIGEN.U4ROLLBACK;
      INSERT INTO UTERROR(CLIENT_ID, APPLIC, WHO, LOGDATE, LOGDATE_TZ, API_NAME, ERROR_MSG)
      VALUES(UNAPIGEN.P_CLIENT_ID, UNAPIGEN.P_APPLIC_NAME, UNAPIGEN.P_USER, CURRENT_TIMESTAMP, CURRENT_TIMESTAMP,
             'GetSdHistory', L_SQLERRM);
      UNAPIGEN.U4COMMIT;
      RETURN(UNAPIGEN.DBERR_GENFAIL);
END GETSDHISTORY;

FUNCTION GETSDHISTORY 
(A_SD                OUT     UNAPIGEN.VC20_TABLE_TYPE,  
 A_WHO               OUT     UNAPIGEN.VC20_TABLE_TYPE,  
 A_WHO_DESCRIPTION   OUT     UNAPIGEN.VC40_TABLE_TYPE,  
 A_WHAT              OUT     UNAPIGEN.VC60_TABLE_TYPE,  
 A_WHAT_DESCRIPTION  OUT     UNAPIGEN.VC255_TABLE_TYPE, 
 A_LOGDATE           OUT     UNAPIGEN.DATE_TABLE_TYPE,  
 A_WHY               OUT     UNAPIGEN.VC255_TABLE_TYPE, 
 A_TR_SEQ            OUT     UNAPIGEN.NUM_TABLE_TYPE,   
 A_EV_SEQ            OUT     UNAPIGEN.NUM_TABLE_TYPE,   
 A_NR_OF_ROWS        IN OUT  NUMBER,                    
 A_WHERE_CLAUSE      IN      VARCHAR2,                  
 A_NEXT_ROWS         IN      NUMBER)                    
RETURN NUMBER IS

L_SD                      VARCHAR2(20);
L_WHO                     VARCHAR2(20);
L_WHO_DESCRIPTION         VARCHAR2(40);
L_WHAT                    VARCHAR2(60);
L_WHAT_DESCRIPTION        VARCHAR2(255);
L_LOGDATE                 TIMESTAMP WITH TIME ZONE;
L_WHY                     VARCHAR2(255);
L_TR_SEQ                  NUMBER;
L_EV_SEQ                  NUMBER;

BEGIN

   IF NVL(A_NR_OF_ROWS,0) = 0 THEN
      A_NR_OF_ROWS := UNAPIGEN.P_DEFAULT_CHUNK_SIZE;
   ELSIF A_NR_OF_ROWS < 0 OR A_NR_OF_ROWS > UNAPIGEN.P_MAX_CHUNK_SIZE THEN
      RETURN(UNAPIGEN.DBERR_NROFROWS);
   END IF;

   IF NVL(A_NEXT_ROWS, 0) NOT IN (-1, 0, 1) THEN
      RETURN(UNAPIGEN.DBERR_NEXTROWS);
   END IF;

   
   IF A_NEXT_ROWS = -1 THEN
      IF DBMS_SQL.IS_OPEN(P_HS_CURSOR) THEN
         DBMS_SQL.CLOSE_CURSOR(P_HS_CURSOR);
      END IF;
      RETURN (UNAPIGEN.DBERR_SUCCESS);
   END IF;

   
   IF A_NEXT_ROWS = 1 THEN
      IF P_HS_CURSOR IS NULL THEN
         RETURN(UNAPIGEN.DBERR_NOCURSOR);
      END IF;
   END IF;

   
   IF NVL(A_NEXT_ROWS,0) = 0 THEN
      
      IF NVL(A_WHERE_CLAUSE, ' ') = ' ' THEN
         RETURN(UNAPIGEN.DBERR_WHERECLAUSE);
      ELSIF UPPER(SUBSTR(A_WHERE_CLAUSE,1,6)) <> 'WHERE ' THEN
         L_WHERE_CLAUSE := 'WHERE sd = ''' || REPLACE(A_WHERE_CLAUSE, '''', '''''') || 
                           ''' ORDER BY logdate DESC';
      ELSE
         L_WHERE_CLAUSE := A_WHERE_CLAUSE; 
      END IF;
      
      L_WHERE_CLAUSE := REPLACE(REPLACE(L_WHERE_CLAUSE, 
                                        'logdate DESC', 
                                        'logdate DESC, ROWID DESC'),
                                'LOGDATE DESC', 
                                'LOGDATE DESC, ROWID DESC');

      IF DBMS_SQL.IS_OPEN(P_HS_CURSOR) THEN
         DBMS_SQL.CLOSE_CURSOR(P_HS_CURSOR);
      END IF;
      P_HS_CURSOR := DBMS_SQL.OPEN_CURSOR;
      L_SQL_STRING := 'SELECT sd, who, who_description, what, what_description, logdate, why, tr_seq, ev_seq '||
                      'FROM dd' || UNAPIGEN.P_DD || '.uvsdhs ' ||
                      L_WHERE_CLAUSE;
      DBMS_SQL.PARSE(P_HS_CURSOR, L_SQL_STRING, DBMS_SQL.V7); 

      DBMS_SQL.DEFINE_COLUMN(P_HS_CURSOR, 1, L_SD, 20);
      DBMS_SQL.DEFINE_COLUMN(P_HS_CURSOR, 2, L_WHO, 20);
      DBMS_SQL.DEFINE_COLUMN(P_HS_CURSOR, 3, L_WHO_DESCRIPTION, 40);
      DBMS_SQL.DEFINE_COLUMN(P_HS_CURSOR, 4, L_WHAT, 60);
      DBMS_SQL.DEFINE_COLUMN(P_HS_CURSOR, 5, L_WHAT_DESCRIPTION, 255);
      DBMS_SQL.DEFINE_COLUMN(P_HS_CURSOR, 6, L_LOGDATE);
      DBMS_SQL.DEFINE_COLUMN(P_HS_CURSOR, 7, L_WHY, 255);
      DBMS_SQL.DEFINE_COLUMN(P_HS_CURSOR, 8, L_TR_SEQ);
      DBMS_SQL.DEFINE_COLUMN(P_HS_CURSOR, 9, L_EV_SEQ);
      L_RESULT := DBMS_SQL.EXECUTE(P_HS_CURSOR);
   END IF;

   L_RESULT := DBMS_SQL.FETCH_ROWS(P_HS_CURSOR);
   L_FETCHED_ROWS := 0;

   LOOP
      EXIT WHEN L_RESULT = 0 OR L_FETCHED_ROWS >= A_NR_OF_ROWS;
      DBMS_SQL.COLUMN_VALUE(P_HS_CURSOR, 1, L_SD);
      DBMS_SQL.COLUMN_VALUE(P_HS_CURSOR, 2, L_WHO);
      DBMS_SQL.COLUMN_VALUE(P_HS_CURSOR, 3, L_WHO_DESCRIPTION);
      DBMS_SQL.COLUMN_VALUE(P_HS_CURSOR, 4, L_WHAT);
      DBMS_SQL.COLUMN_VALUE(P_HS_CURSOR, 5, L_WHAT_DESCRIPTION);
      DBMS_SQL.COLUMN_VALUE(P_HS_CURSOR, 6, L_LOGDATE);
      DBMS_SQL.COLUMN_VALUE(P_HS_CURSOR, 7, L_WHY);
      DBMS_SQL.COLUMN_VALUE(P_HS_CURSOR, 8, L_TR_SEQ);
      DBMS_SQL.COLUMN_VALUE(P_HS_CURSOR, 9, L_EV_SEQ);
      L_FETCHED_ROWS := L_FETCHED_ROWS + 1;
      A_SD(L_FETCHED_ROWS) := L_SD;
      A_WHO(L_FETCHED_ROWS) := L_WHO;
      A_WHO_DESCRIPTION(L_FETCHED_ROWS) := L_WHO_DESCRIPTION;
      A_WHAT(L_FETCHED_ROWS) := L_WHAT;
      A_WHAT_DESCRIPTION(L_FETCHED_ROWS) := L_WHAT_DESCRIPTION;
      A_LOGDATE(L_FETCHED_ROWS) := TO_CHAR(L_LOGDATE);
      A_WHY(L_FETCHED_ROWS) := L_WHY;
      A_TR_SEQ(L_FETCHED_ROWS) := L_TR_SEQ;
      A_EV_SEQ(L_FETCHED_ROWS) := L_EV_SEQ;
      IF L_FETCHED_ROWS < A_NR_OF_ROWS THEN
         L_RESULT := DBMS_SQL.FETCH_ROWS(P_HS_CURSOR);
      END IF;
   END LOOP;

   IF L_FETCHED_ROWS = 0 THEN
      L_RET_CODE := UNAPIGEN.DBERR_NORECORDS;
      DBMS_SQL.CLOSE_CURSOR(P_HS_CURSOR);
   ELSIF L_FETCHED_ROWS < A_NR_OF_ROWS THEN
      A_NR_OF_ROWS := L_FETCHED_ROWS;
      L_RET_CODE := UNAPIGEN.DBERR_SUCCESS;
      DBMS_SQL.CLOSE_CURSOR(P_HS_CURSOR);
   ELSE
      L_RET_CODE := UNAPIGEN.DBERR_SUCCESS;
      A_NR_OF_ROWS := L_FETCHED_ROWS;
   END IF;
   RETURN(L_RET_CODE);

EXCEPTION
WHEN OTHERS THEN
   L_SQLERRM := SQLERRM;
   UNAPIGEN.U4ROLLBACK;
   INSERT INTO UTERROR(CLIENT_ID, APPLIC, WHO, LOGDATE, LOGDATE_TZ, API_NAME, ERROR_MSG)
   VALUES(UNAPIGEN.P_CLIENT_ID, UNAPIGEN.P_APPLIC_NAME, UNAPIGEN.P_USER, CURRENT_TIMESTAMP, CURRENT_TIMESTAMP, 
          'GetSdHistory', L_SQLERRM);
   UNAPIGEN.U4COMMIT;
   IF DBMS_SQL.IS_OPEN(P_HS_CURSOR) THEN
      DBMS_SQL.CLOSE_CURSOR(P_HS_CURSOR);
   END IF;
   RETURN(UNAPIGEN.DBERR_GENFAIL);
END GETSDHISTORY;

FUNCTION GETSDHISTORYDETAILS 
(A_SD                OUT     UNAPIGEN.VC20_TABLE_TYPE,    
 A_TR_SEQ            OUT     UNAPIGEN.NUM_TABLE_TYPE,     
 A_EV_SEQ            OUT     UNAPIGEN.NUM_TABLE_TYPE,     
 A_SEQ               OUT     UNAPIGEN.NUM_TABLE_TYPE,     
 A_DETAILS           OUT     UNAPIGEN.VC4000_TABLE_TYPE,  
 A_NR_OF_ROWS        IN OUT  NUMBER,                      
 A_WHERE_CLAUSE      IN      VARCHAR2,                    
 A_NEXT_ROWS         IN      NUMBER)                      
RETURN NUMBER IS

L_SD                      VARCHAR2(20);
L_TR_SEQ                  NUMBER;
L_EV_SEQ                  NUMBER;
L_SEQ                     NUMBER;
L_DETAILS                 VARCHAR2(4000);

BEGIN

   IF NVL(A_NR_OF_ROWS,0) = 0 THEN
      A_NR_OF_ROWS := UNAPIGEN.P_DEFAULT_CHUNK_SIZE;
   ELSIF A_NR_OF_ROWS < 0 OR A_NR_OF_ROWS > UNAPIGEN.P_MAX_CHUNK_SIZE THEN
      RETURN(UNAPIGEN.DBERR_NROFROWS);
   END IF;

   IF NVL(A_NEXT_ROWS, 0) NOT IN (-1, 0, 1) THEN
      RETURN(UNAPIGEN.DBERR_NEXTROWS);
   END IF;

   
   IF A_NEXT_ROWS = -1 THEN
      IF DBMS_SQL.IS_OPEN(P_HS_DETAILS_CURSOR) THEN
         DBMS_SQL.CLOSE_CURSOR(P_HS_DETAILS_CURSOR);
      END IF;
      RETURN (UNAPIGEN.DBERR_SUCCESS);
   END IF;

   
   IF A_NEXT_ROWS = 1 THEN
      IF P_HS_DETAILS_CURSOR IS NULL THEN
         RETURN(UNAPIGEN.DBERR_NOCURSOR);
      END IF;
   END IF;

   
   IF NVL(A_NEXT_ROWS,0) = 0 THEN

      
      IF NVL(A_WHERE_CLAUSE, ' ') = ' ' THEN
         RETURN(UNAPIGEN.DBERR_WHERECLAUSE);
      ELSIF UPPER(SUBSTR(A_WHERE_CLAUSE,1,6)) <> 'WHERE ' THEN
         L_WHERE_CLAUSE := 'WHERE sd = ''' || REPLACE(A_WHERE_CLAUSE, '''', '''''') || 
                           ''' ORDER BY tr_seq DESC, ev_seq DESC, seq DESC';
      ELSE
         L_WHERE_CLAUSE := A_WHERE_CLAUSE; 
      END IF;

      IF DBMS_SQL.IS_OPEN(P_HS_DETAILS_CURSOR) THEN
         DBMS_SQL.CLOSE_CURSOR(P_HS_DETAILS_CURSOR);
      END IF;
      P_HS_DETAILS_CURSOR := DBMS_SQL.OPEN_CURSOR;
      L_SQL_STRING := 'SELECT sd, tr_seq, ev_seq, seq, details '||
                      'FROM dd' || UNAPIGEN.P_DD || '.uvsdhsdetails ' ||
                      L_WHERE_CLAUSE;
      DBMS_SQL.PARSE(P_HS_DETAILS_CURSOR, L_SQL_STRING, DBMS_SQL.V7); 
      DBMS_SQL.DEFINE_COLUMN(P_HS_DETAILS_CURSOR, 1, L_SD, 20);
      DBMS_SQL.DEFINE_COLUMN(P_HS_DETAILS_CURSOR, 2, L_TR_SEQ);
      DBMS_SQL.DEFINE_COLUMN(P_HS_DETAILS_CURSOR, 3, L_EV_SEQ);
      DBMS_SQL.DEFINE_COLUMN(P_HS_DETAILS_CURSOR, 4, L_SEQ);
      DBMS_SQL.DEFINE_COLUMN(P_HS_DETAILS_CURSOR, 5, L_DETAILS, 4000);   
      L_RESULT := DBMS_SQL.EXECUTE(P_HS_DETAILS_CURSOR);
   END IF;
   
   L_RESULT := DBMS_SQL.FETCH_ROWS(P_HS_DETAILS_CURSOR);
   L_FETCHED_ROWS := 0;

   LOOP
      EXIT WHEN L_RESULT = 0 OR L_FETCHED_ROWS >= A_NR_OF_ROWS;
      DBMS_SQL.COLUMN_VALUE(P_HS_DETAILS_CURSOR, 1, L_SD);
      DBMS_SQL.COLUMN_VALUE(P_HS_DETAILS_CURSOR, 2, L_TR_SEQ);
      DBMS_SQL.COLUMN_VALUE(P_HS_DETAILS_CURSOR, 3, L_EV_SEQ);
      DBMS_SQL.COLUMN_VALUE(P_HS_DETAILS_CURSOR, 4, L_SEQ);
      DBMS_SQL.COLUMN_VALUE(P_HS_DETAILS_CURSOR, 5, L_DETAILS);
      L_FETCHED_ROWS := L_FETCHED_ROWS + 1;
      A_SD(L_FETCHED_ROWS)      := L_SD;
      A_TR_SEQ(L_FETCHED_ROWS)  := L_TR_SEQ;
      A_EV_SEQ(L_FETCHED_ROWS)  := L_EV_SEQ;
      A_SEQ(L_FETCHED_ROWS)     := L_SEQ;
      A_DETAILS(L_FETCHED_ROWS) := L_DETAILS;
      IF L_FETCHED_ROWS < A_NR_OF_ROWS THEN
         L_RESULT := DBMS_SQL.FETCH_ROWS(P_HS_DETAILS_CURSOR);
      END IF;
   END LOOP;

   IF L_FETCHED_ROWS = 0 THEN
      L_RET_CODE := UNAPIGEN.DBERR_NORECORDS;
      DBMS_SQL.CLOSE_CURSOR(P_HS_DETAILS_CURSOR);
   ELSIF L_FETCHED_ROWS < A_NR_OF_ROWS THEN
      A_NR_OF_ROWS := L_FETCHED_ROWS;
      L_RET_CODE := UNAPIGEN.DBERR_SUCCESS;
      DBMS_SQL.CLOSE_CURSOR(P_HS_DETAILS_CURSOR);
   ELSE
      L_RET_CODE := UNAPIGEN.DBERR_SUCCESS;
      A_NR_OF_ROWS := L_FETCHED_ROWS;
   END IF;
   RETURN(L_RET_CODE);

EXCEPTION
   WHEN OTHERS THEN
      L_SQLERRM := SQLERRM;
      UNAPIGEN.U4ROLLBACK;
      INSERT INTO UTERROR(CLIENT_ID, APPLIC, WHO, LOGDATE, LOGDATE_TZ, API_NAME, ERROR_MSG)
      VALUES(UNAPIGEN.P_CLIENT_ID, UNAPIGEN.P_APPLIC_NAME, UNAPIGEN.P_USER, CURRENT_TIMESTAMP, CURRENT_TIMESTAMP,
             'GetSdHistoryDetails', L_SQLERRM);
      UNAPIGEN.U4COMMIT;
      IF DBMS_SQL.IS_OPEN(P_HS_DETAILS_CURSOR) THEN
         DBMS_SQL.CLOSE_CURSOR(P_HS_DETAILS_CURSOR);
      END IF;
      RETURN(UNAPIGEN.DBERR_GENFAIL);
END GETSDHISTORYDETAILS;

FUNCTION SAVESDHISTORY
(A_SD          IN        VARCHAR2,                  
 A_WHO               IN     UNAPIGEN.VC20_TABLE_TYPE,   
 A_WHO_DESCRIPTION   IN     UNAPIGEN.VC40_TABLE_TYPE,   
 A_WHAT              IN     UNAPIGEN.VC60_TABLE_TYPE,   
 A_WHAT_DESCRIPTION  IN     UNAPIGEN.VC255_TABLE_TYPE,  
 A_LOGDATE           IN     UNAPIGEN.DATE_TABLE_TYPE,   
 A_WHY               IN     UNAPIGEN.VC255_TABLE_TYPE,  
 A_TR_SEQ            IN     UNAPIGEN.NUM_TABLE_TYPE,    
 A_EV_SEQ            IN     UNAPIGEN.NUM_TABLE_TYPE,    
 A_NR_OF_ROWS  IN        NUMBER)                    
RETURN NUMBER IS

L_ALLOW_MODIFY      CHAR(1);
L_LOG_HS            CHAR(1);
L_LOG_HS_DETAILS    CHAR(1);
L_ACTIVE            CHAR(1);
L_LC                VARCHAR2(2);
L_LC_VERSION        VARCHAR2(20);
L_SS                VARCHAR2(2);
L_PT_VERSION        VARCHAR2(20);

BEGIN

   IF UNAPIGEN.BEGINTXN(UNAPIGEN.P_SINGLE_API_TXN) <>
      UNAPIGEN.DBERR_SUCCESS THEN
      RAISE STPERROR;
   END IF;

   IF NVL(A_NR_OF_ROWS, -1) < 0 THEN
      UNAPIGEN.P_TXN_ERROR := UNAPIGEN.DBERR_NROFROWS;
      RAISE STPERROR;
   END IF;

   IF NVL(A_SD, ' ') = ' ' THEN
      UNAPIGEN.P_TXN_ERROR := UNAPIGEN.DBERR_NOOBJID;
      RAISE STPERROR;
   END IF;

   L_RET_CODE := UNAPIAUT.GETSDAUTHORISATION(A_SD, L_PT_VERSION, L_LC, L_LC_VERSION, L_SS,
                                             L_ALLOW_MODIFY, L_ACTIVE, L_LOG_HS, L_LOG_HS_DETAILS);
   IF L_RET_CODE <> UNAPIGEN.DBERR_SUCCESS THEN
      UNAPIGEN.P_TXN_ERROR := L_RET_CODE;
      RAISE STPERROR;
   END IF;

   FOR L_SEQ_NO IN 1..A_NR_OF_ROWS LOOP
      UPDATE UTSDHS
      SET WHY = A_WHY(L_SEQ_NO)
      WHERE SD = A_SD
         AND WHO = A_WHO(L_SEQ_NO)
         AND WHO_DESCRIPTION = A_WHO_DESCRIPTION(L_SEQ_NO)
         AND TO_CHAR(LOGDATE) = A_LOGDATE(L_SEQ_NO)
         AND WHAT = A_WHAT(L_SEQ_NO)
         AND WHAT_DESCRIPTION = A_WHAT_DESCRIPTION(L_SEQ_NO)
         AND TR_SEQ = A_TR_SEQ(L_SEQ_NO)
         AND EV_SEQ = A_EV_SEQ(L_SEQ_NO);

      IF SQL%ROWCOUNT = 0 THEN
         UNAPIGEN.P_TXN_ERROR := UNAPIGEN.DBERR_NOTFOUND;
         RAISE STPERROR;
      END IF;
   END LOOP;

   IF UNAPIGEN.ENDTXN <> UNAPIGEN.DBERR_SUCCESS THEN
      RAISE STPERROR;
   END IF;

   RETURN(UNAPIGEN.DBERR_SUCCESS);

EXCEPTION
WHEN OTHERS THEN
   IF SQLCODE <> 1 THEN
      UNAPIGEN.LOGERROR('SaveSdHistory', SQLERRM);
   END IF;
   RETURN(UNAPIGEN.ABORTTXN(UNAPIGEN.P_TXN_ERROR, 'SaveSdHistory'));
END SAVESDHISTORY;

FUNCTION GETSDGROUPKEY
(A_SD              OUT    UNAPIGEN.VC20_TABLE_TYPE,  
 A_GK              OUT    UNAPIGEN.VC20_TABLE_TYPE,  
 A_GK_VERSION      OUT    UNAPIGEN.VC20_TABLE_TYPE,  
 A_VALUE           OUT    UNAPIGEN.VC40_TABLE_TYPE,  
 A_DESCRIPTION     OUT    UNAPIGEN.VC40_TABLE_TYPE,  
 A_IS_PROTECTED    OUT    UNAPIGEN.CHAR1_TABLE_TYPE, 
 A_VALUE_UNIQUE    OUT    UNAPIGEN.CHAR1_TABLE_TYPE, 
 A_SINGLE_VALUED   OUT    UNAPIGEN.CHAR1_TABLE_TYPE, 
 A_NEW_VAL_ALLOWED OUT    UNAPIGEN.CHAR1_TABLE_TYPE, 
 A_MANDATORY       OUT    UNAPIGEN.CHAR1_TABLE_TYPE, 
 A_VALUE_LIST_TP   OUT    UNAPIGEN.CHAR1_TABLE_TYPE, 
 A_DSP_ROWS        OUT    UNAPIGEN.NUM_TABLE_TYPE,   
 A_NR_OF_ROWS      IN OUT NUMBER,                    
 A_WHERE_CLAUSE    IN     VARCHAR2)                  
RETURN NUMBER IS

L_SD                             VARCHAR2(20);
L_GK                             VARCHAR2(20);
L_GK_VERSION                     VARCHAR2(20);
L_VALUE                          VARCHAR2(40);
L_DESCRIPTION                    VARCHAR2(40);
L_IS_PROTECTED                   CHAR(1);
L_VALUE_UNIQUE                   CHAR(1);
L_SINGLE_VALUED                  CHAR(1);
L_NEW_VAL_ALLOWED                CHAR(1);
L_MANDATORY                      CHAR(1);
L_VALUE_LIST_TP                  CHAR(1);
L_DSP_ROWS                       NUMBER(3);
L_BIND_SD_SELECTION              BOOLEAN;
L_BIND_FIXED_SD_FLAG             BOOLEAN;
L_GKDEF_REC                      UNAPIGK.GKDEFINITIONREC;
L_TEMP_RET_CODE                  INTEGER;

BEGIN

   L_BIND_SD_SELECTION := FALSE;
   L_BIND_FIXED_SD_FLAG := FALSE;
   IF NVL(A_NR_OF_ROWS,0) = 0 THEN
      A_NR_OF_ROWS := UNAPIGEN.P_DEFAULT_CHUNK_SIZE;
   ELSIF A_NR_OF_ROWS < 0 OR A_NR_OF_ROWS > UNAPIGEN.P_MAX_CHUNK_SIZE THEN
      RETURN(UNAPIGEN.DBERR_NROFROWS);
   END IF;

   IF NVL(A_WHERE_CLAUSE, ' ') = ' ' THEN
      RETURN(UNAPIGEN.DBERR_WHERECLAUSE);
   ELSIF A_WHERE_CLAUSE = 'SELECTION' THEN
      IF UNAPISD.P_SELECTION_CLAUSE IS NOT NULL THEN 
         IF INSTR(UPPER(UNAPISD.P_SELECTION_CLAUSE), ' WHERE ') <> 0 THEN       
            L_WHERE_CLAUSE := ','||UNAPISD.P_SELECTION_CLAUSE|| ' AND a.sd = gk.sd ORDER BY gk.sd,gk.gkseq'; 
         ELSE
            L_WHERE_CLAUSE := ','||UNAPISD.P_SELECTION_CLAUSE|| ' WHERE a.sd = gk.sd ORDER BY gk.sd,gk.gkseq'; 
         END IF;
         L_BIND_SD_SELECTION := TRUE;
      ELSE
         L_WHERE_CLAUSE := 'ORDER BY gk.sd, gk.gkseq'; 
      END IF;      
   ELSIF UPPER(SUBSTR(A_WHERE_CLAUSE,1,6)) <> 'WHERE ' THEN
      L_BIND_FIXED_SD_FLAG := TRUE;
      L_WHERE_CLAUSE := 'WHERE gk.sd = :sd_val ORDER BY gk.gkseq';
   ELSE
      L_WHERE_CLAUSE := A_WHERE_CLAUSE; 
   END IF;

   IF NOT DBMS_SQL.IS_OPEN(L_SDGK_CURSOR) THEN
      
      L_TEMP_RET_CODE := UNAPIGK.INITGROUPKEYDEFBUFFER('sd');
      IF L_TEMP_RET_CODE <> UNAPIGEN.DBERR_SUCCESS THEN
         RAISE_APPLICATION_ERROR(-20000, 'InitGroupKeyDefBuffer failed with ret_code='||L_TEMP_RET_CODE||' for a_gk_tp=sd');   
      END IF;
      L_SDGK_CURSOR := DBMS_SQL.OPEN_CURSOR;
      L_SQL_STRING := 'SELECT gk.sd, gk.gk, gk.gk_version, gk.value ' ||
                      'FROM dd'|| UNAPIGEN.P_DD ||'.uvsdgk gk '|| L_WHERE_CLAUSE;

      DBMS_SQL.PARSE(L_SDGK_CURSOR, L_SQL_STRING, DBMS_SQL.V7); 
      IF L_BIND_SD_SELECTION THEN
         FOR L_X IN 1..UNAPISD.P_SELECTION_VAL_TAB.COUNT() LOOP
            DBMS_SQL.BIND_VARIABLE(L_SDGK_CURSOR, ':col_val'||L_X , UNAPISD.P_SELECTION_VAL_TAB(L_X)); 
         END LOOP;
      ELSIF L_BIND_FIXED_SD_FLAG THEN
         DBMS_SQL.BIND_VARIABLE(L_SDGK_CURSOR, ':sd_val' , A_WHERE_CLAUSE); 
      END IF;

      DBMS_SQL.DEFINE_COLUMN(L_SDGK_CURSOR, 1, L_SD, 20);
      DBMS_SQL.DEFINE_COLUMN(L_SDGK_CURSOR, 2, L_GK, 20);
      DBMS_SQL.DEFINE_COLUMN(L_SDGK_CURSOR, 3, L_GK_VERSION, 20);
      DBMS_SQL.DEFINE_COLUMN(L_SDGK_CURSOR, 4, L_VALUE, 40);
      L_RESULT := DBMS_SQL.EXECUTE(L_SDGK_CURSOR);
   END IF;
   
   L_RESULT := DBMS_SQL.FETCH_ROWS(L_SDGK_CURSOR);
   L_FETCHED_ROWS := 0;

   LOOP
      EXIT WHEN L_RESULT = 0 OR L_FETCHED_ROWS >= A_NR_OF_ROWS;

      DBMS_SQL.COLUMN_VALUE(L_SDGK_CURSOR, 1, L_SD);
      DBMS_SQL.COLUMN_VALUE(L_SDGK_CURSOR, 2, L_GK);
      DBMS_SQL.COLUMN_VALUE(L_SDGK_CURSOR, 3, L_GK_VERSION);
      DBMS_SQL.COLUMN_VALUE(L_SDGK_CURSOR, 4, L_VALUE);

      L_FETCHED_ROWS := L_FETCHED_ROWS + 1;
      A_SD(L_FETCHED_ROWS) := L_SD;
      A_GK(L_FETCHED_ROWS) := L_GK;
      A_GK_VERSION(L_FETCHED_ROWS) := L_GK_VERSION;
      A_VALUE(L_FETCHED_ROWS) := L_VALUE;

      
      BEGIN
         L_GKDEF_REC := UNAPIGK.P_GK_DEF_BUFFER(L_GK);
         A_DESCRIPTION(L_FETCHED_ROWS) := L_GKDEF_REC.DESCRIPTION;
         A_IS_PROTECTED(L_FETCHED_ROWS) := L_GKDEF_REC.IS_PROTECTED;
         A_VALUE_UNIQUE(L_FETCHED_ROWS) := L_GKDEF_REC.VALUE_UNIQUE;
         A_SINGLE_VALUED(L_FETCHED_ROWS) := L_GKDEF_REC.SINGLE_VALUED;
         A_NEW_VAL_ALLOWED(L_FETCHED_ROWS) := L_GKDEF_REC.NEW_VAL_ALLOWED;
         A_MANDATORY(L_FETCHED_ROWS) := L_GKDEF_REC.MANDATORY;
         A_VALUE_LIST_TP(L_FETCHED_ROWS) := L_GKDEF_REC.VALUE_LIST_TP;
         A_DSP_ROWS(L_FETCHED_ROWS) := L_GKDEF_REC.DSP_ROWS;
      EXCEPTION
      WHEN NO_DATA_FOUND THEN
         
         
         
         A_DESCRIPTION(L_FETCHED_ROWS)     := L_GK;
         A_IS_PROTECTED(L_FETCHED_ROWS)    := '1';
         A_VALUE_UNIQUE(L_FETCHED_ROWS)    := '0';
         A_SINGLE_VALUED(L_FETCHED_ROWS)   := '1';
         A_NEW_VAL_ALLOWED(L_FETCHED_ROWS) := '1';
         A_MANDATORY(L_FETCHED_ROWS)       := '0';
         A_VALUE_LIST_TP(L_FETCHED_ROWS)   := 'F';
         A_DSP_ROWS(L_FETCHED_ROWS)        := 10;      
      END;

      IF L_FETCHED_ROWS < A_NR_OF_ROWS THEN
         L_RESULT := DBMS_SQL.FETCH_ROWS(L_SDGK_CURSOR);
      END IF;
   END LOOP;

   IF L_FETCHED_ROWS = 0 THEN
      L_RET_CODE := UNAPIGEN.DBERR_NORECORDS;
      DBMS_SQL.CLOSE_CURSOR(L_SDGK_CURSOR);
   ELSIF L_FETCHED_ROWS < A_NR_OF_ROWS THEN
      L_RET_CODE := UNAPIGEN.DBERR_SUCCESS;
      A_NR_OF_ROWS := L_FETCHED_ROWS;
      DBMS_SQL.CLOSE_CURSOR(L_SDGK_CURSOR);
   ELSE   
      L_RET_CODE := UNAPIGEN.DBERR_SUCCESS;
      A_NR_OF_ROWS := L_FETCHED_ROWS;
   END IF;

   IF A_WHERE_CLAUSE <> 'SELECTION' AND
      DBMS_SQL.IS_OPEN(L_SDGK_CURSOR) THEN
      DBMS_SQL.CLOSE_CURSOR(L_SDGK_CURSOR);
   END IF;

   IF NOT DBMS_SQL.IS_OPEN(L_SDGK_CURSOR) THEN
      L_TEMP_RET_CODE := UNAPIGK.CLOSEGROUPKEYDEFBUFFER('sd');
      IF L_TEMP_RET_CODE <> UNAPIGEN.DBERR_SUCCESS THEN
         RAISE_APPLICATION_ERROR(-20000, 'CloseGroupKeyDefBuffer failed with ret_code='||L_TEMP_RET_CODE||' for a_gk_tp=sd');   
      END IF;
   END IF;

   RETURN(L_RET_CODE);

EXCEPTION
   WHEN OTHERS THEN
      L_SQLERRM := SQLERRM;
      UNAPIGEN.U4ROLLBACK;
      INSERT INTO UTERROR(CLIENT_ID, APPLIC, WHO, LOGDATE, LOGDATE_TZ, API_NAME, ERROR_MSG)
      VALUES(UNAPIGEN.P_CLIENT_ID, UNAPIGEN.P_APPLIC_NAME, UNAPIGEN.P_USER, CURRENT_TIMESTAMP, CURRENT_TIMESTAMP,
             'GetSdGroupKey', L_SQLERRM);
      UNAPIGEN.U4COMMIT;
      IF DBMS_SQL.IS_OPEN(L_SDGK_CURSOR) THEN
         DBMS_SQL.CLOSE_CURSOR(L_SDGK_CURSOR);
      END IF;
      L_RET_CODE := UNAPIGK.CLOSEGROUPKEYDEFBUFFER('sd');
      
      RETURN(UNAPIGEN.DBERR_GENFAIL);
END GETSDGROUPKEY;

FUNCTION SAVESDGROUPKEY
(A_SD              IN     VARCHAR2,                   
 A_GK              IN     UNAPIGEN.VC20_TABLE_TYPE,   
 A_GK_VERSION      IN OUT UNAPIGEN.VC20_TABLE_TYPE,   
 A_VALUE           IN     UNAPIGEN.VC40_TABLE_TYPE,   
 A_NR_OF_ROWS      IN     NUMBER,                     
 A_MODIFY_REASON   IN     VARCHAR2)                   
RETURN NUMBER IS

L_LC                VARCHAR2(2);
L_LC_VERSION        VARCHAR2(20);
L_SS                VARCHAR2(2);
L_LOG_HS            CHAR(1);
L_LOG_HS_DETAILS    CHAR(1);
L_ALLOW_MODIFY      CHAR(1);
L_ACTIVE            CHAR(1);
L_SD_CURSOR         NUMBER;
L_NEW_SEQ           NUMBER;
L_GK_HANDLE         BOOLEAN_TABLE_TYPE;
L_GK_FOUND          BOOLEAN;
L_WHAT_DESCRIPTION  VARCHAR2(255);
L_HS_SEQ            INTEGER;
L_PT_VERSION        VARCHAR2(20);
L_SKIP              BOOLEAN;

TABLE_DOES_NOT_EXIST EXCEPTION;
PRAGMA EXCEPTION_INIT (TABLE_DOES_NOT_EXIST, -942);

CURSOR L_GK_CURSOR IS
SELECT GK, VALUE
FROM UTSDGK
WHERE SD = A_SD
ORDER BY GKSEQ;

BEGIN

   IF UNAPIGEN.BEGINTXN(UNAPIGEN.P_SINGLE_API_TXN) <>
      UNAPIGEN.DBERR_SUCCESS THEN
      RAISE STPERROR;
   END IF;

   IF NVL(A_NR_OF_ROWS, -1) < 0 THEN
      UNAPIGEN.P_TXN_ERROR := UNAPIGEN.DBERR_NROFROWS;
      RAISE STPERROR;
   END IF;

   IF NVL(A_SD, ' ') = ' ' THEN
      UNAPIGEN.P_TXN_ERROR := UNAPIGEN.DBERR_NOOBJID;
      RAISE STPERROR;
   END IF;

   FOR L_SEQ_NO IN 1..A_NR_OF_ROWS LOOP
      IF NVL(A_GK(L_SEQ_NO), ' ') = ' ' THEN
         UNAPIGEN.P_TXN_ERROR := UNAPIGEN.DBERR_NOOBJID;
         RAISE STPERROR;
      END IF;
      L_GK_HANDLE(L_SEQ_NO) := TRUE;
   END LOOP;

   L_RET_CODE := UNAPIAUT.GETSDAUTHORISATION(A_SD, L_PT_VERSION, L_LC, L_LC_VERSION, L_SS,
                                             L_ALLOW_MODIFY, L_ACTIVE, L_LOG_HS, L_LOG_HS_DETAILS);
   IF L_RET_CODE <> UNAPIGEN.DBERR_SUCCESS THEN
      UNAPIGEN.P_TXN_ERROR := L_RET_CODE;
      RAISE STPERROR;
   END IF;

   UPDATE UTSD
   SET ALLOW_MODIFY = '#'
   WHERE SD = A_SD;

   IF SQL%ROWCOUNT < 1 THEN
      UNAPIGEN.P_TXN_ERROR := UNAPIGEN.DBERR_NOOBJECT;
      RAISE STPERROR ;
   END IF;

   
   
   
   L_EVENT_TP := 'SdGroupKeyUpdated';
   L_EV_SEQ_NR := -1;
   L_EV_DETAILS := 'pt_version=' || L_PT_VERSION;
   L_RESULT := UNAPIEV.INSERTEVENT('SaveSdGroupKey', UNAPIGEN.P_EVMGR_NAME, 'sd', A_SD, 
                                   L_LC, L_LC_VERSION, L_SS, L_EVENT_TP, L_EV_DETAILS, L_EV_SEQ_NR);
   IF L_RESULT <> UNAPIGEN.DBERR_SUCCESS THEN
      UNAPIGEN.P_TXN_ERROR := L_RESULT;
      RAISE STPERROR;
   END IF;

   IF (L_LOG_HS = '1') AND (UNAPIGEN.P_LOG_GK_HS = '1') THEN
      INSERT INTO UTSDHS(SD, WHO, WHO_DESCRIPTION, WHAT, WHAT_DESCRIPTION, LOGDATE, LOGDATE_TZ, WHY, TR_SEQ, EV_SEQ)
      VALUES(A_SD, UNAPIGEN.P_USER, UNAPIGEN.P_USER_DESCRIPTION, L_EVENT_TP, 
             'study "'||A_SD||'" group keys are updated.', 
             CURRENT_TIMESTAMP, CURRENT_TIMESTAMP, A_MODIFY_REASON, UNAPIGEN.P_TR_SEQ, L_EV_SEQ_NR);
   END IF;

   L_HS_SEQ := 0;
   IF L_LOG_HS_DETAILS = '1' THEN
      L_HS_SEQ := L_HS_SEQ + 1;
      INSERT INTO UTSDHSDETAILS(SD, TR_SEQ, EV_SEQ, SEQ, DETAILS)
      VALUES(A_SD, UNAPIGEN.P_TR_SEQ, L_EV_SEQ_NR, L_HS_SEQ, 
             'study "'||A_SD||'" group keys are updated.');
   END IF;

   L_SD_CURSOR := DBMS_SQL.OPEN_CURSOR;
   FOR L_SDGK IN L_GK_CURSOR LOOP
      L_GK_FOUND := FALSE;
      FOR L_SEQ_NO IN 1..A_NR_OF_ROWS LOOP
         
         IF L_SDGK.GK = A_GK(L_SEQ_NO) AND
            (L_SDGK.VALUE = A_VALUE(L_SEQ_NO) OR
             (L_SDGK.VALUE IS NULL AND A_VALUE(L_SEQ_NO) IS NULL)) THEN
            L_GK_HANDLE(L_SEQ_NO) := FALSE;
            L_GK_FOUND := TRUE;
            EXIT;
         END IF;
      END LOOP;

      IF NOT L_GK_FOUND THEN
         
         DELETE FROM UTSDGK
         WHERE SD = A_SD
            AND GK = L_SDGK.GK
            AND VALUE = L_SDGK.VALUE;

         IF L_SDGK.VALUE IS NULL THEN
            
            DELETE FROM UTSDGK
            WHERE SD = A_SD
              AND GK = L_SDGK.GK
              AND VALUE IS NULL;
         END IF;            

         IF (L_LOG_HS_DETAILS = '1') THEN
            L_WHAT_DESCRIPTION := 'Groupkey "'||L_SDGK.GK||'" with value "'||L_SDGK.VALUE||'" is removed from study "'||A_SD||'".';
            L_HS_SEQ := L_HS_SEQ + 1;
            INSERT INTO UTSDHSDETAILS(SD, TR_SEQ, EV_SEQ, SEQ, DETAILS)
            VALUES(A_SD, UNAPIGEN.P_TR_SEQ, L_EV_SEQ_NR, L_HS_SEQ, L_WHAT_DESCRIPTION);
         END IF;   

         L_SQL_STRING := 'DELETE FROM utsdgk' || L_SDGK.GK ||
                         ' WHERE sd = :sd AND ' || 
                         L_SDGK.GK || ' = :value '; 
         BEGIN
            DBMS_SQL.PARSE(L_SD_CURSOR, L_SQL_STRING, DBMS_SQL.V7); 
            DBMS_SQL.BIND_VARIABLE(L_SD_CURSOR, ':sd' , A_SD); 
            DBMS_SQL.BIND_VARIABLE(L_SD_CURSOR, ':value' , L_SDGK.VALUE); 
            L_RESULT := DBMS_SQL.EXECUTE(L_SD_CURSOR);
         EXCEPTION
         WHEN TABLE_DOES_NOT_EXIST THEN
            
            
            NULL;
         END;
      END IF;
   END LOOP;

   FOR L_SEQ_NO IN 1..A_NR_OF_ROWS LOOP
      IF L_GK_HANDLE(L_SEQ_NO) THEN
         L_SKIP := FALSE; 
         IF NVL(A_VALUE(L_SEQ_NO), ' ') <> ' ' THEN
            L_SQL_STRING := 'INSERT INTO utsdgk' || A_GK(L_SEQ_NO) ||
                            ' ('||A_GK(L_SEQ_NO)||', sd)'||
                            ' VALUES (:value, :sd) ';  
            BEGIN
               DBMS_SQL.PARSE(L_SD_CURSOR, L_SQL_STRING, DBMS_SQL.V7); 
               DBMS_SQL.BIND_VARIABLE(L_SD_CURSOR, ':value' , A_VALUE(L_SEQ_NO)); 
               DBMS_SQL.BIND_VARIABLE(L_SD_CURSOR, ':sd' , A_SD); 
               L_RESULT := DBMS_SQL.EXECUTE(L_SD_CURSOR);
            EXCEPTION
            WHEN TABLE_DOES_NOT_EXIST THEN
               
               
               NULL;
            WHEN DUP_VAL_ON_INDEX THEN
               L_SKIP := TRUE;
            END;
         END IF;

         IF NOT L_SKIP THEN
            
            INSERT INTO UTSDGK(SD, GK, GKSEQ, VALUE)
            SELECT A_SD, A_GK(L_SEQ_NO), NVL(MAX(GKSEQ), 0)+1,
                   A_VALUE(L_SEQ_NO)
            FROM UTSDGK
            WHERE SD =A_SD;

            IF (L_LOG_HS_DETAILS = '1') THEN
               L_WHAT_DESCRIPTION := 'Groupkey "'||A_GK(L_SEQ_NO)||'" is added to study "'||A_SD||'", value is "'||A_VALUE(L_SEQ_NO)||'".';
               L_HS_SEQ := L_HS_SEQ + 1;
               INSERT INTO UTSDHSDETAILS(SD, TR_SEQ, EV_SEQ, SEQ, DETAILS)
               VALUES(A_SD, UNAPIGEN.P_TR_SEQ, L_EV_SEQ_NR, L_HS_SEQ, L_WHAT_DESCRIPTION);
            END IF;   
         END IF;   
      END IF;
   END LOOP;

   DBMS_SQL.CLOSE_CURSOR(L_SD_CURSOR);

   IF UNAPIGEN.ENDTXN <> UNAPIGEN.DBERR_SUCCESS THEN
      RAISE STPERROR;
   END IF;

   RETURN(UNAPIGEN.DBERR_SUCCESS);

EXCEPTION
WHEN DUP_VAL_ON_INDEX THEN
   IF DBMS_SQL.IS_OPEN(L_SD_CURSOR) THEN
      DBMS_SQL.CLOSE_CURSOR(L_SD_CURSOR);
   END IF;
   UNAPIGEN.P_TXN_ERROR := UNAPIGEN.DBERR_UNIQUEGK;
   
   
   L_RESULT := UNAPIGEN.ENDTXN; 
   RETURN(UNAPIGEN.P_TXN_ERROR);
WHEN OTHERS THEN
   IF SQLCODE <> 1 THEN
      UNAPIGEN.LOGERROR('SaveSdGroupKey', SQLERRM);
   ELSIF L_SQLERRM IS NOT NULL THEN
      UNAPIGEN.LOGERROR('SaveSdGroupKey', L_SQLERRM);
   END IF;
   IF DBMS_SQL.IS_OPEN(L_SD_CURSOR) THEN
      DBMS_SQL.CLOSE_CURSOR(L_SD_CURSOR);
   END IF;
   RETURN(UNAPIGEN.ABORTTXN(UNAPIGEN.P_TXN_ERROR, 'SaveSdGroupKey'));
END SAVESDGROUPKEY;

FUNCTION SAVE1SDGROUPKEY
(A_SD              IN         VARCHAR2,                   
 A_GK              IN         VARCHAR2,                   
 A_GK_VERSION      IN OUT     VARCHAR2,                   
 A_VALUE           IN         UNAPIGEN.VC40_TABLE_TYPE,   
 A_NR_OF_ROWS      IN         NUMBER,                     
 A_MODIFY_REASON   IN         VARCHAR2)                   
RETURN NUMBER IS

L_LC                VARCHAR2(2);
L_LC_VERSION        VARCHAR2(20);
L_SS                VARCHAR2(2);
L_LOG_HS            CHAR(1);
L_LOG_HS_DETAILS    CHAR(1);
L_ALLOW_MODIFY      CHAR(1);
L_ACTIVE            CHAR(1);
L_SD_CURSOR         NUMBER;
L_NEW_SEQ           NUMBER;
L_GK_HANDLE         BOOLEAN_TABLE_TYPE;
L_GK_FOUND          BOOLEAN;
L_LAST_SEQ          INTEGER;
L_WHAT_DESCRIPTION  VARCHAR2(255);
L_HS_SEQ            INTEGER;
L_PT_VERSION        VARCHAR2(20);
L_SKIP              BOOLEAN;

TABLE_DOES_NOT_EXIST EXCEPTION;
PRAGMA EXCEPTION_INIT (TABLE_DOES_NOT_EXIST, -942);

CURSOR L_GK_CURSOR IS
SELECT VALUE, GKSEQ
FROM UTSDGK
WHERE SD = A_SD
AND GK = A_GK
ORDER BY GKSEQ;

BEGIN

   IF UNAPIGEN.BEGINTXN(UNAPIGEN.P_SINGLE_API_TXN) <>
      UNAPIGEN.DBERR_SUCCESS THEN
      RAISE STPERROR;
   END IF;

   IF NVL(A_NR_OF_ROWS, -1) < 0 THEN
      UNAPIGEN.P_TXN_ERROR := UNAPIGEN.DBERR_NROFROWS;
      RAISE STPERROR;
   END IF;

   IF NVL(A_SD, ' ') = ' ' OR NVL(A_GK, ' ') = ' ' THEN
      UNAPIGEN.P_TXN_ERROR := UNAPIGEN.DBERR_NOOBJID;
      RAISE STPERROR;
   END IF;

   FOR L_SEQ_NO IN 1..A_NR_OF_ROWS LOOP
      L_GK_HANDLE(L_SEQ_NO) := TRUE;
   END LOOP;

   L_RET_CODE := UNAPIAUT.GETSDAUTHORISATION(A_SD, L_PT_VERSION, L_LC, L_LC_VERSION, L_SS,
                                             L_ALLOW_MODIFY, L_ACTIVE, L_LOG_HS, L_LOG_HS_DETAILS);
   IF L_RET_CODE <> UNAPIGEN.DBERR_SUCCESS THEN
      UNAPIGEN.P_TXN_ERROR := L_RET_CODE;
      RAISE STPERROR;
   END IF;

   UPDATE UTSD
   SET ALLOW_MODIFY = '#'
   WHERE SD = A_SD;

   IF SQL%ROWCOUNT < 1 THEN
      UNAPIGEN.P_TXN_ERROR := UNAPIGEN.DBERR_NOOBJECT;
      RAISE STPERROR ;
   END IF;

   L_EVENT_TP := 'SdGroupKeyUpdated';
   L_EV_SEQ_NR := -1;
   L_EV_DETAILS := 'gk='||A_GK||'#gk_version='||A_GK_VERSION||'#pt_version='||L_PT_VERSION;
   L_RESULT := UNAPIEV.INSERTEVENT('Save1SdGroupKey', UNAPIGEN.P_EVMGR_NAME, 'sd', A_SD, L_LC, 
                                   L_LC_VERSION, L_SS, L_EVENT_TP, L_EV_DETAILS, L_EV_SEQ_NR);
   IF L_RESULT <> UNAPIGEN.DBERR_SUCCESS THEN
      UNAPIGEN.P_TXN_ERROR := L_RESULT;
      RAISE STPERROR;
   END IF;

   IF (L_LOG_HS = '1') AND (UNAPIGEN.P_LOG_GK_HS = '1') THEN
      INSERT INTO UTSDHS(SD, WHO, WHO_DESCRIPTION, WHAT, WHAT_DESCRIPTION, LOGDATE, LOGDATE_TZ, WHY, TR_SEQ, EV_SEQ)
      VALUES(A_SD, UNAPIGEN.P_USER, UNAPIGEN.P_USER_DESCRIPTION, L_EVENT_TP, 
             'study "'||A_SD||'" group key "'||A_GK||'" is created/updated.', 
             CURRENT_TIMESTAMP, CURRENT_TIMESTAMP, A_MODIFY_REASON, UNAPIGEN.P_TR_SEQ, L_EV_SEQ_NR);
   END IF;

   L_HS_SEQ := 0;
   IF L_LOG_HS_DETAILS = '1' THEN
      L_HS_SEQ := L_HS_SEQ + 1;
      INSERT INTO UTSDHSDETAILS(SD, TR_SEQ, EV_SEQ, SEQ, DETAILS)
      VALUES(A_SD, UNAPIGEN.P_TR_SEQ, L_EV_SEQ_NR, L_HS_SEQ, 
             'study "'||A_SD||'" group key "'||A_GK||'" is created/updated.');
   END IF;

   L_LAST_SEQ := 499;
   L_SD_CURSOR := DBMS_SQL.OPEN_CURSOR;
   FOR L_SDGK IN L_GK_CURSOR LOOP
      L_GK_FOUND := FALSE;
      FOR L_SEQ_NO IN 1..A_NR_OF_ROWS LOOP
         
         IF L_SDGK.VALUE = A_VALUE(L_SEQ_NO) OR
            (L_SDGK.VALUE IS NULL AND A_VALUE(L_SEQ_NO) IS NULL) THEN
            L_GK_HANDLE(L_SEQ_NO) := FALSE;
            L_GK_FOUND := TRUE;
            EXIT;
         END IF;
      END LOOP;

      IF NOT L_GK_FOUND THEN
         
         DELETE FROM UTSDGK
         WHERE SD = A_SD
            AND GK = A_GK
            AND VALUE = L_SDGK.VALUE;

         
         IF L_SDGK.VALUE IS NULL THEN
            DELETE FROM UTSDGK
            WHERE SD = A_SD
              AND GK = A_GK
              AND VALUE IS NULL;
         END IF;            

         IF (L_LOG_HS_DETAILS = '1') THEN
            L_WHAT_DESCRIPTION := 'Groupkey "'||A_GK||'" with value "'||L_SDGK.VALUE||'" is removed from study "'||A_SD||'".';
            L_HS_SEQ := L_HS_SEQ + 1;
            INSERT INTO UTSDHSDETAILS(SD, TR_SEQ, EV_SEQ, SEQ, DETAILS)
            VALUES(A_SD, UNAPIGEN.P_TR_SEQ, L_EV_SEQ_NR, L_HS_SEQ, L_WHAT_DESCRIPTION);
         END IF;   

         L_SQL_STRING := 'DELETE FROM utsdgk' || A_GK ||
                         ' WHERE sd = :sd AND ' || A_GK || ' = :value '; 
         BEGIN
            DBMS_SQL.PARSE(L_SD_CURSOR, L_SQL_STRING, DBMS_SQL.V7); 
            DBMS_SQL.BIND_VARIABLE(L_SD_CURSOR, ':sd' , A_SD); 
            DBMS_SQL.BIND_VARIABLE(L_SD_CURSOR, ':value' , L_SDGK.VALUE); 
            L_RESULT := DBMS_SQL.EXECUTE(L_SD_CURSOR);
         EXCEPTION
         WHEN TABLE_DOES_NOT_EXIST THEN
            
            
            NULL;
         END;
      ELSE      
         L_LAST_SEQ := L_SDGK.GKSEQ;
      END IF;

   END LOOP;

   FOR L_SEQ_NO IN 1..A_NR_OF_ROWS LOOP
      IF L_GK_HANDLE(L_SEQ_NO) THEN
         L_SKIP := FALSE; 
         IF NVL(A_VALUE(L_SEQ_NO), ' ') <> ' ' THEN
            L_SQL_STRING := 'INSERT INTO utsdgk' || A_GK ||
                            ' ('||A_GK||', sd)'||
                            ' VALUES (:value, :sd) '; 
            BEGIN
               DBMS_SQL.PARSE(L_SD_CURSOR, L_SQL_STRING, DBMS_SQL.V7); 
               DBMS_SQL.BIND_VARIABLE(L_SD_CURSOR, ':value' , A_VALUE(L_SEQ_NO)); 
               DBMS_SQL.BIND_VARIABLE(L_SD_CURSOR, ':sd' , A_SD); 
               L_RESULT := DBMS_SQL.EXECUTE(L_SD_CURSOR);
            EXCEPTION
            WHEN TABLE_DOES_NOT_EXIST THEN
               
               
               NULL;
            WHEN DUP_VAL_ON_INDEX THEN
               L_SKIP := TRUE;
            END;
         END IF;

         IF NOT L_SKIP THEN
            L_LAST_SEQ := L_LAST_SEQ+1;

            
            INSERT INTO UTSDGK(SD, GK, GKSEQ, VALUE)
            VALUES
            (A_SD, A_GK, L_LAST_SEQ, A_VALUE(L_SEQ_NO));

            IF (L_LOG_HS_DETAILS = '1') THEN
               L_WHAT_DESCRIPTION := 'Groupkey "'||A_GK||'" is added to study "'||A_SD||'", value is "'||A_VALUE(L_SEQ_NO)||'".';
               L_HS_SEQ := L_HS_SEQ + 1;
               INSERT INTO UTSDHSDETAILS(SD, TR_SEQ, EV_SEQ, SEQ, DETAILS)
               VALUES(A_SD, UNAPIGEN.P_TR_SEQ, L_EV_SEQ_NR, L_HS_SEQ, L_WHAT_DESCRIPTION);
            END IF;   
         END IF;
      END IF;
   END LOOP;

   DBMS_SQL.CLOSE_CURSOR(L_SD_CURSOR);

   IF UNAPIGEN.ENDTXN <> UNAPIGEN.DBERR_SUCCESS THEN
      RAISE STPERROR;
   END IF;

   RETURN(UNAPIGEN.DBERR_SUCCESS);

EXCEPTION
WHEN DUP_VAL_ON_INDEX THEN
   IF DBMS_SQL.IS_OPEN(L_SD_CURSOR) THEN
      DBMS_SQL.CLOSE_CURSOR(L_SD_CURSOR);
   END IF;
   UNAPIGEN.P_TXN_ERROR := UNAPIGEN.DBERR_UNIQUEGK;
   
    UPDATE UTSD
      SET ALLOW_MODIFY = '1' 
    WHERE SD = A_SD;
   
   
   L_RESULT := UNAPIGEN.ENDTXN; 
   RETURN(UNAPIGEN.P_TXN_ERROR);
WHEN OTHERS THEN
   IF SQLCODE <> 1 THEN
      UNAPIGEN.LOGERROR('Save1SdGroupKey', SQLERRM);
   ELSIF L_SQLERRM IS NOT NULL THEN
      UNAPIGEN.LOGERROR('Save1SdGroupKey', L_SQLERRM);
   END IF;
   IF DBMS_SQL.IS_OPEN(L_SD_CURSOR) THEN
      DBMS_SQL.CLOSE_CURSOR(L_SD_CURSOR);
   END IF;
   RETURN(UNAPIGEN.ABORTTXN(UNAPIGEN.P_TXN_ERROR, 'Save1SdGroupKey'));
END SAVE1SDGROUPKEY;

FUNCTION ADDSDCOMMENT
(A_SD           IN  VARCHAR2, 
 A_COMMENT      IN  VARCHAR2) 
RETURN NUMBER IS

L_ALLOW_MODIFY      CHAR(1);
L_LOG_HS            CHAR(1);
L_LOG_HS_DETAILS    CHAR(1);
L_ACTIVE            CHAR(1);
L_LC                VARCHAR2(2);
L_LC_VERSION        VARCHAR2(20);
L_SS                VARCHAR2(2);
L_HS_DETAILS_SEQ_NR INTEGER;
L_OLD_COMMENT       VARCHAR2(255);
L_PT_VERSION        VARCHAR2(20);

CURSOR L_SDCOMMENTOLD_CURSOR (A_SD IN VARCHAR2) IS
   SELECT LAST_COMMENT
   FROM UTSD A
   WHERE A.SD = A_SD;

BEGIN

   IF UNAPIGEN.BEGINTXN(UNAPIGEN.P_SINGLE_API_TXN) <>
      UNAPIGEN.DBERR_SUCCESS THEN
      RAISE STPERROR;
   END IF;

   IF NVL(A_SD, ' ') = ' ' THEN
      UNAPIGEN.P_TXN_ERROR := UNAPIGEN.DBERR_NOOBJID;
      RAISE STPERROR;
   END IF;

   L_RET_CODE := UNAPIAUT.GETSDAUTHORISATION(A_SD, L_PT_VERSION, L_LC, L_LC_VERSION, L_SS,
                                             L_ALLOW_MODIFY, L_ACTIVE, L_LOG_HS, L_LOG_HS_DETAILS);
   IF L_RET_CODE <> UNAPIGEN.DBERR_SUCCESS THEN
      UNAPIGEN.P_TXN_ERROR := L_RET_CODE;
      RAISE STPERROR;
   END IF;

   
   
   
   OPEN L_SDCOMMENTOLD_CURSOR(A_SD);
   FETCH L_SDCOMMENTOLD_CURSOR
   INTO L_OLD_COMMENT;
   CLOSE L_SDCOMMENTOLD_CURSOR;

   UPDATE UTSD
   SET LAST_COMMENT = A_COMMENT
   WHERE SD = A_SD;

   L_RET_CODE := UNAPIGEN.GETNEXTEVENTSEQNR(L_EV_SEQ_NR);
   IF L_RET_CODE <> 0 THEN
      UNAPIGEN.P_TXN_ERROR := L_RET_CODE;
      RAISE STPERROR;
   END IF;

   IF L_LOG_HS = '1' THEN
      INSERT INTO UTSDHS(SD, WHO, WHO_DESCRIPTION, WHAT, WHAT_DESCRIPTION, LOGDATE, LOGDATE_TZ, WHY, TR_SEQ, EV_SEQ)
      VALUES(A_SD, UNAPIGEN.P_USER, UNAPIGEN.P_USER_DESCRIPTION, 'Comment', 
             'comment is added on study "'||A_SD||'"', 
             CURRENT_TIMESTAMP, CURRENT_TIMESTAMP, A_COMMENT, UNAPIGEN.P_TR_SEQ, L_EV_SEQ_NR);
   END IF;

   L_HS_DETAILS_SEQ_NR := 0;
   IF L_LOG_HS_DETAILS = '1' THEN
      IF NVL((L_OLD_COMMENT <> A_COMMENT), TRUE) AND NOT(L_OLD_COMMENT IS NULL AND A_COMMENT IS NULL)  THEN 
         L_HS_DETAILS_SEQ_NR := L_HS_DETAILS_SEQ_NR + 1;
         INSERT INTO UTSDHSDETAILS(SD, TR_SEQ, EV_SEQ, SEQ, DETAILS)
         VALUES(A_SD, UNAPIGEN.P_TR_SEQ, L_EV_SEQ_NR, L_HS_DETAILS_SEQ_NR, 
                'study "'||A_SD||'" is updated: property <last_comment> changed value from "' || SUBSTR(L_OLD_COMMENT,1,40) || '" to "' || SUBSTR(A_COMMENT,1,40) || '".');
      END IF;
   END IF;

   IF UNAPIGEN.ENDTXN <> UNAPIGEN.DBERR_SUCCESS THEN
      RAISE STPERROR;
   END IF;

   RETURN(UNAPIGEN.DBERR_SUCCESS);

EXCEPTION
WHEN OTHERS THEN
   IF SQLCODE <> 1 THEN
      UNAPIGEN.LOGERROR('AddSdComment', SQLERRM);
   END IF;
   IF L_SDCOMMENTOLD_CURSOR%ISOPEN THEN
      CLOSE L_SDCOMMENTOLD_CURSOR;
   END IF;
   RETURN(UNAPIGEN.ABORTTXN(UNAPIGEN.P_TXN_ERROR, 'AddSdComment'));
END ADDSDCOMMENT;

FUNCTION GETSDCOMMENT
(A_SD               OUT     UNAPIGEN.VC20_TABLE_TYPE,  
 A_LAST_COMMENT     OUT     UNAPIGEN.VC255_TABLE_TYPE, 
 A_NR_OF_ROWS       IN OUT  NUMBER,                    
 A_WHERE_CLAUSE     IN      VARCHAR2,                  
 A_NEXT_ROWS        IN      NUMBER)                    
RETURN NUMBER IS

L_SD                             VARCHAR2(20);
L_LAST_COMMENT                   VARCHAR2(255);
L_FROM_CLAUSE                    VARCHAR2(255);
L_BIND_SD_SELECTION              BOOLEAN;
L_BIND_FIXED_SD_FLAG             BOOLEAN;

BEGIN

   L_BIND_SD_SELECTION := FALSE;
   L_BIND_FIXED_SD_FLAG := FALSE;
   IF NVL(A_NR_OF_ROWS, 0) = 0 THEN
      A_NR_OF_ROWS := UNAPIGEN.P_DEFAULT_CHUNK_SIZE;
   ELSIF A_NR_OF_ROWS < 0 OR A_NR_OF_ROWS > UNAPIGEN.P_MAX_CHUNK_SIZE THEN
      RETURN(UNAPIGEN.DBERR_NROFROWS);
   END IF;

   IF A_WHERE_CLAUSE <> 'SELECTION' THEN
      IF NVL(A_NEXT_ROWS, 0) NOT IN (-1, 0, 1) THEN
         RETURN(UNAPIGEN.DBERR_NEXTROWS);
      END IF;

      
      IF A_NEXT_ROWS = -1 THEN
         IF P_GETSDCOMMENT_CURSOR IS NOT NULL THEN
            DBMS_SQL.CLOSE_CURSOR(P_GETSDCOMMENT_CURSOR);
            P_GETSDCOMMENT_CURSOR := NULL;
         END IF;
         RETURN (UNAPIGEN.DBERR_SUCCESS);
      END IF;

      
      IF A_NEXT_ROWS = 1 THEN
         IF P_GETSDCOMMENT_CURSOR IS NULL THEN
            RETURN(UNAPIGEN.DBERR_NOCURSOR);
         END IF;
      END IF;
   END IF;

   
   IF NVL(A_NEXT_ROWS,0) = 0 OR 
      A_WHERE_CLAUSE = 'SELECTION' THEN
   
      IF P_GETSDCOMMENT_CURSOR IS NULL THEN
         L_SQL_STRING := 'SELECT a.sd, a.last_comment FROM ';

         L_FROM_CLAUSE := 'dd' || UNAPIGEN.P_DD || '.uvsd a ';

         
         L_WHERE_CLAUSE := ' ';
         IF NVL(A_WHERE_CLAUSE, ' ') = ' ' THEN
            RETURN(UNAPIGEN.DBERR_WHERECLAUSE);
         ELSIF A_WHERE_CLAUSE = 'SELECTION' THEN
            
            
            
            
            
            
            
            
            
            IF UNAPISD.P_SELECTION_CLAUSE IS NOT NULL THEN 
               L_FROM_CLAUSE := '';
               L_WHERE_CLAUSE := UNAPISD.P_SELECTION_CLAUSE || 
                                 ' ORDER BY a.sd';
               L_BIND_SD_SELECTION := TRUE;
            ELSE
               L_WHERE_CLAUSE := ' ORDER BY a.sd'; 
            END IF;
         ELSIF UPPER(SUBSTR(A_WHERE_CLAUSE,1,6)) <> 'WHERE ' THEN
             L_BIND_FIXED_SD_FLAG := TRUE;
            L_WHERE_CLAUSE := ' WHERE a.sd = :sd_val ';
         ELSE
            L_WHERE_CLAUSE := A_WHERE_CLAUSE; 
         END IF;

         L_SQL_STRING := L_SQL_STRING || L_FROM_CLAUSE || L_WHERE_CLAUSE;

         P_GETSDCOMMENT_CURSOR := DBMS_SQL.OPEN_CURSOR;

         DBMS_SQL.PARSE(P_GETSDCOMMENT_CURSOR, L_SQL_STRING, DBMS_SQL.V7); 
         IF L_BIND_SD_SELECTION THEN
            FOR L_X IN 1..UNAPISD.P_SELECTION_VAL_TAB.COUNT() LOOP
               DBMS_SQL.BIND_VARIABLE(P_GETSDCOMMENT_CURSOR, ':col_val'||L_X , UNAPISD.P_SELECTION_VAL_TAB(L_X)); 
            END LOOP;
         ELSIF L_BIND_FIXED_SD_FLAG THEN
            DBMS_SQL.BIND_VARIABLE(P_GETSDCOMMENT_CURSOR, ':ws_val' , A_WHERE_CLAUSE); 
         END IF;

         DBMS_SQL.DEFINE_COLUMN(P_GETSDCOMMENT_CURSOR, 1, L_SD, 20);
         DBMS_SQL.DEFINE_COLUMN(P_GETSDCOMMENT_CURSOR, 2, L_LAST_COMMENT, 255);

         L_RESULT := DBMS_SQL.EXECUTE(P_GETSDCOMMENT_CURSOR);

      END IF;
   END IF;
   
   L_RESULT := DBMS_SQL.FETCH_ROWS(P_GETSDCOMMENT_CURSOR);
   L_FETCHED_ROWS := 0;

   LOOP
      EXIT WHEN L_RESULT = 0 OR L_FETCHED_ROWS >= A_NR_OF_ROWS;

      DBMS_SQL.COLUMN_VALUE(P_GETSDCOMMENT_CURSOR, 1, L_SD);
      DBMS_SQL.COLUMN_VALUE(P_GETSDCOMMENT_CURSOR, 2, L_LAST_COMMENT);

      L_FETCHED_ROWS := L_FETCHED_ROWS + 1;

      A_SD(L_FETCHED_ROWS) := L_SD;
      A_LAST_COMMENT(L_FETCHED_ROWS) := L_LAST_COMMENT;

      IF L_FETCHED_ROWS < A_NR_OF_ROWS THEN
         L_RESULT := DBMS_SQL.FETCH_ROWS(P_GETSDCOMMENT_CURSOR);
      END IF;

   END LOOP;

   
   IF (L_FETCHED_ROWS = 0) THEN
       DBMS_SQL.CLOSE_CURSOR(P_GETSDCOMMENT_CURSOR);
       P_GETSDCOMMENT_CURSOR := NULL;
       RETURN(UNAPIGEN.DBERR_NORECORDS);
   ELSIF L_FETCHED_ROWS < A_NR_OF_ROWS THEN
      DBMS_SQL.CLOSE_CURSOR(P_GETSDCOMMENT_CURSOR);
      P_GETSDCOMMENT_CURSOR := NULL;
      A_NR_OF_ROWS := L_FETCHED_ROWS;
   END IF;
   RETURN(UNAPIGEN.DBERR_SUCCESS);

EXCEPTION
   WHEN OTHERS THEN
      L_SQLERRM := SQLERRM;
      UNAPIGEN.U4ROLLBACK;
      INSERT INTO UTERROR(CLIENT_ID, APPLIC, WHO, LOGDATE, LOGDATE_TZ, API_NAME, ERROR_MSG)
      VALUES(UNAPIGEN.P_CLIENT_ID, UNAPIGEN.P_APPLIC_NAME, UNAPIGEN.P_USER, CURRENT_TIMESTAMP, CURRENT_TIMESTAMP,
             'GetSdComment', L_SQLERRM);
      UNAPIGEN.U4COMMIT;
      IF DBMS_SQL.IS_OPEN(P_GETSDCOMMENT_CURSOR) THEN
         DBMS_SQL.CLOSE_CURSOR(P_GETSDCOMMENT_CURSOR);
      END IF;
      RETURN(UNAPIGEN.DBERR_GENFAIL);
END GETSDCOMMENT;

FUNCTION GETSDACCESS
(A_SD             OUT     UNAPIGEN.VC20_TABLE_TYPE,  
 A_DD             OUT     UNAPIGEN.VC3_TABLE_TYPE,   
 A_DATA_DOMAIN    OUT     UNAPIGEN.VC40_TABLE_TYPE,  
 A_ACCESS_RIGHTS  OUT     UNAPIGEN.CHAR1_TABLE_TYPE, 
 A_NR_OF_ROWS     IN OUT  NUMBER,                    
 A_WHERE_CLAUSE   IN      VARCHAR2)                  
RETURN NUMBER IS

L_SD             VARCHAR2(20);
L_DD_DESCRIPTION VARCHAR2(40);
L_AR             UNAPIGEN.CHAR1_TABLE_TYPE; 
L_DD             VARCHAR2(3);
L_DESCRIPTION    UNAPIGEN.VC40_TABLE_TYPE;
L_ROW            INTEGER;
L_DD_CURSOR      UNAPIGEN.CURSOR_REF_TYPE;
L_AR_CURSOR      UNAPIGEN.CURSOR_REF_TYPE;

BEGIN

   IF NVL(A_NR_OF_ROWS,0) = 0 THEN
      A_NR_OF_ROWS := UNAPIGEN.P_DEFAULT_CHUNK_SIZE;
   ELSIF A_NR_OF_ROWS < 0 OR A_NR_OF_ROWS > UNAPIGEN.P_MAX_CHUNK_SIZE THEN
      RETURN(UNAPIGEN.DBERR_NROFROWS);
   END IF;

   IF NVL(A_WHERE_CLAUSE, ' ') = ' ' THEN
      RETURN(UNAPIGEN.DBERR_WHERECLAUSE);
   ELSIF
      UPPER(SUBSTR(A_WHERE_CLAUSE,1,6)) <> 'WHERE ' THEN
      L_WHERE_CLAUSE := 'WHERE sd = ''' || REPLACE(A_WHERE_CLAUSE, '''', '''''') ||''''; 
   ELSE
      L_WHERE_CLAUSE := A_WHERE_CLAUSE; 
   END IF;

   
   
   
   L_SQL_STRING:= 'SELECT dd, description FROM dd'||UNAPIGEN.P_DD||'.uvdd ORDER BY dd';
   OPEN L_DD_CURSOR FOR L_SQL_STRING;
   FOR I IN 1..UNAPIGEN.P_DATADOMAINS LOOP
      FETCH L_DD_CURSOR INTO L_DD, L_DD_DESCRIPTION;
      L_DESCRIPTION(L_DD) := L_DD_DESCRIPTION;
   END LOOP;
   CLOSE L_DD_CURSOR;

   L_SQL_STRING:= 
      'SELECT ar1, ar2, ar3, ar4, ar5, ar6, ar7, ar8, ar9, ar10, ar11, ar12, ar13, ar14, ar15, ar16, ' ||
      'ar17, ar18, ar19, ar20, ar21, ar22, ar23, ar24, ar25, ar26, ar27, ar28, ar29, ar30, ar31, ' ||
      'ar32, ar33, ar34, ar35, ar36, ar37, ar38, ar39, ar40, ar41, ar42, ar43, ar44, ar45, ar46, ' ||
      'ar47, ar48, ar49, ar50, ar51, ar52, ar53, ar54, ar55, ar56, ar57, ar58, ar59, ar60, ar61, ' ||
      'ar62, ar63, ar64, ar65, ar66, ar67, ar68, ar69, ar70, ar71, ar72, ar73, ar74, ar75, ar76, ' ||
      'ar77, ar78, ar79, ar80, ar81, ar82, ar83, ar84, ar85, ar86, ar87, ar88, ar89, ar90, ar91, ' ||
      'ar92, ar93, ar94, ar95, ar96, ar97, ar98, ar99, ar100, ar101, ar102, ar103, ar104, ar105, ' ||
      'ar106, ar107, ar108, ar109, ar110, ar111, ar112, ar113, ar114, ar115, ar116, ar117, ar118, ' ||
      'ar119, ar120, ar121, ar122, ar123, ar124, ar125, ar126, ar127, ar128, sd ' ||
      'FROM udsd ' || 
       L_WHERE_CLAUSE;
   L_FETCHED_ROWS := 0;
   OPEN L_AR_CURSOR FOR L_SQL_STRING;
   LOOP
      FETCH L_AR_CURSOR INTO L_AR(1),L_AR(2),L_AR(3),L_AR(4),L_AR(5),L_AR(6),L_AR(7),L_AR(8),L_AR(9),
      L_AR(10),L_AR(11),L_AR(12),L_AR(13),L_AR(14),L_AR(15),L_AR(16),L_AR(17),L_AR(18),L_AR(19),
      L_AR(20),L_AR(21),L_AR(22),L_AR(23),L_AR(24),L_AR(25),L_AR(26),L_AR(27),L_AR(28),L_AR(29),
      L_AR(30),L_AR(31),L_AR(32),L_AR(33),L_AR(34),L_AR(35),L_AR(36),L_AR(37),L_AR(38),L_AR(39),
      L_AR(40),L_AR(41),L_AR(42),L_AR(43),L_AR(44),L_AR(45),L_AR(46),L_AR(47),L_AR(48),L_AR(49),
      L_AR(50),L_AR(51),L_AR(52),L_AR(53),L_AR(54),L_AR(55),L_AR(56),L_AR(57),L_AR(58),L_AR(59),
      L_AR(60),L_AR(61),L_AR(62),L_AR(63),L_AR(64),L_AR(65),L_AR(66),L_AR(67),L_AR(68),L_AR(69),
      L_AR(70),L_AR(71),L_AR(72),L_AR(73),L_AR(74),L_AR(75),L_AR(76),L_AR(77),L_AR(78),L_AR(79),
      L_AR(80),L_AR(81),L_AR(82),L_AR(83),L_AR(84),L_AR(85),L_AR(86),L_AR(87),L_AR(88),L_AR(89),
      L_AR(90),L_AR(91),L_AR(92),L_AR(93),L_AR(94),L_AR(95),L_AR(96),L_AR(97),L_AR(98),L_AR(99),
      L_AR(100),L_AR(101),L_AR(102),L_AR(103),L_AR(104),L_AR(105),L_AR(106),L_AR(107),L_AR(108),
      L_AR(109),L_AR(110),L_AR(111),L_AR(112),L_AR(113),L_AR(114),L_AR(115),L_AR(116),L_AR(117),
      L_AR(118),L_AR(119),L_AR(120),L_AR(121),L_AR(122),L_AR(123),L_AR(124),L_AR(125),L_AR(126),
      L_AR(127),L_AR(128), L_SD;
      EXIT WHEN L_AR_CURSOR%NOTFOUND;
      
      FOR L_ROW IN 1..UNAPIGEN.P_DATADOMAINS LOOP
          L_FETCHED_ROWS := L_FETCHED_ROWS + 1;
          A_SD(L_FETCHED_ROWS) := L_SD;
          A_DD(L_FETCHED_ROWS) := TO_CHAR(L_ROW);
          A_DATA_DOMAIN(L_FETCHED_ROWS) := L_DESCRIPTION(L_ROW);
          A_ACCESS_RIGHTS(L_FETCHED_ROWS) := L_AR(L_ROW);
          IF L_FETCHED_ROWS >= A_NR_OF_ROWS THEN
             EXIT;
          END IF;
      END LOOP;
      IF L_FETCHED_ROWS >= A_NR_OF_ROWS THEN
         EXIT;
      END IF;
   END LOOP;
   CLOSE L_AR_CURSOR;

   IF L_FETCHED_ROWS = 0 THEN
      L_RET_CODE := UNAPIGEN.DBERR_NORECORDS;
   ELSE
      L_RET_CODE := UNAPIGEN.DBERR_SUCCESS;
      A_NR_OF_ROWS := L_FETCHED_ROWS;
   END IF;

   RETURN(L_RET_CODE);

EXCEPTION
WHEN NO_DATA_FOUND THEN
   IF L_DD_CURSOR%ISOPEN THEN
      CLOSE L_DD_CURSOR;
   END IF;
   IF L_AR_CURSOR%ISOPEN THEN
      CLOSE L_AR_CURSOR;
   END IF;
   RETURN(UNAPIGEN.DBERR_SYSDEFAULTS);
WHEN OTHERS THEN
   L_SQLERRM := SQLERRM;
   IF L_DD_CURSOR%ISOPEN THEN
      CLOSE L_DD_CURSOR;
   END IF;
   IF L_AR_CURSOR%ISOPEN THEN
      CLOSE L_AR_CURSOR;
   END IF;
   UNAPIGEN.U4ROLLBACK;
   INSERT INTO UTERROR(CLIENT_ID, APPLIC, WHO, LOGDATE, LOGDATE_TZ, API_NAME, ERROR_MSG)
   VALUES(UNAPIGEN.P_CLIENT_ID, UNAPIGEN.P_APPLIC_NAME, UNAPIGEN.P_USER, CURRENT_TIMESTAMP, CURRENT_TIMESTAMP,
           'GetSdAccess', L_SQLERRM);
   UNAPIGEN.U4COMMIT;
   RETURN(UNAPIGEN.DBERR_GENFAIL);
END GETSDACCESS;

FUNCTION SAVESDACCESS
(A_SD             IN      VARCHAR2,                  
 A_DD             IN      UNAPIGEN.VC3_TABLE_TYPE,   
 A_ACCESS_RIGHTS  IN      UNAPIGEN.CHAR1_TABLE_TYPE, 
 A_NR_OF_ROWS     IN      NUMBER,                    
 A_MODIFY_REASON  IN      VARCHAR2)                  
RETURN NUMBER IS

L_ALLOW_MODIFY      CHAR(1);
L_LOG_HS            CHAR(1);
L_LOG_HS_DETAILS    CHAR(1);
L_ACTIVE            CHAR(1);
L_LC                VARCHAR2(2);
L_LC_VERSION        VARCHAR2(20);
L_SS                VARCHAR2(2);
L_AR_CURSOR         INTEGER;
L_WRITE_FOUND       BOOLEAN;
L_HS_DETAILS_SEQ_NR INTEGER;
L_PT_VERSION        VARCHAR2(20);

CURSOR L_SDOLD_CURSOR (A_SD IN VARCHAR2) IS
   SELECT A.*
   FROM UDSD A
   WHERE A.SD = A_SD;
L_SDOLD_REC UDSD%ROWTYPE;
L_SDNEW_REC UDSD%ROWTYPE;

BEGIN

   IF UNAPIGEN.BEGINTXN(UNAPIGEN.P_SINGLE_API_TXN) <> UNAPIGEN.DBERR_SUCCESS THEN
      RAISE STPERROR;
   END IF;

   IF NVL(A_NR_OF_ROWS, -1) < 0 THEN
      UNAPIGEN.P_TXN_ERROR := UNAPIGEN.DBERR_NROFROWS;
      RAISE STPERROR;
   END IF;

   
   
   
   IF NVL(A_SD, ' ') = ' ' THEN
      UNAPIGEN.P_TXN_ERROR := UNAPIGEN.DBERR_NOOBJID;
      RAISE STPERROR;
   END IF;

   L_RET_CODE := UNAPIAUT.GETSDAUTHORISATION(A_SD, L_PT_VERSION, L_LC, L_LC_VERSION, L_SS,
                                             L_ALLOW_MODIFY, L_ACTIVE, L_LOG_HS, L_LOG_HS_DETAILS);
   IF L_RET_CODE <> UNAPIGEN.DBERR_SUCCESS THEN
      UNAPIGEN.P_TXN_ERROR := L_RET_CODE;
      RAISE STPERROR;
   END IF;

   UPDATE UTSD
   SET ALLOW_MODIFY = '#'
   WHERE SD = A_SD;

   IF SQL%ROWCOUNT < 1 THEN
      UNAPIGEN.P_TXN_ERROR := UNAPIGEN.DBERR_NOOBJECT;
      RAISE STPERROR ;
   END IF;

   
   
   
   
   L_SQL_STRING := '';
   L_WRITE_FOUND := FALSE;
   FOR L_CTR IN 1..A_NR_OF_ROWS LOOP

      IF NVL(A_ACCESS_RIGHTS(L_CTR), 'N') NOT IN ('R','W','N') THEN
         UNAPIGEN.P_TXN_ERROR := UNAPIGEN.DBERR_AR;
         RAISE STPERROR;
      END IF;

      IF NVL(A_ACCESS_RIGHTS(L_CTR), 'N') = 'W' THEN
         L_WRITE_FOUND := TRUE;
      END IF;

